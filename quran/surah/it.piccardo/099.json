        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6144,
        "text": "In quel Giorno gli uomini usciranno in gruppi, affinché siano mostrate loro le loro opere.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
