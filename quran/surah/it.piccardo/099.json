        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6142,
        "text": "In quel Giorno racconterà le sue storie,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
