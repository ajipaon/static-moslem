{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 92,
    "name": "سُورَةُ اللَّيۡلِ",
    "englishName": "Al-Lail",
    "englishNameTranslation": "The Night",
    "revelationType": "Meccan",
    "numberOfAyahs": 21,
    "ayahs": [
      {
        "number": 6059,
        "text": "Per la notte quando avvolge [con le sue tenebre],",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6060,
        "text": "per il giorno quando risplende,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6061,
        "text": "per Colui Che ha creato il maschio e la femmina.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6062,
        "text": "Invero i vostri sforzi divergono.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6063,
        "text": "A chi sarà stato generoso e timorato",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6064,
        "text": "e avrà attestato la verità della cosa più bella,",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6065,
        "text": "faciliteremo il facile;",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6066,
        "text": "a chi invece sarà stato avaro e avrà creduto di bastare a se stesso",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6067,
        "text": "e tacciato di menzogna la cosa più bella,",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6068,
        "text": "faciliteremo il difficile.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6069,
        "text": "Quando precipiterà [nell'abisso], a nulla gli serviranno i suoi beni!",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6070,
        "text": "In verità spetta a Noi la guida!",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6071,
        "text": "In verità a Noi appartengono l'altra vita e questa vita.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6072,
        "text": "Vi ho dunque messo in guardia da un fuoco fiammeggiante,",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6073,
        "text": "nel quale brucerà solo il malvagio,",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6074,
        "text": "che avrà tacciato di menzogna e avrà girato le spalle;",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6075,
        "text": "mentre ne sarà preservato il timorato,",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6076,
        "text": "che dà dei suoi beni per purificarsi,",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6077,
        "text": "che a nessuno avrà fatto un bene per ottenere ricompensa,",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6078,
        "text": "ma solo per amore del suo Signore l'Altissimo.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6079,
        "text": "Per certo sarà soddisfatto.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      }
    ],
