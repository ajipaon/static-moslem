{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 68,
    "name": "سُورَةُ القَلَمِ",
    "englishName": "Al-Qalam",
    "englishNameTranslation": "The Pen",
    "revelationType": "Meccan",
    "numberOfAyahs": 52,
    "ayahs": [
      {
        "number": 5272,
        "text": "Nun. Pasha lapsin dhe atë çka shkruajnë,",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5273,
        "text": "Ti, me mëshirën e Zotit tënd, nuk je i marrë;",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5274,
        "text": "Ti me siguri do të kesh shpërblim të vazhdueshëm,",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5275,
        "text": "Sepse ti je, me të vërtetë, me moral; të lartë.",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5276,
        "text": "Do ta shohësh edhe ti, a edhe ata do ta shohin,",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5277,
        "text": "Cili prej jush është i marrë?",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5278,
        "text": "Zoti yt e di më së miri se kush ka devijur nga rruga e tij dhe Ai i di mirë të udhëzuarit.",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5279,
        "text": "Prandaj mos i përfill përgënjeshtarët.",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5280,
        "text": "Ata dëshirojnë që ti të gënjesh e të gënjejnë edhe ata.",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5281,
        "text": "Prandaj, mos dëgjo asnjë betues të nënçmuar,",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5282,
        "text": "Shpifës, që vetëm bart fjalët e huaja,",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5283,
        "text": "Penguesin për vepra të mira, mëkatmadhin,",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5284,
        "text": "Zemërprishurin madje edhe kopil,",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5285,
        "text": "Vetëm se ka pasuri dhe shumë djem.",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5286,
        "text": "E kur i kumtohen ajeet tona, thotë: “Këto janë legjenda të lashta!”",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5287,
        "text": "Atë do ta shënojmë në hundë.",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5288,
        "text": "Ata i kemi vënë në sprovë, siç patëm vënë në sprovë pronarët e kopshtit kur u betuan se me siguri herët në agim do ta vjelin.",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5289,
        "text": "Por nuk thanë: “Në dashtë Zoti!”",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5290,
        "text": "Dher deri sa ata flinin, atë (kopshtin) e vizitoi një grup vizitorësh nga Zoti yt.",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5291,
        "text": "Dhe gdhini i vjelur,",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5292,
        "text": "Ndërsa ata në agim e thërrisnin njëri-tjetrin,",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5293,
        "text": "“hpejtoni te arat tuaja, nëse doni t’i vilni”!",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5294,
        "text": "Dheu nisën duke përshpëritur në mes veti,",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5295,
        "text": "Sot kurrsesi mos t’iu hyjë aty ndonjë i varfër.",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5296,
        "text": "Dhe shkuan, të vendosur përzbatimin (e asaj që thanë),",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5297,
        "text": "Por kur e panë, thanë: “Ne paskeshim dështuar!”",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5298,
        "text": "për më tepër edhe jemi dëmtuar (rjepur).",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5299,
        "text": "Njëri, si mëi miri i tyre, tha: “A nuk ju kam thënë, përse nuk falenderoni?”",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5300,
        "text": "“Qoftë lartësuar Zoti ynë – thanë, - ne pa dyshim kemi qenë mizorë”!",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5301,
        "text": "Dhe pastaj filluan të qortojnë njëri-tjetrin.",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5302,
        "text": "“Të mjerët ne! – thonin – Ne me të vërtetë kemi qenë të shfrenuar.",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5303,
        "text": "Zoti ynë mund të na jap më mirë se ajo, ne vetëm te Zoti ynë shpresojmë!”",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5304,
        "text": "Ashtu ka qenë dënimi, kurse dënimi i botës tjetër është edhe më i madh, ta dinë!",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5305,
        "text": "Nuk ka dyshim se të devotshmit do të kenë te Zoti i tyre kënaqësi të xhennetit.",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5306,
        "text": "“A mos sillemi njëlloj me musimanët si me mëkatarët?",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5307,
        "text": "Çka është me ju, si po gjykoni?”",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5308,
        "text": "A keni, ndoshta, libër prej nga mësoni,",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5309,
        "text": "Se do të keni ate çka e pëlqeni ju?",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5310,
        "text": "Ose ju kemi dhënë besën me asi betimi që vlen deri në ditën e kijametit, se do të keni çka ju e caktoni?",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5311,
        "text": "Pyeti ata: “kush ua garanton këtë?”",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5312,
        "text": "Ose, mos ata kanë shokë? Po le t’i sjellin shokët e vet nëse flasin të vërtetën!",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5313,
        "text": "Atë dië kur të shpallet çdo send dhe të thirren për të bërë sexhde e ata nuk do të munden.",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5314,
        "text": "Me shikime të varura dhe me poshtërsi mbuluar, se kanë qenë të thirrur të bijnë në sexhde deri sa kanë qenë të shëndoshë.",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5315,
        "text": "Prandaj, më le mua edhe ata, të cilët e mohojnë këtë Fjalë (Kur’an). Ne dalëngadal, do t’i afrojmë te dënimi prej nga nuk presin.",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5316,
        "text": "Edhe afat do t’u japim, sepse dënimi im është shumë i fortë.",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5317,
        "text": "A lyp ti prej tyre shpërblim, e janë të ngarkuar me ndonjë borxh?",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5318,
        "text": "Apo ata posedojnë ndonjë fshehtësi që ata e shkruajnë?",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5319,
        "text": "Ti duro deri në caktimin e Zotit tënd dhe mos u bën si ai i peshkut (Junusi) që u lut i zemëruar.",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5320,
        "text": "Dhe sikur të mos i mbërrinte mëshirë e Zotit të tij do të isht ehudhur në vend të shkretë dhe i urrejtur,",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5321,
        "text": "Por Zoti i tij e ka zgjedhur dhe e ka bërë një nga ata të mirët.",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5322,
        "text": "Mosbesimtarët gati sa nuk të shafisin me shikimet e tyre, kur dëgjojnë qortimin (Kur’anin) dhe thonë: “Ai me siguri është i marrë”!",
        "numberInSurah": 51,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5323,
        "text": "Ai nuk është tjetër vetëm se vërejtje për tërë botërat!",
        "numberInSurah": 52,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "sq.mehdiu",
      "language": "sq",
      "name": "Feti Mehdiu",
      "englishName": "Feti Mehdiu",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}