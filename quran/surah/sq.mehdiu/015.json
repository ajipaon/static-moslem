{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 15,
    "name": "سُورَةُ الحِجۡرِ",
    "englishName": "Al-Hijr",
    "englishNameTranslation": "The Rock",
    "revelationType": "Meccan",
    "numberOfAyahs": 99,
    "ayahs": [
      {
        "number": 1803,
        "text": "Alif Lam Ra. Këto janë ajete të Librit, Kur’anit të qartë.",
        "numberInSurah": 1,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1804,
        "text": "Shpeshherë mosbesimtarët do të dëshironin të ishin muslimanë.",
        "numberInSurah": 2,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1805,
        "text": "Lëri le të hanë e të kënaqen dhe le t’i mashtrojë shpresa, - kanë për ta ditur ata!",
        "numberInSurah": 3,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1806,
        "text": "Dhe nuk kemi zhdukur asnjë qytet pa i ardhur koha e caktuar.",
        "numberInSurah": 4,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1807,
        "text": "Asnjë popull nuk mundet ta shpejtojë as ta vonojë përfundimin e vet,",
        "numberInSurah": 5,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1808,
        "text": "Dhe thonë, “Ore ti që po të shpallet qortimi (Kur’ani), njëmend je i marrë!",
        "numberInSurah": 6,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1809,
        "text": "Pse nuk na sjell engjuj, nëse je i sinqertë?”",
        "numberInSurah": 7,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1810,
        "text": "Ne engjujt i dërgojmë vetëm me meritë dhe atëherë nuk do të kenë kohë të presin.",
        "numberInSurah": 8,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1811,
        "text": "Ne, me të vërtetë, e kemi shpalur Kur’anin dhe pa dyshim ne jemi mbrojtësit e tij.",
        "numberInSurah": 9,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1812,
        "text": "Ne edhe përpara teje kemi dërguar profetë popujve të mëparshëm,",
        "numberInSurah": 10,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1813,
        "text": "Por atyre nuk u ka ardhur asnjë profet, e ata të mos jenë tallur.",
        "numberInSurah": 11,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1814,
        "text": "Ja, ashtu ne e fusim atë (Kur’anin) në zemrat e mëkatarëve.",
        "numberInSurah": 12,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1815,
        "text": "A nuk i besojnë atij, por dihet se çka u bë me popujt e hershëm.",
        "numberInSurah": 13,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1816,
        "text": "Edhe sikur të kishin hapur në qiell ndonjë derë për ata dhe ata të kalonin nëpër të duke u ngjitur,",
        "numberInSurah": 14,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1817,
        "text": "Prapë do të thoninn: “Sigurisht neve na kanë lënë sytë; ne jemi njerëz të magjepsur!”",
        "numberInSurah": 15,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1818,
        "text": "Dhe ne kemi bërë në qiell sisteme yjesh dhe kemi zbukuruar për shikuesit.",
        "numberInSurah": 16,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1819,
        "text": "Dhe e ruajmë atë prej çdo djalli të mallkuar,",
        "numberInSurah": 17,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1820,
        "text": "Përveç kush vështron tinëzisht dhe e godit një yll i dukshëm.",
        "numberInSurah": 18,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1821,
        "text": "Ndërsa tokën e kemi shtruar dhe nëpër të kemi shpërndarë male të palëvizshëm dhe kemi bërë që në të të mbinë çdo send me masë,",
        "numberInSurah": 19,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1822,
        "text": "Dhe kemi bërë për ju në të burim jetese, si edhe për ataqë nuk i ushqeni ju.",
        "numberInSurah": 20,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1823,
        "text": "Dhe nuk ka asgjë që të mos e ketë burimin te na, por ne nga ajo japim vetëm aqsa është e nevojshme.",
        "numberInSurah": 21,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1824,
        "text": "Ne dërgojmë erërat të mbarësojmë dhe nga qielli lëshojmë shiun me të cilin ju ngimë – dhe ju nuk dispononi me atë.",
        "numberInSurah": 22,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1825,
        "text": "Dhe vetëm ne ngjallim dhe vdesim, dhe vetëm ne mbesim trashëgimtar.",
        "numberInSurah": 23,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1826,
        "text": "Dhe ne e dimë atë çka ka kaluar para jush dhe e dimë edhe atë çka vjen pas.",
        "numberInSurah": 24,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1827,
        "text": "Kurse Ai, Zoti yt, pa dyshim, të gjithë do t’i tubojë. Ai është i urtë dhe shumë i dijshëm.",
        "numberInSurah": 25,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1828,
        "text": "Ne e kemi krijuar njeriun nga balta e argjilit, e formësuar,",
        "numberInSurah": 26,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1829,
        "text": "Kurse exhinët, qysh më parë i kemi krijuar nga zjarri semum (i këndellur).",
        "numberInSurah": 27,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1830,
        "text": "Dhe kur Zoti yt u tha engjujve: “Unë jam krijues i njeriut nga balta, nga balta e tharë,",
        "numberInSurah": 28,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1831,
        "text": "Dhe pasi që i kam dhënë fizionomi dhe i kam dhënë edhe shpirt, ju përuluni!”",
        "numberInSurah": 29,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1832,
        "text": "Dhe të gjithë engjujt, së bashku, iu përulën.",
        "numberInSurah": 30,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1833,
        "text": "Por Iblisi, jo. Ai refuzoi të jetë me ata që iu përulen.",
        "numberInSurah": 31,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1834,
        "text": "“O Iblis, - i tha Ai, - Ç’është me ty që nuk u përule?”",
        "numberInSurah": 32,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1835,
        "text": "Nuk më ka hije, - tha, - t’i përulem një njeriu të cilin e krijove nga balta, nba balta e tharë”.",
        "numberInSurah": 33,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1836,
        "text": "“Atëherë, pra, dil prej aty, - i tha Ai, ti me të vërtetë je i mallkuar.",
        "numberInSurah": 34,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1837,
        "text": "Dhe mallkimi të ndjektë deri në ditën e kijametit!”",
        "numberInSurah": 35,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1838,
        "text": "“O Zoti im, - tha, - më jep afat deri në ditën kur do të ringjallen!”",
        "numberInSurah": 36,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1839,
        "text": "“Të qoftë dhënë afati, - i tha Ai. –",
        "numberInSurah": 37,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1840,
        "text": "Deri në atë ditën e caktuar”.",
        "numberInSurah": 38,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1841,
        "text": "“O Zoti im, - tha, - për atë që më ke shpjerë në humbje, unë atyre në tokë do t’ua zbukuroj punët e tyre, do t’i shpiej gjithsesi në humbje,",
        "numberInSurah": 39,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1842,
        "text": "Përveç atyre robërve të tu të sinqertë”.",
        "numberInSurah": 40,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1843,
        "text": "“Kjo, - tha Ai, - është rruga e drejtë imja.",
        "numberInSurah": 41,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1844,
        "text": "Ti nuk ke kurrfarë kompetence ndaj robërve të mi, përveç atyre të devijuarve, të cilët të kanë pasuar ty”.",
        "numberInSurah": 42,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1845,
        "text": "Për tërë ata vendtakimi është në xhehennem,",
        "numberInSurah": 43,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1846,
        "text": "Që i ka shtatë dyer. Nëpër secilën derë do të kalojë një numër i caktuar.",
        "numberInSurah": 44,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1847,
        "text": "Pa dyshim të devotshmit do të jenë në kopshte dhe burime.",
        "numberInSurah": 45,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1848,
        "text": "“Hyni aty të sigurtë, të liruar nga frika!”",
        "numberInSurah": 46,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1849,
        "text": "Dhe nga krahërorët e tyre do t’ua heqim smirën, ata do të qëndrojnë të gëzuar sikur vëllezër njëri përball tjetrit,",
        "numberInSurah": 47,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1850,
        "text": "Aty nuk i pret kurrfarë lodhje, dhe nuk do të nxirren prej tyre.",
        "numberInSurah": 48,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1851,
        "text": "Lajmëroi robërit e mi se Unë, njëmend, jam ai që fal dhe mëshirëplotë,",
        "numberInSurah": 49,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1852,
        "text": "Por edhe dënimi im është, pa dyshim i dhembshëm.",
        "numberInSurah": 50,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1853,
        "text": "Edhe lajmëroi ata për musafirët e Ibrahimit,",
        "numberInSurah": 51,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1854,
        "text": "Kur hynë dhe i thanë: “Paqe”! E ai u tha: “Ne frikësohemi prej jush”.",
        "numberInSurah": 52,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1855,
        "text": "“Mos u frikëso, - i thanë, - ne të përgëzojmë ty se do të keshë një djalë të mësuar”.",
        "numberInSurah": 53,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1856,
        "text": "“A më përgëzoni tani kur më ka zënë pleqëria? - tha ai. – Me çka po më përgëzoni?”",
        "numberInSurah": 54,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1857,
        "text": "I thanë: “Të përgëzojmë me një realitet dhe për këtë mos e humb shpresën!”",
        "numberInSurah": 55,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1858,
        "text": "Po kush e humb shpresën në mëshirën e Zotit të vet, përveç atyre të devijuarëve e të humburëve.",
        "numberInSurah": 56,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1859,
        "text": "“Po ç’punë keni ju, o të dërguarë! – u tha.",
        "numberInSurah": 57,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1860,
        "text": "“Ne jemi dërguar te një popull mëkatar, - i thanë ata.",
        "numberInSurah": 58,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1861,
        "text": "Përveç familjes së Lutit që do ta shpëtojmë të tërë,",
        "numberInSurah": 59,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1862,
        "text": "Me përjashtim të gruas së tij, e kemi vendosur, ajo do të jetë e dënuar me të tjerët”.",
        "numberInSurah": 60,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1863,
        "text": "Dhe kur erdhën të dërguarit te familja e Lutit.",
        "numberInSurah": 61,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1864,
        "text": "Ai u tha: “Ju jeni, sigurisht, njerëz të panjohur”.",
        "numberInSurah": 62,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1865,
        "text": "“Jo, - i thanë ata, - por po të sjellim atë në çka dyshojnë këta,",
        "numberInSurah": 63,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1866,
        "text": "Dhe të sjellim realitetin që do të ndodhë, dhe ne, sigurisht, jemi të sinqertë.",
        "numberInSurah": 64,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1867,
        "text": "Transfero familjen tënde natën vonë, kurse ti rri në prapavijë të tyre, dhe asnjë prej jush le të mos kthehet, por shkoni atje për ku ju urdhërohet!”",
        "numberInSurah": 65,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1868,
        "text": "Dhe ne ia shpallëm atë që do të ndodhë. Se ata të gjithë, do të zhduken mu në agim.",
        "numberInSurah": 66,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1869,
        "text": "Dhe erdhën bashkëqytetarët, të gëzuar.",
        "numberInSurah": 67,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1870,
        "text": "“Këta janë musafirët e mi, - u tha ai, - mos më turpëroni!",
        "numberInSurah": 68,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1871,
        "text": "Dhe frikësohuni All-llahut, e mos më poshtëroni!”",
        "numberInSurah": 69,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1872,
        "text": "A nuk ta kemi ndaluar, - i thanë, - të pranosh njerëz si musafirë?”",
        "numberInSurah": 70,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1873,
        "text": "“Qe ku i keni vajzat e mia, - tha ai, - nëse dëshironi të bëni diçka.",
        "numberInSurah": 71,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1874,
        "text": "“Pasha ty, ata janë humbur në dehjen e tyre.",
        "numberInSurah": 72,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1875,
        "text": "Dhe ata i përfshiu, me të lindur të diellit, një ushtimë e madhe.",
        "numberInSurah": 73,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1876,
        "text": "Dhe ne e përmbysëm, ajo që ishte lartë e vumë poshtë, dhe mbi ta lëshuam një rrebesh dhe të gurëzuar, sikur shiu.",
        "numberInSurah": 74,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1877,
        "text": "Në këtë ka, njëmend, argumente për ata që i hetojnë.",
        "numberInSurah": 75,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1878,
        "text": "Dhe ai qytet ështëpranë rrugës edhe tash.",
        "numberInSurah": 76,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1879,
        "text": "Në këtë ka mësime për ata që besojnë,",
        "numberInSurah": 77,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1880,
        "text": "Por edhe banorët e Ejkes kanë qenë kriminelë.",
        "numberInSurah": 78,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1881,
        "text": "Prandaj u jemi hakmarrë dhe që të dy janë pranë rrugës dhe shihen haptazi.",
        "numberInSurah": 79,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1882,
        "text": "Edhe banorët e Hixhrit i kanë përgënjeshtruar profetët,",
        "numberInSurah": 80,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1883,
        "text": "Dhe ne u patëm dhënë argumentet tona, por ata ua kthenin shpinën.",
        "numberInSurah": 81,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1884,
        "text": "Dhe skalitnin shtëpiat në shkrepa, të qetë e të sigurtë.",
        "numberInSurah": 82,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1885,
        "text": "Por edhe ata i goditi ushtima e tmerrshme, në agim.",
        "numberInSurah": 83,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1886,
        "text": "Dhe nuk u bëri dobi ajo që kishin fituar",
        "numberInSurah": 84,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1887,
        "text": "Ne nuk i kemi krijuar qiejt dhe tokën edhe çka ka ndërmjet tyre pa arsye. Momenti i fundit, me siguri, do të vijë, prandaj ti sillu me shpirtëgjërësi.",
        "numberInSurah": 85,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1888,
        "text": "Zoti yt është gjithkrijues dhe i gjithdijshëm.",
        "numberInSurah": 86,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1889,
        "text": "Ne të kemi shpallur shtatë ajete që përsëriten, edhe Kur’anin e madhërueshëm.",
        "numberInSurah": 87,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1890,
        "text": "Mos të ikë assesi syri në atë që ne u japim për kënaqësi disave, dhe mos u mërzit për ata, kurse ndaj besimtarëve jij i butë.",
        "numberInSurah": 88,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1891,
        "text": "Dhe thuaj: “Unë jam vetëm qortues i hapët!”",
        "numberInSurah": 89,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1892,
        "text": "Siç u kemi shpallur qortim klasifikuesve,",
        "numberInSurah": 90,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1893,
        "text": "Të cilët e kanë identifikuar Kur’anin me gjini (letrare)",
        "numberInSurah": 91,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1894,
        "text": "Qe pasha Zotin tënd, ata të gjithë do t’i marrim në përgjegjësi,",
        "numberInSurah": 92,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1895,
        "text": "Për atë çka kanë bërë.",
        "numberInSurah": 93,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1896,
        "text": "Ti prediko atë që të urdhërohet dhe hiqu politeistëve.",
        "numberInSurah": 94,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1897,
        "text": "Ne të mjaftojmë ty kundër atyre të cilët tallen.",
        "numberInSurah": 95,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1898,
        "text": "Me All-llahu dhe bëjnë edhe tjetër Zot. Do të shohin ata!",
        "numberInSurah": 96,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1899,
        "text": "Ne mirë e dimë se të vjen rënd ajo që flasin ata.",
        "numberInSurah": 97,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1900,
        "text": "Por madhëro Zotin tënd dhe falenderoje, edhe bëhu nga ata që përulen.",
        "numberInSurah": 98,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1901,
        "text": "Dhe adhuro Zotin tënd, sa të jesh gjallë!",
        "numberInSurah": 99,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      }
    ],
