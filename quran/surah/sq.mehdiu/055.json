{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 55,
    "name": "سُورَةُ الرَّحۡمَٰن",
    "englishName": "Ar-Rahmaan",
    "englishNameTranslation": "The Beneficent",
    "revelationType": "Medinan",
    "numberOfAyahs": 78,
    "ayahs": [
      {
        "number": 4902,
        "text": "I Gjithmëshirshmi",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4903,
        "text": "Të mëson Kur’anin",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4904,
        "text": "E krijon insanin,",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4905,
        "text": "E mëson të flasë.",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4906,
        "text": "Dielli dhe hëna lëvizin sipas një llogarie,",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4907,
        "text": "Edhe bari e lisat përulen,",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4908,
        "text": "Kurse qiellin atë e ka ngritur dhe në të ka vendosur peshojën,",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4909,
        "text": "Që të mos e kaloni kufirin në peshim.",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4910,
        "text": "Andaj matni drejt dhe mos e lini mangut në peshojë!",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4911,
        "text": "Kurse tokën e ka shtuar për krijesat,",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4912,
        "text": "E në të ka pemë, hurma në frute dhe lule,",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4913,
        "text": "Edhe drith me kallinj me lule erëmira,",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4914,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4915,
        "text": "Ai e ka krijuar njeriun nga balta e tharë sikur vorbën,",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4916,
        "text": "Kurse exhinët nga flaka e zjarrit.",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4917,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4918,
        "text": "Zot i dy lindjeve dhe Zot i dy perëndimeve.",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4919,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4920,
        "text": "Dy dete i ka lënë të përpiqen,",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4921,
        "text": "Ndërmjet atyre dyve ka një rrymë që nuk e kapërcejnë,",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4922,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4923,
        "text": "Prej atyre dyve nxirret xhevahir e diamant,",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4924,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4925,
        "text": "Të atij janë edhe anijet që sikur kodra ngrihen lart në det,",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4926,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4927,
        "text": "Çdo send që është në të është e kalueshme,",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4928,
        "text": "dhe mbetet vetëm Zoti yt, i madhërishmi dhe i Nderuari,",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4929,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4930,
        "text": "Atij i luten ata që janë në qiej dhe në tokë, në çdo kohë Ai është i angazhuar me diçka",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4931,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4932,
        "text": "Do të kemi punë me ju për llogari, o të rëndë.",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4933,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4934,
        "text": "O grupe të exhinëve dhe njerëzve, nëse mundeni të depërtoni nëpër kufijt e qiejve dhe të tokës, depërtoni, por nuk mundeni, vetëm se me forcë të madhe,",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4935,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4936,
        "text": "mbi ju do të derdhet flakë e zjarrit dhe bakër i shkrirë, dhe nuk do të mund të mbroheni,",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4937,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4938,
        "text": "Po kur të çahet qielli e kur të bëhet si ngjyrë vjollce,",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4939,
        "text": "Cilën dhunti të Zotit e mohoni?!",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4940,
        "text": "Atë ditë nuk merren në pyetje për mëkatet e veta njerëz as xhind.",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4941,
        "text": "Cilën dhunti të Zotit tuaj e mohoni?!",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4942,
        "text": "Mëkatarët do të njihen sipas karakteristikave të tyre andaj do të rrëmbehen për flokësh e për këmbësh,",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4943,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4944,
        "text": "Ja, ky është xhehennemi të cilin mëkatarët e mohuan!",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4945,
        "text": "Dhe do të sillen vërdallë ndërmjet zjarrit e ujit valë,",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4946,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4947,
        "text": "Kurse ai i cili është frikësuar forcës së Zotit të vet, do të ketë dy kopshte.",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4948,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4949,
        "text": "Të dy plot me lisa me degë.",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4950,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4951,
        "text": "Ku do të rrjedhin dy burime,",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4952,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4953,
        "text": "Aty nga secila pemë do të ketë çifte,",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4954,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4955,
        "text": "Të mbështetur në një shtrojë me mbulesa kadifeje, kurse frytet në të dy kopshtet mund të kapen me dorë.",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4956,
        "text": "E cilën dhunti të Zotit tuaj e mohoni?!",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4957,
        "text": "Aty ka kryeulura të cilat përpara atyre nuk i ka prekur as njeri as exhin,",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4958,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4959,
        "text": "Do të jenë si xhevahirë e dijamantë,",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4960,
        "text": "E ju cilën dhunti të Zotit tuaj e mohoni?!",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4961,
        "text": "A ka tjetër shpërblim mirësia, përveç mirësisë,",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4962,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 61,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4963,
        "text": "E nën ata dy (xhenete) ka edhe dy xhennete,",
        "numberInSurah": 62,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4964,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 63,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4965,
        "text": "Të dy shumë të gjelbërt,",
        "numberInSurah": 64,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4966,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 65,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4967,
        "text": "Aty janë dy burime që gulojnë pa ia da.",
        "numberInSurah": 66,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4968,
        "text": "E cilën dhunti të Zotit tuaj e mohoni?!",
        "numberInSurah": 67,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4969,
        "text": "Aty ka pemë, hurma e kaça,",
        "numberInSurah": 68,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4970,
        "text": "Po cilën dhunti të Zotit tuaj mohoni?!",
        "numberInSurah": 69,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4971,
        "text": "Aty ka të mira fantastike,",
        "numberInSurah": 70,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
