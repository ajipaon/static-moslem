    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6091,
        "text": "A nuk ta hapëm krahërorin tënd,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6092,
        "text": "Dhe ta hoqëm ty barrën,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6093,
        "text": "E cila të rëndonte shpinën tënd,",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6094,
        "text": "Dhe kujtimin për ty lart e ngritëm?",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6095,
        "text": "Nnë vështirësi me të vërtetë ka lehtësi",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6096,
        "text": "Me siguri në vështirësi ka lehtësi!",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6097,
        "text": "E kur të jesh i lirë angazhohu me namaz,",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
