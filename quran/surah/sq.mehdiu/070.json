{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 70,
    "name": "سُورَةُ المَعَارِجِ",
    "englishName": "Al-Ma'aarij",
    "englishNameTranslation": "The Ascending Stairways",
    "revelationType": "Meccan",
    "numberOfAyahs": 44,
    "ayahs": [
      {
        "number": 5376,
        "text": "Dikush ka pyetur për dënimin që zbatohet",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5377,
        "text": "Ndaj mosbesimtarëve – atë s’e mbron dot kush",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5378,
        "text": "Nga All-llahu, Zoti i qiejve,",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5379,
        "text": "Atje ngjiten engjujt dhe Ruhi (Xhibrili) në një ditë që zgjatë pesëdhjetë mijë vjet,",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5380,
        "text": "Prandaj duro si ka hije, pa ankime.",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5381,
        "text": "Atyre u duket se është larg,",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5382,
        "text": "Kurse Ne e shohim se është afër:",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5383,
        "text": "Atë ditë kur qielli të bëhet si fundrrinë rrëshire,",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5384,
        "text": "Kurse malet të bëhen si lesh i lënuar,",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5385,
        "text": "Dhe nuk pyet miku për mik,",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5386,
        "text": "Do t’u mundësohet të shihen. Mëkatari dëshiron që atë ditë ta kompenzojë veten (lirojë) nga dënimi i asaj dite me fëmijët e vet,",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5387,
        "text": "Edhe me bashkëshorten e vet, dhe vëlla të vet,",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5388,
        "text": "Edhe me familje të vet që e ka strehuar,",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5389,
        "text": "Dhe kush është në tokë, të gjithë së bashku, e ta shpëtojë,",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5390,
        "text": "Kurrsesi! Ajo është këndellje,",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5391,
        "text": "Që rrëmben çdo pjesë,",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5392,
        "text": "E thërret atë që ka kthyer shpinën dhe ka ikur,",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5393,
        "text": "Edhe ka tubuar e ka fshehur.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5394,
        "text": "Njeriu, me të vërtetë, është krijuar i pangopur,",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5395,
        "text": "Kur e godit e keqja – është i pikëlluar –",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5396,
        "text": "E kur e godit e mira – është refuzues,",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5397,
        "text": "Me përjashtim të atyre që falen,",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5398,
        "text": "Të cilët në namazet e tyre janë korrektë,",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5399,
        "text": "Edhe ata që në pasurinë e tyre kanë një pjesë të caktuar",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5400,
        "text": "për lypsin dhe për (nevojtarin) që nuk lyp,",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5401,
        "text": "Edhe ata që e vërtetojnë Ditën e kijametit;",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5402,
        "text": "Dhe ata të cilët frikësohen nga dëhimi i Zotit të tyre,",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5403,
        "text": "Se nga dënimi i Zotit të vet, njëmend, askush nuk është i sigurt,",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5404,
        "text": "Edhe ata të cilët ruajnë vendet e turpshme –",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5405,
        "text": "Vetëm për gratë e veta ose për ata që i kanë pasur nën mbikqyrjen e tyre. Ata, me të vërtetë, nuk qortohen,",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5406,
        "text": "Dhe kush kërkon përtej kësaj, ata janë njëmend ligjshkelës,",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5407,
        "text": "Dhe ata të cilët i ruajnë e kujdesen për gjërat që u janë besuar dhe marrëveshjet e tyre,",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5408,
        "text": "Dhe ata të cilët qëndrojnë pas dëshmive të veta,",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5409,
        "text": "Edhe ata të cilët në namazin e tyre janë pedantë.",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5410,
        "text": "Ata do të jenë tënderuar në xhennet.",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5411,
        "text": "Ç\\’është me mosbesimtarët që po të vërsulen ty;",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5412,
        "text": "Nga e djathta dhe e majta, në grupe?",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5413,
        "text": "A lakmon secili prejt yre për të hyrë në xhenet të dhuntive.",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5414,
        "text": "Kurrë! Ne ata i kemi krijuar prej asaj që e dinë.",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5415,
        "text": "Dhe unë betohem në Zotin e lindjes dhe të perëndimit se Ne kemi mundësi",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5416,
        "text": "T’i zëvendësojmë me më të mirë se ata dhe askush nuk mund të na konkurojë në këtë.",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5417,
        "text": "Prandaj leri ata le të llafosen e të zbaviten deri sa të ballafaqohen me ditën që u premtohet atyre,",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5418,
        "text": "Ditën në të cilën do të dalin nga varret me të shpejtë sikur vrapojnë kah idolet,",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5419,
        "text": "Me shikime të frikësuara me poshtërsi mbuluar, ajo është dita e cila u premtohej.",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "sq.mehdiu",
      "language": "sq",
      "name": "Feti Mehdiu",
      "englishName": "Feti Mehdiu",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}