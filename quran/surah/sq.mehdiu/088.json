{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 88,
    "name": "سُورَةُ الغَاشِيَةِ",
    "englishName": "Al-Ghaashiya",
    "englishNameTranslation": "The Overwhelming",
    "revelationType": "Meccan",
    "numberOfAyahs": 26,
    "ayahs": [
      {
        "number": 5968,
        "text": "A të ka ardhur ty lajmi për Ditën e Kijametit?",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5969,
        "text": "Kur disa fytyra do të jenë të përulura,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5970,
        "text": "Të rraskapitura, të stërlodhura,",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5971,
        "text": "Do të digjennë zjarr të këndellur,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5972,
        "text": "Do t’u jepet ujë nga burimet që vlojnë,",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5973,
        "text": "Nuk do të kenë ushqim tjetër përveç nga therrat,",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5974,
        "text": "Që nuk të shuan etjen as urinë.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5975,
        "text": "Disa fytyra at ditë do të jenë të gëzuara,",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5976,
        "text": "Të kënaqura për shpërblimin e punës së vet.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5977,
        "text": "Në xhenet të shkallës së lartë,",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5978,
        "text": "Aty nuk do të dëgjojnë fjalë të kota.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5979,
        "text": "Aty ka burim që rrjedh pa ndal,",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5980,
        "text": "Aty ka edhe shtretër të vendosur lart,",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5981,
        "text": "Edhe gota të vendosura,",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5982,
        "text": "E nënkresë të renduar,",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5983,
        "text": "Edhe qilima të shtruar.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5984,
        "text": "A nuk i shikojnë devet si janë krijuar,",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5985,
        "text": "Edhe qiellin – sa lartë është ngritur,",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5986,
        "text": "Edhe malet – si janë venduar,",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5987,
        "text": "Edhe tokën si është zgjeruar?",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5988,
        "text": "Ti këshillo – ti je vetëm këshillues,",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5989,
        "text": "Ti nuk je ndaj atyre detyrues,",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5990,
        "text": "Por kush kthen shpinën dhe mohon,",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5991,
        "text": "All-llahu atë me mundimin më të madh e mundon.",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5992,
        "text": "Me siguri kthimin tek ne e kanë,",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5993,
        "text": "Dhe para nesh, pa dyshim, llogari për të dhënë kanë!",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "sq.mehdiu",
