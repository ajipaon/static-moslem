        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6100,
        "text": "Edhe Turi Sinanë,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6101,
        "text": "Edhe këtë vend të siguruar!",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6102,
        "text": "Ne e kemi krijuar njeriun në formën më të bukur,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6103,
        "text": "Pastaj e shtyjmë në fund të fundit,",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6104,
        "text": "Përveç atyre të cilët besojnë dhe bëjnë vepra të mira. Ata i pret shpërblim i vazhdueshëm.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6105,
        "text": "E çka po të shtyn pra të përgënjeshtrosh botën tjetër?",
