{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 51,
    "name": "سُورَةُ الذَّارِيَاتِ",
    "englishName": "Adh-Dhaariyat",
    "englishNameTranslation": "The Winnowing Winds",
    "revelationType": "Meccan",
    "numberOfAyahs": 60,
    "ayahs": [
      {
        "number": 4676,
        "text": "Pasha erërat që ngrejnë shumë pluhur,",
        "numberInSurah": 1,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4677,
        "text": "Dhe ata që bartin peshë,",
        "numberInSurah": 2,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4678,
        "text": "Dhe ata të cilët notojnë lehtë,",
        "numberInSurah": 3,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4679,
        "text": "Dhe përndajnë urdhëra,",
        "numberInSurah": 4,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4680,
        "text": "Ajo që u premtohet, njëmend është e vërtetë,",
        "numberInSurah": 5,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4681,
        "text": "Shpërblimi e dënimi do të ndodhin me siguri!",
        "numberInSurah": 6,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4682,
        "text": "pasha qiellin me rrugë yjesh,",
        "numberInSurah": 7,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4683,
        "text": "Ju nuk i keni fjalët më një,",
        "numberInSurah": 8,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4684,
        "text": "Nga ai (besimi) është larguar kush është larguar,",
        "numberInSurah": 9,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4685,
        "text": "U shkatërrofshin (lakmuesit)!",
        "numberInSurah": 10,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4686,
        "text": "Të cilët janë mbytur në injorancë,",
        "numberInSurah": 11,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4687,
        "text": "Pyesin: “Kur do të vijë dita e gjykimit?”",
        "numberInSurah": 12,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4688,
        "text": "Atë ditë kur do të digjen në zjarr,",
        "numberInSurah": 13,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4689,
        "text": "Shijone dëniminnnn tuaj – kjo është ajo që e keni kërkuar më shpejt.",
        "numberInSurah": 14,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4690,
        "text": "Të devotshmit, me siguri, do të vendosen në kopshte dhe burime,",
        "numberInSurah": 15,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4691,
        "text": "Do të marrin atë çka do t’u dhurojë Zoti i tyre, sepse ata më parë kanë qenë mirëbërës,",
        "numberInSurah": 16,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4692,
        "text": "Vetëm pak, natën, kanë fjetur.",
        "numberInSurah": 17,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4693,
        "text": "Kurse në agim janë lutur për falje të mëkateve,",
        "numberInSurah": 18,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4694,
        "text": "Kurse në pasurinë e tyre kanë pjesë edhe ata që lypin edhe ata që nuk lypin.",
        "numberInSurah": 19,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4695,
        "text": "Argumentet në tokë janë për të bindurit,",
        "numberInSurah": 20,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4696,
        "text": "Si edhe në vet ju, a nuk e shihni?",
        "numberInSurah": 21,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4697,
        "text": "Kurse në qiell keni furnizimin dhe atë që u premtohet.",
        "numberInSurah": 22,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4698,
        "text": "Pasha Zotin e qiellit dhe të tokës, kjo është e vërtetë, ashtu siç është e vërtetë se ju flisni!",
        "numberInSurah": 23,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4699,
        "text": "A të ka arritur rrëfimi për musafirët e nderuar të Ibrahimit,",
        "numberInSurah": 24,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4700,
        "text": "Kur hynë dhe thanë: “Paqe!” Dhe ai u tha: “Paqe, o njerëz të panjohur!”",
        "numberInSurah": 25,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4701,
        "text": "Dhe ai shkoi, pa u hutuar, te familja e vet dhe solli një viç të majmë,",
        "numberInSurah": 26,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4702,
        "text": "Ua afroi dhe u tha: “A nuk hani?”",
        "numberInSurah": 27,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4703,
        "text": "Dhe u dridh nga frika prej tyre “Mos u frikëso!”, i thanë, dhe e përgëzuan me një djalosh të dijshëm.",
        "numberInSurah": 28,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4704,
        "text": "Kurse gruaja e tij u paraqit duke klithur e gërvishur fytyrën, “Vallë, a unë – tha një plakë shterpë!”",
        "numberInSurah": 29,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4705,
        "text": "“Ashtu ka thënë Zoti yt, ithanë ata, Ai është i urtë dhe i gjithëdijshëm”.",
        "numberInSurah": 30,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4706,
        "text": "“Po çka thuani ju o të deleguar?” – u tha (Ibrahimi),",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4707,
        "text": "“Ne jemi dërguar te një popull mizor” i thanë ata,",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4708,
        "text": "T’i mbulojmë ata me gurë – topa dheu,",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4709,
        "text": "I shenjuar te Zoti yt si teprues në të këqia”.",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4710,
        "text": "Dhe Ne, kush qe aty nga besimtarët, i nxorrëm,",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4711,
        "text": "Por aty gjetëm vetëm një shtëpi muslimane,",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4712,
        "text": "Dhe aty kemi lënë shenj për ata që frikësohen nga dënimi i padurueshëm.",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4713,
        "text": "Edhe te Musa, kur e dërguam te Faraoni me argumente të qarta,",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4714,
        "text": "E ai, duke shpresuar në forcën e vet, i ktheu shpinën dhe tha: “Është magjistar ose i marrë”!",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4715,
        "text": "Dhe e kapëm atë dhe ushtrinë e tij dhe i hudhëm në det. Ai e pat merituar.",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4716,
        "text": "Edhe te Ad-i, kur mbi ta lëshuam erën shkatërruese,",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4717,
        "text": "Pranë çfarëdo sendi që kaloi, asgjë nuk kurseu, të gjithë i shndërroi në kalbësirë.",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4718,
        "text": "Edhe te Themudi, kur iu tha: “kënaquni edhe një kohë!”",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4719,
        "text": "Dhe u treguan kryelartë ndaj urdhërit të Zotit të vet, prandaj i përfshiu vetëtima që e panë me sytë e tyre,",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4720,
        "text": "Dhe nuk mundën as të ngrihen e as të mbrohen.",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4721,
        "text": "Edhe populli i Nuhit, shumë kohë më parë, ai, njëmend ishte një popull i mbrapsht.",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4722,
        "text": "Ne kemi ngritur qiellin me forcën tonë, por me të vërtetë mundemi edhe më tepër.",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4723,
        "text": "Edhe tokën e kemi shtruar – çfarë shtruesi i mirë!",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4724,
        "text": "Dhe prej çdo sendi kemi krijuar nga një çift, ndoshta do të mendoni.",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4725,
        "text": "“Prandaj, shpejtoni te All-llahu, unë nga ai jam (i dërguar) t’iu tërheq vërejtjen haptazi.",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4726,
        "text": "Dhe mos bëni tjetër Zot me All-llahun, unë jam nga ana e Tij t’iu tërheq vërejtjen haptazi!”",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4727,
        "text": "Dhe ashtu, as atyre më përpara nuk u ka ardhur asnjë profet e të mos thonë: “Është magjistar ose i marrë!”",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4728,
        "text": "Ose ia linin njëri-tjetrit me porosi. Jo, por ata janë njerëz që e kanë tepruar,",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4729,
        "text": "Prandaj hiqu sish se nuk do të qortohesh,",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4730,
        "text": "Dhe vazhdo e këshillo, se këshillimi, me të vërtetë, u bën dobi besimtarëve.",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4731,
        "text": "Kurse exhinët dhe njerëzit nuk i kam krijuar për tjetër vetëm se të më adhurojnë.",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4732,
        "text": "Nuk kërkoj prej tyre furnizim as që dëshiroj të më ushqejnë.",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4733,
        "text": "All-llahu është me të vërtetë furnizuesi më i madh, i fortë i pathyeshëm!",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4734,
        "text": "Ata të cilët kanë bërë mizori e kanë dënimin siç u takon shokëve të tyre, andaj le të mos më ngutin,",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4735,
        "text": "Të mjerët ata mosbesimtarë për atë ditë që u premtohet!",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "sq.mehdiu",
      "language": "sq",
      "name": "Feti Mehdiu",
      "englishName": "Feti Mehdiu",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}