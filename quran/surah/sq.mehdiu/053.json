{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 53,
    "name": "سُورَةُ النَّجۡمِ",
    "englishName": "An-Najm",
    "englishNameTranslation": "The Star",
    "revelationType": "Meccan",
    "numberOfAyahs": 62,
    "ayahs": [
      {
        "number": 4785,
        "text": "Pasha yllin kur perëndon,",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4786,
        "text": "Shoku juaj nuk është larguar nga rruga e drejtë dhe nuk është i humbur,",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4787,
        "text": "Ai nuk flet me hamendje,",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4788,
        "text": "Ajo nuk është tjetër vetëm se shpallje që shpallet",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4789,
        "text": "E mëson një engjull me forcë të madhe,",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4790,
        "text": "Mendjelartë, që është paraqitur në forcën e vet,",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4791,
        "text": "Në horizontin më të lartin,",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4792,
        "text": "Pastaj iu afrua dhe iu afrua shumë",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4793,
        "text": "Afër sa dy harqe apo më afër,",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4794,
        "text": "Dhe i shpalli çka i shpalli robit të vet,",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4795,
        "text": "Zemra nuk e mohoi atë çka e ka parë.",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4796,
        "text": "A po ziheni me të për ata që i ka para?",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4797,
        "text": "Ai e ka parë edhe herën tjetër,",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4798,
        "text": "Në Sidre-i Munteha,",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4799,
        "text": "Aty është xhenneti Ma’va,",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4800,
        "text": "Kur Sidren e mbuoi çka e mbuloi,",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4801,
        "text": "Shikimin nuk ia ktheu e as e hutoi!",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4802,
        "text": "Pa mrekullitë më madhështore të Zotit të vet",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4803,
        "text": "A i patë Laten dhe Uzzan",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4804,
        "text": "Edhe Menatin, të tretin, të fundit?",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4805,
        "text": "A djemtë janë për ju e për Atë – vajzat?!",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4806,
        "text": "Ajo është, pra, ndarje e padrejtë.",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4807,
        "text": "Këta janë vetëm emra me të cilat i keni emëruar ju dhe të parët tuaj. All-llahu për ata nuk u ka dërguar kurrfarë argumenti. Ata pasojnë vetëm paragjykimet dhe ç’dëshirojnë epshet e tyre, por nga Zoti i tyre u pat ardhur udhëzim.",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4808,
        "text": "A i takon njeriut gjithçka dëshiron?!",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4809,
        "text": "All-llahut i takon kjo botë edhe bota tjetër.",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4810,
        "text": "Sa e sa engjuj ka në qiej ndërmjetësimi i të cilëve nuk do t’iu bëjë dobi derisa All-llahu t’i lejojë atij që do Ai dhe që është i kënaqur.",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4811,
        "text": "Ata që nuk besojnë në botën tjetër i emërtojnë engjujt me emra femrash.",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4812,
        "text": "E për atë nuk dinë kurrgjë, pasojnë vetëm me hamendje, kurse hamendja aspak nuk e pasuron të vërtetën.",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4813,
        "text": "Prandaj ti lragohu prej atyre që i kthejnë shpinën Kur’anit dhe që nuk dëshirojnë tjetër vetëm jetën në këtë botë.",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4814,
        "text": "Ajo është kulminacioni i dijenisë së tyre. Zoti yt, pa dyshim, e di më së miri ata që janë larguar nga rruga e tij dhe ai e di mirë ata që janë në rrugë të drejtë.",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4815,
        "text": "Të All-llahut janë gjithçka ka në qiej dhe çka ka në tokë – për t’i shpërblyer ata që kanë punuar keq sipas asaj që kanë punuar, kurse ata që bëjnë mirë me shpërblim edhe më të mirë.",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4816,
        "text": "Ata që largohen prej mëkateve të mëdha e posaçërisht prej amoralitetit, përveç të voglave. Zoti yt me të vërtetë shumë falë, Ai iu njeh shumë mirë juve prej se ju ka krijuar prej dheut dhe qysh kur ishit embrion në barqet e nënave tuaja. Prandaj mos mburreni fort me vetveten. Ai e di më mirë atë që ruhet.",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4817,
        "text": "A e pe ti atë që ktheu shpinën,",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4818,
        "text": "Dhe jep pak lëmoshë,",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4819,
        "text": "A e di ai atë të padukshmen dhe e sheh?",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4820,
        "text": "A nuk u njoftua me atë që është në suhufet e Musaut",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4821,
        "text": "Dhe të Ibrahimit, i cili zbatonte çdo detyrim;",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4822,
        "text": "Se asnjë mëkatar nuk do t’i bartë mëkatet e tjetrit,",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4823,
        "text": "Dhe se njeriu ka vetëm atë që e punon vetë,",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4824,
        "text": "Dhe se mundi i tij do të shihet,",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4825,
        "text": "Pastaj do të shpërblehet me shpërblim të merituar,",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4826,
        "text": "Dhe se te Zoti yt do të kthehen,",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4827,
        "text": "Dhe Ai është që të bën të qeshësh dhe të qashë,",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4828,
        "text": "Dhe se Ai është që të vdes dhe të ngjallë!,",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4829,
        "text": "Dhe se Ai ka krijuar çift, mashkull e femër,",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4830,
        "text": "Nga pika e farës së hedhur,",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4831,
        "text": "Dhe ata përsëri do t’i ngjallë,",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4832,
        "text": "Dhe se Ai i pasuon dhe zbukuron,",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4833,
        "text": "Dhe se Ai është Zot i Sirijusit,",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4834,
        "text": "Dhe se Ai e shkatërroi Ad-in e lashtë,",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4835,
        "text": "Dhe Themudin, dhe nuk ka lënë askë.",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4836,
        "text": "Kurse popullin e Nuhit, më herët, i cili ka qenë më mizori dhe më i humburi,",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4837,
        "text": "Edhe vendbanimet e shkatërruara i përmbysi,",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4838,
        "text": "Dhe i gjeti çka i gjeti.",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4839,
        "text": "E në cilën dhunti të Zotit tënd ti po dyshon?!",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4840,
        "text": "Ky (Kur’an) është vërejtje si vërejtjet e mëparshme.",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4841,
        "text": "Kijameti po afrohet,",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4842,
        "text": "Përveç All-llahut atë nuk ka kush e zbulon.",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4843,
        "text": "A këtij fjalimi i çuditen,",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4844,
        "text": "Dhe qesheni e nuk qani (në vend se të qani),",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4845,
        "text": "Madje edhe me mendjemadhësi?",
        "numberInSurah": 61,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4846,
        "text": "Bini në sexhde për All-llahun dhe adhurone!",
        "numberInSurah": 62,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": {
          "id": 13,
          "recommended": false,
          "obligatory": true
        }
      }
    ],
