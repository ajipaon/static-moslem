{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 44,
    "name": "سُورَةُ الدُّخَانِ",
    "englishName": "Ad-Dukhaan",
    "englishNameTranslation": "The Smoke",
    "revelationType": "Meccan",
    "numberOfAyahs": 59,
    "ayahs": [
      {
        "number": 4415,
        "text": "Ha Mim",
        "numberInSurah": 1,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4416,
        "text": "Pasha Librin e qartë,",
        "numberInSurah": 2,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4417,
        "text": "Ne atë e zbritëm në një natë të bekuar. Ne me siguri tërheqim vërejtjen",
        "numberInSurah": 3,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4418,
        "text": "Kur zgjidhet çdo çështje e urtë",
        "numberInSurah": 4,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4419,
        "text": "Sipas urdhëit Tonë! Ne me të vërtetë kemi dërguar profetë",
        "numberInSurah": 5,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4420,
        "text": "Nga mëshira e Zotit tënd, Ai me të vërtetë dëgjon dhe i di të gjitha.",
        "numberInSurah": 6,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4421,
        "text": "Zotit të qiejve dhe të tokës dhe asaj që është ndërmjet tyre, nëse jeni të bindur.",
        "numberInSurah": 7,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4422,
        "text": "Nuk ka tjetër Zot përveç Atij, ngjall dhe vdes. Zot i juaj dhe Zot i të parëve tuaj të hershëm,",
        "numberInSurah": 8,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4423,
        "text": "Por ata dyshojnë dhe luajnë.",
        "numberInSurah": 9,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4424,
        "text": "prandaj prite ditën kur t’iu sjellë qielli një tym haptazi",
        "numberInSurah": 10,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4425,
        "text": "Që do t’i mbulojë njerëzit. “Ky është dënim i dhembshëm!",
        "numberInSurah": 11,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4426,
        "text": "O Zoti ynë, hiqna dënimin, ne me siguri jemi besimtarë!”",
        "numberInSurah": 12,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4427,
        "text": "Ku e gjetën ata këshillën! Atyre u ka ardhur profet haptazi",
        "numberInSurah": 13,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4428,
        "text": "E prap se prapë ia kthejnë shpinën dhe thonë: “I mësuar, i prishur mendësh”!",
        "numberInSurah": 14,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4429,
        "text": "Ne do t’iu largojmë pak dënimin e ju me siguri dot ë ktheheni prap,",
        "numberInSurah": 15,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4430,
        "text": "Por atë ditë kur do t’i rrëmbejmë me tërë forcën, njëmend do t’u hakmerremi.",
        "numberInSurah": 16,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4431,
        "text": "Përpara tyre Ne e kemi vënë në sprovë popullin e Faraonit. Atyre u erdh Profet fisnik.",
        "numberInSurah": 17,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4432,
        "text": "“Përgjigjuni o robërit e All-llahut, sepse unë jam për ju Profet i sigurt.",
        "numberInSurah": 18,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4433,
        "text": "Dhe mos bëheni më të lartë se All-llahu, unë ju sjell argument të qartë,",
        "numberInSurah": 19,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4434,
        "text": "Dhe unë kërkoj mbështetje te Zoti im dhe Zoti juaj që të mos më vritni me gur.",
        "numberInSurah": 20,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4435,
        "text": "Por nëse nuk më besoni, atëherë hiquni prej meje!",
        "numberInSurah": 21,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4436,
        "text": "Dhe e thirri Zotin e vt, sepse ata janë, me të vërtetë, popull mizor”.",
        "numberInSurah": 22,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4437,
        "text": "Tërhiqu me robërit e Mi natën, se do të jeni të ndjekur,",
        "numberInSurah": 23,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4438,
        "text": "Dhe lëre detin të qetë! Ata janë një ushtir e përmbytur.",
        "numberInSurah": 24,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4439,
        "text": "Sa kopshte e burime kanë lënë,",
        "numberInSurah": 25,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4440,
        "text": "Edhe sa ara të mbjellura e vende fisnike,",
        "numberInSurah": 26,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4441,
        "text": "dhe dhuntitë që i kanë gëzuar.",
        "numberInSurah": 27,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4442,
        "text": "Dhe ashtu, ia lëmë atë në trashëgim një populli tjetër.",
        "numberInSurah": 28,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4443,
        "text": "Për ata nuk qanë as qielli as toka dhe nuk iu dha afat,",
        "numberInSurah": 29,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4444,
        "text": "Kurse izraelitët i patën shpëtuar nga vuajtja e poshtëruar,",
        "numberInSurah": 30,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4445,
        "text": "Nga Faraoni. Ai ishte, me të vërtetë, një teprues i madh,",
        "numberInSurah": 31,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4446,
        "text": "Dhe i patëm zgjedhur me dije ndër bashkëkohësit,",
        "numberInSurah": 32,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4447,
        "text": "Dhe u dhamë argumentet përplot përvoja të qarta,",
        "numberInSurah": 33,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4448,
        "text": "Ata me siguri do të thonë:",
        "numberInSurah": 34,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4449,
        "text": "“Ajo është vetëm se vdekja jonë e parë, ne nuk do të ringjallemi;",
        "numberInSurah": 35,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4450,
        "text": "Sillna baballarët, nëse jeni të sinqertë!”",
        "numberInSurah": 36,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4451,
        "text": "A janë ata më të mirë apo populli Tubba dhe ata përpara atyre? Ata i zhdukëm. Ishin me të vërtetë mizorë.",
        "numberInSurah": 37,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4452,
        "text": "Nuk i kemi krijuar qiejt dhe tokën dhe ç’ka ndërmjet tyre për të luajtur,",
        "numberInSurah": 38,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4453,
        "text": "Ne ata dy i kemi krijuar me urtësi por shumcia prej tyre nuk e dinë.",
        "numberInSurah": 39,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4454,
        "text": "Dita e Kijametit për të gjithë është caktuar,",
        "numberInSurah": 40,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4455,
        "text": "Dita kur miku mikut aspak nuk i bën dobi dhe as do t’u ndihmohet,",
        "numberInSurah": 41,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4456,
        "text": "Përveç atyre që i mëshiron All-llahu. Ai është me të vërtetë i fortë dhe mëshirëplotë.",
        "numberInSurah": 42,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4457,
        "text": "Druri zekkum,",
        "numberInSurah": 43,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4458,
        "text": "Do të jetë ushqim përmëkatarin,",
        "numberInSurah": 44,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4459,
        "text": "Do të vlojë në barqe si rrëshirë.",
        "numberInSurah": 45,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4460,
        "text": "Si vlon uji valë.",
        "numberInSurah": 46,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4461,
        "text": "Rrëmbene dhe futne në mes të zjarrit,",
        "numberInSurah": 47,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4462,
        "text": "E pastaj, si dënim, derdhnju mbi krye ujin e valë!",
        "numberInSurah": 48,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4463,
        "text": "Shijoe! Ti je fisnik i fortë.",
        "numberInSurah": 49,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4464,
        "text": "Ky është në të cilin jy dyshonit.",
        "numberInSurah": 50,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4465,
        "text": "Të devotshmit, pa mëdyshje, kanë vend të sigurt,",
        "numberInSurah": 51,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4466,
        "text": "Në kopshte me burime,",
        "numberInSurah": 52,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4467,
        "text": "Të veshur në mëndafsh e kadife, përballë njëri-tjetrit.",
        "numberInSurah": 53,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4468,
        "text": "Gjithashtu do t’i kurorëzojmë me hyri symëdha.",
        "numberInSurah": 54,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4469,
        "text": "Aty mund të kërkojnë, të sigurt, çfarëdo lloj pemësh që të dëshirojnë;",
        "numberInSurah": 55,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4470,
        "text": "Aty nuk do të shijojnë vdekjen përveç vdekjes së parë dhe Ai do t’i ruajë nga dënimi me zjarr.",
        "numberInSurah": 56,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4471,
        "text": "Mirësi nga Zoti yt, ai është sukses i madh.",
        "numberInSurah": 57,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4472,
        "text": "Dhe Ne e kemi bërë të lehtë atë (Kur’anin) në gjuhën tënde, ashtu që ata të marrin mësim,",
        "numberInSurah": 58,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4473,
        "text": "Prandaj prit, edhe ata presin!",
        "numberInSurah": 59,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "sq.mehdiu",
      "language": "sq",
      "name": "Feti Mehdiu",
      "englishName": "Feti Mehdiu",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}