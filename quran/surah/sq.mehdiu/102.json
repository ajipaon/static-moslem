  "data": {
    "number": 102,
    "name": "سُورَةُ التَّكَاثُرِ",
    "englishName": "At-Takaathur",
    "englishNameTranslation": "Competition",
    "revelationType": "Meccan",
    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6169,
        "text": "Ju preokupon shumimi i pasurisë,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6170,
        "text": "Derisa të shkoni në varre.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6171,
        "text": "Ani, do ta merrni vesh!",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6172,
        "text": "Edhe njëherë ani! Me siguri do ta dini!",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6173,
        "text": "Po, si jo! Sikur ta dinit, me siguri dhe me bindje,",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6174,
        "text": "Gjithsesi do ta shihni xhehennemin.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6175,
        "text": "Edhe një herë, me siguri do shihni me të dy sytë!",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6176,
        "text": "Pastaj, atë ditë, do të pyetni patjetër për kënaqësitë.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      }
