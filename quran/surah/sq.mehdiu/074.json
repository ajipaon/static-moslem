{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 74,
    "name": "سُورَةُ المُدَّثِّرِ",
    "englishName": "Al-Muddaththir",
    "englishNameTranslation": "The Cloaked One",
    "revelationType": "Meccan",
    "numberOfAyahs": 56,
    "ayahs": [
      {
        "number": 5496,
        "text": "O i mbuluar!",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5497,
        "text": "Ngritu dhe qorto (thirr)!",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5498,
        "text": "dhe Zotin tënd madhëro!",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5499,
        "text": "Dhe rrobet tua pastro!",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5500,
        "text": "Dhe nga të këqiat u largo!",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5501,
        "text": "Dhe mos u kënaq as me shumë, por këshillo!",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5502,
        "text": "Dhe për hirë të Zotit tënd duro!",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5503,
        "text": "E kur të fryhet në sure,",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5504,
        "text": "Ajo atëherë do të jetë ditë e rëndë",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5505,
        "text": "Për mosbesimtarët, pa lehtësi.",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5506,
        "text": "M’i lë mua dhe atë që i kam krijuar vetë",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5507,
        "text": "Dhe i kam dhënë pasuri të madhe,",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5508,
        "text": "Edhe djem që janë me ta,",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5509,
        "text": "Edhe shumë të mira kam dhuruar;",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5510,
        "text": "Pastaj lakmon t’ia shtojë edhe më tepër,",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5511,
        "text": "Kurrsesi! Ai, me të vërtetë, u kundëdrvihet fakteve tona.",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5512,
        "text": "Do ta ngarkoj shumë rëndë,",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5513,
        "text": "Sepse ai ka menduar dhe planifikuar,",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5514,
        "text": "Qoftë i mallkuar si e ka planifikuar!",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5515,
        "text": "Prapë qoftë i mallkuar si e ka planifikuar!",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5516,
        "text": "Pastaj ka shikuar,",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5517,
        "text": "E mandej u ngrys e u mrrol,",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5518,
        "text": "E pastaj ktheu shpinën me mendjemadhësi",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5519,
        "text": "Dhe tha: “Kjo nuk është kurrgjë përveç një magji q transmetohet,",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5520,
        "text": "Këto janë vetëm fjalë njeriu”.",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5521,
        "text": "Atë do ta hudhë në Sekar,",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5522,
        "text": "A e di ç’është Sekar?",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5523,
        "text": "Aio asgjë nuk kursen,",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5524,
        "text": "Lëkurët njerëzve ua nxinë,",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5525,
        "text": "Mbi të vigjëlon nëntëmbëdhjetë,",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5526,
        "text": "Dhe nuk kemi bërë rojtar të zjarrit tjetër kënd, veç engjujve dhe numrin e tyre e kemi caktuar si sprovë për ata që nuk besojnë, për t’i bindur ata që u është dhënë Libri, kurse ata që besojnë t’u forcohet besimi, ndërsa ata të cilëve u është dhënë Libri dhe besimtarët, të mos dyshojnë, dhe që ata zemrat e të cilëve janë të sëmura, dhe ata që janë mosbesimtarë të thonë: “Çka ka dashur All-llahu me këtë shembull?” Ashtu All-llahu e lë të humbur kë të dojë dhe udhëzon kë të dojë, dhe askush nuk i di ushtritë e Zotit tënd përveç Atij. Kurse ai (Sekari) është vetëm qortim për njerëzimin.",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5527,
        "text": "Po, si jo! Pasha Hënën,",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5528,
        "text": "Edhenatën kur kalon,",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5529,
        "text": "E agimi kur agon,",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5530,
        "text": "Ajo është, njëmend, një nga (fatkeqësi) të mëdhatë.",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5531,
        "text": "Qortim për njerëzit,",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5532,
        "text": "Për atë nga mesi juaj i cili do të shpëtojë ose të vonojë,",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5533,
        "text": "Secili është përgjegjës për atë çka ka fituar (ç’ka bërë),",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5534,
        "text": "Përveç të djathtëve.",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5535,
        "text": "Ata në xhennete do të bisedojnë",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5536,
        "text": "Për mizorët – mëkatarët.",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5537,
        "text": "“Çka ju solli në Sekar”?",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5538,
        "text": "Do të thonë: “Nuk kemi pas kryer namazin,",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5539,
        "text": "Dhe të varfërit nuk i kemi ushqyer,",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5540,
        "text": "Dhe kemi llomotitur me llomotitësit,",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5541,
        "text": "Edhe e kemi pas mohuar ditën e kijametit,",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5542,
        "text": "Derisa nuk na erdhi vdekja”.",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5543,
        "text": "Atyre nuk u bën dobi ndërmjetësimi i ndërmjetësuesve.",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5544,
        "text": "E pse ata largohen nga këshilla,",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5545,
        "text": "Sikur gomarë të tërhuzur,",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5546,
        "text": "Që ikin nga ndjekësi!",
        "numberInSurah": 51,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5547,
        "text": "Po! Secili njeri prej tyre dëshiron t’i jepen broshura të shpallura qartë.",
        "numberInSurah": 52,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5548,
        "text": "Kurrë! Sepse ata nuk i frikësohen asaj bote.",
        "numberInSurah": 53,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5549,
        "text": "S’ka dyshim, ai është këshillë.",
        "numberInSurah": 54,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5550,
        "text": "Dhe kush do të mund, ta ketë në mend.",
        "numberInSurah": 55,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5551,
        "text": "Por në mend mund ta ketë vetëm nëse do All-llahu. Ai është i denjë për t’iu friëksuar dhe kompetentë për të falur.",
        "numberInSurah": 56,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "sq.mehdiu",
      "language": "sq",
      "name": "Feti Mehdiu",
      "englishName": "Feti Mehdiu",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}