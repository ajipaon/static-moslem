{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 75,
    "name": "سُورَةُ القِيَامَةِ",
    "englishName": "Al-Qiyaama",
    "englishNameTranslation": "The Resurrection",
    "revelationType": "Meccan",
    "numberOfAyahs": 40,
    "ayahs": [
      {
        "number": 5552,
        "text": "Betohem, seriozisht, në Ditën e Kijametit,",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5553,
        "text": "Dhe betohem, seriozisht, në shpirtin që veten e qorton.",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5554,
        "text": "A mendon njeriu se nuk do t’i tubojmë eshtrat e tij?",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5555,
        "text": "Po, si jo! Ne kemi mundësi të rikrijojmë eedhe majet e thonjve të gishtërinjve të tij.",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5556,
        "text": "Por njeriu dëshiron të gabojë në jetë.",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5557,
        "text": "Ai pyet: “Kur do të bëhet kijameti”?",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5558,
        "text": "Por kur t’i shtanget shikimi,",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5559,
        "text": "Edhe hëna të errësohet,",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5560,
        "text": "Dhe Dielli me hënë të bashkohet,",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5561,
        "text": "Atë ditë njeriu do të thotë: “Kah të iket?!”",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5562,
        "text": "Askund! Nuk ka strehim.",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5563,
        "text": "Atë ditë cakur është te Zoti yt,",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5564,
        "text": "Atë ditë njeriu do të informohet për atë që e ka shpejtuar ose e ka vonuar,",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5565,
        "text": "Por njeriu është dëshmitarë kundër vetes,",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5566,
        "text": "Edhe pse përpiqet të sjellë arsyetimet e tij.",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5567,
        "text": "Atë (Kur’anin) mos e shqipto me gjuhë për ta shpejtuar,",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5568,
        "text": "Ne kemi detyrë tubimin dhe leximin e tij,",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5569,
        "text": "E kur e lexojmë, ti përcille leximin e tij,",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5570,
        "text": "E pastaj, është detyrë e jona ta shkoqisim.",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5571,
        "text": "Por ani! Ju e dashuroni këtë jetë të kalueshme,",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5572,
        "text": "Dhe e leni pas shpine atë tjetrën.",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5573,
        "text": "Atë ditë disa fytyra do të shkelqejnë",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5574,
        "text": "E disa fytyra atë ditë do të jenë të ngrysura.",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5575,
        "text": "Zotin e vet do ta shikojnë,",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5576,
        "text": "Duke menduar se do t’i godit fatkeqësi.",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5577,
        "text": "Po, si jo! Kur (shpirti) depërton në eshtrat kyqe,",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5578,
        "text": "dhe të thuhet, “Kush është shërues”?",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5579,
        "text": "Dhe mendoi se është momenti i ndarjes,",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5580,
        "text": "Dhe këmba me këmbë ndërlidhet...",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5581,
        "text": "Te Zoti yt do të sillen atë ditë,",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5582,
        "text": "“Dhe nuk ka besuar as e ka kryer faljen,",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5583,
        "text": "por ka kthyer shpinën dhe ka përgënjeshtruar.",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5584,
        "text": "E pastaj shkon tek të vetët, mendjemadh”.",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5585,
        "text": "I mjeri ti! I mjeri ti!",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5586,
        "text": "Dhe prapë, i mjeri ti! I mjeri ti!",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5587,
        "text": "A mendon njeriu se do të lihet në vullnet të vetin, pa përgjegjësi?",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5588,
        "text": "A nuk ka qenë një pikë fare që hudhet,",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5589,
        "text": "Pastaj ka qenë gjak i trashur dhe e ka krijuar në formën e përsosur, të fundit,",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5590,
        "text": "Dhe prej atij bëri dy qifte, mashkull dhe femër.",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5591,
        "text": "Po a nuk ka Ai mundësi të ngjallë të vdekurin?",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "sq.mehdiu",
      "language": "sq",
      "name": "Feti Mehdiu",
      "englishName": "Feti Mehdiu",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}