{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 52,
    "name": "سُورَةُ الطُّورِ",
    "englishName": "At-Tur",
    "englishNameTranslation": "The Mount",
    "revelationType": "Meccan",
    "numberOfAyahs": 49,
    "ayahs": [
      {
        "number": 4736,
        "text": "Pasha Turin,",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4737,
        "text": "Pasha Librin në rreshta të shkruar,",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4738,
        "text": "Në lëkurë të përpunuar!",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4739,
        "text": "Pasha haremin përplot të banuar,",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4740,
        "text": "Pasha kulmin (qiellin) e ngritur",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4741,
        "text": "Dhe pasha detin e mbushur.",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4742,
        "text": "Dënimi i Zotit tënd, me siguri, do të ndodh.",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4743,
        "text": "Atë nuk e ndal dot kush,",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4744,
        "text": "Atë ditë kur qielli të shqetësohet keq",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4745,
        "text": "Dhe malet të lëkunden nga vendi;",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4746,
        "text": "Atë ditë mjerë për gënjeshtarët!",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4747,
        "text": "Të cilët janë zhytur (në gënjeshtra) e luajnë!",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4748,
        "text": "Atë ditë kur me rrëmbim të hudhen në zjarr të xhehennemit,",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4749,
        "text": "“Ky është zjarri që nuk e keni besuar,",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4750,
        "text": "Po a është kjo magji apo ju nuk shihni?",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4751,
        "text": "Digjuni në të, për ju është njëlloj, duruat apo nuk duruat. Po shpërbleheni për atë çka keni punuar”.",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4752,
        "text": "Kurse të devotshmit do të jenë në xhenete, në lumturi,",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4753,
        "text": "Të kënaqur me atë që u ka dhënë Zoti i tyre. Ata Zoti i tyre do t’i ruajë nga vuajtjet në zjarr.",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4754,
        "text": "“Hani dhe pini, u bëftë mirë! Për atë që keni punuar!”",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4755,
        "text": "Të mbështetur në kanape të rradhitura dhe do t’i martojmë me hyri symëdha.",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4756,
        "text": "Ata të cilët kanë besuar dhe të cilët pasardhësit e vet i pasojnë në besim, edhe fëmijët e tyre do t’ua bashkangjesim, kurse veprat e tyre kurrgjë nuk ua zvogëlojmë çdo kush është përgjegjës për atë që e bënë vet.",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4757,
        "text": "Do t’u afrojmë pemë e mish, sipas dëshirës së tyre;",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4758,
        "text": "Aty njëri-tjetrin do ta shërbejnë me gota plot, pa fjalë të kota dhe pa mëkate.",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4759,
        "text": "Do të sillen rreth tyre djelmosha si margaritarë të fshehur.",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4760,
        "text": "Do t’i qasen njëri-tjetrit, duke e pyetur",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4761,
        "text": "Dhe duke thënë: “Më herët kemi pasur frikë në familjen tonë",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4762,
        "text": "E All-llahu Na dhuroi mëshirë dhe na ruajti nga vuajtja në zjarr.",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4763,
        "text": "Ne i jemi falur edhe më parë, Ai është me të vërtetë bamirës dhe i mëshirshëm”.",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4764,
        "text": "Prandaj, ti qortoi, sepse ti, me mëshirën e Zotit tënd, nuk je as magjistar as i marrë.",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4765,
        "text": "Kurse ata thonë: “Ai është poet, të presim vdekjen (fundin e tij).",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4766,
        "text": "Ti thuaj: “Pritni! Edhe unë me ju do të pres!”",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4767,
        "text": "A u vjen ajo nga ëndërrat e tyre apo janë kokëfortë që e teprojnë?",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4768,
        "text": "A thua ata të flasin: “E trillon!” Jo, por ata nuk besojnë.",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4769,
        "text": "Po le të sjellin një fjalim të ngjashëm si ai (Kur’ani), nëse flasin të vërtetën!",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4770,
        "text": "A janë të krijuar par kurrgjë apo ata janë krijues?",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4771,
        "text": "A thua ata i krijuan qiejt dhe tokën? Jo, por ata nuk janë të bindur.",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4772,
        "text": "A mos e kanë ata pasurinë e Zotit tënd, apo mos janë ata vallë dirigjues!?",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4773,
        "text": "A mos kanë ata shkallë, me të cilat përgjojnë?! Le të sjellë ndonjë përgjues i tyre një argument të qartë!",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4774,
        "text": "A për Atë (All-llahun) janë vajzat e për ju djemt?",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4775,
        "text": "A mos kërkon ti prej tyre shpërblim dhe janë të rënduar me tatim?",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4776,
        "text": "A mos po disponojnë me të padukshmen dhe mos po shkruajnë?",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4777,
        "text": "Apo dëshirojnë ndonjë kurthë? Por në kurthë do të zihen mua ta që nuk besojnë!",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4778,
        "text": "A kanë ata tjetër Zot veç All-llahut? Qoftë lartësuar All-llahu nga ata që ia bëjnë shok!",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4779,
        "text": "Dhe sikur të shihnin se po bjen një copë qielli, do të thonin: “Re të dendura”!",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4780,
        "text": "Prandaj lëshoi deri sa të ballafaqohen me Ditën kur do të shtangen,",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4781,
        "text": "Me ditën kur dredhitë e tyre aspak nuk do t’u bëjnë dobi dhe nuk do të ndihmohen.",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4782,
        "text": "Pa dyshim ata të cilët kanë bërë mizori, kanë dënim përveç atij (të kësaj bote), por shumcia sish nuk e dinë.",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4783,
        "text": "Andaj duro e prit gjykimin e Zotit tënd, Ne të kemi parasysh dhe lavdëro e madhëro Zotin tënd kur të ngrihesh,",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4784,
        "text": "Edhe natën. Por ta madhërosh edhe kur humbin yjtë!",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "sq.mehdiu",
      "language": "sq",
      "name": "Feti Mehdiu",
      "englishName": "Feti Mehdiu",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}