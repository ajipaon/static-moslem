        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6095,
        "text": "For indeed, with hardship [will be] ease.",
        "numberInSurah": 5,
        "juz": 30,
