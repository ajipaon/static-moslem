  "data": {
    "number": 93,
    "name": "سُورَةُ الضُّحَىٰ",
    "englishName": "Ad-Dhuhaa",
    "englishNameTranslation": "The Morning Hours",
    "revelationType": "Meccan",
    "numberOfAyahs": 11,
    "ayahs": [
      {
        "number": 6080,
        "text": "Pelas horas da manhã,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6081,
        "text": "E pela noite, quando é serena,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6082,
        "text": "Que o teu Senhor não te abandonou, nem te odiou.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6083,
        "text": "E sem dúvida que a outra vida será melhor, para ti, do que a presente.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6084,
        "text": "Logo o teu Senhor te agraciará, de um modo que te satisfaça.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6085,
        "text": "Porventura, não te encontrou órgão e te amparou?",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6086,
        "text": "Não te encontrou extraviado e te encaminhou?",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6087,
        "text": "Não te achou necessitado e te enriqueceu?",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6088,
        "text": "Portanto, não maltrates o órfão,",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6089,
        "text": "Nem tampouco repudies o mendigo,",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6090,
        "text": "Mas divulga a mercê do teu Senhor, em teu discurso.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      }
    ],
    "edition": {
