    "number": 107,
    "name": "سُورَةُ المَاعُونِ",
    "englishName": "Al-Maa'un",
    "englishNameTranslation": "Almsgiving",
    "revelationType": "Meccan",
    "numberOfAyahs": 7,
    "ayahs": [
      {
        "number": 6198,
        "text": "Weet jij wie degene is die (de Dag van) het Oordeel loochent?",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6199,
        "text": "Dat is degene die de wees wegduwt.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6200,
        "text": "En hij spoort niet aan tot het geven van voedsel aan de behoeftigen.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6201,
        "text": "Wee dan de verrichters van de shalât,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6202,
        "text": "Degenen die onachtzaam zijn met hun shalât.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6203,
        "text": "Degenen die er een vertoning van maken.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6204,
        "text": "En die de levensbenodigdheden tegenhouden.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      }
    ],
