{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 83,
    "name": "سُورَةُ المُطَفِّفِينَ",
    "englishName": "Al-Mutaffifin",
    "englishNameTranslation": "Defrauding",
    "revelationType": "Meccan",
    "numberOfAyahs": 36,
    "ayahs": [
      {
        "number": 5849,
        "text": "Wee de zwendelaars!",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5850,
        "text": "Degenen die wanneer zij mensen voor zich laten wegen de volle maat eisen.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5851,
        "text": "Maar wanneer zij voor anderen afmeten of voor hen afwegen, benadelen zij (hen).",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5852,
        "text": "Zijn diegenen dan er niet van overtuigd dat zij opgewekt zullen worden?",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5853,
        "text": "Op een geweldige Dag?",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5854,
        "text": "Op de Dag waarop de mensen voor de Heer der Werelden staan?",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5855,
        "text": "Nee, voorwaar, het boek van de zondigen is in Siddjîen.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5856,
        "text": "En wat doet jou weten wat Siddjîen is?",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5857,
        "text": "Een volbeschreven boek.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5858,
        "text": "Wee die Dag de loochenaars!",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5859,
        "text": "Degenen die de Dag des Oordeels loochenen.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5860,
        "text": "En niemand loochent die behalve elke zondige overtreder.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5861,
        "text": "Wanneer Onze Verzen aan hem worden voorgedragen, zegt hij: \"Fabels van de vroegeren!\"",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5862,
        "text": "Nee! Wat zij plachten te doen heeft zelfs hun harten bedekt.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5863,
        "text": "Nee, voorwaar, zij zullen zeker op die Dag van hun Heer afgescheiden zijn.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5864,
        "text": "Vervolgens zullen zij zeker Djahîm (de Hel) binnengaan.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5865,
        "text": "Daarop wordt gezegd: \"Dit is dat wat jullie plachten te loochenen.\"",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5866,
        "text": "Nee! Voorwaar, het boek van de deugdzamen is zeker in 'Illiyyôen.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5867,
        "text": "En wat doet jou weten wat 'Illiyyôen is?",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5868,
        "text": "Een volbeschreven Boek.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5869,
        "text": "De bij (Allah) gebrachten zijn er getuigen van.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5870,
        "text": "Voorwaar, de deugdzamen zullen zeker in Na'im (het Paradijs) vertoeven.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5871,
        "text": "Op rustbanken kijken zij toe.",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5872,
        "text": "Jij herkent in hun gezichten de stralende gelukzaligheid",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5873,
        "text": "Hun wordt verzegeld drinken ingeschonken.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5874,
        "text": "Waarvan het zegel van muskus is, en laten de wedijveraars hierom dan wedijveren.",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5875,
        "text": "En zijn mengdrank is van (de bron) Tasmîm.",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5876,
        "text": "Een bron waarvan de nabijgebrachten drinken.",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5877,
        "text": "Voorwaar, degenen die zondigden plachten over degenen die geloofden te lachen.",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5878,
        "text": "En wanneer zij aan hen voorbijgingen, knipoogden zij naar elkaar.",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5879,
        "text": "En wanneer zij terugkeerden naar hun volk, keerden zij verheugd terug.",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5880,
        "text": "En wanneer zij hen zagen, zeiden zij: \"Voorwaar, zij zijn zeker dwalend.\"",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5881,
        "text": "En zij zijn niet als bewakers over hen gezonden.",
        "numberInSurah": 33,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5882,
        "text": "Maar op deze Dag zullen degenen die geloven lachen over de ongelovigen.",
        "numberInSurah": 34,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5883,
        "text": "Op rustbanken kijken zij toe.",
        "numberInSurah": 35,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5884,
        "text": "Worden de ongelovigen niet vergolden voor wat zij plachten te doen?",
        "numberInSurah": 36,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "nl.siregar",
      "language": "nl",
      "name": "Siregar",
      "englishName": "Siregar",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}