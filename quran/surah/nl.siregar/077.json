{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 77,
    "name": "سُورَةُ المُرۡسَلَاتِ",
    "englishName": "Al-Mursalaat",
    "englishNameTranslation": "The Emissaries",
    "revelationType": "Meccan",
    "numberOfAyahs": 50,
    "ayahs": [
      {
        "number": 5623,
        "text": "Bij de met goedheid uitgezondenen.",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5624,
        "text": "Bij de met spoed voortspoedenden.",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5625,
        "text": "En bij de verspreid verspreidenden.",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5626,
        "text": "Bij de scheidend scheidenden.",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5627,
        "text": "Bij de doorgevenden van de Vermaning.",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5628,
        "text": "Als een afwijzing van verontschuldigingen of als een waarschuwing.",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5629,
        "text": "Voorwaar, wat jullie is aangezegd, zal zeker gebeuren.",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5630,
        "text": "Wanneer dan de sterren gedoofd worden.",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5631,
        "text": "En wanneer de hemel gespleten wordt.",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5632,
        "text": "En wanneer de bergen verpulverd worden.",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5633,
        "text": "En wanneer voor de Boodschappers de tijd vastgesteld is.",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5634,
        "text": "(Er wordt gezegd:) \"Tot welke dag is er uitstel gegeven?\"",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5635,
        "text": "Tot de Dag van de Beoordeling.",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5636,
        "text": "En wat doet jullie weten wat Dag van de Beoordeling is?",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5637,
        "text": "Wee die Dag de loochenaars!",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5638,
        "text": "Hebben Wij de vroegeren niet vernietigd?",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5639,
        "text": "Waarna Wij hen deden opvolgen door de lateren?",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5640,
        "text": "Zo behandelen Wij de misdadigers.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5641,
        "text": "Wee die Dag de loochenaars!",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5642,
        "text": "Hebben Wij jullie niet uit een onaanzienlijk water geschapen?",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5643,
        "text": "Toen plaatsten Wij het in een beschermende rustplaats.",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5644,
        "text": "Tot een bekend tijdstip.",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5645,
        "text": "Wij beschikten (erover) en Wij zijn de beste Beschikkers.",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5646,
        "text": "Wee die Dag de loochenaars!",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5647,
        "text": "Hebben Wij de aarde niet tot een plaats ven verzameling gemaakt?",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5648,
        "text": "Zowel voor levenden en doden?",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5649,
        "text": "En Wij plaatsten daarop stevige bergen en Wij schonken jullie helder water.",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5650,
        "text": "Wee die Dag de loochenaars!",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5651,
        "text": "Gaat naar dat wat jullie plachten to loochenen!",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5652,
        "text": "Gaat naar een schaduw (van rook) die drie kolommen heeft.",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5653,
        "text": "Die geen schaduw geeft en die niet baat tegen het vlammende Vuur.",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5654,
        "text": "Zij (de Hel) werpt vonken als kastelen.",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5655,
        "text": "Alsof zij gele kamelen waren.",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5656,
        "text": "Wee die Dag de loochenaars!",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5657,
        "text": "Dit is een Dag waarop zij niet spreken.",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5658,
        "text": "En er wordt hun niet toegestaan zich te verontschuldigen.",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5659,
        "text": "Wee die Dag de loochenaars!",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5660,
        "text": "Dit is de Dag van de Beoordeling, Wij verzamelen jullie en de vroegeren.",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5661,
        "text": "Als jullie dan een list hebben, voert die dan uit.",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5662,
        "text": "Wee die Dag de loochenaars!",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5663,
        "text": "Voorwaar, de Moettaqôen verkeren in schaduwen en bij bronnen.",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5664,
        "text": "En er zijn vruchten die zij wensen.",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5665,
        "text": "(Er wordt gezegd:) \"Eet en drinkt smakelijk voor wat jullie plachten te verrichten.\"",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5666,
        "text": "Voorwaar, zo belonen Wij de weldoeners.",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5667,
        "text": "Wee die Dag de loochenaars!",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5668,
        "text": "Eet en geniet even: voorwaar, jullie zijn misdadigers.",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5669,
        "text": "Wee die Dag de loochenaars!",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5670,
        "text": "En wanneer er tot hen gezegd wordt: \"Buigt jullie (in de shalât),\" dan buigen zij niet.",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5671,
        "text": "Wee die Dag de loochenaars!",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5672,
        "text": "In welk Woord na hem (de Koran) zullen zij dan geloven?",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "nl.siregar",
      "language": "nl",
      "name": "Siregar",
      "englishName": "Siregar",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}