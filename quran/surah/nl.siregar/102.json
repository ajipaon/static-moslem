    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6169,
        "text": "Jullie gaan op in de wedijver om meer.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6170,
        "text": "Totdat jullie in de graven terechtkomen.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6171,
        "text": "Nee, jullie zullen het weten.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6172,
        "text": "Nogmaals, nee! Jullie zullen het weten.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6173,
        "text": "Nee! Als jullie het maar met zekere kennis zouden weten!",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6174,
        "text": "Jullie zullen zeker de Hel zien.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6175,
        "text": "Nogmaals, jullie zullen haar met een zeker oog zien.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6176,
        "text": "Dan zullen jullie op die Dag ondervraagd worden over de genietingen.",
