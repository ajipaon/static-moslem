{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 86,
    "name": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq",
    "englishNameTranslation": "The Morning Star",
    "revelationType": "Meccan",
    "numberOfAyahs": 17,
    "ayahs": [
      {
        "number": 5932,
        "text": "Bij de hemel en de verlichtende ster.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5933,
        "text": "En wat doet jou weten wat de verlichtende ster is?",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5934,
        "text": "De doordringende ster.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5935,
        "text": "Voorwaar, er is geen ziel of er is een bewaker over aangesteld.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5936,
        "text": "Laat de mens er daarom over denken waarvan hij geschapen is.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5937,
        "text": "Geschapen is hij van een spurtend water.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5938,
        "text": "Dat komt van tussen de lendenen en de ribben.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5939,
        "text": "Voorwaar, Hij heeft zeker macht over zijn terugkeer.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5940,
        "text": "Op de Dag dat de harten beproefd worden.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5941,
        "text": "Dan heeft hij geen kracht en geen helper.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5942,
        "text": "Bij de hemel, regen bevattend.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5943,
        "text": "En bij de aarde, planten bevattend.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5944,
        "text": "Voorwaar, hij (de Koran) is zeker een beslissend Woord.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5945,
        "text": "En hij bevat geen scherts.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5946,
        "text": "Voorwaar, zij beramen een plan.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5947,
        "text": "En Ik beraam een plan.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5948,
        "text": "Geef daarom de ongelovigen uitstel, geef hun even uitstel.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      }
    ],
    "edition": {
