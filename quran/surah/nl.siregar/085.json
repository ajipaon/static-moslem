  "data": {
    "number": 85,
    "name": "سُورَةُ البُرُوجِ",
    "englishName": "Al-Burooj",
    "englishNameTranslation": "The Constellations",
    "revelationType": "Meccan",
    "numberOfAyahs": 22,
    "ayahs": [
      {
        "number": 5910,
        "text": "Bij de hemel en zijn sterrenstelsels.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5911,
        "text": "Bij de aangezegde Dag.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5912,
        "text": "Bij de getuige en waarvan getuigd wordt.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5913,
        "text": "Verdoemd zijn de gravers van de kuil.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5914,
        "text": "Van het vuur met zijn brandhout.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5915,
        "text": "Toen zij er omheen zaten.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5916,
        "text": "En zij getuige waren van wat zij de gelovigen aandeden.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5917,
        "text": "En zij wreekten zich alleen op hen omdat zij geloofden in Allah, de Geweldige, de Geprezene.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5918,
        "text": "Degene Die de heerschappij over de hemelen en de aarde heeft. En Allah is Getuige over alle zaken.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5919,
        "text": "Voorwaar, degenen die de gelovige mannen en vrouwen bestraften en daarna geen berouw toonden, voor hen is de bestraffing van de Hel en voor hen is de verbrandende bestraffing.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5920,
        "text": "Voorwaar, degenen die geloven en goede daden verrichten, voor hen is het Paradijs waar de rivieren onder door stromen. Dat is de grote overwinning.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5921,
        "text": "Voorwaar, de greep ven jouw Heer is zeker hard.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5922,
        "text": "Voorwaar, Hij is het die schept en doet herleven.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5923,
        "text": "Hij is de Vergevensgezinde, de Liefdevolle.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5924,
        "text": "Bezitter van de Troon, de Meest Vrijgevige.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5925,
        "text": "Uitvoerder van wat Hij wil.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5926,
        "text": "Heeft het bericht van de legers jou bereikt?",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5927,
        "text": "Van Fir'aun en de Tsamôed?",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5928,
        "text": "Degenen die niet geloven gaan zelfs door met loochenen.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5929,
        "text": "En Allah omsingelt hen van achteren.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5930,
        "text": "Het is zelfs een glorierijke Koran.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5931,
        "text": "In de Lauhoelmahfôezh.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      }
    ],
