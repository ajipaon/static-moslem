{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 89,
    "name": "سُورَةُ الفَجۡرِ",
    "englishName": "Al-Fajr",
    "englishNameTranslation": "The Dawn",
    "revelationType": "Meccan",
    "numberOfAyahs": 30,
    "ayahs": [
      {
        "number": 5994,
        "text": "Bij de dageraad.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5995,
        "text": "Bij de tien nachten. (De eerste tien dagen en nachten van de maand Dzoelhiddjah)",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5996,
        "text": "Bij het even en het oneven.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5997,
        "text": "Bij de nacht wanneer hij voorbijgaat.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5998,
        "text": "Is daarin geen eed voor de bezitter van verstand?",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5999,
        "text": "Heb jij niet vernomen hoe jouw Heer de 'Âd heeft behandeld?",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6000,
        "text": "Van de stad Iram met zijn zuilen?",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6001,
        "text": "Zoals nog nooit een stad is geschapen in de landen?",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6002,
        "text": "En de Tsamôed die de rotsen uithieuwen in de vallei?",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6003,
        "text": "En Fir'aun, de bezitter van de pinnen?",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6004,
        "text": "Degenen die overtraden in het land?",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6005,
        "text": "En daarin veelvuldig verderf zaaiden?",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6006,
        "text": "Toen deed jouw Heer de gesel van de bestraffing op hen neerdalen.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6007,
        "text": "Voorwaar, jouw Heer is zeker waakzaam.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6008,
        "text": "Wat de mens betreft, wanneer zijn Heer hem op de proef stelt en hem aanzien geeft en hem genietingen schenkt, dan zegt hij: \"Mijn Heer heeft mij geëerd.\"",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6009,
        "text": "Maar wanneer Hij hem beproeft, en dan zijn voorzieningen beperkt, dan zegt hij: \"Mijn Heer heeft mij vernederd.\"",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6010,
        "text": "Nee! Jullie ondersteunen immers de wees niet.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6011,
        "text": "En jullie sporen elkaar niet aan tot het voeden van de behoeftigen.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6012,
        "text": "En jullie verteren het erfdeel inhalig.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6013,
        "text": "En jullie beminnen het bezit met overdreven liefde.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6014,
        "text": "Nee, wanneer de aarde met klappen verpulverd wordt.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6015,
        "text": "En jouw Heer komt, en de Engelen, rij na rij.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6016,
        "text": "En op de Dag dat de Hel wordt getoond, op die Dag zal de mens zich (zijn slechte daden) herinneren, maar wat baat hem dan nog de herinnering?",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6017,
        "text": "Hij zegt: \"Wee, had ik maar goede (daden) verricht tijdens mijn leven.\"",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6018,
        "text": "Maar op die Dag is er niemand die straft zoals Hij.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6019,
        "text": "En niemand knevelt zoals Hij knevelt.",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6020,
        "text": "O tot rust gekomen ziel!",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6021,
        "text": "Keer terug tot jouw Heer, behaagd en welbehaagd (ontvangen door Hem).",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6022,
        "text": "En treed binnen onder Mijn dienaren.",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6023,
        "text": "En treed binnen in Mijn Paradijs.",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "nl.siregar",
      "language": "nl",
      "name": "Siregar",
      "englishName": "Siregar",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}