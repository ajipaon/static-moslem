    "englishNameTranslation": "Almsgiving",
    "revelationType": "Meccan",
    "numberOfAyahs": 7,
    "ayahs": [
      {
        "number": 6198,
        "text": "あなたは，審判を嘘であるとする者を見たか。",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6199,
        "text": "かれは，孤児に手荒くする者であり，",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6200,
        "text": "また貧者に食物を与えることを勧めない者である。",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6201,
        "text": "災いなるかな，礼拝する者でありながら，",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6202,
        "text": "自分の礼拝を忽せにする者。",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6203,
        "text": "（人に）見られるための礼拝をし，",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6204,
        "text": "慈善を断わる者に。",
        "numberInSurah": 7,
        "juz": 30,
