  "data": {
    "number": 90,
    "name": "سُورَةُ البَلَدِ",
    "englishName": "Al-Balad",
    "englishNameTranslation": "The City",
    "revelationType": "Meccan",
    "numberOfAyahs": 20,
    "ayahs": [
      {
        "number": 6024,
        "text": "سوگند به اين شهر مى‌خورم‌",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6025,
        "text": "و تو [يكچند] در اين شهر [حكمرواى مطلق و] دست گشاده‌اى‌",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6026,
        "text": "و سوگند به پدر و فرزندى كه پديد آورده است‌",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6027,
        "text": "به راستى انسان را در رنج و محنت كشيدن آفريده‌ايم‌",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6028,
        "text": "آيا مى‌پندارد كه هرگز كسى بر او دست ندارد؟",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6029,
        "text": "گويد مالى فراوان [در راه مبارزه با پيامبر] بر باد دادم‌",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6030,
        "text": "آيا مى‌پندارد كه هيچ كس [كار و بار] او را نديده است؟",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6031,
        "text": "آيا براى او دو چشم نيافريده‌ايم؟",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6032,
        "text": "و زبانى و دو لب‌",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6033,
        "text": "و او را بر سر [هر يك از] دو راه [خير و شر] نياورده‌ايم؟",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6034,
        "text": "ولى خود در پى عقبه نبود",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6035,
        "text": "و تو چه دانى عقبه چيست‌",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6036,
        "text": "آزادسازى برده‌",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6037,
        "text": "يا اطعامى [از بينوايان‌] در روزى گرسنگى زده‌",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6038,
        "text": "در حق يتيمى خويشاوند",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6039,
        "text": "يا بينوايى خاك‌نشين‌",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6040,
        "text": "تا سپس از كسانى باشد كه ايمان آورده‌اند و همديگر را به شكيبايى و همديگر را به مهربانى سفارش كرده‌اند",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6041,
        "text": "اينانند كه اصحاب يمين‌اند",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6042,
        "text": "و كسانى كه به آيات ما انكار ورزيده‌اند، آنانند كه اصحاب شمال‌اند",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6043,
        "text": "بر آنان آتشى چند لايه، [چيره‌] باشد",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      }
    ],
