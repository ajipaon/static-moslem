  "data": {
    "number": 92,
    "name": "سُورَةُ اللَّيۡلِ",
    "englishName": "Al-Lail",
    "englishNameTranslation": "The Night",
    "revelationType": "Meccan",
    "numberOfAyahs": 21,
    "ayahs": [
      {
        "number": 6059,
        "text": "سوگند به شب چون فرو پوشاند",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6060,
        "text": "و سوگند به روز چون روشن شود",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6061,
        "text": "و سوگند به آنكه نرينه و مادينه آفريد",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6062,
        "text": "كه كوشش شما پراكنده [و گونه‌گون‌] است‌",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6063,
        "text": "حال اگر كسى [مالى‌] بخشيد و پروا و پرهيز ورزيد",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6064,
        "text": "و [وعده‌] بهشت را استوار داشت‌",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6065,
        "text": "زودا كه راهش را به سوى خير و آسانى هموار كنيم‌",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6066,
        "text": "و اما كسى كه دريغ‌ورزيد و بى‌نيازى نمود",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6067,
        "text": "و وعده بهشت را دروغ انگاشت‌",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6068,
        "text": "زودا كه راهش را به سوى شر و دشوارى هموار كنيم‌",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6069,
        "text": "و چون به هلاكت افتد مالش به كار او نيايد",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6070,
        "text": "بى‌گمان راهنمايى [بندگان‌] بر ماست‌",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6071,
        "text": "و بى‌گمان [جهان‌] واپسين و نخستين ما راست‌",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6072,
        "text": "پس شما را از آتشى كه زبانه زند هشدار دهم‌",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6073,
        "text": "كه جز شقاوت پيشه وارد آن نشود",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6074,
        "text": "همان كسى كه [حق را] انكار كرد و روى بر تافت‌",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6075,
        "text": "و تقواپيشه از آن دور داشته خواهد شد",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6076,
        "text": "همان كسى كه مالش را مى‌بخشد كه پاكدلى يابد",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6077,
        "text": "و بر او [منت از] نعمت كسى نيست كه بايد جزايش را بدهد",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6078,
        "text": "[و نمى‌كند آنچه مى‌كند] مگر در طلب خشنودى پروردگار بلندمرتبه‌اش‌",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6079,
        "text": "و زودا كه خشنود شود",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      }
    ],
