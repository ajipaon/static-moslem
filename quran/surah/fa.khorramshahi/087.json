  "data": {
    "number": 87,
    "name": "سُورَةُ الأَعۡلَىٰ",
    "englishName": "Al-A'laa",
    "englishNameTranslation": "The Most High",
    "revelationType": "Meccan",
    "numberOfAyahs": 19,
    "ayahs": [
      {
        "number": 5949,
        "text": "به نام پروردگارت كه بلندمرتبه است، تسبيح گوى‌",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5950,
        "text": "همانكه آفريد و سامان بخشيد",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5951,
        "text": "همان كسى كه اندازه آفرينى و رهنمايى كرد",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5952,
        "text": "و همان كسى كه چراگاهها[ى سبز و خرم‌] رويانيد",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5953,
        "text": "و سرانجام آن را خاشاكى [پژمرده و] سياه گردانيد",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5954,
        "text": "زودا كه تو را به خواندن آوريم و از ياد مگذارى‌",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5955,
        "text": "مگر آنچه خدا خواهد، كه او آشكار و نهان را مى‌داند",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5956,
        "text": "و در كار تو آسانى پديد آوريم‌",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5957,
        "text": "پس اندرز ده، اگر [دهى‌] اندرز سود دهد",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5958,
        "text": "زودا كسى كه خشيت پيشه كرده است، پند گيرد",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5959,
        "text": "و شقاوت‌پيشه از آن دورى كند",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5960,
        "text": "همان كسى كه به آتش سترگ درآيد",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5961,
        "text": "آنگاه در آنجا نه بميرد و نه زنده بماند",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5962,
        "text": "به راستى هر كس پاكدلى پيشه كرد، رستگار شد",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5963,
        "text": "و [هر كس‌] كه نام پروردگارش را ياد كرد و نيايش كرد",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5964,
        "text": "حق اين است كه شما زندگانى دنيا را بر مى‌گزينيد",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5965,
        "text": "و حال آنكه آخرت بهتر و پاينده‌تر است‌",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5966,
        "text": "اين [نكته‌] در كتب آسمانى پيشين هم هست‌",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5967,
        "text": "در صحف ابراهيم و موسى‌",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      }
    ],
