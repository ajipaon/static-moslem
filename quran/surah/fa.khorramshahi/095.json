        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6102,
        "text": "به راستى كه انسان را در بهترين قوام آفريده‌ايم‌",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
