{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 78,
    "name": "سُورَةُ النَّبَإِ",
    "englishName": "An-Naba",
    "englishNameTranslation": "The Announcement",
    "revelationType": "Meccan",
    "numberOfAyahs": 40,
    "ayahs": [
      {
        "number": 5673,
        "text": "Birbirlerine neyi soruyorlar?",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5674,
        "text": "O büyük haberden (kıyametten) mi?",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5675,
        "text": "Ki onlar onda ayrılığa düşmektedirler.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5676,
        "text": "Hayır, ilerde bilecekler.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5677,
        "text": "Hayır hayır, ilerde bilecekler.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5678,
        "text": "Biz yeryüzünü bir beşik yapmadık mı?",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5679,
        "text": "Dağları da birer kazık kılmadık mı?",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5680,
        "text": "Sizleri çift çift yarattık.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5681,
        "text": "Uykunuzu bir dinlenme yaptık.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5682,
        "text": "Geceyi bir örtü yaptık.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5683,
        "text": "Gündüzü de bir geçim zamanı yaptık.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5684,
        "text": "Üstünüze yedi sağlam bina (gök) çattık.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5685,
        "text": "İçlerine ışık saçan bir kandil astık.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5686,
        "text": "Yoğunlaşmış bulutlardan şarıl şarıl bir su indirdik.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5687,
        "text": "Onunla taneler ve otlar çıkaralım diye.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5688,
        "text": "Ve sarmaş dolaş bağlar bahçeler (çıkaralım diye).",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5689,
        "text": "Kuşkusuz o hüküm günü kararlaştırılmış bir vakit olmuştur.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5690,
        "text": "O gün Sûr'a üflenir, bölük bölük gelirsiniz.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5691,
        "text": "Gök de açılmış, kapı kapı olmuştur.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5692,
        "text": "Dağlar yürütülmüş, serap olmuştur.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5693,
        "text": "Kuşkusuz Cehennem gözetleme yeri olmuştur.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5694,
        "text": "Azgınlar için son varılacak yer olmuştur.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5695,
        "text": "Orada çağlarca kalacaklardır.",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5696,
        "text": "Orada ne bir serinlik tadacaklar, ne de içecek bir şey.",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5697,
        "text": "Ancak bir kaynar su ve irin (içecekler).",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5698,
        "text": "Bir ceza ki tam yaptıklarına uygun.",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5699,
        "text": "Çünkü onlar hiçbir hesap ummazlardı.",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5700,
        "text": "Âyetlerimizi yalanlaya yalanlaya tam bir yalancı olmuşlardı.",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5701,
        "text": "Biz ise herşeyi sayıp bir kitaba geçirmişiz.",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5702,
        "text": "(Onlara): \"Şimdi tadın (cezanızı). Artık size azabınızı artırmaktan başka bir şey yapmayacağız\" (denir).",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5703,
        "text": "Kuşkusuz takva sahipleri için bir kurtuluş var.",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5704,
        "text": "Bahçeler var, bağlar var.",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5705,
        "text": "Memeleri tomurcuklanmış yaşıt kızlar var.",
        "numberInSurah": 33,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5706,
        "text": "Dopdolu kadehler var.",
        "numberInSurah": 34,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5707,
        "text": "Orada ne boş bir söz işitirler, ne de bir yalan.",
        "numberInSurah": 35,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5708,
        "text": "(Bunlar) Rabbinden yeterli bir bağış olarak (verilir).",
        "numberInSurah": 36,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5709,
        "text": "O, göklerin, yerin ve bu ikisi arasındakilerin Rabbidir. Rahmân'dır. Hiç kimse ondan bir hitaba mâlik olamaz.",
        "numberInSurah": 37,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5710,
        "text": "O gün Ruh ve melekler sıra sıra dururlar. Rahmân'ın izin verdikleri dışında hiç kimse konuşamaz. İzin verilen de doğruyu söyler.",
        "numberInSurah": 38,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5711,
        "text": "İşte bu hak gündür. Artık dileyen Rabbine bir yol tutar.",
        "numberInSurah": 39,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5712,
        "text": "Biz sizi yakın bir azap ile uyardık. O gün kişi ellerinin ne takdim ettiğine bakacak ve kâfir diyecek ki: \"Ah ne olaydı, ben bir toprak olaydım.\"",
        "numberInSurah": 40,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "tr.yazir",
      "language": "tr",
      "name": "Elmalılı Hamdi Yazır",
      "englishName": "Elmalili Hamdi Yazir",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}