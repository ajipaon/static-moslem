{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 53,
    "name": "سُورَةُ النَّجۡمِ",
    "englishName": "An-Najm",
    "englishNameTranslation": "The Star",
    "revelationType": "Meccan",
    "numberOfAyahs": 62,
    "ayahs": [
      {
        "number": 4785,
        "text": "İnmekte olan yıldıza andolsun ki,",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4786,
        "text": "Arkadaşınız (Muhammed) sapmadı, azmadı.",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4787,
        "text": "O, hevâdan (arzularına göre) konuşmaz.",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4788,
        "text": "O(nun konuşması kendisine ) vahyedilenden başkası değildir.",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4789,
        "text": "Onu, müthiş kuvvetleri olan biri öğretti",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4790,
        "text": "(Ki o) akıl ve görüşünde kuvvetli (bir melek)dir. Hemen (gerçek meleklik şekliyle) doğruldu.",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4791,
        "text": "O, en yüksek ufukta idi.",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4792,
        "text": "Sonra (Cebrail ona) yaklaştı ve (aşağıya doğru) sarktı.",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4793,
        "text": "Onunla arasındaki mesafe, iki yay kadar, yahut daha az kaldı.",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4794,
        "text": "(Allah), kuluna verdiği vahyi verdi.",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4795,
        "text": "Onun gördüğünü kalb(i) yalanlamadı.",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4796,
        "text": "Onun gördükleri hakkında şimdi kendisi ile tartışacak mısınız.",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4797,
        "text": "Andolsun onu bir kez daha görmüştü.",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4798,
        "text": "Sidretü'lMüntehâ'nın yanında.",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4799,
        "text": "Ki Cennetü'lMe'vâ onun yanındadır.",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4800,
        "text": "Sidre'yi kaplayan kaplıyordu.",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4801,
        "text": "(Peygamberin) gözü şaşmadı ve sınırı aşmadı.",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4802,
        "text": "Andolsun ki o, Rabbinin âyetlerinden en büyüğünü gördü.",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4803,
        "text": "Siz de gördünüz değil mi o Lât ve Uzza'yı?",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4804,
        "text": "Ve üçüncü olarak da öteki (put) Menat'ı?",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4805,
        "text": "Size erkek O'na dişi öyle mi?",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4806,
        "text": "Öyle ise bu çok insafsızca bir taksim.",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4807,
        "text": "Onlar hiçbir şey değil, sırf sizin ve babalarınızın taktığınız (boş) isimlerdir. Allah onlar hakkında hiçbir delil indirmedi. Onlar yalnız zanna ve nefislerin sevdasına uyuyorlar. Halbuki onlara Rableri tarafından yol gösterici gelmiştir.",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4808,
        "text": "Yoksa her arzu ettiği şey, insanın kendisinin mi (olacak) dir?",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4809,
        "text": "Son da ilk de (ahiret de dünya da) Allah'ındır.",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4810,
        "text": "Göklerde nice melek var ki Allah'ın dileyip razı olduğuna izin vermeden önce onların şefaatları hiç bir işe yaramaz.",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4811,
        "text": "Ahirete iman etmeyenler meleklere dişilerin adlarını takıp duruyorlar",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4812,
        "text": "Onların bu hususta bir bilgileri yoktur. Sadece zanna uyuyorlar. Zan ise, şüphesiz hakikat bakımından birşey ifade etmez.",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4813,
        "text": "Onun için bizi anmaktan yüz çeviren ve dünya hayatından başka bir şey istemeyenlerden yüz çevir.",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4814,
        "text": "İşte onların ilimden erişebilecekleri (son sınır) budur. Şüphesiz, Rabbin, yolundan sapanı da iyi bilir; O, hidayette olanı da iyi bilir.",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4815,
        "text": "Göklerde ve yerde bulunanlar hep Allah'ındır. Akıbet (sonuçta) kötülük yapanları yaptıkları ile cezalandıracak, güzel davrananları da daha güzeliyle mükafatlandıracaktır.",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4816,
        "text": "Onlar ki günahın büyüklerinden ve çirkin işlerden kaçınırlar, yalnız bazı küçük kusurlar hariç. Şüphesiz Rabbinin affı geniştir. O, sizi daha topraktan yarattığı zaman ve siz annelerinizin karınlarında bulunduğunuz sırada, sizi en iyi bilendir. Bunun için kendinizi temize çıkarmayın. Çünkü O, kötülükten sakınanı daha iyi bilir.",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4817,
        "text": "Şimdi gördün mü O yüz çevireni?",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4818,
        "text": "Azıcık verip (sonra vermemekte) direneni?",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4819,
        "text": "Gaybın bilgisi kendi yanındadır da, o mu görüyor?",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4820,
        "text": "Yoksa haber verilmedi mi Musa'nın sahifelerinde yazılı olanlar?",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4821,
        "text": "Ve çok vefakâr olan İbrahim'in sahifelerindekiler?",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4822,
        "text": "Ki hiçbir günahkâr başkasının günah yükünü yüklenmez.",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4823,
        "text": "Doğrusu insana çalışmasından başka bir şey yoktur.",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4824,
        "text": "Ve çalışması da yakında görülecektir.",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4825,
        "text": "Sonra ona karşılığı tastamam verilecektir.",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4826,
        "text": "Ve şüphesiz en son varış, Rabbinedir.",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4827,
        "text": "Doğrusu güldüren de ağlatan da O'dur.",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4828,
        "text": "Öldüren de dirilten de O'dur.",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4829,
        "text": "Şüphesiz erkeği, dişiyi iki eş yaratan O'dur,",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4830,
        "text": "Atıldığı zaman bir nutfeden.",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4831,
        "text": "Şüphesiz tekrar diriltmek de O'na aittir.",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4832,
        "text": "Şüphesiz zengin eden de sermaye veren de O'dur.",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4833,
        "text": "Doğrusu Şi'râ yıldızının Rabbi O'dur.",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4834,
        "text": "O, helak etti önce gelen Âd'ı.",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4835,
        "text": "Ve Semûd'u da bırakmadı.",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4836,
        "text": "Önceden de Nuh kavmini (helak etmişti), çünkü onlar zulmetmiş ve azmıştı.",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4837,
        "text": "Altı üstüne getirilmiş şehirleri devirip yıktı.",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4838,
        "text": "Onları neler kapladı neler!",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4839,
        "text": "O halde Rabbinin hangi nimetinden kuşku duyuyorsun.",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4840,
        "text": "Bu da ilk uyarıcılardan bir uyarıcıdır.",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4841,
        "text": "Yaklaşan yaklaştı.",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4842,
        "text": "Onu Allah'tan başka açığa çıkaracak yoktur.",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4843,
        "text": "Şimdi siz bu sözden mi hayret ediyorsunuz?",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4844,
        "text": "Gülüyorsunuz da ağlamıyorsunuz?",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4845,
        "text": "Ve siz mi kafa tutuyorsunuz ey gafiller?",
        "numberInSurah": 61,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4846,
        "text": "Haydi Allah için secdeye kapanın ve O'na kulluk edin.",
        "numberInSurah": 62,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": {
          "id": 13,
          "recommended": false,
          "obligatory": true
        }
      }
    ],
