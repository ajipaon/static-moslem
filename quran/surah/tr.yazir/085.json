  "data": {
    "number": 85,
    "name": "سُورَةُ البُرُوجِ",
    "englishName": "Al-Burooj",
    "englishNameTranslation": "The Constellations",
    "revelationType": "Meccan",
    "numberOfAyahs": 22,
    "ayahs": [
      {
        "number": 5910,
        "text": "Burçlar sahibi gökyüzüne,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5911,
        "text": "Vaad olunan o güne,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5912,
        "text": "Şahitlik edene ve edilene andolsun ki,",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5913,
        "text": "Kahroldu o hendeğin sahipleri,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5914,
        "text": "O çıralı ateşin,",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5915,
        "text": "Hani o ateşin başına oturmuşlar,",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5916,
        "text": "Müminlere yaptıklarını seyrediyorlardı.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5917,
        "text": "Müminlere kızmalarının sebebi de, onların yalnız çok güçlü ve övgüye lâyık olan Allah'a iman etmeleri idi.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5918,
        "text": "O Allah ki, göklerin ve yerin hükümranlığı O'nundur ve Allah her şeye şahittir.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5919,
        "text": "İnanan erkek ve kadınlara işkence yapıp sonra da tevbe etmeyenlere cehennem azabı ve yangın azabı vardır.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5920,
        "text": "İnanan ve iyi amel yapanlar için de altından ırmaklar akan cennetler vardır. İşte büyük kurtuluş odur.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5921,
        "text": "Kuşkusuz Rabbinin yakalaması serttir.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5922,
        "text": "Yoktan o yaratır ve tekrar o diriltir.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5923,
        "text": "Bununla beraber çok bağışlayandır, çok sevendir.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5924,
        "text": "Arş'ın sahibidir, yücedir.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5925,
        "text": "Dilediğini yapandır.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5926,
        "text": "O orduların kıssası sana geldi mi?",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5927,
        "text": "Yani Firavun ve Semud'un?",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5928,
        "text": "Fakat o inkarcılar hâlâ bir yalanlama içinde.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5929,
        "text": "Oysa Allah onları arkalarından kuşatmıştır.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5930,
        "text": "Hayır o şerefli bir Kur'ân'dır.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5931,
        "text": "Levhi Mahfuz'dadır.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      }
    ],
