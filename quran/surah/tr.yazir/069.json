{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 69,
    "name": "سُورَةُ الحَاقَّةِ",
    "englishName": "Al-Haaqqa",
    "englishNameTranslation": "The Reality",
    "revelationType": "Meccan",
    "numberOfAyahs": 52,
    "ayahs": [
      {
        "number": 5324,
        "text": "(Gerçekleşecek) Kıyamet!",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5325,
        "text": "Nedir, o Kıyamet?",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5326,
        "text": "Gerçekleşenin (Kıaymetin) ne olduğunu sen nerden bileceksin?",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5327,
        "text": "Semûd ve Âd, kapılarını çalacak olan o felaketi yalan saymışlardı.",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5328,
        "text": "Semûd kavmi korkunç bir sesle yok edildi.",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5329,
        "text": "Âd kavmi ise gürültülü ve azgın bir fırtına ile yok edildiler.",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5330,
        "text": "Allah o fırtınayı üzerlerine yedi gece sekiz gündüz musallat etmişti. Öyle ki, o kavmi içi boş hurma kütükleri gibi oracıkta yere serilmiş halde görürdün.",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5331,
        "text": "Bak şimdi görebilir misin onlardan bir kalıntı?",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5332,
        "text": "Firavun, ondan öncekiler ve altı üstüne getirilen beldeler de hep o hatayı işleyegeldiler.",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5333,
        "text": "Hep Rablerinin elçilerine karşı geldiler. O da onları pek şiddetli bir şekilde yakalayıverdi.",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5334,
        "text": "Kuşkusuz, sular kabarınca sizi gemide biz taşıdık.",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5335,
        "text": "Onu size bir ibret yapalım ve belleyici kulaklar bellesin diye.",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5336,
        "text": "Sûr'a bir tek üfleme üflendiği,",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5337,
        "text": "Arz ve dağlar yerlerinden kaldırılıp şiddetle birbirine çarpılarak darmadağın olduğu zaman,",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5338,
        "text": "İşte o gün olacak olur.",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5339,
        "text": "O gün gök yarılmış, sarkmıştır.",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5340,
        "text": "Melekler de onun etrafındadır, O gün Rabbinin Arşını bunların da üstünde sekiz melek yüklenir.",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5341,
        "text": "O gün (hesap için Allah'a) arz olunursunuz, öyle ki gizli bir haliniz kalmaz.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5342,
        "text": "Kitabı sağından verilen, \"alın okuyun kitabımı..\"",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5343,
        "text": "\"Çünkü ben hesabıma kavuşacağımı sezmiştim\" der.",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5344,
        "text": "Artık o hoşnut bir hayattadır.",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5345,
        "text": "Yüksek bir cennettedir.",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5346,
        "text": "Ki o cennetin meyveleri sarkmıştır.",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5347,
        "text": "\"Geçmiş günlerde yaptığınız işlerden ötürü afiyetle yeyin, için.\" (denir).",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5348,
        "text": "Kitabı sol tarafından verilen ise der ki: \"Keşke kitabım verilmeseydi de,",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5349,
        "text": "Hesabımın ne olduğunu bilmeseydim,",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5350,
        "text": "Ne olurdu o ölüm, iş bitirici olsaydı.",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5351,
        "text": "Malım bana hiç fayda vermedi.",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5352,
        "text": "Gücüm de benden yok olup gitti.\"",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5353,
        "text": "(Zebanilere şöyle denir): \"Onu yakalayın da bağlayın.\"",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5354,
        "text": "\"Sonra cehenneme atın onu.\"",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5355,
        "text": "\"Sonra da boyu yetmiş arşın zincir içerisinde onu oraya sokun.\"",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5356,
        "text": "Çünkü o, büyük Allah'a inanmıyordu.",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5357,
        "text": "Yoksula yedirmeye teşvik etmiyordu.",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5358,
        "text": "Bu sebeple bugün burada onun candan bir dostu yoktur.",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5359,
        "text": "Bir irinden başka yiyecek de yok.",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5360,
        "text": "Onu günahkârlardan başkası yemez.",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5361,
        "text": "Andolsun gördüklerinize,",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5362,
        "text": "Ve görmediklerinize..",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5363,
        "text": "Kuşkusuz Kur'ân, şerefli bir peygamberin (Allah'tan) getirdiği sözdür.",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5364,
        "text": "O bir şair sözü değildir, siz çok az inanıyorsunuz.",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5365,
        "text": "Bir kâhin sözü de değildir, ne de az düşünüyorsunuz!",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5366,
        "text": "O, âlemlerin Rabbi tarafından indirilmedir.",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5367,
        "text": "O, bize isnâden bazı sözler uydurmaya kalkışsaydı,",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5368,
        "text": "Elbette biz onu bundan dolayı kuvvetle yakalardık.",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5369,
        "text": "Sonra da onun şah damarını keser atardık.",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5370,
        "text": "O vakit sizden hiçbiriniz ona siper de olamazdınız.",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5371,
        "text": "O hiç kuşkusuz, takva sahipleri için unutulmayacak bir öğüttür.",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5372,
        "text": "Bununla beraber biz biliyoruz ki sizden inanmayanlar var.",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5373,
        "text": "Kuşkusuz bu Kur'ân kafirler için bir pişmanlık vesilesidir.",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5374,
        "text": "Gerçekten o, şüphe götürmez bir bilgidir.",
        "numberInSurah": 51,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5375,
        "text": "O halde, haydi tesbih et Rabbinin yüce ismiyle",
        "numberInSurah": 52,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "tr.yazir",
      "language": "tr",
      "name": "Elmalılı Hamdi Yazır",
      "englishName": "Elmalili Hamdi Yazir",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}