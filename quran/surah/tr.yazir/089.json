{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 89,
    "name": "سُورَةُ الفَجۡرِ",
    "englishName": "Al-Fajr",
    "englishNameTranslation": "The Dawn",
    "revelationType": "Meccan",
    "numberOfAyahs": 30,
    "ayahs": [
      {
        "number": 5994,
        "text": "Andolsun fecre.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5995,
        "text": "On geceye (Zilhicce ayının ilk on gecesine).",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5996,
        "text": "Çifte ve teke.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5997,
        "text": "Gitmekte olan geceye.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5998,
        "text": "Nasıl, bunlarda bir akıl sahibi için yemin var değil mi?",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5999,
        "text": "Görmedin mi Rabbin ne yaptı Âd kavmine?",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6000,
        "text": "Sütunlar sahibi İrem'e?",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6001,
        "text": "Ki ülkeler içinde onun benzeri yaratılmamıştı.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6002,
        "text": "Vâdide kayaları yontan Semud kavmine?",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6003,
        "text": "Kazıklar sahibi (güçlü, kuvvetli) Firavun'a?",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6004,
        "text": "Bunlar ülkelerde azmışlardı.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6005,
        "text": "Oralarda çok bozgunculuk yapmışlardı.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6006,
        "text": "Bu yüzden Rabbin onların üstüne azap kamçısı yağdırdı.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6007,
        "text": "Kuşkusuz Rabbin her an gözetlemededir.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6008,
        "text": "Ama insan, her ne zaman Rabbi onu sınayıp da ikramda bulunur, nimet verirse, \"Rabbim bana ikram etti.\" der.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6009,
        "text": "Ama her ne zaman da sınayıp rızkını daraltırsa, o vakit de, \"Rabbim beni zillete düşürdü.\" der.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6010,
        "text": "Hayır hayır, doğrusu siz yetime ikram etmiyorsunuz.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6011,
        "text": "Birbirinizi yoksulu yedirmeye teşvik etmiyorsunuz.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6012,
        "text": "Oysa mirası öyle bir yiyorsunuz ki, haramhelal gözetmeden.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6013,
        "text": "Malı öyle bir seviyorsunuz ki, yığmacasına.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6014,
        "text": "Hayır hayır, yer birbiri ardınca sarsılıp dümdüz olduğu zaman,",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6015,
        "text": "Rabbinin emri gelip melekler sıra sıra dizildiği zaman,",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6016,
        "text": "Ki cehennem de o gün getirilmiştir. İşte o gün insan anlar. Fakat bu anlamanın ona ne yararı var?",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6017,
        "text": "\"Keşke hayatım için bir şeyler yapıp gönderseydim.\" der.",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6018,
        "text": "Artık o gün Allah'ın edeceği azabı kimse edemez.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6019,
        "text": "Onun vuracağı bağı kimse vuramaz.",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6020,
        "text": "Ey, Rabbine, itaat edip huzura eren nefis!",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6021,
        "text": "Hem hoşnut edici, hem de hoşnut edilmiş olarak Rabbine dön.",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6022,
        "text": "Kullarımın arasına gir.",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6023,
        "text": "Cennetime gir.",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "tr.yazir",
      "language": "tr",
      "name": "Elmalılı Hamdi Yazır",
      "englishName": "Elmalili Hamdi Yazir",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}