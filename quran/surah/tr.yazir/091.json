  "data": {
    "number": 91,
    "name": "سُورَةُ الشَّمۡسِ",
    "englishName": "Ash-Shams",
    "englishNameTranslation": "The Sun",
    "revelationType": "Meccan",
    "numberOfAyahs": 15,
    "ayahs": [
      {
        "number": 6044,
        "text": "Güneş'e ve onun parıltısına,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6045,
        "text": "Güneş'in ardından gelen Ay'a,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6046,
        "text": "Güneş'i açıp ortaya çıkaran gündüze,",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6047,
        "text": "Onu örten geceye,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6048,
        "text": "Göğe ve onu bina edene,",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6049,
        "text": "Yere ve onu döşeyene,",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6050,
        "text": "Nefse ve onu biçimlendirene,",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6051,
        "text": "Sonra da ona kötülük ve takva kabiliyetini verene yemin olsun ki,",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6052,
        "text": "Elbette nefsini temizleyip parlatan kurtulmuştur.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6053,
        "text": "Onu kirletip gömen de ziyan etmiştir.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6054,
        "text": "Semud, azgınlığıyla Hakk'ı yalanladı,",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6055,
        "text": "En azgınları ileri atılınca,",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6056,
        "text": "Allah'ın Rasulü (Salih peygamber) onlara: \"Allah'ın devesini ve onun su nöbetini gözetin.\" demişti.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6057,
        "text": "Fakat onlar peygamberi yalanlayıp deveyi kestiler. Rableri de günahlarını başlarına geçiriverdi de orayı dümdüz etti.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6058,
        "text": "Öyle ya, Allah bu işin sonundan korkacak değil ya.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "tr.yazir",
      "language": "tr",
      "name": "Elmalılı Hamdi Yazır",
      "englishName": "Elmalili Hamdi Yazir",
      "format": "text",
