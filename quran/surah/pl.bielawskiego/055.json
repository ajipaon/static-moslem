{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 55,
    "name": "سُورَةُ الرَّحۡمَٰن",
    "englishName": "Ar-Rahmaan",
    "englishNameTranslation": "The Beneficent",
    "revelationType": "Medinan",
    "numberOfAyahs": 78,
    "ayahs": [
      {
        "number": 4902,
        "text": "Miłosierny!",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4903,
        "text": "On nauczył Koranu.",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4904,
        "text": "On stworzył człowieka.",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4905,
        "text": "On nauczył go jasnego wyrażania się.",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4906,
        "text": "Słońce i księżyc podlegają pewnemu rachunkowi.",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4907,
        "text": "Trawy i drzewa wybijają pokłony.",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4908,
        "text": "I niebo - On wzniósł je i ustawił wagę,",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4909,
        "text": "Abyście ważąc nie oszukiwali.",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4910,
        "text": "Ustawiajcie wagi sprawiedliwie i nie powodujcie straty na wadze!",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4911,
        "text": "I ziemię - On ją rozpostarł dla stworzeń.",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4912,
        "text": "Na niej są owoce i palmy z wiechami,",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4913,
        "text": "Ziarna w kłosach i rośliny aromatyczne.",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4914,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4915,
        "text": "On stworzył człowieka z gliny jak glina garncarzy;",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4916,
        "text": "On stworzył dżiny z czystego ognia.",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4917,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4918,
        "text": "On - Pan dwóch wschodów, On - Pan dwóch zachodów!",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4919,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4920,
        "text": "On puścił wolno dwa morza, aby mogły się spotkać;",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4921,
        "text": "Między nimi jest przegroda, której nie przekraczają.",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4922,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4923,
        "text": "Z nich obu pochodzą perły i korale.",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4924,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4925,
        "text": "Do Niego należą okręty wysoko wzniesione na morzu, jak góry.",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4926,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4927,
        "text": "Wszystko, co jest na niej, przeminie,",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4928,
        "text": "A pozostanie oblicze twego Pana, pełne majestatu i godne czci.",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4929,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4930,
        "text": "Błagają Go ci, którzy są w niebiosach i na ziemi; On każdego dnia tworzy jakieś dzieło.",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4931,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4932,
        "text": "My niebawem będziemy mieć czas dla was, o wy, dwa ciężary!",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4933,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4934,
        "text": "O zgromadzenie dżinów i ludzi! Jeśli potraficie przeniknąć przez regiony niebios i ziemi, to przenikajcie! Lecz wy nie potraficie przeniknąć inaczej, jak tylko posiadając władzę.",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4935,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4936,
        "text": "I będzie wysłany przeciwko wam płomień ognia i miedź roztopiona,. a wy nie otrzymacie pomocy.",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4937,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4938,
        "text": "A kiedy niebo rozerwie się i stanie się szkarłatne jak czerwona skóra",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4939,
        "text": "- Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo? -",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4940,
        "text": "Tego Dnia nie zostaną zapytani o swoje grzechy ani ludzie, ani dżiny.",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4941,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4942,
        "text": "Grzesznicy będą rozpoznani przez swoje znamiona i zostaną pochwyceni za czupryny i za nogi.",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4943,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4944,
        "text": "Oto Gehenna, którą grzesznicy uważają za kłamstwo!",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4945,
        "text": "Krążą oni między nią i wrzącą wodą.",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4946,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4947,
        "text": "A tego, który się lęka stanąć przed swoim Panem, czekają dwa Ogrody",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4948,
        "text": "- Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo? -",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4949,
        "text": "Posiadające bujne gałęzie.",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4950,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4951,
        "text": "W nich są dwa płynące źródła.",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4952,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4953,
        "text": "W nich będą dwa rodzaje wszystkich owoców.",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4954,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4955,
        "text": "Będą oni wypoczywali podparci na łokciach na łożach wyszywanych brokatem, a owoce tych dwóch Ogrodów będą blisko.",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4956,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4957,
        "text": "Będą tam dziewice o skromnym spojrzeniu, których nie dotknął przed nimi ani żaden człowiek, ani dżin.",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4958,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4959,
        "text": "Będą one jak rubin i koral.",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4960,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4961,
        "text": "Czy zapłata za dobro może być inna niż dobro?",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4962,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 61,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4963,
        "text": "Oprócz tych dwóch będą jeszcze inne dwa Ogrody",
        "numberInSurah": 62,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4964,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 63,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4965,
        "text": "- dwa Ogrody o ciemnej zieleni.",
        "numberInSurah": 64,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4966,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 65,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4967,
        "text": "W nich będą dwa źródła tryskające obficie wodą.",
        "numberInSurah": 66,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4968,
        "text": "Przeto które z dobrodziejstw waszego Pana uważacie za kłamstwo?",
        "numberInSurah": 67,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4969,
        "text": "W nich będą owoce i palmy, i granaty.",
        "numberInSurah": 68,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4970,
