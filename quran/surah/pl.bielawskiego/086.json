  "data": {
    "number": 86,
    "name": "سُورَةُ الطَّارِقِ",
    "englishName": "At-Taariq",
    "englishNameTranslation": "The Morning Star",
    "revelationType": "Meccan",
    "numberOfAyahs": 17,
    "ayahs": [
      {
        "number": 5932,
        "text": "Na niebo i na gwiazdę nocną!",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5933,
        "text": "A co ciebie pouczy, co to jest gwiazda nocna?",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5934,
        "text": "To jest gwiazda przenikająca.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5935,
        "text": "Każda dusza ma nad sobą stróża!",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5936,
        "text": "Niech więc rozważy człowiek, z czego on został stworzony!",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5937,
        "text": "Został stworzony z cieczy wytryskającej,",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5938,
        "text": "Która wychodzi spomiędzy lędźwi i żeber.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5939,
        "text": "Zaprawdę, On jest władny sprowadzić go",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5940,
        "text": "W Dniu, kiedy będą badane skryte myśli.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5941,
        "text": "Nie będzie on miał wówczas ani siły, ani pomocnika!",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5942,
        "text": "Na niebo, które włada powrotem!",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5943,
        "text": "Na ziemię posiadającą rozpadliny!",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5944,
        "text": "Zaprawdę, to jest słowo rozstrzygające,",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5945,
        "text": "Nie żadna zabawa!",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5946,
        "text": "Zaprawdę, oni zamyślają podstęp!",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5947,
        "text": "I Ja zamyślam podstęp!",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5948,
        "text": "Daj więc nieco wytchnienia niewiernym; daj im jeszcze chwilę zwłoki!",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      }
    ],
