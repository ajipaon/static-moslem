{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 74,
    "name": "سُورَةُ المُدَّثِّرِ",
    "englishName": "Al-Muddaththir",
    "englishNameTranslation": "The Cloaked One",
    "revelationType": "Meccan",
    "numberOfAyahs": 56,
    "ayahs": [
      {
        "number": 5496,
        "text": "O ty, okryty płaszczem!",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5497,
        "text": "Powstań i ostrzegaj!",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5498,
        "text": "I twego Pana wysławiaj!",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5499,
        "text": "I twoje szaty oczyszczaj!",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5500,
        "text": "Od obrzydliwości - uciekaj!",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5501,
        "text": "Nie obdarzaj, by otrzymać więcej!",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5502,
        "text": "I dla twojego Pana bądź cierpliwy!",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5503,
        "text": "A kiedy zadmą w trąbę,",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5504,
        "text": "Tego Dnia dzień będzie trudny;",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5505,
        "text": "Dla niewiernych - nie będzie on łatwy.",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5506,
        "text": "Pozostaw Mnie samego z tym, którego stworzyłem!",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5507,
        "text": "Ja dałem mu rozległy majątek",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5508,
        "text": "I synów tu obecnych.",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5509,
        "text": "I wyrównałem dla niego wszystko,",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5510,
        "text": "A on pożąda, abym mu dał więcej.",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5511,
        "text": "Otóż nie! On okazał się dla Naszych znaków oporny!",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5512,
        "text": "Ja nałożę na niego nieustanne cierpienie.",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5513,
        "text": "Oto on - pomyślał i zdecydował.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5514,
        "text": "Przeto niech zginie, jak zdecydował!",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5515,
        "text": "Jeszcze raz niech zginie, jak zdecydował!",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5516,
        "text": "Potem on popatrzył!",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5517,
        "text": "Potem zmarszczył brwi i zasępił się!",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5518,
        "text": "Potem odwrócił się, wbił się w pychę",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5519,
        "text": "I powiedział: \"To nic innego, jak tylko czary, które się przekazuje!",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5520,
        "text": "To nic innego, jak tylko słowo ludzi śmiertelnych!\"",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5521,
        "text": "Ja narażę go na palenie się w ogniu-sakar.",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5522,
        "text": "A co ciebie pouczy, co to jest sakar?",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5523,
        "text": "On niczego nie zostawia i niczego nie oszczędza,",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5524,
        "text": "Palący na czarno skórę.",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5525,
        "text": "Nad nim czuwa dziewiętnastu.",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5526,
        "text": "Uczyniliśmy stróżami ognia tylko aniołów. Uczyniliśmy taką ich liczbę tylko po to, aby doświadczyć tych, którzy nie uwierzyli; aby przekonali się o prawdzie ci, którym została dana Księga; aby powiększyła się wiara tych, którzy uwierzyli; aby nie wątpili ci, którzy otrzymali Księgę, oraz wierni; aby powiedzieli ci, w których sercach tkwi choroba, oraz niewierni: \"Jaki Bóg chciał dać przykład przez to?\" W ten sposób Bóg prowadzi w zabłądzenie, kogo chce, i On prowadzi drogą prostą, kogo chce. Nikt nie zna wojsk twego Pana, jak tylko On. To jest tylko przypomnienie dla ludzi.",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5527,
        "text": "Ależ nie! Na księżyc!",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5528,
        "text": "I na noc, kiedy się cofa!",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5529,
        "text": "I na zorzę poranną, kiedy się zjawia!",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5530,
        "text": "Zaprawdę, on jest jednym z największych!",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5531,
        "text": "Jest ostrzeżeniem dla ludzi,",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5532,
        "text": "Dla tych spośród was, którzy chcą się posuwać naprzód lub którzy chcą się cofać.",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5533,
        "text": "Każda dusza jest poręką na to, co sobie zyskała",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5534,
        "text": "- z wyjątkiem ludzi będących po prawicy.",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5535,
        "text": "Oni w Ogrodach będą się nawzajem wypytywać",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5536,
        "text": "O grzeszników:",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5537,
        "text": "\"Co doprowadziło was do ognia-sakar?\"",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5538,
        "text": "Powiedzą oni: \"Nie byliśmy wśród modlących się;",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5539,
        "text": "Nie dawaliśmy pożywienia ubogim;",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5540,
        "text": "Pogrążaliśmy się w daremnej dyspucie wraz z pogrążającymi się;",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5541,
        "text": "I uznaliśmy za kłamstwo Dzień Sądu,",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5542,
        "text": "- aż przyszła do nas pewność.\"",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5543,
        "text": "I nie pomoże im wstawiennictwo wstawiających się.",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5544,
        "text": "Cóż im się stało, że odwracają się od napomnienia,",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5545,
        "text": "Jak gdyby byli wystraszonymi osłami,",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5546,
        "text": "Które uciekają przed lwem?",
        "numberInSurah": 51,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5547,
        "text": "A przecież każdy z nich chce, aby mu dano karty rozpostarte.",
        "numberInSurah": 52,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5548,
        "text": "Ależ nie! Oni nie obawiają się życia ostatecznego!",
        "numberInSurah": 53,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5549,
        "text": "Ależ nie! Zaprawdę, to jest napomnienie!",
        "numberInSurah": 54,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5550,
        "text": "Przeto ten, kto zechce, przypomni to sobie!",
        "numberInSurah": 55,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5551,
        "text": "Lecz oni przypomną sobie tylko, jeśli zechce Bóg. On jest godny bojaźni i On włada przebaczeniem.",
        "numberInSurah": 56,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "pl.bielawskiego",
