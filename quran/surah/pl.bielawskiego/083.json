{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 83,
    "name": "سُورَةُ المُطَفِّفِينَ",
    "englishName": "Al-Mutaffifin",
    "englishNameTranslation": "Defrauding",
    "revelationType": "Meccan",
    "numberOfAyahs": 36,
    "ayahs": [
      {
        "number": 5849,
        "text": "Biada oszustom!",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5850,
        "text": "Tym, którzy kiedy otrzymują, domagają się od ludzi pełnej miary;",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5851,
        "text": "A kiedy mierzą lub ważą dla nich, to przyczyniają się do ich straty.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5852,
        "text": "Czyż oni nie myślą, iż będą wskrzeszeni",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5853,
        "text": "Dnia wielkiego,",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5854,
        "text": "Dnia, kiedy ludzie staną przed Panem światów?",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5855,
        "text": "Wcale nie! Zaprawdę, księga ludzi występnych znajduje się w Sidżdżinie!",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5856,
        "text": "A co ciebie pouczy, co to jest Sidżdżin?",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5857,
        "text": "To jest księga zapisana.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5858,
        "text": "Biada tego Dnia tym, którzy pomawiają o kłamstwo;",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5859,
        "text": "Tym, którzy uważają za kłamstwo Dzień Sądu!",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5860,
        "text": "Za kłamstwo uważa go tylko każdy człowiek występny, grzeszny.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5861,
        "text": "Kiedy mu recytują Nasze znaki, on mówi: \"To są baśnie praojców!\"",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5862,
        "text": "Ależ nie! Przeciwnie, rdza uczynków pokryła ich serca.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5863,
        "text": "Wcale nie! Zaprawdę, oni tego Dnia będą oddzieleni od swego Pana!",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5864,
        "text": "Potem będą się palić w piekle!",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5865,
        "text": "Wtedy będzie im powiedziane: \"To jest to, co uważaliście za kłamstwo!\"",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5866,
        "text": "Wcale nie tak! Zaprawdę, księga sprawiedliwych jest w Illijjun!",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5867,
        "text": "A co ciebie pouczy, co to jest Illijjun?",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5868,
        "text": "To jest księga zapisana.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5869,
        "text": "Będą świadkami jej owi blisko dopuszczeni.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5870,
        "text": "Zaprawdę, sprawiedliwi zaznają szczęśliwości;",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5871,
        "text": "Spoczywając na łożach, będą spoglądać;",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5872,
        "text": "Na ich twarzach poznasz blask szczęśliwości.",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5873,
        "text": "Będą pojeni winem szlachetnym, pieczętowanym;",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5874,
        "text": "Pieczęcią jego będzie piżmo - niech się starają pragnący tego, współzawodnicząc pilnie w staraniach -",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5875,
        "text": "A domieszką jego będzie woda z Tasnim",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5876,
        "text": "- źródła, z którego pić będą owi blisko dopuszczeni.",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5877,
        "text": "Zaprawdę, ci, którzy zgrzeszyli, naśmiewali się z tych, którzy uwierzyli;",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5878,
        "text": "Kiedy przechodzili mimo nich, wymieniali między sobą spojrzenia;",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5879,
        "text": "A kiedy powracali do swoich domowników, stroili żarty.",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5880,
        "text": "A kiedy ich widzieli, mówili: \"Zaprawdę, oni są zbłąkani!\"",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5881,
        "text": "Lecz oni nie zostali posłani, aby być ich stróżami.",
        "numberInSurah": 33,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5882,
        "text": "Jednak dzisiaj ci, którzy uwierzyli, śmieją się z niewiernych;",
        "numberInSurah": 34,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5883,
        "text": "Spoczywając na łożach, spoglądają.",
        "numberInSurah": 35,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5884,
        "text": "Czyż niewierni zostali wynagrodzeni za to, co czynili?",
        "numberInSurah": 36,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      }
    ],
