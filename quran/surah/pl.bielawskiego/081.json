{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 81,
    "name": "سُورَةُ التَّكۡوِيرِ",
    "englishName": "At-Takwir",
    "englishNameTranslation": "The Overthrowing",
    "revelationType": "Meccan",
    "numberOfAyahs": 29,
    "ayahs": [
      {
        "number": 5801,
        "text": "Kiedy słońce będzie spowite ciemnością",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5802,
        "text": "I kiedy gwiazdy będą zamglone;",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5803,
        "text": "Kiedy góry będą z miejsca poruszone;",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5804,
        "text": "Kiedy wielbłądzice w dziesiątym miesiącu będą całkowicie opuszczone;",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5805,
        "text": "Kiedy dzikie zwierzęta będą zebrane;",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5806,
        "text": "Kiedy morza będą wzburzone;",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5807,
        "text": "Kiedy dusze zostaną w pary złączone;",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5808,
        "text": "Kiedy żywcem pogrzebana będzie zapytana,",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5809,
        "text": "Za jaki grzech została zamordowana;",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5810,
        "text": "Kiedy karty księgi zostaną rozpostarte;",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5811,
        "text": "Kiedy niebo zostanie rozsunięte;",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5812,
        "text": "Kiedy ogień piekielny będzie rozpalony;",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5813,
        "text": "Kiedy Ogród będzie przybliżony -",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5814,
        "text": "Wtedy dowie się dusza, co sobie przygotowała.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5815,
        "text": "Lecz nie! Przysięgam na planety,",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5816,
        "text": "Wędrujące i znikające;",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5817,
        "text": "Na noc, kiedy ciemność zapada;",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5818,
        "text": "Na poranek, kiedy zaczyna oddychać!",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5819,
        "text": "To, zaprawdę, słowo Posłańca szlachetnego,",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5820,
        "text": "Posiadającego moc u Władcy Tronu, zdecydowanego",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5821,
        "text": "Słuchanego, ponadto godnego zaufania!",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5822,
        "text": "Wasz towarzysz nie jest opętany!",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5823,
        "text": "Z pewnością on zobaczył go na jasnym horyzoncie!",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5824,
        "text": "On nie skąpi tego, co skryte.",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5825,
        "text": "Nie jest to słowo szatana przeklętego.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5826,
        "text": "Dokąd więc idziecie?",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5827,
        "text": "To jest tylko napomnienie dla światów,",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5828,
        "text": "Dla tych spośród was, którzy chcą iść prosto.",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5829,
        "text": "Lecz wy nie będziecie chcieli, jeśli nie zechce Bóg, Pan światów!",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      }
    ],
