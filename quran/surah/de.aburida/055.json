{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 55,
    "name": "سُورَةُ الرَّحۡمَٰن",
    "englishName": "Ar-Rahmaan",
    "englishNameTranslation": "The Beneficent",
    "revelationType": "Medinan",
    "numberOfAyahs": 78,
    "ayahs": [
      {
        "number": 4902,
        "text": "Der Allerbarmer",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4903,
        "text": "hat den Quran gelehrt.",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4904,
        "text": "Er hat den Menschen erschaffen.",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4905,
        "text": "Er hat ihm das deutliche Reden beigebracht.",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4906,
        "text": "Die Sonne und der Mond kreisen nach einer festgesetzten Berechnung.",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4907,
        "text": "Und die Sterne und Bäume fallen (vor Ihm) anbetend nieder.",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4908,
        "text": "Und den Himmel hat Er emporgehoben. Und Er hat das (richtige) Abwiegen zum Gebot gemacht",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4909,
        "text": "auf daß ihr euch in der Waage nicht vergeht.",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4910,
        "text": "so setzt das Gewicht in gerechter Weise und betrügt nicht beim Wiegen.",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4911,
        "text": "Und Er hat die Erde für die Geschöpfe bereitet",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4912,
        "text": "auf ihr sind Früchte und Palmen mit Fruchthüllen",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4913,
        "text": "und Korn auf Halmen und duftende Pflanzen.",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4914,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4915,
        "text": "Er hat den Menschen aus Ton erschaffen, gleich einer Töpferware.",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4916,
        "text": "Und die Ginn schuf Er aus rauchloser Feuerflamme.",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4917,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4918,
        "text": "(Er ist) der Herr der beiden Osten und der Herr der beiden Westen.",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4919,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4920,
        "text": "Er hat den beiden Gewässern, die einander begegnen, freien Lauf gelassen.",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4921,
        "text": "Zwischen ihnen steht eine Scheidewand, so daß sie nicht ineinander übergreifen.",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4922,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4923,
        "text": "Perlen kommen aus beiden hervor und Korallen.",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4924,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4925,
        "text": "Und Sein sind die hochragenden Schiffe auf dem Meer, gleich Berge.",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4926,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4927,
        "text": "Alles, was auf (Erden) ist, wird vergehen.",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4928,
        "text": "Aber das Angesicht deines Herrn bleibt bestehen - des Herrn der Majestät und der Ehre.",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4929,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4930,
        "text": "Ihn bitten alle, die in den Himmeln und auf Erden sind. Er ist tagtäglich in jeglichem Einsatz.",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4931,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4932,
        "text": "Bald aber werden Wir Uns um euch kümmern, ihr beiden Gewichtigen!",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4933,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4934,
        "text": "O Wesen der Ginn und der Menschen! Wenn ihr imstande seid, die Grenzen der Himmel und der Erde zu durchdringen, dann dringt hindurch. Doch ihr werdet nicht imstande sein durchzudringen, es sei denn mit der Macht (eures Herrn)",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4935,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4936,
        "text": "Entsandt werden soll gegen euch eine lodernde Flamme aus Feuer und Kupfer; dann werdet ihr beide euch nicht zu helfen wissen.",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4937,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4938,
        "text": "Und wenn der Himmel sich spaltet und so geworden ist wie eine Rose, gleich rotem Leder.",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4939,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4940,
        "text": "An jenem Tage werden weder Menschen noch Ginn nach ihren Sünden befragt.",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4941,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4942,
        "text": "Die Schuldigen werden an ihren Merkmalen erkannt werden, und sie werden an ihren Stirnlocken und Füßen erfaßt werden.",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4943,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4944,
        "text": "Das ist Gahannam, die die Schuldigen leugnen.",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4945,
        "text": "Zwischen ihr und siedend heißem Wasser werden sie die Runde machen.",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4946,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4947,
        "text": "Und dem aber, der sich vor der Gegenwart seines Herrn fürchtet, werden zwei Gärten zuteil sein.",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4948,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4949,
        "text": "In beiden (Gärten) wachsen vielerlei Bäume.",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4950,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4951,
        "text": "In beiden (Gärten) fließen zwei Quellen.",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4952,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4953,
        "text": "Darin wird es von jeglicher Fruchtart zwei Arten geben.",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4954,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4955,
        "text": "Sie ruhen auf Kissen, die mit Brokat gefüttert sind. Und die Früchte der beiden Gärten sind nahe zur Hand.",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4956,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4957,
        "text": "Darin sind (keusche Mädchen) mit züchtigem Blick, die weder Menschen noch Ginn vor ihnen berührt haben.",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4958,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4959,
        "text": "(Es scheint,) als wären sie (die Mädchen) Rubine und Korallen.",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4960,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4961,
        "text": "Kann der Lohn für Güte (etwas) anderes sein als Güte.",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4962,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 61,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4963,
        "text": "Und neben diesen beiden gibt es noch zwei (andere) Gärten.",
        "numberInSurah": 62,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4964,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 63,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4965,
        "text": "(Es sind Gärten) mit dunkelgrünem Blattwerk.",
        "numberInSurah": 64,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4966,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 65,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4967,
        "text": "Darin fließen zwei Quellen, die reichlich Wasser spenden.",
        "numberInSurah": 66,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4968,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 67,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4969,
        "text": "In beiden (Gärten) sind Früchte und Dattelpalmen und Granatäpfel.",
        "numberInSurah": 68,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4970,
        "text": "Welche der Wohltaten eures Herrn wollt ihr beide da leugnen?",
        "numberInSurah": 69,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4971,
        "text": "Darin sind gute und schöne (Mädchen)",
        "numberInSurah": 70,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 468,
