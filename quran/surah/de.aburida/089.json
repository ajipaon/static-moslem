{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 89,
    "name": "سُورَةُ الفَجۡرِ",
    "englishName": "Al-Fajr",
    "englishNameTranslation": "The Dawn",
    "revelationType": "Meccan",
    "numberOfAyahs": 30,
    "ayahs": [
      {
        "number": 5994,
        "text": "Bei der Morgenröte",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5995,
        "text": "und bei den zehn Nächten",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5996,
        "text": "und beim (an Zahl) Geraden und Ungeraden",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5997,
        "text": "und bei der Nacht, wenn sie vergeht!",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5998,
        "text": "Ist hierin ein ausreichender Beweis für einen, der Verstand hat?",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5999,
        "text": "Hast du nicht gesehen, wie dein Herr mit den `Ad verfuhr",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6000,
        "text": "mit Iram, der Säulenreichen",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6001,
        "text": "dergleichen nicht erschaffen wurde in (anderen) Ländern?",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6002,
        "text": "Und den Tamud, die die Felsen im Tal aushöhlten?",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6003,
        "text": "Und Pharao mit seinen bodenfesten Bauten?",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6004,
        "text": "Denjenigen, die im Lande gewalttätig waren",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6005,
        "text": "und dort viel Verderbnis stifteten?",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6006,
        "text": "Darum ließ dein Herr die Geißel der Strafe auf sie schütten.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6007,
        "text": "Wahrlich, dein Herr ist auf der Hut.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6008,
        "text": "Wenn aber der Mensch von seinem Herrn geprüft wird, indem Er ihm Wohltaten erweist und Gnaden auf ihn häuft, dann sagt er: \"Mein Herr hat mich gewürdigt.\"",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6009,
        "text": "Wenn Er ihn aber prüft, indem Er ihm seine Versorgung verkürzt, dann sagt er: \"Mein Herr hat mich erniedrigt.\"",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6010,
        "text": "Nein, ihr seid nicht freigebig gegen die Waise",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6011,
        "text": "und treibt einander nicht an, den Armen zu speisen.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6012,
        "text": "Und ihr verzehrt das Erbe (anderer) ganz und gar.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6013,
        "text": "Und ihr liebt den Reichtum mit übermäßiger Liebe.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6014,
        "text": "Nicht aber so wenn die Erde kurz und klein zermalmt wird",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6015,
        "text": "und dein Herr kommt und (auch) die Engel in Reihen auf Reihen (kommen)",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6016,
        "text": "und Gahannam an jenem Tage nahegebracht wird. An jenem Tage wird der Mensch bereit sein, sich mahnen zu lassen; aber was wird ihm dann das Mahnen nutzen?",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6017,
        "text": "Er wird sagen: \"O hätte ich doch im voraus für (dieses) mein Leben Sorge getragen!\"",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6018,
        "text": "An jenem Tag wird niemand so bestrafen wie Er",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6019,
        "text": "und niemand wird so festbinden wie Er.",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6020,
        "text": "O du ruhige Seele!",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6021,
        "text": "Komm zufrieden zurück zu deinem Herrn und mit (Allahs) Wohlwollen",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6022,
        "text": "so schließ' dich dem Kreis Meiner Diener an.",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6023,
        "text": "Und tritt ein in Mein Paradies.",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "de.aburida",
      "language": "de",
      "name": "Abu Rida",
      "englishName": "Abu Rida Muhammad ibn Ahmad ibn Rassoul",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}