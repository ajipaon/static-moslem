{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 80,
    "name": "سُورَةُ عَبَسَ",
    "englishName": "Abasa",
    "englishNameTranslation": "He frowned",
    "revelationType": "Meccan",
    "numberOfAyahs": 42,
    "ayahs": [
      {
        "number": 5759,
        "text": "Er runzelte die Stirn und wandte sich ab",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5760,
        "text": "als der blinde Mann zu ihm kam.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5761,
        "text": "Was läßt dich aber wissen, daß er sich nicht reinigen wollte",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5762,
        "text": "oder daß er Ermahnung suchte und ihm somit die Lehre nützlich würde?",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5763,
        "text": "Wer aber es nicht für nötig hält",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5764,
        "text": "dem kommst du (bereitwillig) entgegen",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5765,
        "text": "ohne dir etwas daraus zu machen, daß er sich nicht reinigen will.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5766,
        "text": "Was aber den anbelangt, der in Eifer zu dir kommt",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5767,
        "text": "und gottesfürchtig ist",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5768,
        "text": "um den kümmerst du dich nicht.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5769,
        "text": "Nicht so. Wahrlich, dies ist eine Ermahnung",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5770,
        "text": "so möge, wer da will, diesem eingedenk sein.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5771,
        "text": "Auf geehrten Seiten, die in Ehren gehalten werden",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5772,
        "text": "sind sie emporgehoben, rein",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5773,
        "text": "in den Händen rechtschaffener Sendboten",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5774,
        "text": "die edel und tugendhaft sind.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5775,
        "text": "Verderben auf den Menschen! Wie undankbar ist er!",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5776,
        "text": "Woraus hat Er ihn erschaffen?",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5777,
        "text": "Aus einem Samenerguß hat Er ihn erschaffen und gebildet.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5778,
        "text": "Dann ermöglicht Er ihm den Ausgang.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5779,
        "text": "Dann läßt Er ihn sterben und läßt ihn ins Grab bringen.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5780,
        "text": "Dann, wenn Er will, erweckt Er ihn wieder.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5781,
        "text": "Wahrlich; er hat nicht getan, was Er ihm geboten hat.",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5782,
        "text": "So soll der Mensch doch seine Nahrung betrachten.",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5783,
        "text": "Siehe, Wir gossen das Wasser in Fülle aus.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5784,
        "text": "alsdann spalteten Wir die Erde in wunderbarer Weise",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5785,
        "text": "und ließen Korn in ihr wachsen",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5786,
        "text": "und Reben und Gezweig",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5787,
        "text": "und Ölbäume und Palmen",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5788,
        "text": "und dicht bepflanzte Gartengehege",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5789,
        "text": "und Obst und Futtergras",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5790,
        "text": "als Versorgung für euch und euer Vieh.",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5791,
        "text": "Doch wenn das betäubende Getöse kommt",
        "numberInSurah": 33,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5792,
        "text": "am Tage, da der Mensch seinen Bruder fluchtartig verläßt",
        "numberInSurah": 34,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5793,
        "text": "sowie seine Mutter und seinen Vater",
        "numberInSurah": 35,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5794,
        "text": "und seine Frau und seine Söhne",
        "numberInSurah": 36,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5795,
        "text": "an jenem Tage wird jeder eigene Sorgen genug haben, die ihn beschäftigen.",
        "numberInSurah": 37,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5796,
        "text": "An jenem Tage werden manche Gesichter strahlend sein",
        "numberInSurah": 38,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5797,
        "text": "heiter und freudig.",
        "numberInSurah": 39,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5798,
        "text": "Und andere Gesichter, an jenem Tage, werden staubbedeckt sein.",
        "numberInSurah": 40,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5799,
        "text": "Trübung wird darauf liegen.",
        "numberInSurah": 41,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5800,
        "text": "Das sind die Ungläubigen, die Unverschämten.",
        "numberInSurah": 42,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "de.aburida",
      "language": "de",
      "name": "Abu Rida",
      "englishName": "Abu Rida Muhammad ibn Ahmad ibn Rassoul",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}