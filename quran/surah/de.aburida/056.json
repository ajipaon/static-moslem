{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 56,
    "name": "سُورَةُ الوَاقِعَةِ",
    "englishName": "Al-Waaqia",
    "englishNameTranslation": "The Inevitable",
    "revelationType": "Meccan",
    "numberOfAyahs": 96,
    "ayahs": [
      {
        "number": 4980,
        "text": "Wenn das Ereignis eintrifft",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4981,
        "text": "gibt es keinen, der sein Eintreffen verleugnen könnte.",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4982,
        "text": "Dann wird es (die einen) erniedrigen, (andere) wird es erhöhen.",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4983,
        "text": "Wenn die Erde heftig geschüttelt wird",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4984,
        "text": "und die Berge gänzlich zerbröckelt werden",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4985,
        "text": "dann sollen sie zu weithin zerstreutem Staub werden.",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4986,
        "text": "Und ihr sollt in drei Gattungen (gegliedert) werden",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4987,
        "text": "(In) die zur Rechten was (wißt ihr) von denen die zur Rechten sein werden?",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4988,
        "text": "Und (in) die zur Linken - was (wißt ihr) von denen, die zur Linken sein werden.",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4989,
        "text": "Und (in) die Vordersten - (sie) werden die Vordersten sein.",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4990,
        "text": "Das sind die, die Allah nahe sein werden",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4991,
        "text": "in den Gärten der Wonne.",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4992,
        "text": "(Dies sind) eine große Schar der Früheren",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4993,
        "text": "und einige wenige der Späteren.",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4994,
        "text": "Auf Polstern, die mit Gold durchwoben sind",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4995,
        "text": "lehnen (sie) auf diesen einander gegenüber.",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4996,
        "text": "Bedient werden sie von Jünglingen, die nicht altern",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4997,
        "text": "mit Bechern und Krü gen aus einer fließenden Quelle.",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4998,
        "text": "Keinen Kopfschmerz werden sie davon bekommen, noch werden sie das Bewußtsein verlieren.",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4999,
        "text": "Und Früchte, die sie sich wünschen",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5000,
        "text": "und Fleisch vom Geflügel, das sie begehren",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5001,
        "text": "und Huris",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5002,
        "text": "wohlbehüteten Perlen gleich",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5003,
        "text": "(werden sie erhalten) als Belohnung für das, was sie zu tun pflegten.",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5004,
        "text": "Sie werden dort weder leeres Gerede noch Anschuldigung der Sünde hören",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5005,
        "text": "nur das Wort: \"Frieden, Frieden!\"",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5006,
        "text": "Und die zur Rechten - was (wißt ihr) von denen, die zur Rechten sein werden?",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5007,
        "text": "(Sie werden) unter dornlosen Lotusbäumen (sein)",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5008,
        "text": "und gebüschelten Bananen",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5009,
        "text": "und endlosem Schatten",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5010,
        "text": "bei fließendem Wasser",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5011,
        "text": "und vielen Früchten",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5012,
        "text": "die weder zu Ende gehen, noch für verboten erklärt werden",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5013,
        "text": "und auf erhöhten Ruhekissen.",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5014,
        "text": "Wir haben sie (die Huris) in herrlicher Schöpfung gestaltet",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5015,
        "text": "und sie zu Jungfrauen gemacht",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5016,
        "text": "zu liebevollen Altersgenossinnen",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5017,
        "text": "derer zur Rechten.",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5018,
        "text": "(Dies sind) eine große Schar der Früheren",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5019,
        "text": "und eine große Schar der Späteren.",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5020,
        "text": "Und die zur Linken - was (wißt ihr) von denen, die zur Linken sein werden?",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5021,
        "text": "(Sie werden) inmitten von glühenden Winden und siedendem Wasser (sein)",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5022,
        "text": "und im Schatten schwarzen Rauches",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5023,
        "text": "der weder kühl noch erfrischend ist.",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5024,
        "text": "Vor diesem (Schicksal) wurden sie in der Tat mit Wohlleben verwöhnt",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5025,
        "text": "und (sie) verharrten in großer Sünde.",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5026,
        "text": "Und sie pflegten zu sagen: \"Wie? Wenn wir tot sind und zu Staub und Gebeinen geworden sind, dann sollen wir wirklich auferweckt werden?",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5027,
        "text": "Und unsere Vorväter auch?\"",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5028,
        "text": "Sprich: \"Wahrlich, die Früheren und die Späteren",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5029,
        "text": "werden alle zur gesetzten Frist eines bestimmten Tages versammelt werden.",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5030,
        "text": "Dann, o ihr Irregegangenen und Leugner",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5031,
        "text": "sollt ihr vom Baume Zaqqum essen",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5032,
        "text": "und damit eure Bäuche füllen",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5033,
        "text": "und darauf von sie dendem Wasser trinken.",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5034,
        "text": "Dann trinkt (ihr,) wie die durstigen Kamele trinken.\"",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5035,
        "text": "Das wird ihre Bewirtung am Tage des Gerichts sein.",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5036,
        "text": "Wir haben euch erschaffen. Warum wollt ihr da nicht die Wahrheit zugeben.",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5037,
        "text": "Habt ihr betrachtet, was euch an Sperma entfließt?",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5038,
        "text": "Erschafft ihr es oder sind Wir die Schöpfer?",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5039,
        "text": "Wir haben für euch den Tod verordnet, und Wir sind nicht unfähig dazu",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5040,
        "text": "an eurer Stelle andere wie euch hervorzubringen und euch in einen Zustand zu versetzen, den ihr nicht kennt.",
        "numberInSurah": 61,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5041,
        "text": "Und ihr kennt doch gewiß die erste Schöpfung. Warum also wollt ihr euch nicht besinnen?",
        "numberInSurah": 62,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5042,
        "text": "Habt ihr betrachtet, was ihr aussät?",
        "numberInSurah": 63,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5043,
        "text": "Seid ihr es, die es wachsen lassen, oder lassen Wir es wachsen?",
        "numberInSurah": 64,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5044,
        "text": "Wollten Wir, könnten Wir es in brüchiges Zeug verwandeln; dann würdet ihr nicht aufhören, euch zu beklagen",
        "numberInSurah": 65,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5045,
        "text": "\" Wir sind zugrunde gerichtet!",
        "numberInSurah": 66,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5046,
        "text": "Nein, wir sind beraubt.\"",
        "numberInSurah": 67,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5047,
        "text": "Habt ihr das Was ser betrachtet, das ihr trinkt?",
        "numberInSurah": 68,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5048,
        "text": "Seid ihr es, die es aus den Wolken niedersenden, oder sind Wir es, die es niedersenden?",
        "numberInSurah": 69,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5049,
        "text": "Wollten Wir, könnten Wir es bittersalzig machen. Warum also dankt ihr (Mir) nicht?",
        "numberInSurah": 70,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5050,
        "text": "Habt ihr das Feuer betrachtet, das ihr entzündet?",
        "numberInSurah": 71,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5051,
        "text": "Seid ihr es, die den Baum dazu hervorbrachten, oder sind Wir dessen Urheber?",
        "numberInSurah": 72,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5052,
        "text": "Wir haben das (Feuer) zur Ermahnung (vor der Hölle) erschaffen und zum Nutzen für die Wanderer durch die Wildnisse.",
        "numberInSurah": 73,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5053,
        "text": "Darum preise den Namen deines Großen Herrn.",
        "numberInSurah": 74,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5054,
        "text": "Ich schwöre bei den Stationen der Sterne -",
        "numberInSurah": 75,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5055,
        "text": "und wahrlich, das ist ein großer Schwur, wenn ihr es nur wüßtet",
        "numberInSurah": 76,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5056,
        "text": "daß dies wahrlich ein edler Quran ist",
        "numberInSurah": 77,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5057,
        "text": "in einer wohlaufbewahrten Urschrift.",
        "numberInSurah": 78,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5058,
        "text": "Keiner kann sie berühren, außer den Reinen.",
        "numberInSurah": 79,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5059,
        "text": "(Sie ist) eine Offenbarung vom Herrn der Welten.",
        "numberInSurah": 80,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5060,
        "text": "Wollt ihr denn gegenüber dieser Verkündigung undankbar sein?",
        "numberInSurah": 81,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5061,
        "text": "Und daß ihr (sie) leugnet, soll das euer Dank sein?",
        "numberInSurah": 82,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5062,
        "text": "Warum wohl, wenn (die Seele des Sterbenden) zur Kehle steigt",
        "numberInSurah": 83,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5063,
        "text": "und ihr in jenem Augenblick zuschaut?",
        "numberInSurah": 84,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5064,
        "text": "Und Wir sind ihm näher als ihr, nur daß ihr es nicht seht.",
        "numberInSurah": 85,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5065,
        "text": "Warum wohl, wenn ihr nicht zur Re chenschaft gezogen werden sollt?",
        "numberInSurah": 86,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5066,
        "text": "Zwingt ihr sie nicht zurück (in den Leib), wenn ihr wahrhaftig seid?",
        "numberInSurah": 87,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5067,
        "text": "Wenn er nun zu denen gehört, die (Allah) nahe sind",
        "numberInSurah": 88,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5068,
        "text": "dann (wird er) Glückseligkeit (genießen) und Düfte und Gärten der Wonne.",
        "numberInSurah": 89,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5069,
        "text": "Und wenn er zu denen gehört, die zur Rechten sind",
        "numberInSurah": 90,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5070,
        "text": "(wird ein) \"Friede sei auf dir\" von denen, die der Rechten angehö ren(, zugerufen).",
        "numberInSurah": 91,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5071,
        "text": "Wenn er aber zu den Leugnern, Irregegangenen gehört",
        "numberInSurah": 92,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5072,
        "text": "dann (wird ihm) eine Bewirtung mit siedendem Wasser zuteil",
        "numberInSurah": 93,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5073,
        "text": "und Brennen in der Gahim.",
        "numberInSurah": 94,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5074,
        "text": "Wahrlich, dies ist die Wahrheit in aller Gewißheit.",
        "numberInSurah": 95,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5075,
        "text": "Lobpreise darum den Namen deines Großen Herrn.",
        "numberInSurah": 96,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      }
