{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 79,
    "name": "سُورَةُ النَّازِعَاتِ",
    "englishName": "An-Naazi'aat",
    "englishNameTranslation": "Those who drag forth",
    "revelationType": "Meccan",
    "numberOfAyahs": 46,
    "ayahs": [
      {
        "number": 5713,
        "text": "ގަދަފަދަވެގެންވާ ގޮތުގައި (كافر ންގެ) ފުރާނަތައް ގެންދަވާ ملائكة ން ގަންދެއްވަމެވެ.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5714,
        "text": "އަދި އޯގާވެރިކަމާއި، ފަސޭހަކަމާއެކު (مؤمن ންގެ) ފުރާނަތައް ގެންދަވާ ملائكة ން ގަންދެއްވަމެވެ.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5715,
        "text": "އަދި ކަލާނގެ أمر ފުޅުތަކާއިގެން ދަތުރުކުރައްވާ ملائكة ން ގަންދެއްވަމެވެ.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5716,
        "text": "އަދި (مؤمن ންގެ) ފުރާނަތަކާއިގެން ކުރިއަރައިދާ ملائكة ންނާއިއެވެ.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5717,
        "text": "އަދި (ދުނިޔޭގެ) ކަންތައްތައް تدبير ކުރެއްވުމުގައި އުޅުއްވާ ملائكة ންނާއިއެވެ.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5718,
        "text": "(ތާޅަފިލި ފުރަތަމަފަހަރު ފުންމަވައިލެއްވުމުން) ބިންގަނޑު ހެލިގެންދާ ދުވަހެވެ.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5719,
        "text": "އެއާ ވިދިގެން އޭގެފަހަތުން ދެވަނަ ފުންމެވުން ފުންމަވާ ދުވަހެވެ.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5720,
        "text": "އެދުވަހުން ބިރުގެންފައިވާ ހިތްތަކެއް ވެއެވެ.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5721,
        "text": "އެއުރެންގެ ލޯތައް ތިރިވެ ނިކަމެތިވެގެންވެއެވެ.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5722,
        "text": "އެއުރެން ބުނެތެވެ. އިހުއުޅުނު ދުނިޔޭގެ ދިރިއުޅުމަށް، އަލުން ރައްދުކުރައްވާނޭ ބަޔަކު ކަމުގައި ހަމަކަށަވަރުން، ތިމަންމެން ވާނޭހެއްޔެވެ؟",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5723,
        "text": "ފީވެ، އަޅިޔަށްވެފައިވާ ކަށިތަކެއް ކަމުގައި، ތިމަންމެން ވުމަށްފަހުގައިވެސް ހެއްޔެވެ؟",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5724,
        "text": "އެއުރެން ބުނޫއެވެ. އެހެންވިއްޔާ އެއީ، ގެއްލެނިވެގެންވާ އެނބުރި ދިޔުމެކެވެ.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5725,
        "text": "ފަހެ، އެކަންތައްވާކަން ކަށަވަރީ، އެންމެ ފުމެލެއްވުމެއްގެ އަޑު ކަމުގައެވެ.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5726,
        "text": "ފަހެ، އެހާރުން އެންމެން ތިބޭހުށީ، އަލުން ދިރި، محشر ބިމުގައެވެ.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5727,
        "text": "موسى ގެފާނުގެ ވާހަކަ ކަލޭގެފާނާ ހަމަޔަށް އައިސްފައިވޭތޯއެވެ؟",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5728,
        "text": "طوى އޭ ކިޔުނު مقدّس ވާދީގައި ހުންނަވަނިކޮށް، އެކަލޭގެފާނުގެ ވެރިރަސްކަލާނގެ، އެކަލޭގެފާނަށް ނިދާ އިއްވެވި ހިނދެވެ.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5729,
        "text": "فرعون ގެ ގާތަށް ކަލޭގެފާނު ވަޑައިގަންނަވާށެވެ! ހަމަކަށަވަރުން، ފުރެދުމުގައި އޭނާ ހައްދުފަހަނަޅައި ހިނގައްޖެއެވެ.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5730,
        "text": "އަދި ކަލޭގެފާނު (އޭނާޔަށް) ވިދާޅުވާށެވެ! كافر ކަމުން طاهر ވެގަތުމަށް ކަލޭ އެދެމުހެއްޔެވެ؟",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5731,
        "text": "އަދި ކަލޭގެ ވެރިރަސްކަލާނގެ حضرة އަށް އަހުރެން މަގު ދެއްކުމަށްވެސް މެއެވެ. އެއީ (އެކަލާނގެއަށް) ކަލޭ ބިރުވެތިވުމަށްޓަކައެވެ.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5732,
        "text": "ފަހެ، އެކަލޭގެފާނު، އޭނާޔަށް ބޮޑުވެގެންވާ معجزة ދެއްކެވިއެވެ.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5733,
        "text": "ފަހެ، އޭނާ ދޮގުކުރިއެވެ. އަދި އުރެދުނެވެ.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5734,
        "text": "ދެން އޭނާ އެނބުރި، ފުރަގަސްދީ (ދެކޮޅުވެރިކަމުގެ) މަސައްކަތް ކުރަންފެށިއެވެ.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5735,
        "text": "ފަހެ، (އޭނާގެ لشكر) އެއްކުޅައެވެ. އަދި ގޮވައިލިއެވެ.",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5736,
        "text": "ފަހެ، އޭނާ ބުންޏެވެ. ތިމަންނާއީ، ތިޔަބައިމީހުންގެ އެންމެ މަތިވެރިވެގެންވާ ކަލާނގެއެވެ.",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5737,
        "text": "ދެން آخرة އާއި، ދުނިޔޭގެ عقوبات އެއްގެ ގޮތުން اللَّه އޭނާ ހިއްޕެވިއެވެ.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5738,
        "text": "ބިރުވެތިވާ މީހަކަށް ހަމަކަށަވަރުން، އެކަމުގައި عبرة އެއްވާކަން ކަށަވަރެވެ.",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5739,
        "text": "ހެއްދެވުމުގައި އުނދަގޫކަންބޮޑީ، ތިޔަބައިމީހުން ހެއްޔެވެ؟ ނުވަތަ އުޑު ހެއްޔެވެ؟ އެ އުޑު ބިނާކުރެއްވީ އެކަލާނގެއެވެ.",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5740,
        "text": "އެކަލާނގެ އެ އުޑުގެ ބިނާ އުފުއްލެވިއެވެ. ދެން އެ އުޑު ފުރިހަމަކުރެއްވިއެވެ.",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5741,
        "text": "އަދި އެ އުޑުގެ ރޭގަނޑު އަނދިރިކުރެއްވިއެވެ. އަދި އެ އުޑުގެ ދުވާލު ފާޅުކުރެއްވިއެވެ.",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5742,
        "text": "އެއަށްފަހު، ބިން ފެތުރުއްވިއެވެ.",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5743,
        "text": "އެ ބިމުގެ ފެނާއި، ފަތްޕިލާވެލި، އެ ބިމުން ނެރުއްވިއެވެ.",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5744,
        "text": "އަދި ފަރުބަދަތައް (އެ ބިމުގައި) ހަރުލެއްވިއެވެ.",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5745,
        "text": "(އެއީ) ތިޔަބައިމީހުންނަށާއި، ތިޔަބައިމީހުންގެ نعم ސޫފިތަކަށް ދިރިއުޅުމުގެ ވަގުތީ އުފަލެއްގެ ގޮތުގައެވެ.",
        "numberInSurah": 33,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5746,
        "text": "ފަހެ، ބޮޑުވެގެންވާ ކަންތައްގަނޑު އަންނަހިނދެވެ. (އެބަހީ: قيامة ދުވަސް އަންނަހިނދެވެ.)",
        "numberInSurah": 34,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5747,
        "text": "އިންސާނާ ކުޅަހާ ކަމެއް، ހަނދުމަވާ ދުވަހެވެ.",
        "numberInSurah": 35,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5748,
        "text": "އަދި ދެކޭމީހަކަށް ނަރަކަ ފާޅުކުރައްވާނެތެވެ.",
        "numberInSurah": 36,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5749,
        "text": "އަދި ދެކޭމީހަކަށް ނަރަކަ ފާޅުކުރައްވާނެތެވެ.",
        "numberInSurah": 37,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5750,
        "text": "ފަހެ، ފުރެދުމުގައި ހައްދުފަހަނަޅައި، އަދި ދުނިޔޭގެ ދިރިއުޅުމަށް އިސްކަންދިން މީހާ ދަންނާށެވެ!",
        "numberInSurah": 38,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5751,
        "text": "ފަހެ، ހަމަކަށަވަރުން، އެމީހާ އުޅޭނެ ތަނަކީ، ނަރަކަޔެވެ.",
        "numberInSurah": 39,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5752,
        "text": "އަދި އެމީހާގެ ވެރިރަސްކަލާނގެ حضرة ގައި ހުންނަންޖެހޭ ހުރުމަށް ބިރުވެތިވެ، އެމީހެއްގެ نفس އަމިއްލަ އެދުންތަކުން ދުރުކޮށްފިމީހާ ދަންނާށެވެ!",
        "numberInSurah": 40,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5753,
        "text": "ފަހެ، ހަމަކަށަވަރުން، އެމީހާ އުޅޭނެ ތަނަކީ، ސުވަރުގެއެވެ.",
        "numberInSurah": 41,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5754,
        "text": "قيامة ގެ وقت އަންނާނީ ކޮންއިރަކުންކަމާމެދު އެއުރެން ކަލޭގެފާނާ سؤال ކުރެތެވެ.",
        "numberInSurah": 42,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5755,
        "text": "އެކަން بيان ކޮށްދެއްވުމަކީ، ކަލޭގެފާނަށް (ކުރެއްވެން) އޮތްކަމެއްތޯއެވެ؟",
        "numberInSurah": 43,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5756,
        "text": "އެކަން ދެނެވޮޑިގަތުންވަނީ، ހަމައެކަނި، ކަލޭގެފާނުގެ ވެރިރަސްކަލާނގެއަށެވެ.",
        "numberInSurah": 44,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5757,
        "text": "ކަލޭގެފާނުވާކަން ކަށަވަރީ، އެދުވަހަށް ބިރުވެތިވާ މީހަކަށް إنذار ކުރައްވާ ބޭކަލަކު ކަމުގައެވެ.",
        "numberInSurah": 45,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5758,
        "text": "އެއުރެން އެކަންތައް ދެކޭދުވަހު، ހަވީރެއް، ނުވަތަ ހެނދުނެއްގައި މެނުވީ، އެއުރެން ލަސްވެ ނުތިބޭކަހަލައެވެ.",
        "numberInSurah": 46,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      }
