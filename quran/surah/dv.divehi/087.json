  "status": "OK",
  "data": {
    "number": 87,
    "name": "سُورَةُ الأَعۡلَىٰ",
    "englishName": "Al-A'laa",
    "englishNameTranslation": "The Most High",
    "revelationType": "Meccan",
    "numberOfAyahs": 19,
    "ayahs": [
      {
        "number": 5949,
        "text": "ކަލޭގެފާނުގެ ވެރި، އެންމެ މަތިވެރި ރަސްކަލާނގެ اسم ފުޅުން ކަލޭގެފާނު تسبيح ވިދާޅުވާށެވެ!",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5950,
        "text": "އެކަލާނގެއީ (އެންމެހާ ތަކެތި) ހައްދަވައި، ދެން އެ ހެއްދެވުން ފުރިހަމަކުރެއްވި ރަސްކަލާނގެއެވެ.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5951,
        "text": "އަދި އެކަލާނގެއީ (އެންމެހާ ތަކެތި) މިންވަރުކުރައްވައި، ދެން (އެތަކެއްޗަށް) މަގުދެއްކެވި ރަސްކަލާނގެއެވެ.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5952,
        "text": "އަދި އެކަލާނގެއީ، ފަތްޕިލާވެލި ފެޅުއްވި ރަސްކަލާނގެއެވެ.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5953,
        "text": "އަދި އެކަލާނގެ އެތަކެތި، ކަޅުކުލައިގެ ހިކި ކުނޑިތަކެއް ކަމުގައި ލެއްވިއެވެ.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5954,
        "text": "ތިމަންރަސްކަލާނގެ، ކަލޭގެފާނު ކިޔަވައިވިދާޅުވާ ބޭކަލަކު ކަމުގައި ލައްވާހުށީމެވެ. ފަހެ، ކަލޭގެފާނު (އެ قرآن) ހަނދުމަފުޅެއް ނުނެތޭނެތެވެ.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5955,
        "text": "(ހަނދުމަފުޅު ނެތުމަށް) اللَّه އިރާދަކުރެއްވި އެއްޗެއް މެނުވީއެވެ. ހަމަކަށަވަރުން، ފާޅުވެގެންވާ ކަންތަކާއި، ވަންހަނާވެގެންވާ ކަންތައް، އެކަލާނގެ ދެނެވޮޑިގެންވެތެވެ.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5956,
        "text": "އަދި ފަސޭހަވެގެންވާ މަގަށް، ތިމަންރަސްކަލާނގެ ކަލޭގެފާނަށްޓަކައި މަގު ފަހިކޮށްދެއްވާހުށީމެވެ.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5957,
        "text": "ފަހެ، ހަނދުމަކޮށްދިނުން މަންފާކުރާނަމަ، ކަލޭގެފާނު (އެއުރެންނަށް) ހަނދުމަކޮށްދެއްވާށެވެ!",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5958,
        "text": "(اللَّه އަށް) ބިރުވެތިވާ މީހާ، ހަނދުމަކޮށް އުޅޭނެތެވެ.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5959,
        "text": "އަދި އަބާއްޖަވެރިކަން އެންމެ ބޮޑުމީހާ، އެކަމުން ދުރުހެލިވާނެތެވެ.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5960,
        "text": "އެމީހާއީ، ބޮޑުވެގެންވާ ނަރަކައަށް ވަންނާނޭ މީހެކެވެ.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5961,
        "text": "ދެން އެމީހަކު އެތާނގައި މަރެއްވެސް ނުވާނެއެވެ. އަދި (އަރާމުހުރި ދިރުމަކުން) ނުވެސް ދިރޭނޭމެއެވެ.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5962,
        "text": "ދެން އެމީހަކު އެތާނގައި މަރެއްވެސް ނުވާނެއެވެ. އަދި (އަރާމުހުރި ދިރުމަކުން) ނުވެސް ދިރޭނޭމެއެވެ.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5963,
        "text": "(كافر ކަމުން) طاهر ވެ، އަދި އެމީހާގެ ވެރިރަސްކަލާނގެ اسم ފުޅު ހަނދުމަކޮށް، ނަމާދުކުޅަމީހާ ކާމިޔާބުވެއްޖެކަން ކަށަވަރެވެ.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5964,
        "text": "(كافر ކަމުން) طاهر ވެ، އަދި އެމީހާގެ ވެރިރަސްކަލާނގެ اسم ފުޅު ހަނދުމަކޮށް، ނަމާދުކުޅަމީހާ ކާމިޔާބުވެއްޖެކަން ކަށަވަރެވެ.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5965,
        "text": "އަދި ކިއެއްތަ! آخرة ގެ ހެޔޮކަން ބޮޑުވެފައި، ދެމިހުރުން ބޮޑުވެގެންވާ حال ވެސް، ތިޔަބައިމީހުން ދުނިޔޭގެ ދިރިއުޅުން އިސްކުރަމުއެވެ.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5966,
        "text": "ހަމަކަށަވަރުން، މިވާހަކަތައް، އިހުގެ ފަތްފުށްތަކުގައިވާކަން ކަށަވަރެވެ.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5967,
        "text": "إبراهيم ގެފާނާއި، موسى ގެފާނަށް ބާވައިލެއްވި ފަތްފުށްތަކުގައެވެ.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      }
    ],
