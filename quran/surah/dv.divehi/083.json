{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 83,
    "name": "سُورَةُ المُطَفِّفِينَ",
    "englishName": "Al-Mutaffifin",
    "englishNameTranslation": "Defrauding",
    "revelationType": "Meccan",
    "numberOfAyahs": 36,
    "ayahs": [
      {
        "number": 5849,
        "text": "މިނެކިރުމުގައި އުނިކުރާ މީހުންނަށް، ގެއްލުމާއި ހަލާކު ހުށްޓެވެ.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5850,
        "text": "އެއުރެންނީ، އެހެންމީހުންގެ ކިބައިން މިނާނަމަ، ހަމަކުރަން އެދޭމީހުންނެވެ.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5851,
        "text": "އަދި އެހެންމީހުންނަށް މިނެދޭނަމަ، ނުވަތަ އެހެންމީހުންނަށް ކިރައިދޭނަމަ، ގެއްލުންކުރާ މީހުންނެވެ.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5852,
        "text": "އަދި އެހެންމީހުންނަށް މިނެދޭނަމަ، ނުވަތަ އެހެންމީހުންނަށް ކިރައިދޭނަމަ، ގެއްލުންކުރާ މީހުންނެވެ.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5853,
        "text": "ބޮޑުވެގެންވާ ދުވަހަކަށް އެއުރެން ފޮނުއްވާނޭ ކަމުގައި އެއުރެން يقين ނުކުރަނީ ހެއްޔެވެ؟",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5854,
        "text": "(އެއީ) عالم ތަކުގެ ވެރިރަސްކަލާނގެ حضرة ގައި މީސްތަކުން قائم ވެ ތިބޭނޭ ދުވަހެވެ.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5855,
        "text": "ހަމަހިލާ އެހެންނަކުން ނުވާނެއެވެ. ހަމަކަށަވަރުން، ނުލަފާ كافر ންގެ عمل ތަކުގެ ފަތްވަނީ سِجّين ގައިކަން ކަށަވަރެވެ.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5856,
        "text": "سِجّين އަކީ ކޮންއެއްޗެއްކަން ކަލޭގެފާނަށް އަންގައިދިނީ ކޮންއެއްޗެއްތޯއެވެ؟",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5857,
        "text": "(އެއީ) ލިޔުއްވައި، ފާހަގަކުރައްވާފައިވާ ފޮތެކެވެ.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5858,
        "text": "ދޮގުކުރާ މީހުންނަށް އެދުވަހުން ގެއްލުމާއި ހަލާކު ހުށްޓެވެ.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5859,
        "text": "އެއުރެންނީ، قيامة ދުވަސް ދޮގުކުރާ މީހުންނެވެ.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5860,
        "text": "ހައްދުފަހަނަޅައިފައިވާ، ފާފަކުރުންގިނަ ކޮންމެ މީހަކު މެނުވީ، އެދުވަސް ދޮގެއް ނުކުރާނެތެވެ.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5861,
        "text": "ތިމަންރަސްކަލާނގެ آية ތައް އޭނާޔަށް ކިޔައިދެއްވާހިނދު، އޭނާ ބުނެތެވެ. (ތިޔައީ) އެވޭލާ މީހުން ދައްކައިއުޅުނު ފުލޯކު ވާހަކަތަކެވެ.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5862,
        "text": "ހަމަހިލާ އެހެންނަކުން ނުވާނެއެވެ. އަދި ކީއްހެއްޔެވެ؟ އެއުރެން ހޯދައިއުޅުނު عمل ތަކުގެ ސަބަބުން އެއުރެންގެ ހިތްތަކުގައި ދަބަރުޖަހާފައިވެއެވެ.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5863,
        "text": "ހަމަހިލާ އެހެންނަކުން ނުވާނެއެވެ. ހަމަކަށަވަރުން، އެއުރެންގެ ވެރިރަސްކަލާނގެ حضرة ން އެދުވަހު، އެއުރެން ފަރުދާ ކުރެވިގެންވާނޭކަން ކަށަވަރެވެ.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5864,
        "text": "ދެން ހަމަކަށަވަރުން، އެއުރެން ނަރަކަޔަށް ވަންނާނެކަން ކަށަވަރެވެ.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5865,
        "text": "ދެން (އެއުރެންނަށް) އަންގަވާނެތެވެ. ތިޔަބައިމީހުން ދޮގުކޮށް އުޅުނު ކަންތަކަކީ މިއީއެވެ.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5866,
        "text": "ހަމަހިލާ އެހެންނަކުން ނުވާނެއެވެ. ހަމަކަށަވަރުން، ހެޔޮލަފާ މީހުންގެ عمل ތަކުގެ ފަތްވަނީ عِلّـيّون ގައި ކަންކަށަވަރެވެ.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5867,
        "text": "عِلـيّون އަކީ، ކޮންއެއްޗެއްކަން ކަލޭގެފާނަށް އަންގައިދިނީ ކޮންއެއްޗެއްތޯއެވެ؟",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5868,
        "text": "(އެއީ) ލިޔުއްވައި ފާހަގަކުރައްވާފައިވާ ފޮތެކެވެ.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5869,
        "text": "ކުއްތަންކުރައްވާފައިވާ ملائكة ން އެ ފޮތް ދެކެވަޑައިގަންނަވާނެތެވެ.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5870,
        "text": "ހަމަކަށަވަރުން، ހެޔޮލަފާ މީހުންވަނީ، نعمة ގެ ސުވަރުގޭގައިކަން ކަށަވަރެވެ.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5871,
        "text": "ދާންތަކުގެ މަތީގައި (نعمة ތަކަށް) ބަލަބަލައެވެ.",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5872,
        "text": "އެއުރެންގެ މޫނުތަކުގެމަތިން، نعمة ގެ އުފާވެރިކަން ތިބާޔަށް އެނގޭނެތެވެ.",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5873,
        "text": "ސިއްކަގައްސަވައި ބަންދުކުރައްވާފައިވާ (މަސްތުވުމެއްނެތް) ސާފު ރާގެ ބުއިމަކުން، އެއުރެންނަށް ބޯންދެއްވާނެތެވެ.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5874,
        "text": "އެބުއިން ނިމޭހުށީ، ކަސްތޫރި ވަހުންނެވެ. ވާދަކޮށް ކުރިއަރައިދާ މީހުން އެއާމެދުގައި ވާދަކޮށް ކުރިއަރައިދާ ހުށިކަމެވެ!",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5875,
        "text": "އެއާ އެއްކުރައްވާފައިވަނީ تسنيم އެވެ.",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5876,
        "text": "(އެއީ) ކުއްތަންވެގެންވާ އަޅުން އެއިން ބޯނޭ އާރެކެވެ.",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5877,
        "text": "ހަމަކަށަވަރުން، ކުށްވެރި، ފާފަވެރިން (ދުނިޔޭގައި) إيمان ވި މީހުންނާ ދިމާޔަށް (މަލާމާތުގެގޮތުން) ހީނއުޅޭ ކަމުގައިވޫއެވެ.",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5878,
        "text": "އަދި އެއުރެން އެ مؤمن ންގެ ކައިރިންދާހިނދު، ލޮލުގެ إشارة ން ފުރައްސާރަ ކުރެތެވެ.",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5879,
        "text": "އަދި އެއުރެންގެ أهل ވެރިންގެގާތަށް އެނބުރިދާހިނދު، ހީނސަމާސާ ކުރަމުންދެއެވެ.",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5880,
        "text": "އަދި އެއުރެން مؤمن ން ދެކޭހިނދު ބުނެތެވެ. ހަމަކަށަވަރުން، މިބައިމީހުންނީ، މަގު ގެއްލިފައިވާ ބަޔެއްކަން ކަށަވަރެވެ.",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5881,
