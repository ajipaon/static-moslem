{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 75,
    "name": "سُورَةُ القِيَامَةِ",
    "englishName": "Al-Qiyaama",
    "englishNameTranslation": "The Resurrection",
    "revelationType": "Meccan",
    "numberOfAyahs": 40,
    "ayahs": [
      {
        "number": 5552,
        "text": "قيامة ދުވަސް ގަންދެއްވައި ހުވާ وحى ކުރައްވަމެވެ.",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5553,
        "text": "އަދި النفس اللّوّامة ގަންދެއްވައި ހުވާ وحى ކުރައްވަމެވެ. \t (ނޯޓު: النفس اللّوّامة އަކީ، ނުބައިކަންތައް ކުރާކަމުގެ މައްޗަށާއި، ހެޔޮކަންތައް ނުކުރާކަމުގެ މައްޗަށް، އަމިއްލަ نفس އަށް މަލާމާތް ރައްދުކޮށްކޮށް ހުންނަ نفس އެކެވެ.)",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5554,
        "text": "އިންސާނާ ހީކުރަނީ، ތިމަންރަސްކަލާނގެ އޭނާގެ ކަށިތައް އެއްނުކުރައްވާނެ ކަމަށްހެއްޔެވެ؟",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5555,
        "text": "އެހެންނަކުން ނޫނެވެ. އެ އިންސާނާގެ އިނގިލިތަކުގެ ކުރިތައް ހަމަކުރެއްވުމަށްވެސް، ތިމަންރަސްކަލާނގެ ކުޅަދުންވަންތައެވެ.",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5556,
        "text": "އަދި ކިއެއްތަ! އިންސާނާ އެދެނީ، އޭނާގެ ކުރިޔަށް އޮތްތާނގައިވެސް ފާފަތައް ކޮށްއުޅުމަށެވެ.",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5557,
        "text": "އޭނާ سؤال ކުރާނެތެވެ. قيامة ދުވަސް އަންނާނީ ކޮންއިރަކުން ހެއްޔެވެ؟",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5558,
        "text": "ފަހެ، ލޯތަކަށް حيران ކަންލިބި، ތަރަތަރަލާހިނދެވެ.",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5559,
        "text": "އަދި ހަނދުގެ އަލިކަން ފިލައިދާހިނދެވެ.",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5560,
        "text": "އަދި އިރާއި، ހަނދު އެއްތަންކުރައްވާހިނދެވެ.",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5561,
        "text": "އެދުވަހުން އިންސާނާ ބުނާނެތެވެ. ފިލަންދާނީ ކޮންތާކަށް ހެއްޔެވެ؟",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5562,
        "text": "ހަމަހިލާ އެހެންނަކުން ނުވާނެއެވެ. ރައްކާވެ ތިބެންދާނެ ތަނެއްނެތެވެ.",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5563,
        "text": "އެދުވަހުންގޮސް ހަމަޖެހުންވަނީ، ކަލޭގެފާނުގެ ވެރިރަސްކަލާނގެ حضرة އަށެވެ.",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5564,
        "text": "އިންސާނާ އިސްކުޅަހާ ކަމަކާއި، ފަސްކުޅަހާ ކަމެއްގެ ވާހަކައިން އެ އިންސާނާއަށް އެދުވަހުން خبر ދެއްވާނެތެވެ.",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5565,
        "text": "އަދި ކިއެއްތަ! އެ އިންސާނާވަނީ އޭނާގެ އަމިއްލަ نفس ގެ މައްޗަށްވާ ހެތްކެއް ކަމުގައެވެ.",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5566,
        "text": "އެ އިންސާނާ، އޭނާގެ عذر ތައް ކުރިމަތިކުޅަނަމަވެސް މެއެވެ.",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5567,
        "text": "އެ قرآن ލިބިވަޑައިގަތުން އަވަސްކުރެއްވުމަށް ކަލޭގެފާނުގެ ދޫފުޅު، އެ قرآن އާއިގެން حركات ތެރި ނުކުރައްވާށެވެ!",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5568,
        "text": "ހަމަކަށަވަރުން، އެ قرآن (ކަލޭގެފާނުގެ ހިތްޕުޅަށް) جمع ކުރެއްވުމާއި، އަދި އެ قرآن ކިޔަވައިވިދާޅުވުމުގެ ކުޅަދާނަކަން ދެއްވުމަކީ، ތިމަންރަސްކަލާނގެއަށް އޮތްކަމެކެވެ.",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5569,
        "text": "ފަހެ، ތިމަންރަސްކަލާނގެ (جبريل ގެފާނު މެދުވެރިކުރައްވައި) އެ قرآن ކިޔަވައިދެއްވާހިނދު، ފަހެ، ކަލޭގެފާނު އެ ކިޔަވައިދެއްވުމާ تبع ވެވަޑައިގަންނަވާށެވެ!",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5570,
        "text": "ދެން ހަމަކަށަވަރުން، އެ قرآن ބަޔާންކޮށްދެއްވުން ވަނީވެސް ތިމަންރަސްކަލާނގެއަށެވެ.",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5571,
        "text": "ހަމަހިލާ އެހެންނަކުން ނުވާނެއެވެ. އަދި ކިއެއްތަ! ތިޔަބައިމީހުން ލޯބިކުރަނީ ދުނިޔޭގެ ކަންތަކަށެވެ.",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5572,
        "text": "އަދި ތިޔަބައިމީހުން آخرة ދޫކޮށްލަމުއެވެ.",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5573,
        "text": "އެދުވަހުން މޫނުތަކެއް ރީތިވެ، އަލިވެގެންވެއެވެ.",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5574,
        "text": "އެ މޫނުތަކުގެ ވެރިރަސްކަލާނގެއަށް ބަލަބަލައި ތިބެއެވެ.",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5575,
        "text": "އަދި އެދުވަހުން މޫނުތަކެއް ހުތުރުވެ، މޮޅިވެގެންވެއެވެ.",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5576,
        "text": "މައިކަށީގެ ބަދަތައް ބިނދޭވަރުގެކަމެއް ކުރެވޭނެކަމަށް (އެ މޫނުތަކުގެ ވެރިން) ހީކުރެތެވެ.",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5577,
        "text": "ހަމަހިލާ އެހެންނަކުން ނުވާނެއެވެ. ވައިނޮޅިކަށިގަނޑާ ހަމަޔަށް ފުރާނަ ފޯރައްޖެހިނދެވެ.",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5578,
        "text": "އަދި އަންގަވާހިނދެވެ. ފަންޑިތައެއް ހަދައި، ފަރުވާއެއް ކުރާނީ ކާކުހެއްޔެވެ؟",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5579,
        "text": "އަދި އެއީ ވަކިވުން ކަމުގައި يقين ކޮށްފި ހިނދެވެ.",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5580,
        "text": "އަދި (މަރުގެ ސަކަރާތުގައި) އެއްފައި އަނެއްފަޔާ އޮޅައިލެވޭހިނދެވެ.",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5581,
        "text": "އެދުވަހުން އެމީހަކު ގެންދެވުންވަނީ، ކަލޭގެފާނުގެ ވެރިރަސްކަލާނގެ حضرة އަށެވެ.",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5582,
        "text": "ފަހެ، އޭނާ (اللَّه ގެ ފޮތް) ތެދެއްވެސް ނުކުރެތެވެ. އަދި ނަމާދެއްވެސް ނުކުރެތެވެ.",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5583,
        "text": "އެހެނެއްކަމަކު އޭނާ ދޮގުކުރިއެވެ. އަދި އޭނާ ފުރަގަސްދީ އެނބުރިދިޔައެވެ.",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5584,
        "text": "ދެން އޭނާގެ أهل ވެރިންގެ ގާތަށް ބޮޑާކަންމަތީ ހިނގަމުންދިޔައެވެ.",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5585,
        "text": "ކަލެޔަށް ހަލާކު ހުށްޓެވެ. ފަހެ، (ކަލެޔަށް) ހަލާކު ހުށްޓެވެ.",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5586,
        "text": "ދެންވެސް ކަލެޔަށް ހަލާކު ހުށްޓެވެ. ފަހެ، (ކަލެޔަށް) ހަލާކު ހުށްޓެވެ.",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5587,
        "text": "ބޭކާރުގޮތުގައި އޭނާ ދޫކޮށްލައްވާނެކަމަށް އިންސާނާ ހީކުރަނީ ހެއްޔެވެ؟",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5588,
        "text": "އޮހޮރައިލެވޭ މަނީގެ ތިއްކެއް ކަމުގައި އޭނާ ނުވޭހެއްޔެވެ؟",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5589,
        "text": "ދެން ގަނޑުލޭކޮޅެއް ކަމުގައި އޭނާ ވިއެވެ. ފަހެ، އެކަލާނގެ އެ އިންސާނާ ހައްދަވައި، ފުރިހަމަކުރެއްވިއެވެ.",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5590,
        "text": "ފަހެ، އޭނާގެ ކިބައިން، ފިރިހެން އަންހެން ދެބާވަތް އުފެއްދެވިއެވެ.",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5591,
        "text": "އެކަން އެގޮތަށް ކުރެއްވި ރަސްކަލާނގެ، މަރުވެފައިވާ މީހުން އަލުން ދިރުއްވުމަށް ކުޅަދުންވަންތަވެވޮޑިގެން ނުވޭހެއްޔެވެ؟",
