{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 84,
    "name": "سُورَةُ الانشِقَاقِ",
    "englishName": "Al-Inshiqaaq",
    "englishNameTranslation": "The Splitting Open",
    "revelationType": "Meccan",
    "numberOfAyahs": 25,
    "ayahs": [
      {
        "number": 5885,
        "text": "އުޑުފަޅަފަޅައިގެންދާ ހިނދެވެ.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5886,
        "text": "އަދި އެ އުޑުގެ ވެރިރަސްކަލާނގެ أمر ފުޅަށް އެ އުޑު ކިޔަމަންވާހިނދެވެ. އެ އުޑަށް އެކަން حق ވެގެންވެއެވެ.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5887,
        "text": "އަދި ބިންގަނޑު ދެމި، ފެތުރިގެންދާ ހިނދެވެ.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5888,
        "text": "އަދި އެ ބިމުގެ ތެރޭގައިވާ ތަކެތި އުކާލައި، އެ ބިން ހުސްވެގެންދާ ހިނދެވެ.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5889,
        "text": "އަދި އެ ބިމުގެ ވެރިރަސްކަލާނގެ أمر ފުޅަށް އެ ބިން ކިޔަމަންވާ ހިނދެވެ. އެ ބިމަށް އެކަން حق ވެގެންވެއެވެ.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5890,
        "text": "އޭ އިންސާނާއެވެ! ހަމަކަށަވަރުން، ތިބާއީ، ތިބާގެ ވެރިރަސްކަލާނގެ حضرة އަށް ދިޔުމަށް ވަރަށްބޮޑަށް ގަދައަޅައި މަސައްކަތްކުރާ މީހެކެވެ. ފަހެ، ތިބާ (އެ މަސައްކަތާއިގެން) އެކަލާނގެއާ ބައްދަލުވާނެތެވެ.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5891,
        "text": "ފަހެ، އެމީހެއްގެ ފަތް، އެމީހެއްގެ ކަނާތަށް ލިބުނު މީހާ ދަންނާށެވެ!",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5892,
        "text": "ފަހެ، ލުއިފަސޭހަވެގެންވާ حساب ބެއްލެވުމަކުން، ނިކަންހުރެ އެމީހަކާމެދު حساب ބައްލަވާނެތެވެ.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5893,
        "text": "އަދި އުފަލުންހުރެ، އޭނާގެ أهل ވެރިންގެގާތަށް (ސުވަރުގެޔަށް) އެނބުރިދާނެތެވެ.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5894,
        "text": "އަދި އެމީހެއްގެ ފަތް، އެމީހެއްގެ ފުރަގަހުން (ވާތަށް) ލިބުނުމީހާ ދަންނާށެވެ.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5895,
        "text": "ފަހެ، ނިކަންހުރެ އެމީހަކު ގޮވާނެތެވެ. އޭ ހުރި ހަލާކެއްގެ ބޮޑުކަމާއެވެ!",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5896,
        "text": "އަދި އަނދައަނދާ ހުންނަ ނަރަކަޔަކަށް އޭނާ ވަންނާނެތެވެ.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5897,
        "text": "ހަމަކަށަވަރުން، އޭނާގެ أهل ވެރިންގެ ގާތުގައި އުފާވެރިވެގެންވާ މީހަކު ކަމުގައި (ދުނިޔޭގައި) އޭނާވިއެވެ.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5898,
        "text": "ހަމަކަށަވަރުން (އޭނާގެ ވެރިރަސްކަލާނގެ حضرة އަށް) އެނބުރި ނުދާނޭކަމަށް އޭނާ ހީކުޅައެވެ.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5899,
        "text": "އެހެންނަކުންނޫނެވެ. ހަމަކަށަވަރުން، އޭނާގެ ވެރިރަސްކަލާނގެ އޭނާ ދެކެވޮޑިގެންވާ ކަމުގައިވެއެވެ.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5900,
        "text": "ފަހެ، ތިމަންރަސްކަލާނގެ ރަތްވިލާ ގަންދެއްވަމެވެ.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5901,
        "text": "އަދި ރޭގަނޑާއި، އެ ރޭގަނޑު އެއްކުޅަހާ ތަކެތި ގަންދެއްވަމެވެ.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5902,
        "text": "އަދި ހަނދުގެ އަލިކަން ފުރިހަމަވާހިނދު، އެ ހަނދު ގަންދެއްވަމެވެ.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5903,
        "text": "ހަމަކަށަވަރުން، ތިޔަބައިމީހުން އެއް حالة އަކަށްފަހު، އަނެއް حالة އަކަށް ބަދަލުވަމުން ދާނޭކަން ކަށަވަރެވެ.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5904,
        "text": "އެއުރެން إيمان ނުވަނީ ފަހެ، އެއުރެންނަށް ކީއްވެގެން ހެއްޔެވެ؟",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5905,
        "text": "އަދި އެއުރެންނަށް قرآن ކިޔަވައިދެވޭހިނދު، އެއުރެން سجدة އެއް ނުކުރެތެވެ.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": {
          "id": 14,
          "recommended": true,
          "obligatory": false
        }
      },
      {
        "number": 5906,
        "text": "އަދި ކިއެއްތަ! كافر ވި މީހުން (އެ قرآن) ދޮގުކުރޫއެވެ.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5907,
        "text": "އަދި އެއުރެންގެ ހިތްތަކުގައި ފޮރުވާފައިވާ ކަންތައް، اللَّه ދެނެވޮޑިގަތުން ބޮޑެވެ.",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5908,
        "text": "ފަހެ، ވޭންދެނިވި عذاب ހުރިކަމުގެ ވާހަކައިން ކަލޭގެފާނު އެއުރެންނަށް خبر ދެއްވާށެވެ!",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5909,
        "text": "إيمان ވެ صالح عمل ތައް ކުޅަމީހުން މެނުވީއެވެ. އެއުރެންނަށް ކެނޑިގެންނުދާނޭ ދަރުމަހުށްޓެވެ.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      }
    ],
