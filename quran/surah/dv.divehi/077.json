{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 77,
    "name": "سُورَةُ المُرۡسَلَاتِ",
    "englishName": "Al-Mursalaat",
    "englishNameTranslation": "The Emissaries",
    "revelationType": "Meccan",
    "numberOfAyahs": 50,
    "ayahs": [
      {
        "number": 5623,
        "text": "ވިދިވިދިގެން އަންނަ ވައިރޯޅިތަކާއި،",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5624,
        "text": "އަދި ގަދަޔަށް އަންނަ ބާރު ވައިރޯޅިތަކާއި،",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5625,
        "text": "އަދި ވިލާތައް ފަރާތްފަރާތަށް ފަތުރާ ވައިރޯޅިތަކާއި،",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5626,
        "text": "حق އާއި باطل ވަކިކުރާ (قرآن ގެ) آية ތަކާއި،",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5627,
        "text": "އަދި ހަނދުމަކޮށްދޭ آية ތައް (رسول ންނަށް) ގެނެސްދެއްވާ ملائكة ން ގަންދެއްވަމެވެ.",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5628,
        "text": "عذ ر ނެތިކުރެއްވުމަށާއި، إنذار ކުރެއްވުމަށެވެ.",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5629,
        "text": "ހަމަކަށަވަރުން، ތިޔަބައިމީހުންނަށް وعد ކުރައްވާ ކަންތަކީ، ވާނޭކަންތައްކަން ކަށަވަރެވެ.",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5630,
        "text": "ފަހެ، ތަރިތަކުން އޭގެ އަލިކަން ފުއްސަވައިފިހިނދެވެ.",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5631,
        "text": "އަދި އުޑު ފަޅައިގެން ހިނގައްޖެހިނދެވެ.",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5632,
        "text": "އަދި ފަރުބަދަތައް ކުނޑިކުނޑިވެގެން ހިނގައްޖެހިނދެވެ.",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5633,
        "text": "އަދި رسول ން ކަނޑައެޅިގެންވާ ވަގުތަށް އެއްކުރައްވައިފިހިނދެވެ.",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5634,
        "text": "އެކަންތައްތައް ފަސްކުރައްވާފައިވަނީ، ކޮންދުވަހަކަށް ހެއްޔެވެ؟",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5635,
        "text": "ނިޔާކުރައްވާ ދުވަހަށެވެ.",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5636,
        "text": "އަދި ނިޔާކުރައްވާ ދުވަހަކީ، ކޮންދުވަހެއްކަން ކަލޭގެފާނަށް އަންގައިދިނީ ކޮންއެއްޗެއްތޯއެވެ؟",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5637,
        "text": "ދޮގުކުރާ މީހުންނަށް އެދުވަހުން ގެއްލުމާއި ހަލާކު ހުށްޓެވެ.",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5638,
        "text": "ތިމަންރަސްކަލާނގެ، އިސްވެދިޔަ މީހުން ހަލާކު ނުކުރައްވަންހެއްޔެވެ؟",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5639,
        "text": "ދެން ތިމަންރަސްކަލާނގެ އެއުރެންގެ ފަހަތުން ފަހުގެ މީހުން ގެންދަވާހުށީމެވެ.",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5640,
        "text": "ކުށްވެރިންނާމެދު، ތިމަންރަސްކަލާނގެ ކަންތައް ކުރައްވަނީ އެފަދައިންނެވެ.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5641,
        "text": "ދޮގުކުރާ މީހުންނަށް އެދުވަހުން ގެއްލުމާއި، ހަލާކު ހުށްޓެވެ.",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5642,
        "text": "ދެރަވެގެންވާ ފެނަކުން (އެބަހީ: މަނިފޮދަކުން) ތިމަންރަސްކަލާނގެ ތިޔަބައިމީހުން ނަހައްދަވަން ހެއްޔެވެ؟",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5643,
        "text": "ފަހެ، ރައްކާތެރިވެގެންވާ ތަނެއްގައި، (އެބަހީ: رحم ގައި) ތިމަންރަސްކަލާނގެ އެ ފެން ލެއްވީމެވެ.",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5644,
        "text": "އެނގިގެންވާ މުއްދަތަކަށެވެ.",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5645,
        "text": "ފަހެ، ތިމަންރަސްކަލާނގެ (އެހުރިހާކަމެއް) މިންވަރު ކުރެއްވީމެވެ. ފަހެ، މިންވަރުކުރައްވާ ކަލާނގެ މޮޅުކަމާއެވެ!",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5646,
        "text": "ދޮގުކުރާ މީހުންނަށް އެދުވަހުން ގެއްލުމާއި ހަލާކުހުށްޓެވެ.",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5647,
        "text": "ދޮގުކުރާ މީހުންނަށް އެދުވަހުން ގެއްލުމާއި ހަލާކުހުށްޓެވެ.",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5648,
        "text": "ދިރިތިބި މީހުންނާއި، މަރުވެފައިވާ މީހުން އެތަނަށް އެއްކުރައްވާނެ ތަނެއް ކަމުގައި، ތިމަންރަސްކަލާނގެ ބިން ނުލައްވަން ހެއްޔެވެ؟",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5649,
        "text": "އަދި ތިމަންރަސްކަލާނގެ އެ ބިމުގައި ހަރުލައިހުންނަ އުސްފަރުބަދަތައް ލެއްވީމެވެ. އަދި ތިމަންރަސްކަލާނގެ ތިޔަބައިމީހުންނަށް މީރުފެން ބޯންދެއްވީމެވެ.",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5650,
        "text": "ދޮގުކުރާ މީހުންނަށް އެދުވަހުން ގެއްލުމާއި ހަލާކު ހުށްޓެވެ.",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5651,
        "text": "(އެއުރެންނަށް އަންގަވާނެތެވެ.) ތިޔަބައިމީހުން ދޮގުކޮށްއުޅުނު ކަންތަކަށް ތިޔަބައިމީހުން ހިނގައިދާށެވެ!",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5652,
        "text": "ތިންކަތްޕަށް ކަފިވެފައިވާ ހިޔަންޏެއްގެ ތެރެޔަށް (އެބަހީ: ނަރަކައިގެ އަލިފާންގަނޑުގެ ދުމުގެ ތެރެޔަށް) ތިޔަބައިމީހުން ހިނގައިދާށެވެ!",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5653,
        "text": "(އެއިން) ހިޔަލެއްވެސް ނުދެތެވެ. އަދި ހުޅުގަނޑުގެ ހޫނުން ސަލާމަތްކޮށެއްވެސް ނުމެދޭމެއެވެ.",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5654,
        "text": "ހަމަކަށަވަރުން، ބޮޑެތި ގަނޑުވަރުފަދަ ކަނިތައް، އެ ހުޅުގަނޑު އުކައެވެ.",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5655,
        "text": "އެ ކަނިތައް، ރީނދޫކުލައިގެ جمل ތަކެއް ފަދައެވެ.",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5656,
        "text": "ދޮގުކުރާ މީހުންނަށް އެދުވަހުން ގެއްލުމާއި ހަލާކު ހުށްޓެވެ.",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5657,
        "text": "މިއީ އެއުރެންނަށް ބަސްނުމޮށެވޭނޭ ދުވަހެވެ.",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5658,
        "text": "އަދި އެއުރެން عذر ދެއްކުމަކަށް، އެއުރެންނަކަށް إذن އެއް ނުމެދެއްވާނޭމެއެވެ.",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5659,
        "text": "ދޮގުކުރާ މީހުންނަށް އެދުވަހުން ގެއްލުމާއި، ހަލާކު ހުށްޓެވެ.",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5660,
        "text": "(وحى ކުރައްވާނެތެވެ.) މިއީ ނިޔާކުރެއްވުމުގެ ދުވަހެވެ. ތިމަންރަސްކަލާނގެ ތިޔަބައިމީހުންނާއި، އަދި ކުރީގެ މީހުން އެއްކުރެއްވީމެވެ.",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5661,
        "text": "ފަހެ، ތިޔަބައިމީހުންނަށް ހަދާނޭ حيلة އެއް ލިބިގެންވާނަމަ، ފަހެ، ތިމަންރަސްލާނގެއާމެދު ތިޔަބައިމީހުން حيلة ހަދައިބަލާށެވެ!",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5662,
        "text": "ދޮގުކުރާ މީހުންނަށް އެދުވަހުން ގެއްލުމާއި، ހަލާކު ހުށްޓެވެ.",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5663,
        "text": "ހަމަކަށަވަރުން، تقوى ވެރިންވަނީ، ހިޔާތަކާއި، ފެން އާރުތަކުގެ نعمة ގައެވެ.",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5664,
        "text": "އަދި އެއުރެން ހިތްއެދޭ މޭވާތަކުގެ ތެރޭގައެވެ.",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5665,
        "text": "(އަދި އަންގަވާނެތެވެ.) ތިޔަބައިމީހުން ކޮށްއުޅުނު عمل ތަކުގެ ސަބަބުން، އަރާމު ލިބެނިވިގޮތުގައި ތިޔަބައިމީހުން ކައިބޮއި އުޅޭށެވެ!",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5666,
        "text": "إحسان ތެރިންނަށް ހަމަކަށަވަރުން، ތިމަންރަސްކަލާނގެ ޖަޒާ ދެއްވަނީ އެފަދައިންނެވެ.",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5667,
        "text": "ދޮގުކުރާ މީހުންނަށް އެދުވަހުން ގެއްލުމާއި، ހަލާކު ހުށްޓެވެ.",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5668,
        "text": "(ދުނިޔޭގައި) ކުޑަހިނދުކޮޅަކަށް ތިޔަބައިމީހުން ކައި، އަރާމުކޮށް އުޅޭށެވެ! ހަމަކަށަވަރުން، ތިޔަބައިމީހުންނީ، ކުށްވެރިންނެވެ.",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5669,
        "text": "ދޮގުކުރާ މީހުންނަށް އެދުވަހުން ގެއްލުމާއި، ހަލާކު ހުށްޓެވެ.",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5670,
        "text": "ތިޔަބައިމީހުން (ނަމާދުކޮށް) ركوع ކޮށްއުޅޭށޭ އެއުރެންނަށް ބުނެފިނަމަ، އެއުރެން ركوع ކޮށެއް ނޫޅެތެވެ.",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5671,
        "text": "ދޮގުކުރާ މީހުންނަށް އެދުވަހުން ގެއްލުމާއި، ހަލާކުހުށްޓެވެ.",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5672,
        "text": "ފަހެ، އެ قرآن އަށްފަހު، އެއުރެން إيمان ވަނީ، ކޮން ވާހަކައަކަށް ހެއްޔެވެ؟",
