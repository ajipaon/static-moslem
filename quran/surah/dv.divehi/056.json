{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 56,
    "name": "سُورَةُ الوَاقِعَةِ",
    "englishName": "Al-Waaqia",
    "englishNameTranslation": "The Inevitable",
    "revelationType": "Meccan",
    "numberOfAyahs": 96,
    "ayahs": [
      {
        "number": 4980,
        "text": "قيامة ވެއްޖެހިނދެވެ.",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4981,
        "text": "އެކަންވާނޭކަމާމެދު إنكار ކޮށް ދޮގުކުރާނޭ ފަރާތެއް ނުވެއެވެ.",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4982,
        "text": "އެއްބަޔަކު ދަށްކުރަމުން، އަނެއްބަޔަކު މަތިކުރަމުންނެވެ. (އެބަހީ: އަބާއްޖަވެރިން ދަށްކުރައްވައި، ބާއްޖަވެރިން މަތިވެރިކުރައްވާ ދުވަހެއްގެ ގޮތުގައެވެ.)",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4983,
        "text": "ބިންގަނޑު ގަދަފަދައަށް ހެލިގެންފިހިނދެވެ.",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4984,
        "text": "އަދި ފަރުބަދަތައް ފުނޑުފުނޑުވެ، ހިމުންވެއްޖެހިނދެވެ.",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4985,
        "text": "ފަހެ، ވަޔާ ބުރަބުރައިގެންދާ ކުނޑިތަކެއްކަމުގައި އެ ފަރުބަދަތައް ވެއްޖެހިނދެވެ.",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4986,
        "text": "އަދި ތިޔަބައިމީހުން (އެދުވަހުން) ތިންބާވަތަކަށް ވެދެއެވެ.",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4987,
        "text": "ފަހެ، ކަނާތްފަރާތުގެ މީހުންނެވެ. ކަނާތްފަރާތުގެ މީހުންނީ، ކޮންބަޔެއްހެއްޔެވެ؟",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4988,
        "text": "އަދި ވާތްފަރާތުގެ މީހުންނެވެ. ވާތްފަރާތުގެ މީހުންނީ، (ދަރަޖައިގެގޮތުން) ކޮންބަޔެއްހެއްޔެވެ؟",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4989,
        "text": "އަދި ކުރިއަރައިދިޔަ މީހުންނީ، (ދަރަޖައިގެގޮތުން) ކުރިއަރައިދިޔަ މީހުންނެވެ.",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4990,
        "text": "ކުއްތަންކުރައްވާފައިވާ މީހުންނީ، އެއުރެންނެވެ.",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4991,
        "text": "نعمة ގެ ސުވަރުގެތަކުގައެވެ.",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4992,
        "text": "އިސްވެދިޔަމީހުންގެ ތެރެއިން (އެބަހީ: އިސްވެދިޔަ أمّة ތަކުގެ މީހުންގެ ތެރެއިން) ގިނަބަޔެކެވެ.",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4993,
        "text": "އަދި ފަހުގެ މީހުންގެ ތެރެއިން (އެބަހީ: އިސްވެދިޔަ أمّة ތަކާ އަޅައިބަލާއިރު، محمد ނަބިއްޔާގެ أمّة ގެ މީހުންގެ ތެރެއިން) މަދުބަޔެކެވެ.",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4994,
        "text": "ރަނާއި، ޖަވާހިރުން އަތްކަން އަޅުއްވާފައިވާ ދާންތަކެއްގެ މަތީގައެވެ.",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4995,
        "text": "އެ ދާންތަކުގެ މަތީގައި، އެކަކު އަނެކަކާ ކުރިމަތިލައި، ލެނގިގެންވާ حال ގައެވެ.",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4996,
        "text": "ޒުވާންކަންމަތީ ދެމިތިބޭ ޒުވާން ފިރިހެންކުދިން، އެއުރެންގެ خدمة ގައި ބުރުޖަހާނެތެވެ.",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4997,
        "text": "ތަށިތަކަކާއި، ކުރާތަކަކާއިގެންނެވެ. އަދި ހިނގަހިނގާ ހުންނަ އާރަކުން ނެގޭ ރާތަށިތަކަކާއިގެންނެވެ.",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4998,
        "text": "އޭގެސަބަބުން އެއުރެންނަށް ބޮލުގެ ރިހުމެއް ނުލިބޭނެތެވެ. އަދި މަސްތުވުމެއްވެސް ނުމެލިބޭނޭމެއެވެ.",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4999,
        "text": "އަދި އެއުރެން ބޭނުންގޮތަކަށް ނަގާނޭ މޭވާތަކާއިގެންނެވެ.",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5000,
        "text": "އަދި އެއުރެން ހިތްއެދޭ ބާވަތްތަކުގެ ދޫނިސޫފާސޫފީގެ މަހާއިގެންނެވެ.",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5001,
        "text": "އަދި ކަޅިރަވަ ރީތި، حورالعين ން ތިބޭނެއެވެ.",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5002,
        "text": "(އެކަނބަލުން) ރައްކާތެރިކުރައްވާފައިވާ ރަންމުތްތަކަކާ އެއްފަދަވެގެންވެއެވެ.",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5003,
        "text": "(އެއީ) އެއުރެން ކޮށްއުޅުނު عمل ތަކަށް ޖަޒާއެއް ކަމުގައެވެ.",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5004,
        "text": "ބޭކާރު ބަސްތަކެއްވެސް، އަދި ކުށްވެރިކުރަނިވި ބަސްތަކެއްވެސް، އެތާކު އެއުރެންނަކަށް ނީވޭނެތެވެ.",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5005,
        "text": "އެހެނެއްކަމަކު (އެއުރެންނަށް އިވޭނީ) سلام ހުށްޓޭ، سلام ހުށްޓޭ، ބުނެވޭ އަޑެވެ.",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5006,
        "text": "ކަނާތްފަރާތުގެ މީހުންނެވެ. ކަނާތްފަރާތުގެ މީހުންނީ، ކޮންބަޔެއްހެއްޔެވެ؟",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5007,
        "text": "(އެއުރެންވަނީ) ކަށިނެތް، ކުންނާރު ގަސްތަކުގެ ތެރޭގައެވެ.",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5008,
        "text": "ކެޔޮވައްތައް އެތުރިފައިވާ ފަތްކެޔޮ ގަސްތަކުގެ ތެރޭގައެވެ.",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5009,
        "text": "އަދި ފެތުރިގެންވާ ހިޔަލުގައެވެ.",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5010,
        "text": "އަދި އަބަދަށް އޮހިއޮހިހުންނަ ފެން، އެއުރެންނަށް ލިބޭނެއެވެ.",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5011,
        "text": "އަދި ގިނަގުނަވެގެންވާ މޭވާތައްވެސްމެއެވެ.",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5012,
        "text": "ހުސްވުމެއްނުވާ، އަދި މަނާވުމެއްނުވާ މޭވާއެވެ.",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5013,
        "text": "އަދި އުފުލިގެންވާ ދާންތަކެވެ.",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5014,
        "text": "ހަމަކަށަވަރުން، އަލުން އާ އުފެއްދެވުމަކުން ތިމަންރަސްކަލާނގެ އެކަނބަލުން (އެބަހީ: ސުވަރުގޭގެ އަންހެނުން) އުފެއްދެވީމެވެ.",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5015,
        "text": "ފަހެ، بِكر ވެރި ބަޔަކު ކަމުގައި ތިމަންރަސްކަލާނގެ އެކަނބަލުން ލެއްވީމެވެ.",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5016,
        "text": "ފިރިންދެކެ ލޯބިވާ، އެއްފުރާގެ ބަޔަކު ކަމުގައެވެ.",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5017,
        "text": "ކަނާތްފަރާތުގެ މީހުންނަށްޓަކައެވެ.",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5018,
        "text": "އިސްވެދިޔަ މީހުންގެ ތެރެއިން (އެބަހީ: އިސްވެދިޔަ أمّة ތަކުގެ މީހުންގެ ތެރެއިން) ގިނަބަޔެކެވެ.",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5019,
        "text": "އަދި ފަހުގެ މީހުންގެ ތެރެއިންވެސް (އެބަހީ: محمد ނަބިއްޔާގެ أمّة ގެ މީހުންގެ ތެރެއިންވެސް) ގިނަބަޔެކެވެ.",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5020,
        "text": "ވާތްފަރާތުގެ މީހުންނެވެ. ވާތްފަރާތުގެ މީހުންނީ، ކޮންބަޔެއްހެއްޔެވެ؟",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5021,
        "text": "(އެއުރެންވަނީ، ލޯލިތަކުގެ ބުޑުތައް ފޫވައްޓައިފަނިވި) ހޫނު ވައިގެތެރޭގައެވެ. އަދި ކެކިކެކިހުންނަ ހޫނުފެނުގެ ތެރޭގައެވެ.",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5022,
        "text": "އަދި ކަޅުކުލައިގެ ދުމަކުން އުފެދޭ ހިޔަނިގަނޑެއްގެ ތެރޭގައެވެ.",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5023,
        "text": "ފިންޏެއްވެސް ނޫނެވެ. އަދި އަރާމެއްވެސް ނޫނެވެ.",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5024,
        "text": "ހަމަކަށަވަރުން، އޭގެކުރިން އެއުރެންވީ، ތަނަވަސްކަމުގައި ހައްދުފަހަނަޅައިފައިވާ ބަޔަކު ކަމުގައެވެ.",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5025,
        "text": "އަދި ބޮޑުވެގެންވާ ފާފަވެރިކަމުގެ މަތީގައި ދެމިތިބޭ ބަޔަކުކަމުގައި އެއުރެންވޫއެވެ.",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5026,
        "text": "އަދި އެއުރެން ބުނާކަމުގައިވިއެވެ. ތިމަންމެން މަރުވެ، ވެއްޔަށާއި ކަށިތަކަކަށް ވެއްޖެހިނދު، އަލުން ދިރުމަކަށް ފޮނުއްވާނޭ ބަޔަކުކަމުގައި ހަމަކަށަވަރުން، ތިމަންމެން ވޭހެއްޔެވެ؟",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5027,
        "text": "އަދި ތިމަންމެންގެ އިސްވެދިޔަ ކާބަފައިންވެސް ހެއްޔެވެ؟",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5028,
        "text": "އަދި ތިމަންމެންގެ އިސްވެދިޔަ ކާބަފައިންވެސް ހެއްޔެވެ؟",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5029,
        "text": "ކަލޭގެފާނު ވިދާޅުވާށެވެ! ހަމަކަށަވަރުން، އިސްވެދިޔަ މީހުންނާއި، ފަހުން އަންނާނޭ މީހުންނީ، އެނގިގެންވާ ދުވަހެއްގެ ކަނޑައަޅުއްވާފައިވާ ވަގުތަކަށް އެއްކުރައްވާނޭ ބަޔަކުކަން ކަށަވަރެވެ.",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5030,
        "text": "ކަލޭގެފާނު ވިދާޅުވާށެވެ! ހަމަކަށަވަރުން، އިސްވެދިޔަ މީހުންނާއި، ފަހުން އަންނާނޭ މީހުންނީ، އެނގިގެންވާ ދުވަހެއްގެ ކަނޑައަޅުއްވާފައިވާ ވަގުތަކަށް އެއްކުރައްވާނޭ ބަޔަކުކަން ކަށަވަރެވެ.",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5031,
        "text": "އޭ މަގުފުރެދިގެންވާ، އަދި ދޮގުކުރާ މީހުންނޭވެ! ދެން ހަމަކަށަވަރުން، ތިޔަބައިމީހުން زقّوم ގަހުން ކާނޭކަން ކަށަވަރެވެ.",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5032,
        "text": "ފަހެ، އެއިން ބަނޑުތައް ފުރައިލާނޭމެއެވެ.",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5033,
        "text": "ފަހެ، އޭގަމައްޗަށް ކެކިކެކިހުންނަ ހޫނުފެން ބޯނޭމެއެވެ.",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5034,
        "text": "ފަހެ، ފެންބޮވައިގެންފައިވާ جمل ތައް ފެންބޯފަދައިންނެވެ.",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5035,
        "text": "قيامة ދުވަހުން އެއުރެންގެ މެހުމާންދާރީއަކީ މިއެވެ.",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5036,
        "text": "ތިޔަބައިމީހުން ހެއްދެވީ ތިމަންރަސްކަލާނގެއެވެ. ފަހެ ތިޔަބައިމީހުން ތެދުނުކުރަނީ ކީއްވެހެއްޔެވެ؟",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5037,
        "text": "ތިޔަބައިމީހުން ބޭރުކުރާ މަންޏާމެދު، ތިޔަބައިމީހުން ފަހެ، ވިސްނައި ބެލީމުހެއްޔެވެ؟",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5038,
        "text": "އެތަކެތި ހަދަނީ، ތިޔަބައިމީހުން ހެއްޔެވެ؟ ނުވަތަ ހައްދަވާ ފަރާތަކީ ތިމަންރަސްކަލާނގެ ހެއްޔެވެ؟",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5039,
        "text": "އެތަކެތި ހަދަނީ، ތިޔަބައިމީހުން ހެއްޔެވެ؟ ނުވަތަ ހައްދަވާ ފަރާތަކީ ތިމަންރަސްކަލާނގެ ހެއްޔެވެ؟",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5040,
        "text": "ތިޔަބައިމީހުންގެ މެދުގައި ތިމަންރަސްކަލާނގެ މަރު، މިންވަރުކުރެއްވީމެވެ. ތިޔަބައިމީހުންގެ ބަދަލުގައި، ތިޔަބައިމީހުންފަދަ އެހެންބަޔަކު ގެންނެވުމުގެ މައްޗަށާއި، ތިޔަބައިމީހުން ނުދަންނަ ސިފައެއްގައި، ތިޔަބައިމީހުން އަލުން އުފެއްދެވުމަށް ނުކުޅެދިގެންވާ ކަލަކުކަމުގައި ތިމަންރަސްކަލާނގެ ނުވަމެވެ.",
        "numberInSurah": 61,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5041,
        "text": "ހަމަކަށަވަރުން، ފުރަތަމަ އުފެއްދެވުން ތިޔަބައިމީހުންނަށް އެނގިފައިވެއެވެ. ފަހެ، ތިޔަބައިމީހުން ހަނދުމަނުކުރަނީ ކީއްވެހެއްޔެވެ؟",
        "numberInSurah": 62,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5042,
        "text": "ތިޔަބައިމީހުން ގޮވާންކުރާ ތަކެއްޗާމެދު، ފަހެ، ތިޔަބައިމީހުން ވިސްނައި ބެލީމުހެއްޔެވެ؟",
        "numberInSurah": 63,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5043,
        "text": "އެތަކެތި ބިމުން ފަޅުވަނީ، ތިޔަބައިމީހުންހެއްޔެވެ؟ ނުވަތަ އެތަކެތި ބިމުން ފަޅުއްވާ ފަރާތަކީ، ތިމަންރަސްކަލާނގެ ހެއްޔެވެ؟",
        "numberInSurah": 64,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5044,
        "text": "ތިމަންރަސްކަލާނގެ އިރާދަކުރައްވާނަމަ، ހިކި ކުނޑި ހުއިތަކެއް ކަމުގައި އެތަކެތި ލައްވައިފީމުސްކަން ކަށަވަރެވެ. ދެން އެކަމާމެދު ތިޔަބައިމީހުން حيران ވެފައި ތިބެއްޖައީހެވެ.",
        "numberInSurah": 65,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5045,
        "text": "(ތިޔަބައިމީހުން ބުނާނޫއެވެ.) ހަމަކަށަވަރުން، ތިމަންމެންނީ، ގެއްލުންވެގެންވާ ބަޔަކީމުއެވެ.",
        "numberInSurah": 66,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5046,
        "text": "އަދި ކިއެއްތަ! ތިމަންމެންނީ (އެތަކެތިން) محروم ވެގެންވާ ބަޔަކީމުއެވެ.",
        "numberInSurah": 67,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5047,
        "text": "ތިޔަބައިމީހުން ބޯ ފެނާމެދު، ތިޔަބައިމީހުން ފަހެ ވިސްނާ ބެލީމުހެއްޔެވެ؟",
        "numberInSurah": 68,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5048,
        "text": "ވާރޭވިލާތަކުން އެ ފެން ވެއްސީ، ތިޔަބައިމީހުންހެއްޔެވެ؟ ނުވަތަ އެެ ފެން ބާވައިލެއްވި ފަރާތަކީ، ތިމަންރަސްކަލާނގެ ހެއްޔެވެ؟",
        "numberInSurah": 69,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5049,
        "text": "ތިމަންރަސްކަލާނގެ އިރާދަކުރައްވާނަމަ، އެ ފެން ބޯން ނުކެރޭފަދަ ލޮނު ފެނެއް ކަމުގައި ލައްވައިފީމުހެވެ. ފަހެ، ތިޔަބައިމީހުން شكر ނުކުރަނީ ކީއްވެހެއްޔެވެ؟",
        "numberInSurah": 70,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5050,
        "text": "ތިޔަބައިމީހުން ރޯކުރާ އަލިފާނާމެދު، ތިޔަބައިމީހުން ފަހެ، ވިސްނައި ބެލީމުހެއްޔެވެ؟",
        "numberInSurah": 71,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5051,
        "text": "އޭގެ ގަސް އުފެއްދީ ތިޔަބައިމީހުން ހެއްޔެވެ؟ ނުވަތަ އެ އުފެއްދެވި ފަރާތަކީ ތިމަންރަސްކަލާނގެ ހެއްޔެވެ؟",
        "numberInSurah": 72,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5052,
        "text": "ހަނދުމަކުރުމެއް ކަމުގައްޔާއި، ދަތުރުފަތުރުކުރާ މީހުންނަށާއި، ދަތުރުނުކޮށް قائم ވެތިބޭ މީހުންނަށް ބޭނުންހިފޭ އެއްޗެއްކަމުގައި، ތިމަންރަސްކަލާނގެ އެ އަލިފާން ލެއްވީމެވެ.",
        "numberInSurah": 73,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5053,
        "text": "ފަހެ، ކަލޭގެފާނުގެ ވެރި، މާތްވަންތަ ރަސްކަލާނގެ اسم ފުޅުން ކަލޭގެފާނު تسبيح ވިދާޅުވާށެވެ!",
        "numberInSurah": 74,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5054,
        "text": "ފަހެ، ހަމަކަށަވަރުން، ތަރިތައް އޮސްފުހޭ ތަންތަން ގަންދެއްވައި ތިމަންރަސްކަލާނގެ ހުވާ وحى ކުރައްވަމެވެ.",
        "numberInSurah": 75,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5055,
        "text": "އަދި ތިޔަބައިމީހުން ދަންނަނީނަމަ، ހަމަކަށަވަރުން، އެއީ ބޮޑުވެގެންވާ ހުވަޔެއްކަން ކަށަވަރެވެ.",
        "numberInSurah": 76,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5056,
        "text": "ހަމަކަށަވަރުން، އެއީ މާތްވެގެންވާ قرآن ކަން ކަށަވަރެވެ.",
        "numberInSurah": 77,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5057,
        "text": "ރައްކާތެރިކުރައްވާފައިވާ ފޮތެއްގައެވެ.",
        "numberInSurah": 78,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5058,
        "text": "طاهر ވެގެންވާ މީހުން މެނުވީ، އެ ފޮތުގައި ނުބީހޭނެތެވެ.",
        "numberInSurah": 79,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5059,
        "text": "(އެއީ) عالم ތަކުގެ ވެރިރަސްކަލާނގެ حضرة ން ބާވައިލެއްވި ފޮތެކެވެ.",
        "numberInSurah": 80,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5060,
        "text": "ތިޔަބައިމީހުން ދޮގުކޮށް ފަރުވާކުޑަކުރަނީ، މިވާހަކައާމެދުގައި (އެބަހީ: މި قرآن އާ މެދުގައި) ހެއްޔެވެ؟",
        "numberInSurah": 81,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5061,
        "text": "އަދި ތިޔަބައިމީހުންނަށް رزق ދެއްވިކަމުގެ މައްޗަށްކުރާ شكر އަކީ (އެ رزق ދެއްވި ރަސްކަލާނގެ) ދޮގުކުރުން ކަމުގައި ތިޔަބައިމީހުން ހަދަމުއެވެ.",
        "numberInSurah": 82,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5062,
        "text": "ފަހެ، މަރުވުމުގެ ވަގުތުގައި، ކަރާހަމަޔަށް ފުރާނައައިސް ނޭވާހޮޅިޔަށް އެޅިއްޖެހިނދެވެ.",
        "numberInSurah": 83,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5063,
        "text": "އަދި އެހިނދު، ތިޔަބައިމީހުން ބަލަބަލައި ތިބެމުއެވެ.",
        "numberInSurah": 84,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5064,
        "text": "އަދި ތިޔަބައިމީހުންނަށްވުރެ އެމީހާއާ (އެބަހީ: މަރުވާ މީހާއާ) ގާތްކަންބޮޑީ، ތިމަންރަސްކަލާނގެއެވެ. އެހެނެއްކަމަކު، ތިޔަބައިމީހުންނަކަށް ނުފެނެތެވެ.",
        "numberInSurah": 85,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5065,
        "text": "އަދި ތިޔަބައިމީހުންނަށްވުރެ އެމީހާއާ (އެބަހީ: މަރުވާ މީހާއާ) ގާތްކަންބޮޑީ، ތިމަންރަސްކަލާނގެއެވެ. އެހެނެއްކަމަކު، ތިޔަބައިމީހުންނަކަށް ނުފެނެތެވެ.",
        "numberInSurah": 86,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5066,
        "text": "ފަހެ، ތިޔަބައިމީހުންނީ، عمل ތަކަށް ޖަޒާ ނުލިބޭނެ ބަޔަކުކަމުގައިވާނަމަ، ތިޔަބައިމީހުން އެ ފުރާނަ އަނބުރައި ނުގެންނަނީ ކީއްވެގެން ހެއްޔެވެ؟ ތިޔަބައިމީހުންނީ، ތެދުވެރިން ކަމުގައިވާނަމައެވެ.",
        "numberInSurah": 87,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5067,
        "text": "ފަހެ، އެ (މަރުވި) މީހާ ކުއްތަންކުރައްވާފައިވާ މީހުންގެ ތެރެއިންވާނަމަ، ދަންނާށެވެ!",
        "numberInSurah": 88,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5068,
        "text": "ފަހެ، އެމީހަކަށް އަރާމާއި، ތަނަވަސްވެގެންވާ رزق އާއި، نعمة ގެ ސުވަރުގެ ހުށްޓެވެ.",
        "numberInSurah": 89,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5069,
        "text": "އަދި އެމީހަކު ކަނާތްފަރާތުގެ މީހުންގެ ތެރެއިންވާނަމަ، ދަންނާށެވެ!",
        "numberInSurah": 90,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5070,
        "text": "ފަހެ، (އެމީހަކަށް އަންގަވާނެތެވެ.) ކަނާތްފަރާތުގެ މީހުންގެ ކިބައިން ތިބާޔަށް سلام ހުށްޓެވެ.",
        "numberInSurah": 91,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5071,
        "text": "އަދި ދޮގުކޮށް، މަގުފުރެދޭ މީހުންގެ ތެރެއިން އެމީހަކުވާނަމަ، ދަންނާށެވެ!",
        "numberInSurah": 92,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5072,
        "text": "ފަހެ، އެމީހަކަށްހުރީ، ކެކިކެކިހުންނަ ހޫނުފެނުގެ މެހުމާންދާރީއެކެވެ.",
        "numberInSurah": 93,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5073,
        "text": "އަދި ނަރަކަޔަށް ވެއްދެވުމެކެވެ.",
        "numberInSurah": 94,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5074,
        "text": "ހަމަކަށަވަރުން، މިއީ يقين ވެގެންވާ حق ތެދުކަން ކަށަވަރެވެ.",
        "numberInSurah": 95,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5075,
        "text": "ފަހެ، ކަލޭގެފާނު ގެ ވެރި، މާތްވަންތަ ރަސްކަލާނގެ اسم ފުޅުން ކަލޭގެފާނު تسبيح ވިދާޅުވާށެވެ!",
        "numberInSurah": 96,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      }
    ],
