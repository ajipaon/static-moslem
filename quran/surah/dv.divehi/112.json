    "numberOfAyahs": 4,
    "ayahs": [
      {
        "number": 6222,
        "text": "ކަލޭގެފާނު ވިދާޅުވާށެވެ! އެއީ އެއްކައުވަންތަ اللَّه އެވެ.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 554,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6223,
        "text": "اللَّه އީ، އެންމެހާ ތަކެތި އެކަލާނގެއަށް ބޭނުންތެރިވެގެންވާ، އަދި އެއްވެސް އެއްޗަކަށް، އެކަލާނގެ ބޭނުންވެވޮޑިގެންނުވާ ރަސްކަލާނގެއެވެ.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 554,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6224,
        "text": "އެކަލާނގެއަކަށް އެއްވެސް އެއްޗެއް އުފަންވެގެނެއް ނުވެތެވެ. އަދި އެއްވެސް އެއްޗަކަށް އެކަލާނގެ އުފަންވެވޮޑިގެނެއް ނުމެވޭމެއެވެ.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 554,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6225,
        "text": "އަދި އެކަލާގެއާ އެއްފަދަވެގެންވާ، ހަމައެކަކުވެސް ނުވެތެވެ.",
        "numberInSurah": 4,
