{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 81,
    "name": "سُورَةُ التَّكۡوِيرِ",
    "englishName": "At-Takwir",
    "englishNameTranslation": "The Overthrowing",
    "revelationType": "Meccan",
    "numberOfAyahs": 29,
    "ayahs": [
      {
        "number": 5801,
        "text": "އިރުގެ އަލިކަން ފިލައި، ހިނގައިދާ ހިނދެވެ.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5802,
        "text": "އަދި ތަރިތަކުން އަލިކަން ފިލައި، ބުރަބުރައިގެންދާ ހިނދެވެ.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5803,
        "text": "އަދި ފަރުބަދަތައް ލުހި، ފުނޑުފުނޑުވާ ހިނދެވެ.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5804,
        "text": "އަދި މާބަނޑު ޖަމަލުތައް އަޅާނުލައި ދޫކޮށްލާ ހިނދެވެ.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5805,
        "text": "އަދި ވަލު ޖަނަވާރުތައް (އެއްތަނަކަށް) އެއްކުރައްވާ ހިނދެވެ.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5806,
        "text": "އަދި ކަނޑުތައް ރޯވެ، އަނދައިގަންނަ ހިނދެވެ.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5807,
        "text": "އަދި ފުރާނަތައް، އޭގެ ހަށިތަކާ ގުޅިދާހިނދެވެ.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5808,
        "text": "އަދި ދިރިއޮއްވައި ވަޅުލި އަންހެން ދަރިޔާއާ سؤال ކުރައްވާ ހިނދެވެ.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5809,
        "text": "އޭނާ މަރައިލީ ކޮން ކުށެއްގެ ސަބަބުން ހެއްޔެވެ؟",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5810,
        "text": "އަދި ފަތްފުށްތައް ނިއުޅައިލައްވާ ހިނދެެވެ.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5811,
        "text": "އަދި އުޑު ގެންދަވާ ހިނދެވެ.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5812,
        "text": "އަދި ނަރަކަ ރޯކޮށްލައްވާ ހިނދެވެ.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5813,
        "text": "އަދި ސުވަރުގެ ގާތްކުރައްވާ ހިނދެވެ.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5814,
        "text": "ކޮންމެ نفس އަކަށްމެ، އެ نفس އެއް حاضر ކުޅަކަމެއް (އެހިނދު) އެނގޭނެތެވެ.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5815,
        "text": "ކޮންމެ نفس އަކަށްމެ، އެ نفس އެއް حاضر ކުޅަކަމެއް (އެހިނދު) އެނގޭނެތެވެ.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5816,
        "text": "ފަހެ، ރޭގަނޑަށް ފާޅުކަންބޮޑު، ދުވާލު ވަންހަނާވާ، ހިނގަހިނގާ ހުންނަ ތަރިތައް ގަންދެއްވަމެވެ.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5817,
        "text": "އަދި ރޭގަނޑު އަނދިރިކަމާއިގެން ފަސްދީދާހިނދު، އެ ރޭގަނޑު ގަންދެއްވަމެވެ.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5818,
        "text": "އަދި ފަތިހުގެ އަލިކަން ފާޅުވާހިނދު، އެ ފަތިސް ގަންދެއްވަމެވެ.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5819,
        "text": "ހަމަކަށަވަރުން، އެއީ މާތްވެގެންވާ رسول އަކު (އެބަހީ: جبريل ގެފާނު) ގެންނެވި ބަސްފުޅުކަން ކަށަވަރެވެ.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5820,
        "text": "عرش ގެ ވެރިރަސްކަލާނގެ حضرة ގައި ބާރެއްގެ ވެރި ދަރަޖަވެރިޔެކެވެ.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5821,
        "text": "އެތާނގައި (އެބަހީ: އުޑުތަކުގައި) ކިޔަމަންވެވޭ އަމާނާތްތެރިޔެކެވެ.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5822,
        "text": "އަދި ތިޔަބައިމީހުންގެ އެކުވެރިޔާއީ (އެބަހީ: محمد ގެފާނީ) މޮޔައަކު ކަމުގައި ނުވެއެވެ.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5823,
        "text": "އަދި ހަމަކަށަވަރުން، އެކަލޭގެފާނު (އެބަހީ: محمد ގެފާނު) ފާޅުވެގެންވާ އުދަރެހުން އެކަލޭގެފާނު (އެބަހީ: جبريل ގެފާނު) ދެކެވަޑައިގަތެވެ.",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5824,
        "text": "އަދި غيب ގެ ކަންތައް ފޮރުވައި، ހިފަހައްޓަވާ ބޭކަލަކު ކަމުގައި އެކަލޭގެފާނު (އެބަހީ: محمد ގެފާނު) ނުވެއެވެ.",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5825,
        "text": "އަދި رحمة ން ބޭރުކުރައްވާފައިވާ شيطان އެއްގެ ބަސްކަމުގައިވެސް އެ قرآن ނުވެއެވެ.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5826,
        "text": "ފަހެ، ތިޔަބައިމީހުން ތިޔަދަނީ ކޮންތާކަށް ހެއްޔެވެ؟",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5827,
        "text": "عالم ތަކަށް ހަނދާންކޮށްދިނުމެއް ކަމުގައި މެނުވީ އެ قرآن ނުވެއެވެ.",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5828,
        "text": "ތިޔަބައިމީހުންކުރެ ސީދާވެގަތުމަށް އެދޭ މީހަކަށްޓަކައެވެ.",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5829,
        "text": "عالم ތަކުގެ ވެރި اللَّه އިރާދަކުރެއްވުމުން މެނުވީ، ތިޔަބައިމީހުންނަކަށް (އެއްވެސް ކަމެއް) ނޭދެވޭނެތެވެ.",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      }
    ],
