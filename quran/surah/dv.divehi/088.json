{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 88,
    "name": "سُورَةُ الغَاشِيَةِ",
    "englishName": "Al-Ghaashiya",
    "englishNameTranslation": "The Overwhelming",
    "revelationType": "Meccan",
    "numberOfAyahs": 26,
    "ayahs": [
      {
        "number": 5968,
        "text": "ބިރުވެރިކަމާއިގެން އެންމެންނަށް ކުރިމަތިވެގެންއަންނާނޭ ކަންތަކުގެ ވާހަކަ) އެބަހީ: قيامة ގެ ވާހަކަ (ކަލޭގެފާނާ ހަމަޔަށް އައިސްފައިވޭތޯއެވެ؟",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5969,
        "text": "އެދުވަހުން، ދެރަވެ ނިކަމެތިވެގެންވާ މޫނުތަކެއް ވެއެވެ.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5970,
        "text": "މަސައްކަތްތެރި، ވަރުބަލި މޫނުތަކެކެވެ.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5971,
        "text": "ހޫނުކަން ގަދަވެގެންވާ ނަރަކަޔަށް އެއުރެން ވަންނާނެތެވެ.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5972,
        "text": "ހޫނުކަން ނިހާޔަތަށް ގޮސްފައިވާ ފެންއާރަކުން އެއުރެންނަށް ބޯންދެއްވާނެތެވެ.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5973,
        "text": "ކަށިހުރި، ވިހަ މޭވާޔަކުން މެނުވީ، އެއުރެންނަކަށް ކާނާއެއް ނުވެއެވެ.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5974,
        "text": ") އެ ކެއުމަކުން (ހަށިގަނޑަކަށް ބާރެއް ނުދޭނެތެވެ. އަދި ބަނޑުހައި ހޫނުކަން ފިލުވައެއް ނުމެދޭނޭމެއެވެ.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5975,
        "text": "އެދުވަހުން نعمة ލިބިގެންވާ، އުފާވެރި މޫނުތަކެއް ވެއެވެ.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5976,
        "text": "ދުނިޔޭގައި އެއުރެންކުޅަ މަސައްކަތަށް ރުހިގެންވެއެވެ.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5977,
        "text": "މަތިވެރިވެގެންވާ ސުވަރުގޭގައެވެ.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5978,
        "text": ") ގެއްލުންދެނިވި (ބޭކާރު ވާހަކައެއް، އެތާނގައި އެއުރެން ނާހާނެތެވެ.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5979,
        "text": "ހިނގަހިނގާ ހުންނަ ފެންއާރެއް އެތާނގައިވެއެވެ.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5980,
        "text": "އުފުލިގެންވާ ދާންތައް އެތާނގައިވެއެވެ.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5981,
        "text": "އަދި ބެހެއްޓިގެންވާ ތަށިތައްވެސްމެއެވެ.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5982,
        "text": "އަދި އެތުރިގެންވާ ބާލީސްތައްވެސްމެއެވެ.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5983,
        "text": "އަދި ފެތުރިގެންވާ ދޫލަތައްވެސްމެއެވެ.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5984,
        "text": "ފަހެ، ޖަމަލު ހައްދަވާފައިވަނީ، ކޮންފަދައަކުން ކަމާމެދު އެއުރެން ޖަމަލަށް ނުބަލަނީހެއްޔެވެ.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5985,
        "text": "އަދި އުޑު އުފުއްލަވާފައިވަނީ، ކޮންފަދައަކުން ކަމާމެދު އެއުރެން އުޑަށް ނުބަލަނީހެއްޔެވެ؟",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5986,
        "text": "އަދި ފަރުބަދަތައް ކޮޅަށް ނަންގަވާފައިވަނީ، ކޮންފަދައަކުން ކަމާމެދު އެއުރެން ފަރުބަދަތަކަށް ނުބަލަނީހެއްޔެެވެ؟",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5987,
        "text": "އަދި ބިން ފަތުރުއްވާފައިވަނީ، ކޮންފަދައަކުން ކަމާމެދު އެއުރެން ބިމަށް ނުބަލަނީހެއްޔެވެ؟",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5988,
        "text": "ފަހެ، ކަލޭގެފާނު) އެއުރެންނަށް (ހަނދުމަކޮށް ދެއްވާށެވެ! ކަލޭގެފާނު ވާކަން ކަށަވަރީ، ހަނދުމަކޮށްދެއްވާ ބޭކަލަކު ކަމުގައެވެ.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5989,
        "text": "އެއުރެންގެ މައްޗަށް ބާރު ހިންގަވާ ބޭކަލަކު ކަމުގައި ކަލޭގެފާނު ނުވެއެވެ.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5990,
        "text": "ފުރަގަސްދީ، كافر ވި މީހަކަށް މެނުވީއެވެ.",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5991,
        "text": "ފަހެ، اللّه، އެމީހަކަށް ބޮޑުވެގެންވާ عذاب ން عذاب ދެއްވާނެތެވެ.",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5992,
        "text": "ހަމަކަށަވަރުން، އެއުރެން އެނބުރި އައުންވަނީ، ތިމަންރަސްކަލާނގެ حضرة އަށެވެ.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5993,
        "text": "ދެން އެއުރެންގެ حساب ބެއްލެވުން ވަނީވެސް، ހަމަކަށަވަރުން، ތިމަންރަސްކަލާނގެއަށެވެ.",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      }
    ],
