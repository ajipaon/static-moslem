    "numberOfAyahs": 5,
    "ayahs": [
      {
        "number": 6226,
        "text": "ކަލޭގެފާނު ވިދާޅުވާށެވެ! ފަތިހުގެ ވެރިރަސްކަލާނގެ حضرة ން ރައްކާތެރިކަން ލިބިގަންނަމެވެ.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 555,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6227,
        "text": "އެކަލާނގެ ހައްދަވާފައިވާހާ ތަކެތީގެ ނުބައިކަމުންނެވެ.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 555,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6228,
        "text": "އަދި ރޭގަނޑުގެ އަނދިރިކަން ގަދަވެގެންފިހިނދު، އެ ރޭގަނޑުގެ ނުބައިކަމުންވެސްމެއެވެ.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 555,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6229,
        "text": "އަދި ގޮށްތަކަށްފުމޭ މީހުން (އެބަހީ: سحر ހަދާމީހުން) ގެ ނުބައިކަމުންވެސްމެއެވެ.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 555,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6230,
        "text": "އަދި حسد ވެރިޔަކު حسد ވެރިވެއްޖެހިނދު، އޭނާގެ ނުބައިކަމުންވެސްމެއެވެ.",
