{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 74,
    "name": "سُورَةُ المُدَّثِّرِ",
    "englishName": "Al-Muddaththir",
    "englishNameTranslation": "The Cloaked One",
    "revelationType": "Meccan",
    "numberOfAyahs": 56,
    "ayahs": [
      {
        "number": 5496,
        "text": "އޭ ފޭރާންތައް (ގައިކޮޅުގައި) އޮޅުއްވައިގެން އޮންނެވި ކަލޭގެފާނެވެ!",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5497,
        "text": "ކަލޭގެފާނު ތެދުވެވަޑައިގެން إنذار ކުރައްވާށެވެ!",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5498,
        "text": "އަދި ކަލޭގެފާނުގެ ވެރިރަސްކަލާނގެއަށް تكبير ވިދާޅުވާށެވެ!",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5499,
        "text": "އަދި ކަލޭގެފާނުގެ ފޭރާންކޮޅުތައް طاهر ކުރައްވާށެވެ!",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5500,
        "text": "އަދި ބުދަށް އަޅުކަންކުރުމާ ދުރުވެވަޑައިގަންނަވާށެވެ!",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5501,
        "text": "އަދި ގިނައިން ލިބުން އެދިވަޑައިގެން (މީހަކަށް) އެއްޗެއް ނުދެއްވާށެވެ!",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5502,
        "text": "އަދި ކަލޭގެފާނުގެ ވެރިރަސްކަލާނގެއަށްޓަކައި ކަލޭގެފާނު ކެތްތެރިވެ ވަޑައިގަންނަވާށެވެ!",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5503,
        "text": "ފަހެ، ތާޅަފިލި ފުމެލައްވާ ހިނދެވެ.",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5504,
        "text": "ފަހެ، އެދުވަސް އެއީ އުނދަގޫވެގެންވާ ދުވަހެކެވެ.",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5505,
        "text": "كافر ންގެ މައްޗަށް ފަސޭހަކަމެއް ނުވާ ދުވަހެކެވެ.",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5506,
        "text": "ހަމައެކަނި ތިމަންރަސްކަލާނގެ ހެއްދެވި މީހާ، ކަލޭގެފާނު ތިމަންރަސްކަލާނގެއަށް ދޫކޮށްލައްވާށެވެ!",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5507,
        "text": "ހަމައެކަނި ތިމަންރަސްކަލާނގެ ހެއްދެވި މީހާ، ކަލޭގެފާނު ތިމަންރަސްކަލާނގެއަށް ދޫކޮށްލައްވާށެވެ!",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5508,
        "text": "އަދި ގިނަވެގެންވާ މުދަލާއި ކައިރީގައި ތިބޭނެ ފިރިހެންދަރިން، ތިމަންރަސްކަލާނގެ އޭނާޔަށް ދެއްވީމެވެ.",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5509,
        "text": "އަދި (ދުނިޔޭގެ ހުރިހާ ކަމެއް) ތަނަވަސްކޮށްދެއްވައި، އޭނާޔަށް ފަސޭހަކޮށްދެއްވީމެވެ.",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5510,
        "text": "ދެންވެސް އޭނާ އެދެނީ، ތިމަންރަސްކަލާނގެ އިތުރުކޮށްދެއްވުމަށެވެ.",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5511,
        "text": "ހަމަހިލާ އެހެންނަކުން ނުވާނެއެވެ. ހަމަކަށަވަރުން، އޭނާވީ، ތިމަންރަސްކަލާނގެ آية ތަކަށް ދެކޮޅުހަދާ މީހެއް ކަމުގައެވެ.",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5512,
        "text": "ނިކަންހުރެ ތިމަންރަސްކަލާނގެ، ގަދަފަދަވެގެންވާ عذاب އަކަށް ކުރިމަތިލުމަށް އޭނާއަށް ޖައްސަވާހުށީމެވެ.",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5513,
        "text": "ހަމަކަށަވަރުން، އޭނާ ވިސްނައި فكر ކޮށް ރޭވިއެވެ.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5514,
        "text": "ފަހެ، އޭނާޔަށް لعنة ލެއްވިއެވެ. އޭނާ ރޭވީ ކޮންފަދައަކުން ހެއްޔެވެ؟",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5515,
        "text": "ދެން އޭނާޔަށް لعنة ލެއްވިއެވެ. އޭނާ ރޭވީ ކޮންފަދައަކުން ހެއްޔެވެ؟",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5516,
        "text": "ދެން އޭނާ ބަލައި ވިސްނިއެވެ.",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5517,
        "text": "ދެން އޭނާ މޫނު ކުނިކޮށްލިއެވެ. އަދި މޫނު ހެދިއެވެ.",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5518,
        "text": "ދެން އޭނާ ފުރަގަސްދިނެވެ. އަދި ބޮޑާވެގަތެވެ.",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5519,
        "text": "ފަހެ، އޭނާ ބުންޏެވެ. ޖާދޫގަރުންގެ ކިބައިން އައިސްފައިވާ ޖާދޫއެއް ކަމުގައި މެނުވީ މި قرآن ނުވެއެވެ.",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5520,
        "text": "آدم ގެދަރިންގެ ބަސްކަމުގައި މެނުވީ މި قرآن ނުވެއެވެ.",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5521,
        "text": "ނިކަންހުރެ ތިމަންރަސްކަލާނގެ އޭނާ ނަރަކަޔަށް ވައްދަވާހުށީމެވެ.",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5522,
        "text": "އެ ނަރަކަޔަކީ ކޮންއެއްޗެއްކަން ކަލޭގެފާނަށް އަންގައިދިނީ ކޮންއެއްޗެއްތޯއެވެ؟",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5523,
        "text": "އެ ނަރަކަ (ނާންދާ) އެއްވެސް އެއްޗެއް ނުބަހައްޓަތެވެ. އަދި ދޫކޮށެއްވެސް ނުލަތެވެ.",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5524,
        "text": "ގައިގެ ހަން އަންދައި ކަޅުކޮށްލަތެވެ.",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5525,
        "text": "(ބެލެހެއްޓެވުމަށް) އެ ނަރަކައިގެ މައްޗަށް އޮނަވިހި ملائكة ންވެއެވެ.",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5526,
        "text": "އަދި ނަރަކައިގެ ބަނޑޭރިން ކަމުގައި، ތިމަންރަސްކަލާނގެ ملائكة ން މެނުވީ ނުލައްވަމެވެ. އަދި كافر ވި މީހުންނަށް امتحان އެއްކަމުގައި މެނުވީ އެ ملائكة ންގެ عدد ނުލައްވަމެވެ. (އެއީ) ފޮތް ލިބުނު މީހުންނަށް يقين ވުމަށާއި، އަދި إيمان ވި މީހުންގެ إيمان ކަން އިތުރުވުމަށާއި، އަދި ފޮތް ލިބުނު މީހުންނާއި إيمان ވި މީހުން شك ނުކުރުމަށްޓަކައެވެ. އަދި އެއުރެންގެ ހިތްތަކުގައި ބަލިމަޑުކަމެއްވާ މީހުންނާއި، كافر ން ބުނުމަށްޓަކައެވެ. މި مثال ން اللَّه އިރާދަކުރެއްވީ ކޮންއެއްޗެއް ބާވައެވެ؟ އެފަދައިން اللَّه އިރާދަކުރައްވާ މީހަކު މަގުފުރައްދަވަތެވެ. އަދި އެކަލާނގެ އިރާދަކުރައްވާ މީހަކަށް ތެދުމަގު ދައްކަވަތެވެ. އަދި ކަލޭގެފާނުގެ ވެރިރަސްކަލާނގެ لشكر ގެ عدد، އެކަލާނގެ މެނުވީ ދެނެވޮޑިގެން ނުވެތެވެ. އަދި آدم ގެދަރިންނަށް عبرة ތެރި ހަނދާންކޮށްދިނުމެއް ކަމުގައި މެނުވީ، އެ ނަރަކަ ނުވެތެވެ.",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5527,
        "text": "ހަމަަހިލާ އެހެންނަކުން ނުވާނެއެވެ. ހަނދު ގަންދެއްވަމެވެ.",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5528,
        "text": "އަދި ރޭގަނޑު ފަސްދީދާހިނދު، އެ ރޭގަނޑު ގަންދެއްވަމެވެ.",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5529,
        "text": "އަދި އަލިވެގެންދާހިނދު، ފަތިސް ގަންދެއްވަމެވެ.",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5530,
        "text": "ހަމަކަށަވަރުން، އެ ނަރަކައީ، ބޮޑެތި ބަލާތަކުގެ ތެރެއިން ބަލާއެއްކަން ކަށަވަރެވެ.",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5531,
        "text": "آدم ގެދަރިންނަށް إنذار ކުރާ އެއްޗެއްގެ ގޮތުގައެވެ.",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5532,
        "text": "ތިޔަބައިމީހުންކުރެ ބޭނުން މީހަކަށް ކުރިއެރުންވެއެވެ. ނުވަތަ ފަސްޖެހުންވެއެވެ.",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5533,
        "text": "ކޮންމެ نفس އެއް، އެ نفس އެއް ހޯދައިގަތްކަމަކުން ބަނދެވިގެންވެއެވެ.",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5534,
        "text": "ކަނާތްފަރާތުގެ أهل ވެރިން މެނުވީއެވެ.",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5535,
        "text": "ކަނާތްފަރާތުގެ أهل ވެރިން މެނުވީއެވެ.",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5536,
        "text": "(އެއުރެންވަނީ) ސުވަރުގެތަކެއްގައެވެ. ކުށްވެރިންނާ މެދުގައި އެކަކު އަނެކަކާ سؤال ކުރާ حال ގައެވެ.",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5537,
        "text": "ތިޔަބައިމީހުން ނަރަކަޔަށް ވެއްދީ ކޮންކަމެއް ހެއްޔެވެ؟",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5538,
        "text": "އެއުރެން ބުނާނެތެވެ. ނަމާދުކުރާ މީހުންގެ ތެރެޔަކު ތިމަންމެން ނުވަމުއެވެ.",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5539,
        "text": "އަދި ތިމަންމެން مسكين ންނަށް ކާންދީއެއް ނޫޅެމުއެވެ.",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5540,
        "text": "އަދި باطل ވާހަކަ ދައްކާ މީހުންނާ އެކުގައި، ތިމަންމެން باطل ވާހަކަ ދައްކައި އުޅުނީމުއެވެ.",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5541,
        "text": "އަދި ތިމަންމެން قيامة ދުވަސް ދޮގުކޮށް އުޅުނީމުއެވެ.",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5542,
        "text": "يقين ކަން (އެބަހީ: މަރު) ތިމަންމެންނާ ހަމަޔަށް އަތުވެއްޖައުމަށް ދާނދެނެވެ.",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5543,
        "text": "ފަހެ، شفاعة ތެރިވާމީހުންގެ شفاعة، އެއުރެންނަކަށް މަންފާއެއް ނުކުރާނެތެވެ.",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5544,
        "text": "ފަހެ، ހަނދާންކޮށްދިނުމުން (އެބަހީ: قرآن ން) އެއުރެން އެއްކިބާވެ ދުރަށްދަނީ ކީއްވެގެން ހެއްޔެވެ؟",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5545,
        "text": "ފަހެ، ހަނދާންކޮށްދިނުމުން (އެބަހީ: قرآن ން) އެއުރެން އެއްކިބާވެ ދުރަށްދަނީ ކީއްވެގެން ހެއްޔެވެ؟",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5546,
        "text": "އެއުރެން ސިންގާއަކަށް ފިލަންދުވާ وحشى حمار ތަކެއް ފަދައެވެ.",
        "numberInSurah": 51,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5547,
        "text": "އަދި ކިއެއްތަ! އެއުރެންކުރެ ކޮންމެ މީހަކު އެދެނީ، ލިޔުއްވާފައިވާ ފަތްފުށްތަކެއް އެމީހަކަށް ބާވައިލެއްވުމަށެވެ.",
        "numberInSurah": 52,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5548,
        "text": "ހަމަހިލާ އެހެންނަކުން ނުވާނެއެވެ. އަދި ކިއެއްތަ! އެއުރެން آخرة އަކަށް ބިރެއް ނުގަނެތެވެ.",
        "numberInSurah": 53,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5549,
        "text": "ހަމަހިލާ އެހެންނަކުން ނުވާނެއެވެ. ހަމަކަށަވަރުން، އެ قرآن އަކީ ހަނދާންކޮށްދިނުމެކެވެ.",
        "numberInSurah": 54,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5550,
        "text": "ފަހެ، ބޭނުން މީހަކު އެ قرآن ހަނދުމަކުރާހުށިކަމެވެ!",
        "numberInSurah": 55,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5551,
        "text": "އަދި اللَّه އިރާދަކުރެއްވިއްޔާ މެނުވީ، އެއުރެންނަކަށް ހަނދާނެއް ނުކުރެވޭނެތެވެ. އެކަލާނގެއީ (މީސްތަކުން އެކަލާނގެއަށް ބިރުވެތިވެ) تقوى ވެރިވުން حق ވެގެންވާ، އަދި (ކިޔަމަންގަތް އަޅުންނަށް) ފާފަތައް ފުއްސެވުމުގެ ވެރިރަސްކަލާނގެއެވެ.",
        "numberInSurah": 56,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      }
    ],
