  "status": "OK",
  "data": {
    "number": 85,
    "name": "سُورَةُ البُرُوجِ",
    "englishName": "Al-Burooj",
    "englishNameTranslation": "The Constellations",
    "revelationType": "Meccan",
    "numberOfAyahs": 22,
    "ayahs": [
      {
        "number": 5910,
        "text": "برج ތަކުގެވެރި އުޑު ގަންދެއްވަމެވެ.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5911,
        "text": "އަދި وعد ކުރައްވާފައިވާ ދުވަސް ގަންދެއްވަމެވެ.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5912,
        "text": "އަދި ހެކިދޭ ފަރާތާއި، ހެކިދެވޭ ފަރާތް ގަންދެއްވަމެވެ.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5913,
        "text": "ވަޅުގަނޑުގެ އެކުވެރިންނަށް لعنة ލެއްވިއެވެ.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5914,
        "text": "(އެ ވަޅުގަނޑަކީ) ދަރުއަޅައި ރޯކޮށްފައިވާ އަލިފާންގަނޑެވެ.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5915,
        "text": "އެއުރެން އޭގެކައިރީގައި އިށީނދެ ތިބިހިނދެވެ.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5916,
        "text": "مؤمن ންނަށް އެއުރެން ކަންތައްކުރާގޮތް، އެއުރެން ދެކޭންތިބި حال ގައެވެ.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5917,
        "text": "އަދި عزيز ވަންތަ، ގިނަގިނައިން حمد ލިބިވޮޑިގެންވާ اللَّه އަށް އެއުރެން إيمان ވާކަމަށްޓަކައި މެނުވީ، އެ كافر ން އެއުރެންނާމެދު ނުރުހުންތެރިއެއް ނުވެތެވެ.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5918,
        "text": "އެކަލާނގެއީ، އުޑުތަކާއި، ބިމުގެ ވެރިކަން އެކަލާނގެއަށް މިލްކުވެގެންވާ ރަސްކަލާނގެއެވެ. اللَّه އީ، ކޮންމެ ކަމެއްގެ މައްޗަށް ހެކިވެވޮޑިގެންވާ ރަސްކަލާނގެއެވެ.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5919,
        "text": "ހަމަކަށަވަރުން، مؤمن ފިރިހެނުންނާއި، مؤمن އަންހެނުން، دين ން އެއްކިބާކުރުމަށް އަނިޔާދީ އުނދަގޫކޮށް، ދެން توبة ނުވި މީހުން (ދަންނާށެވެ!) ފަހެ، އެއުރެންނަށް ނަރަކައިގެ عذاب ހުށްޓެވެ. އަދި އެއުރެންނަށް އަނދައަނދާ ހުންނަ އަލިފާނުގެ عذاب ހުށްޓެވެ.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5920,
        "text": "ހަމަކަށަވަރުން، إيمان ވެ صالح عمل ތައްކުޅަމީހުންނަށް، އެތާނގެ ދަށުން ކޯރުތައް ހިނގަހިނގާ ހުންނަ ސުވަރުގެތައް ހުށްޓެވެ. ބޮޑުވެގެންވާ نصيب ލިބުމަކީ އެއީއެވެ.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5921,
        "text": "ހަމަކަށަވަރުން، ކަލޭގެފާނުގެ ވެރިރަސްކަލާނގެ ހިއްޕަވާ ހިއްޕެވުމަކީ، ގަދަފަދަވެގެންވާ ހިއްޕެވުމެއްކަން ކަށަވަރެވެ.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5922,
        "text": "ހަމަކަށަވަރުން، ފުރަތަމަ އުފައްދަވަނީވެސް، އަދި އަލުން (ދިރުއްވައި) إعادة ކުރައްވާނީވެސް ހަމަ އެކަލާނގެއެވެ.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5923,
        "text": "އަދި އެކަލާނގެއީ، ގިނަގިނައިން ފާފަފުއްސަވާ، ނުހަނު އޯގާވަންތަ ރަސްކަލާނގެއެވެ.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5924,
        "text": "عرش ގެ ވެރި، މާތްވަންތަ ރަސްކަލާނގެއެވެ.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5925,
        "text": "އިރާދަކުރައްވާ ކޮންމެ ކަމެއް ކުރައްވާ ރަސްކަލާނގެއެވެ.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5926,
        "text": "އިރާދަކުރައްވާ ކޮންމެ ކަމެއް ކުރައްވާ ރަސްކަލާނގެއެވެ.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5927,
        "text": "فرعون އާއި، ثمود ބާގައިގެ لشكر ގެ ވާހަކަ، ކަލޭގެފާނާ ހަމަޔަށް އައިސްފައިވޭތޯއެވެ؟",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5928,
        "text": "އަދި ކިއެއްތަ! كافر ވިމީހުން ވަނީ، ދޮގުކުރުމެއްގައެވެ.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5929,
        "text": "اللَّه އެއުރެންގެ ހުރިހާކަމެއް ފުރިހަމަޔަށް ދެނެވޮޑިގެންވެތެވެ.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5930,
        "text": "اللَّه އެއުރެންގެ ހުރިހާކަމެއް ފުރިހަމަޔަށް ދެނެވޮޑިގެންވެތެވެ.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5931,
        "text": "އަދި ކިއެއްތަ! އެއީ ރައްކާތެރި ކުރައްވާފައިވާ ފިލަޔެއްގައިވާ (އެބަހީ: اللّوح المحفوظ ގައިވާ) މާތް قرآن އެވެ.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      }
    ],
