    "ayahs": [
      {
        "number": 6189,
        "text": "އެތުގެ އެކުވެރިންނާމެދު (އެބަހީ: فيل ބާގައިމީހުންނާމެދު (ކަލޭގެފާނުގެ ވެރިރަސްކަލާނގެ، ކަންތައް ކުރެއްވިިގޮތް ކަލޭގެފާނަށް އެނގިވަޑައި ނުގަންނަވާތޯއެވެ؟",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 547,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6190,
        "text": "އެކަލާނގެ އެއުރެންގެ މަކަރާއި حيلة، ގެއްލުމެއްގެ ތެރޭގައި ނުލައްވާތޯއެވެ؟",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 547,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6191,
        "text": "އަދި އެކަލާނގެ އެއުރެންގެ މައްޗަށް ބައިބަޔަށް ދޫނިތަކެއް ފޮނުއްވިއެވެ.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 547,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6192,
        "text": "ގާވެފައިވާ މަށީގެ ހިލަތަކެއް، އެ ދޫނިތައް އެއުރެންނާ ދިމާޔަށް އުކަތެވެ.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 547,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6193,
        "text": "ފަހެ، ޖަނަވާރު ކައިފައިހުރި ހުއިތަކެއް ފަދައިން އެކަލާނގެ އެއުރެން ލެއްވިއެވެ.",
