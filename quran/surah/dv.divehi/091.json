  "data": {
    "number": 91,
    "name": "سُورَةُ الشَّمۡسِ",
    "englishName": "Ash-Shams",
    "englishNameTranslation": "The Sun",
    "revelationType": "Meccan",
    "numberOfAyahs": 15,
    "ayahs": [
      {
        "number": 6044,
        "text": "އިރާއި، އޭގެ އަލިކަން ގަންދެއްވަމެވެ.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6045,
        "text": "އަދި ހަނދު އޭގެ ފަހަތުންދާހިނދު ހަނދު، ގަންދެއްވަމެވެ.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6046,
        "text": "އަދި ދުވާލު، އެ އިރު ފާޅުކޮށްފިހިނދު، އެ ދުވާލު ގަންދެއްވަމެވެ.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6047,
        "text": "އަދި ރޭގަނޑު، އެ އިރު ނިވާކުރާހިނދު، އެ ރޭގަނޑު ގަންދެއްވަމެވެ.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6048,
        "text": "އަދި އުޑާއި، އެ އުޑު ބިނާކުރެއްވި ކަލާނގެ ގަންދެއްވަމެވެ.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6049,
        "text": "އަދި ބިމާއި، އެ ބިން ފެތުރުއްވި ކަލާނގެ ގަންދެއްވަމެވެ.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6050,
        "text": "އަދި نفس އާއި، އެ نفس ފުރިހަމަކުރެއްވި ކަލާނގެ ގަންދެއްވަމެވެ.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6051,
        "text": "ފަހެ، އެކަލާނގެ އެ نفس އަށް (ގެއްލުންހުރި) ނުބައިގޮތާއި (ފައިދާހުރި) ހެޔޮގޮތް އެނގުމުގެ ވިސްނުން ދެއްވިއެވެ.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6052,
        "text": "އެ نفس طاهر ކޮށްފިމީހާ، ކާމިޔާބުވެއްޖެކަން ކަށަވަރެވެ.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6053,
        "text": "އަދި އެ نفس އަށް އުނިކަން ދީފިމީހާ، ނާކާމިޔާބުވެއްޖެކަން ކަށަވަރެވެ.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6054,
        "text": "ثمود ބާގައިމީހުން، އެއުރެންގެ އަނިޔާވެރިކަމުން (އެއުރެންގެ ނަބިއްޔާ) ދޮގުކުރޫއެވެ.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6055,
        "text": "އެއުރެންކުރެ އެންމެ އަބާއްޖަވެރިމީހާ (ޖަމަލު މެރުމަށް) އަވަސްވެގަތްހިނދެވެ.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6056,
        "text": "ފަހެ، އެއުރެންނަށް اللَّه ގެ رسول އާ (އެބަހީ: صالح ގެފާނު) ވިދާޅުވިއެވެ. اللَّه ގެ ޖަމަލަށާއި، އޭގެ ބުއިމަށް ރައްކާތެރިވާށެވެ!",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6057,
        "text": "ފަހެ، އެއުރެން އެކަލޭގެފާނުގެ ބަސްފުޅު ދޮގުކުޅައޫއެވެ. ދެން އެއުރެން އެ ޖަމަލު މަރައިލޫއެވެ. ފަހެ، އެއުރެންގެ ފާފައިގެ ސަބަބުން އެއުރެންގެ ވެރިރަސްކަލާނގެ އެއުރެންގެ މައްޗަށް عذاب ފޮނުއްވިއެވެ. އަދި އެއުރެން އެންމެން، އެ عذاب ގައި ހަމަހަމަ ކުރެއްވިއެވެ.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6058,
        "text": "އަދި އެ عذاب ގެ ނަތީޖާއާމެދު، އެކަލާނގެ ބިރުވެތިވެވޮޑިގެނެއްނުވެއެވެ.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      }
    ],
