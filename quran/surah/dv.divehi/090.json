  "code": 200,
  "status": "OK",
  "data": {
    "number": 90,
    "name": "سُورَةُ البَلَدِ",
    "englishName": "Al-Balad",
    "englishNameTranslation": "The City",
    "revelationType": "Meccan",
    "numberOfAyahs": 20,
    "ayahs": [
      {
        "number": 6024,
        "text": "އަދި ތިމަންރަސްކަލާނގެ ސުވަރުގެޔަށް ވަންނާށެވެ!",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6025,
        "text": "ކަލޭގެފާނު މިރަށުގައި ހުންނަވާ حال، މިރަށް) އެބަހީ: مكة (ގަންދެއްވައި ހުވާ وحى ކުރައްވަމެވެ.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6026,
        "text": "އަދި އުފަންކުރި މީހާއާއި، އޭނާޔަށް އުފަންވީހާ ބަޔަކު ގަންދެއްވަމެވެ. (އެބަހީ: آدم ގެފާނާއި آدم ގެ ދަރިންނެވެ.)",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6027,
        "text": "ހަމަކަށަވަރުން، ތިމަންރަސްކަލާނގެ އިންސާނާ ހައްދަވައި، ވަރުބަލިވުމާއި ގަދަ މަސައްކަތުގެ ތެރެޔަށް ލެއްވީމެވެ.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6028,
        "text": "އެކަކުވެސް އެ އިންސާނާގެ މައްޗަށް ބާރު ހިންގުމަށް ކުޅަދާނަވެގެން ނުވާނޭކަމަށް އޭނާ ހީކުރަނީހެއްޔެވެ؟",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6029,
        "text": "އޭނާ ބުނެތެވެ. ގިނަވެގެންވާ މުދާ ތިމަން خرد ކޮށް ނެތިކޮށްފީމެވެ.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6030,
        "text": "އެކަކުވެސް އޭނާ ނުދެކޭކަމަށް އޭނާ ހީކުރަނީހެއްޔެެވެ؟",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6031,
        "text": "އެކަކުވެސް އޭނާ ނުދެކޭކަމަށް އޭނާ ހީކުރަނީހެއްޔެެވެ؟",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6032,
        "text": "ތިމަންރަސްކަލާނގެ އޭނާޔަށް، ދެލޮލާއި، ދުލަކާއި، ދެތުންފަތް ނުލައްވަން ހެއްޔެވެ؟",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6033,
        "text": "އަދި ދެމަގަށް ތިމަންރަސްކަލާނގެ އޭނާޔަށް މަގުދެއްކެވީމެވެ.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6034,
        "text": "ފަހެ، ދަތިކަންހުރިމަގަށް އެ އިންސާނާވަދެ، އެމަގުން ނުހިނގީ ކީއްވެގެންހެއްޔެވެ؟",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6035,
        "text": "ދަތިކަންހުރި މަގަކީ، ކޮންމަގެއްކަން، ކަލޭގެފާނަށް އަންގައިދިނީ ކޮންއެއްޗެއްތޯއެވެ؟",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6036,
        "text": ") އެއީ (އަޅަކު މިނިވަންކުރުމެވެ.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6037,
        "text": "ނުވަތަ ބަނޑުހައިހޫނުކަން ފެތުރިފައިވާ ދުވަހެއްގައި ކާންދިނުމެވެ.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6038,
        "text": "ގާތްތިމާގެ ޔަތީމަކަށެވެ.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6039,
        "text": "ނުވަތަ އަތްމަތި ދަތިވެފައިވާ މިސްކީނަކަށެވެ.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6040,
        "text": "ދެން إيمان ވެ، އަދި ކެތްތެރިވުމަށް އެކަކު އަނެކަކަށް وصية ކޮށް، އަދި މީސްތަކުންނަށް رحم ބެހެއްޓުމަށް އެކަކު އަނެކަކަށް وصية ކޮށް އުޅުނުމީހުންގެ ތެރެއިން އޭނާ ވާނެތެވެ.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6041,
        "text": "ކަނާތްފަރާތުގެ މީހުންނީ، އެއުރެންނެވެ.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6042,
        "text": "އަދި ތިމަންރަސްކަލާނގެ آية ތަކަށް كافر ވި މީހުންނީ، އެއީ ވާތްފަރާތުގެ މީހުންނެވެ.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6043,
        "text": "އެއުރެންނަށް ހުރީ، ބަންދުކުރައްވާފައިވާ ނަރަކަޔެވެ.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      }
    ],
