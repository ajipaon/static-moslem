    "name": "سُورَةُ الضُّحَىٰ",
    "englishName": "Ad-Dhuhaa",
    "englishNameTranslation": "The Morning Hours",
    "revelationType": "Meccan",
    "numberOfAyahs": 11,
    "ayahs": [
      {
        "number": 6080,
        "text": "Клянусь утром",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6081,
        "text": "и ночью, когда она густеет!",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6082,
        "text": "Не покинул тебя твой Господь и не возненавидел.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6083,
        "text": "Ведь последнее для тебя - лучше, чем первое.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6084,
        "text": "Ведь даст тебе твой Господь, и ты будешь доволен.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6085,
        "text": "Разве не нашел Он тебя сиротой - и приютил?",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6086,
        "text": "И нашел тебя заблудшим - и направил на путь?",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6087,
        "text": "И нашел тебя бедным и обогатил?",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6088,
        "text": "И вот сироту ты не притесняй,",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6089,
        "text": "а просящего не отгоняй,",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6090,
        "text": "а о милости твоего Господа возвещай.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      }
