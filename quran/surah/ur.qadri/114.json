    "ayahs": [
      {
        "number": 6231,
        "text": "آپ عرض کیجئے کہ میں (سب) انسانوں کے رب کی پناہ مانگتا ہوں،",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 556,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6232,
        "text": "جو (سب) لوگوں کا بادشاہ ہے،",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 556,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6233,
        "text": "جو (ساری) نسلِ انسانی کا معبود ہے،",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 556,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6234,
        "text": "وسوسہ انداز (شیطان) کے شر سے جو (اﷲ کے ذکر کے اثر سے) پیچھے ہٹ کر چھپ جانے والا ہے،",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 556,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6235,
        "text": "جو لوگوں کے دلوں میں وسوسہ ڈالتا ہے،",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 556,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6236,
        "text": "خواہ وہ (وسوسہ انداز شیطان) جنات میں سے ہو یا انسانوں میں سے،",
        "numberInSurah": 6,
