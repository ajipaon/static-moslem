  "data": {
    "number": 99,
    "name": "سُورَةُ الزَّلۡزَلَةِ",
    "englishName": "Az-Zalzala",
    "englishNameTranslation": "The Earthquake",
    "revelationType": "Medinan",
    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6139,
        "text": "جب زمین اپنے سخت بھونچال سے بڑی شدت کے ساتھ تھرتھرائی جائے گی،",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6140,
        "text": "اور زمین اپنے (سب) بوجھ نکال باہر پھینکے گی،",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6141,
        "text": "اور انسان (حیران و ششدر ہو کر) کہے گا: اسے کیا ہوگیا ہے،",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6142,
        "text": "اس دن وہ اپنے حالات خود ظاہر کر دے گی،",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6143,
        "text": "اس لئے کہ آپ کے رب نے اس کے لئے تیز اشاروں (کی زبان) کو مسخر فرما دیا ہوگا،",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6144,
        "text": "اس دن لوگ مختلف گروہ بن کر (جدا جدا حالتوں کے ساتھ) نکلیں گے تاکہ انہیں ان کے اَعمال دکھائے جائیں،",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6145,
        "text": "تو جس نے ذرہ بھر نیکی کی ہوگی وہ اسے دیکھ لے گا،",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6146,
        "text": "اور جس نے ذرہ بھر برائی کی ہوگی وہ اسے (بھی) دیکھ لے گا،",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      }
    ],
