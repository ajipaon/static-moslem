    "number": 113,
    "name": "سُورَةُ الفَلَقِ",
    "englishName": "Al-Falaq",
    "englishNameTranslation": "The Dawn",
    "revelationType": "Meccan",
    "numberOfAyahs": 5,
    "ayahs": [
      {
        "number": 6226,
        "text": "آپ عرض کیجئے کہ میں (ایک) دھماکے سے انتہائی تیزی کے ساتھ (کائنات کو) وجود میں لانے والے رب کی پناہ مانگتا ہوں،",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 555,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6227,
        "text": "ہر اس چیز کے شر (اور نقصان) سے جو اس نے پیدا فرمائی ہے،",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 555,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6228,
        "text": "اور (بالخصوص) اندھیری رات کے شر سے جب (اس کی) ظلمت چھا جائے،",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 555,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6229,
        "text": "اور گرہوں میں پھونک مارنے والی جادوگرنیوں (اور جادوگروں) کے شر سے،",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 555,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6230,
        "text": "اور ہر حسد کرنے والے کے شر سے جب وہ حسد کرے،",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 555,
        "hizbQuarter": 240,
        "sajda": false
      }
