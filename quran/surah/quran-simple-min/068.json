    "numberOfAyahs": 52,
    "ayahs": [
      {
        "number": 5272,
        "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ ن ۚ وَالقَلَمِ وَما يَسطُرونَ",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5273,
        "text": "ما أَنتَ بِنِعمَةِ رَبِّكَ بِمَجنونٍ",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5274,
        "text": "وَإِنَّ لَكَ لَأَجرًا غَيرَ مَمنونٍ",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5275,
        "text": "وَإِنَّكَ لَعَلىٰ خُلُقٍ عَظيمٍ",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5276,
        "text": "فَسَتُبصِرُ وَيُبصِرونَ",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5277,
        "text": "بِأَييِكُمُ المَفتونُ",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5278,
        "text": "إِنَّ رَبَّكَ هُوَ أَعلَمُ بِمَن ضَلَّ عَن سَبيلِهِ وَهُوَ أَعلَمُ بِالمُهتَدينَ",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5279,
        "text": "فَلا تُطِعِ المُكَذِّبينَ",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5280,
        "text": "وَدّوا لَو تُدهِنُ فَيُدهِنونَ",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5281,
        "text": "وَلا تُطِع كُلَّ حَلّافٍ مَهينٍ",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5282,
        "text": "هَمّازٍ مَشّاءٍ بِنَميمٍ",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5283,
        "text": "مَنّاعٍ لِلخَيرِ مُعتَدٍ أَثيمٍ",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5284,
        "text": "عُتُلٍّ بَعدَ ذٰلِكَ زَنيمٍ",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5285,
        "text": "أَن كانَ ذا مالٍ وَبَنينَ",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5286,
        "text": "إِذا تُتلىٰ عَلَيهِ آياتُنا قالَ أَساطيرُ الأَوَّلينَ",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5287,
        "text": "سَنَسِمُهُ عَلَى الخُرطومِ",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5288,
        "text": "إِنّا بَلَوناهُم كَما بَلَونا أَصحابَ الجَنَّةِ إِذ أَقسَموا لَيَصرِمُنَّها مُصبِحينَ",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5289,
        "text": "وَلا يَستَثنونَ",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5290,
        "text": "فَطافَ عَلَيها طائِفٌ مِن رَبِّكَ وَهُم نائِمونَ",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5291,
        "text": "فَأَصبَحَت كَالصَّريمِ",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5292,
        "text": "فَتَنادَوا مُصبِحينَ",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5293,
        "text": "أَنِ اغدوا عَلىٰ حَرثِكُم إِن كُنتُم صارِمينَ",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5294,
        "text": "فَانطَلَقوا وَهُم يَتَخافَتونَ",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5295,
        "text": "أَن لا يَدخُلَنَّهَا اليَومَ عَلَيكُم مِسكينٌ",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5296,
        "text": "وَغَدَوا عَلىٰ حَردٍ قادِرينَ",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5297,
        "text": "فَلَمّا رَأَوها قالوا إِنّا لَضالّونَ",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5298,
        "text": "بَل نَحنُ مَحرومونَ",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5299,
        "text": "قالَ أَوسَطُهُم أَلَم أَقُل لَكُم لَولا تُسَبِّحونَ",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5300,
        "text": "قالوا سُبحانَ رَبِّنا إِنّا كُنّا ظالِمينَ",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5301,
        "text": "فَأَقبَلَ بَعضُهُم عَلىٰ بَعضٍ يَتَلاوَمونَ",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5302,
        "text": "قالوا يا وَيلَنا إِنّا كُنّا طاغينَ",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5303,
        "text": "عَسىٰ رَبُّنا أَن يُبدِلَنا خَيرًا مِنها إِنّا إِلىٰ رَبِّنا راغِبونَ",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5304,
        "text": "كَذٰلِكَ العَذابُ ۖ وَلَعَذابُ الآخِرَةِ أَكبَرُ ۚ لَو كانوا يَعلَمونَ",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5305,
        "text": "إِنَّ لِلمُتَّقينَ عِندَ رَبِّهِم جَنّاتِ النَّعيمِ",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5306,
        "text": "أَفَنَجعَلُ المُسلِمينَ كَالمُجرِمينَ",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5307,
        "text": "ما لَكُم كَيفَ تَحكُمونَ",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5308,
        "text": "أَم لَكُم كِتابٌ فيهِ تَدرُسونَ",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5309,
        "text": "إِنَّ لَكُم فيهِ لَما تَخَيَّرونَ",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5310,
        "text": "أَم لَكُم أَيمانٌ عَلَينا بالِغَةٌ إِلىٰ يَومِ القِيامَةِ ۙ إِنَّ لَكُم لَما تَحكُمونَ",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5311,
        "text": "سَلهُم أَيُّهُم بِذٰلِكَ زَعيمٌ",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5312,
        "text": "أَم لَهُم شُرَكاءُ فَليَأتوا بِشُرَكائِهِم إِن كانوا صادِقينَ",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5313,
        "text": "يَومَ يُكشَفُ عَن ساقٍ وَيُدعَونَ إِلَى السُّجودِ فَلا يَستَطيعونَ",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5314,
        "text": "خاشِعَةً أَبصارُهُم تَرهَقُهُم ذِلَّةٌ ۖ وَقَد كانوا يُدعَونَ إِلَى السُّجودِ وَهُم سالِمونَ",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5315,
        "text": "فَذَرني وَمَن يُكَذِّبُ بِهٰذَا الحَديثِ ۖ سَنَستَدرِجُهُم مِن حَيثُ لا يَعلَمونَ",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5316,
        "text": "وَأُملي لَهُم ۚ إِنَّ كَيدي مَتينٌ",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5317,
        "text": "أَم تَسأَلُهُم أَجرًا فَهُم مِن مَغرَمٍ مُثقَلونَ",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5318,
        "text": "أَم عِندَهُمُ الغَيبُ فَهُم يَكتُبونَ",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5319,
        "text": "فَاصبِر لِحُكمِ رَبِّكَ وَلا تَكُن كَصاحِبِ الحوتِ إِذ نادىٰ وَهُوَ مَكظومٌ",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5320,
        "text": "لَولا أَن تَدارَكَهُ نِعمَةٌ مِن رَبِّهِ لَنُبِذَ بِالعَراءِ وَهُوَ مَذمومٌ",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5321,
        "text": "فَاجتَباهُ رَبُّهُ فَجَعَلَهُ مِنَ الصّالِحينَ",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5322,
        "text": "وَإِن يَكادُ الَّذينَ كَفَروا لَيُزلِقونَكَ بِأَبصارِهِم لَمّا سَمِعُوا الذِّكرَ وَيَقولونَ إِنَّهُ لَمَجنونٌ",
        "numberInSurah": 51,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
