  "data": {
    "number": 67,
    "name": "سُورَةُ المُلۡكِ",
    "englishName": "Al-Mulk",
    "englishNameTranslation": "The Sovereignty",
    "revelationType": "Meccan",
    "numberOfAyahs": 30,
    "ayahs": [
      {
        "number": 5242,
        "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ تَبارَكَ الَّذي بِيَدِهِ المُلكُ وَهُوَ عَلىٰ كُلِّ شَيءٍ قَديرٌ",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 562,
        "ruku": 496,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5243,
        "text": "الَّذي خَلَقَ المَوتَ وَالحَياةَ لِيَبلُوَكُم أَيُّكُم أَحسَنُ عَمَلًا ۚ وَهُوَ العَزيزُ الغَفورُ",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 562,
        "ruku": 496,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5244,
        "text": "الَّذي خَلَقَ سَبعَ سَماواتٍ طِباقًا ۖ ما تَرىٰ في خَلقِ الرَّحمٰنِ مِن تَفاوُتٍ ۖ فَارجِعِ البَصَرَ هَل تَرىٰ مِن فُطورٍ",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 562,
        "ruku": 496,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5245,
        "text": "ثُمَّ ارجِعِ البَصَرَ كَرَّتَينِ يَنقَلِب إِلَيكَ البَصَرُ خاسِئًا وَهُوَ حَسيرٌ",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 562,
        "ruku": 496,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5246,
        "text": "وَلَقَد زَيَّنَّا السَّماءَ الدُّنيا بِمَصابيحَ وَجَعَلناها رُجومًا لِلشَّياطينِ ۖ وَأَعتَدنا لَهُم عَذابَ السَّعيرِ",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 562,
        "ruku": 496,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5247,
        "text": "وَلِلَّذينَ كَفَروا بِرَبِّهِم عَذابُ جَهَنَّمَ ۖ وَبِئسَ المَصيرُ",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 562,
        "ruku": 496,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5248,
        "text": "إِذا أُلقوا فيها سَمِعوا لَها شَهيقًا وَهِيَ تَفورُ",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 562,
        "ruku": 496,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5249,
        "text": "تَكادُ تَمَيَّزُ مِنَ الغَيظِ ۖ كُلَّما أُلقِيَ فيها فَوجٌ سَأَلَهُم خَزَنَتُها أَلَم يَأتِكُم نَذيرٌ",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 562,
        "ruku": 496,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5250,
        "text": "قالوا بَلىٰ قَد جاءَنا نَذيرٌ فَكَذَّبنا وَقُلنا ما نَزَّلَ اللَّهُ مِن شَيءٍ إِن أَنتُم إِلّا في ضَلالٍ كَبيرٍ",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 562,
        "ruku": 496,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5251,
        "text": "وَقالوا لَو كُنّا نَسمَعُ أَو نَعقِلُ ما كُنّا في أَصحابِ السَّعيرِ",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 562,
        "ruku": 496,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5252,
        "text": "فَاعتَرَفوا بِذَنبِهِم فَسُحقًا لِأَصحابِ السَّعيرِ",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 562,
        "ruku": 496,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5253,
        "text": "إِنَّ الَّذينَ يَخشَونَ رَبَّهُم بِالغَيبِ لَهُم مَغفِرَةٌ وَأَجرٌ كَبيرٌ",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 562,
        "ruku": 496,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5254,
        "text": "وَأَسِرّوا قَولَكُم أَوِ اجهَروا بِهِ ۖ إِنَّهُ عَليمٌ بِذاتِ الصُّدورِ",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 563,
        "ruku": 496,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5255,
        "text": "أَلا يَعلَمُ مَن خَلَقَ وَهُوَ اللَّطيفُ الخَبيرُ",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 563,
        "ruku": 496,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5256,
        "text": "هُوَ الَّذي جَعَلَ لَكُمُ الأَرضَ ذَلولًا فَامشوا في مَناكِبِها وَكُلوا مِن رِزقِهِ ۖ وَإِلَيهِ النُّشورُ",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 563,
        "ruku": 497,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5257,
        "text": "أَأَمِنتُم مَن فِي السَّماءِ أَن يَخسِفَ بِكُمُ الأَرضَ فَإِذا هِيَ تَمورُ",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 563,
        "ruku": 497,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5258,
        "text": "أَم أَمِنتُم مَن فِي السَّماءِ أَن يُرسِلَ عَلَيكُم حاصِبًا ۖ فَسَتَعلَمونَ كَيفَ نَذيرِ",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 563,
        "ruku": 497,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5259,
        "text": "وَلَقَد كَذَّبَ الَّذينَ مِن قَبلِهِم فَكَيفَ كانَ نَكيرِ",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 563,
        "ruku": 497,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5260,
        "text": "أَوَلَم يَرَوا إِلَى الطَّيرِ فَوقَهُم صافّاتٍ وَيَقبِضنَ ۚ ما يُمسِكُهُنَّ إِلَّا الرَّحمٰنُ ۚ إِنَّهُ بِكُلِّ شَيءٍ بَصيرٌ",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 563,
        "ruku": 497,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5261,
        "text": "أَمَّن هٰذَا الَّذي هُوَ جُندٌ لَكُم يَنصُرُكُم مِن دونِ الرَّحمٰنِ ۚ إِنِ الكافِرونَ إِلّا في غُرورٍ",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 563,
        "ruku": 497,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5262,
        "text": "أَمَّن هٰذَا الَّذي يَرزُقُكُم إِن أَمسَكَ رِزقَهُ ۚ بَل لَجّوا في عُتُوٍّ وَنُفورٍ",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 563,
        "ruku": 497,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5263,
        "text": "أَفَمَن يَمشي مُكِبًّا عَلىٰ وَجهِهِ أَهدىٰ أَمَّن يَمشي سَوِيًّا عَلىٰ صِراطٍ مُستَقيمٍ",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 563,
        "ruku": 497,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5264,
        "text": "قُل هُوَ الَّذي أَنشَأَكُم وَجَعَلَ لَكُمُ السَّمعَ وَالأَبصارَ وَالأَفئِدَةَ ۖ قَليلًا ما تَشكُرونَ",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 563,
        "ruku": 497,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5265,
        "text": "قُل هُوَ الَّذي ذَرَأَكُم فِي الأَرضِ وَإِلَيهِ تُحشَرونَ",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 563,
        "ruku": 497,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5266,
        "text": "وَيَقولونَ مَتىٰ هٰذَا الوَعدُ إِن كُنتُم صادِقينَ",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 563,
        "ruku": 497,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5267,
        "text": "قُل إِنَّمَا العِلمُ عِندَ اللَّهِ وَإِنَّما أَنا نَذيرٌ مُبينٌ",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 563,
        "ruku": 497,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5268,
        "text": "فَلَمّا رَأَوهُ زُلفَةً سيئَت وُجوهُ الَّذينَ كَفَروا وَقيلَ هٰذَا الَّذي كُنتُم بِهِ تَدَّعونَ",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 497,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5269,
        "text": "قُل أَرَأَيتُم إِن أَهلَكَنِيَ اللَّهُ وَمَن مَعِيَ أَو رَحِمَنا فَمَن يُجيرُ الكافِرينَ مِن عَذابٍ أَليمٍ",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 497,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5270,
        "text": "قُل هُوَ الرَّحمٰنُ آمَنّا بِهِ وَعَلَيهِ تَوَكَّلنا ۖ فَسَتَعلَمونَ مَن هُوَ في ضَلالٍ مُبينٍ",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 497,
        "hizbQuarter": 225,
        "sajda": false
      },
      {
        "number": 5271,
        "text": "قُل أَرَأَيتُم إِن أَصبَحَ ماؤُكُم غَورًا فَمَن يَأتيكُم بِماءٍ مَعينٍ",
        "numberInSurah": 30,
        "juz": 29,
