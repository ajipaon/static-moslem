  "data": {
    "number": 20,
    "name": "سُورَةُ طه",
    "englishName": "Taa-Haa",
    "englishNameTranslation": "Taa-Haa",
    "revelationType": "Meccan",
    "numberOfAyahs": 135,
    "ayahs": [
      {
        "number": 2349,
        "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ طه",
        "numberInSurah": 1,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2350,
        "text": "ما أَنزَلنا عَلَيكَ القُرآنَ لِتَشقىٰ",
        "numberInSurah": 2,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2351,
        "text": "إِلّا تَذكِرَةً لِمَن يَخشىٰ",
        "numberInSurah": 3,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2352,
        "text": "تَنزيلًا مِمَّن خَلَقَ الأَرضَ وَالسَّماواتِ العُلَى",
        "numberInSurah": 4,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2353,
        "text": "الرَّحمٰنُ عَلَى العَرشِ استَوىٰ",
        "numberInSurah": 5,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2354,
        "text": "لَهُ ما فِي السَّماواتِ وَما فِي الأَرضِ وَما بَينَهُما وَما تَحتَ الثَّرىٰ",
        "numberInSurah": 6,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2355,
        "text": "وَإِن تَجهَر بِالقَولِ فَإِنَّهُ يَعلَمُ السِّرَّ وَأَخفَى",
        "numberInSurah": 7,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2356,
        "text": "اللَّهُ لا إِلٰهَ إِلّا هُوَ ۖ لَهُ الأَسماءُ الحُسنىٰ",
        "numberInSurah": 8,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2357,
        "text": "وَهَل أَتاكَ حَديثُ موسىٰ",
        "numberInSurah": 9,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2358,
        "text": "إِذ رَأىٰ نارًا فَقالَ لِأَهلِهِ امكُثوا إِنّي آنَستُ نارًا لَعَلّي آتيكُم مِنها بِقَبَسٍ أَو أَجِدُ عَلَى النّارِ هُدًى",
        "numberInSurah": 10,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2359,
        "text": "فَلَمّا أَتاها نودِيَ يا موسىٰ",
        "numberInSurah": 11,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2360,
        "text": "إِنّي أَنا رَبُّكَ فَاخلَع نَعلَيكَ ۖ إِنَّكَ بِالوادِ المُقَدَّسِ طُوًى",
        "numberInSurah": 12,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2361,
        "text": "وَأَنَا اختَرتُكَ فَاستَمِع لِما يوحىٰ",
        "numberInSurah": 13,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2362,
        "text": "إِنَّني أَنَا اللَّهُ لا إِلٰهَ إِلّا أَنا فَاعبُدني وَأَقِمِ الصَّلاةَ لِذِكري",
        "numberInSurah": 14,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2363,
        "text": "إِنَّ السّاعَةَ آتِيَةٌ أَكادُ أُخفيها لِتُجزىٰ كُلُّ نَفسٍ بِما تَسعىٰ",
        "numberInSurah": 15,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2364,
        "text": "فَلا يَصُدَّنَّكَ عَنها مَن لا يُؤمِنُ بِها وَاتَّبَعَ هَواهُ فَتَردىٰ",
        "numberInSurah": 16,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2365,
        "text": "وَما تِلكَ بِيَمينِكَ يا موسىٰ",
        "numberInSurah": 17,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2366,
        "text": "قالَ هِيَ عَصايَ أَتَوَكَّأُ عَلَيها وَأَهُشُّ بِها عَلىٰ غَنَمي وَلِيَ فيها مَآرِبُ أُخرىٰ",
        "numberInSurah": 18,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2367,
        "text": "قالَ أَلقِها يا موسىٰ",
        "numberInSurah": 19,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2368,
        "text": "فَأَلقاها فَإِذا هِيَ حَيَّةٌ تَسعىٰ",
        "numberInSurah": 20,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2369,
        "text": "قالَ خُذها وَلا تَخَف ۖ سَنُعيدُها سيرَتَهَا الأولىٰ",
        "numberInSurah": 21,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2370,
        "text": "وَاضمُم يَدَكَ إِلىٰ جَناحِكَ تَخرُج بَيضاءَ مِن غَيرِ سوءٍ آيَةً أُخرىٰ",
        "numberInSurah": 22,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2371,
        "text": "لِنُرِيَكَ مِن آياتِنَا الكُبرَى",
        "numberInSurah": 23,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2372,
        "text": "اذهَب إِلىٰ فِرعَونَ إِنَّهُ طَغىٰ",
        "numberInSurah": 24,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2373,
        "text": "قالَ رَبِّ اشرَح لي صَدري",
        "numberInSurah": 25,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2374,
        "text": "وَيَسِّر لي أَمري",
        "numberInSurah": 26,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2375,
        "text": "وَاحلُل عُقدَةً مِن لِساني",
        "numberInSurah": 27,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2376,
        "text": "يَفقَهوا قَولي",
        "numberInSurah": 28,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2377,
        "text": "وَاجعَل لي وَزيرًا مِن أَهلي",
        "numberInSurah": 29,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2378,
        "text": "هارونَ أَخِي",
        "numberInSurah": 30,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2379,
        "text": "اشدُد بِهِ أَزري",
        "numberInSurah": 31,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2380,
        "text": "وَأَشرِكهُ في أَمري",
        "numberInSurah": 32,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2381,
        "text": "كَي نُسَبِّحَكَ كَثيرًا",
        "numberInSurah": 33,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2382,
        "text": "وَنَذكُرَكَ كَثيرًا",
        "numberInSurah": 34,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2383,
        "text": "إِنَّكَ كُنتَ بِنا بَصيرًا",
        "numberInSurah": 35,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2384,
        "text": "قالَ قَد أوتيتَ سُؤلَكَ يا موسىٰ",
        "numberInSurah": 36,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2385,
        "text": "وَلَقَد مَنَنّا عَلَيكَ مَرَّةً أُخرىٰ",
        "numberInSurah": 37,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2386,
        "text": "إِذ أَوحَينا إِلىٰ أُمِّكَ ما يوحىٰ",
        "numberInSurah": 38,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2387,
        "text": "أَنِ اقذِفيهِ فِي التّابوتِ فَاقذِفيهِ فِي اليَمِّ فَليُلقِهِ اليَمُّ بِالسّاحِلِ يَأخُذهُ عَدُوٌّ لي وَعَدُوٌّ لَهُ ۚ وَأَلقَيتُ عَلَيكَ مَحَبَّةً مِنّي وَلِتُصنَعَ عَلىٰ عَيني",
        "numberInSurah": 39,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2388,
        "text": "إِذ تَمشي أُختُكَ فَتَقولُ هَل أَدُلُّكُم عَلىٰ مَن يَكفُلُهُ ۖ فَرَجَعناكَ إِلىٰ أُمِّكَ كَي تَقَرَّ عَينُها وَلا تَحزَنَ ۚ وَقَتَلتَ نَفسًا فَنَجَّيناكَ مِنَ الغَمِّ وَفَتَنّاكَ فُتونًا ۚ فَلَبِثتَ سِنينَ في أَهلِ مَديَنَ ثُمَّ جِئتَ عَلىٰ قَدَرٍ يا موسىٰ",
        "numberInSurah": 40,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2389,
        "text": "وَاصطَنَعتُكَ لِنَفسِي",
        "numberInSurah": 41,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2390,
        "text": "اذهَب أَنتَ وَأَخوكَ بِآياتي وَلا تَنِيا في ذِكرِي",
        "numberInSurah": 42,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2391,
        "text": "اذهَبا إِلىٰ فِرعَونَ إِنَّهُ طَغىٰ",
        "numberInSurah": 43,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2392,
        "text": "فَقولا لَهُ قَولًا لَيِّنًا لَعَلَّهُ يَتَذَكَّرُ أَو يَخشىٰ",
        "numberInSurah": 44,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2393,
        "text": "قالا رَبَّنا إِنَّنا نَخافُ أَن يَفرُطَ عَلَينا أَو أَن يَطغىٰ",
        "numberInSurah": 45,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2394,
        "text": "قالَ لا تَخافا ۖ إِنَّني مَعَكُما أَسمَعُ وَأَرىٰ",
        "numberInSurah": 46,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2395,
        "text": "فَأتِياهُ فَقولا إِنّا رَسولا رَبِّكَ فَأَرسِل مَعَنا بَني إِسرائيلَ وَلا تُعَذِّبهُم ۖ قَد جِئناكَ بِآيَةٍ مِن رَبِّكَ ۖ وَالسَّلامُ عَلىٰ مَنِ اتَّبَعَ الهُدىٰ",
        "numberInSurah": 47,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2396,
        "text": "إِنّا قَد أوحِيَ إِلَينا أَنَّ العَذابَ عَلىٰ مَن كَذَّبَ وَتَوَلّىٰ",
        "numberInSurah": 48,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2397,
        "text": "قالَ فَمَن رَبُّكُما يا موسىٰ",
        "numberInSurah": 49,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2398,
        "text": "قالَ رَبُّنَا الَّذي أَعطىٰ كُلَّ شَيءٍ خَلقَهُ ثُمَّ هَدىٰ",
        "numberInSurah": 50,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2399,
        "text": "قالَ فَما بالُ القُرونِ الأولىٰ",
        "numberInSurah": 51,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2400,
        "text": "قالَ عِلمُها عِندَ رَبّي في كِتابٍ ۖ لا يَضِلُّ رَبّي وَلا يَنسَى",
        "numberInSurah": 52,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2401,
        "text": "الَّذي جَعَلَ لَكُمُ الأَرضَ مَهدًا وَسَلَكَ لَكُم فيها سُبُلًا وَأَنزَلَ مِنَ السَّماءِ ماءً فَأَخرَجنا بِهِ أَزواجًا مِن نَباتٍ شَتّىٰ",
        "numberInSurah": 53,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2402,
        "text": "كُلوا وَارعَوا أَنعامَكُم ۗ إِنَّ في ذٰلِكَ لَآياتٍ لِأُولِي النُّهىٰ",
        "numberInSurah": 54,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2403,
        "text": "۞ مِنها خَلَقناكُم وَفيها نُعيدُكُم وَمِنها نُخرِجُكُم تارَةً أُخرىٰ",
        "numberInSurah": 55,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2404,
        "text": "وَلَقَد أَرَيناهُ آياتِنا كُلَّها فَكَذَّبَ وَأَبىٰ",
        "numberInSurah": 56,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2405,
        "text": "قالَ أَجِئتَنا لِتُخرِجَنا مِن أَرضِنا بِسِحرِكَ يا موسىٰ",
        "numberInSurah": 57,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2406,
        "text": "فَلَنَأتِيَنَّكَ بِسِحرٍ مِثلِهِ فَاجعَل بَينَنا وَبَينَكَ مَوعِدًا لا نُخلِفُهُ نَحنُ وَلا أَنتَ مَكانًا سُوًى",
        "numberInSurah": 58,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2407,
        "text": "قالَ مَوعِدُكُم يَومُ الزّينَةِ وَأَن يُحشَرَ النّاسُ ضُحًى",
        "numberInSurah": 59,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2408,
        "text": "فَتَوَلّىٰ فِرعَونُ فَجَمَعَ كَيدَهُ ثُمَّ أَتىٰ",
        "numberInSurah": 60,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2409,
        "text": "قالَ لَهُم موسىٰ وَيلَكُم لا تَفتَروا عَلَى اللَّهِ كَذِبًا فَيُسحِتَكُم بِعَذابٍ ۖ وَقَد خابَ مَنِ افتَرىٰ",
        "numberInSurah": 61,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2410,
        "text": "فَتَنازَعوا أَمرَهُم بَينَهُم وَأَسَرُّوا النَّجوىٰ",
        "numberInSurah": 62,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2411,
        "text": "قالوا إِن هٰذانِ لَساحِرانِ يُريدانِ أَن يُخرِجاكُم مِن أَرضِكُم بِسِحرِهِما وَيَذهَبا بِطَريقَتِكُمُ المُثلىٰ",
        "numberInSurah": 63,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2412,
        "text": "فَأَجمِعوا كَيدَكُم ثُمَّ ائتوا صَفًّا ۚ وَقَد أَفلَحَ اليَومَ مَنِ استَعلىٰ",
        "numberInSurah": 64,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2413,
        "text": "قالوا يا موسىٰ إِمّا أَن تُلقِيَ وَإِمّا أَن نَكونَ أَوَّلَ مَن أَلقىٰ",
        "numberInSurah": 65,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2414,
        "text": "قالَ بَل أَلقوا ۖ فَإِذا حِبالُهُم وَعِصِيُّهُم يُخَيَّلُ إِلَيهِ مِن سِحرِهِم أَنَّها تَسعىٰ",
        "numberInSurah": 66,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2415,
        "text": "فَأَوجَسَ في نَفسِهِ خيفَةً موسىٰ",
        "numberInSurah": 67,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2416,
        "text": "قُلنا لا تَخَف إِنَّكَ أَنتَ الأَعلىٰ",
        "numberInSurah": 68,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2417,
        "text": "وَأَلقِ ما في يَمينِكَ تَلقَف ما صَنَعوا ۖ إِنَّما صَنَعوا كَيدُ ساحِرٍ ۖ وَلا يُفلِحُ السّاحِرُ حَيثُ أَتىٰ",
        "numberInSurah": 69,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2418,
        "text": "فَأُلقِيَ السَّحَرَةُ سُجَّدًا قالوا آمَنّا بِرَبِّ هارونَ وَموسىٰ",
        "numberInSurah": 70,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2419,
        "text": "قالَ آمَنتُم لَهُ قَبلَ أَن آذَنَ لَكُم ۖ إِنَّهُ لَكَبيرُكُمُ الَّذي عَلَّمَكُمُ السِّحرَ ۖ فَلَأُقَطِّعَنَّ أَيدِيَكُم وَأَرجُلَكُم مِن خِلافٍ وَلَأُصَلِّبَنَّكُم في جُذوعِ النَّخلِ وَلَتَعلَمُنَّ أَيُّنا أَشَدُّ عَذابًا وَأَبقىٰ",
        "numberInSurah": 71,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2420,
        "text": "قالوا لَن نُؤثِرَكَ عَلىٰ ما جاءَنا مِنَ البَيِّناتِ وَالَّذي فَطَرَنا ۖ فَاقضِ ما أَنتَ قاضٍ ۖ إِنَّما تَقضي هٰذِهِ الحَياةَ الدُّنيا",
        "numberInSurah": 72,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2421,
        "text": "إِنّا آمَنّا بِرَبِّنا لِيَغفِرَ لَنا خَطايانا وَما أَكرَهتَنا عَلَيهِ مِنَ السِّحرِ ۗ وَاللَّهُ خَيرٌ وَأَبقىٰ",
        "numberInSurah": 73,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2422,
        "text": "إِنَّهُ مَن يَأتِ رَبَّهُ مُجرِمًا فَإِنَّ لَهُ جَهَنَّمَ لا يَموتُ فيها وَلا يَحيىٰ",
        "numberInSurah": 74,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2423,
        "text": "وَمَن يَأتِهِ مُؤمِنًا قَد عَمِلَ الصّالِحاتِ فَأُولٰئِكَ لَهُمُ الدَّرَجاتُ العُلىٰ",
        "numberInSurah": 75,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2424,
        "text": "جَنّاتُ عَدنٍ تَجري مِن تَحتِهَا الأَنهارُ خالِدينَ فيها ۚ وَذٰلِكَ جَزاءُ مَن تَزَكّىٰ",
        "numberInSurah": 76,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2425,
        "text": "وَلَقَد أَوحَينا إِلىٰ موسىٰ أَن أَسرِ بِعِبادي فَاضرِب لَهُم طَريقًا فِي البَحرِ يَبَسًا لا تَخافُ دَرَكًا وَلا تَخشىٰ",
        "numberInSurah": 77,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2426,
        "text": "فَأَتبَعَهُم فِرعَونُ بِجُنودِهِ فَغَشِيَهُم مِنَ اليَمِّ ما غَشِيَهُم",
        "numberInSurah": 78,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2427,
        "text": "وَأَضَلَّ فِرعَونُ قَومَهُ وَما هَدىٰ",
        "numberInSurah": 79,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2428,
        "text": "يا بَني إِسرائيلَ قَد أَنجَيناكُم مِن عَدُوِّكُم وَواعَدناكُم جانِبَ الطّورِ الأَيمَنَ وَنَزَّلنا عَلَيكُمُ المَنَّ وَالسَّلوىٰ",
        "numberInSurah": 80,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2429,
        "text": "كُلوا مِن طَيِّباتِ ما رَزَقناكُم وَلا تَطغَوا فيهِ فَيَحِلَّ عَلَيكُم غَضَبي ۖ وَمَن يَحلِل عَلَيهِ غَضَبي فَقَد هَوىٰ",
        "numberInSurah": 81,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2430,
        "text": "وَإِنّي لَغَفّارٌ لِمَن تابَ وَآمَنَ وَعَمِلَ صالِحًا ثُمَّ اهتَدىٰ",
        "numberInSurah": 82,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2431,
        "text": "۞ وَما أَعجَلَكَ عَن قَومِكَ يا موسىٰ",
        "numberInSurah": 83,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2432,
        "text": "قالَ هُم أُولاءِ عَلىٰ أَثَري وَعَجِلتُ إِلَيكَ رَبِّ لِتَرضىٰ",
        "numberInSurah": 84,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2433,
        "text": "قالَ فَإِنّا قَد فَتَنّا قَومَكَ مِن بَعدِكَ وَأَضَلَّهُمُ السّامِرِيُّ",
        "numberInSurah": 85,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2434,
        "text": "فَرَجَعَ موسىٰ إِلىٰ قَومِهِ غَضبانَ أَسِفًا ۚ قالَ يا قَومِ أَلَم يَعِدكُم رَبُّكُم وَعدًا حَسَنًا ۚ أَفَطالَ عَلَيكُمُ العَهدُ أَم أَرَدتُم أَن يَحِلَّ عَلَيكُم غَضَبٌ مِن رَبِّكُم فَأَخلَفتُم مَوعِدي",
        "numberInSurah": 86,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2435,
        "text": "قالوا ما أَخلَفنا مَوعِدَكَ بِمَلكِنا وَلٰكِنّا حُمِّلنا أَوزارًا مِن زينَةِ القَومِ فَقَذَفناها فَكَذٰلِكَ أَلقَى السّامِرِيُّ",
        "numberInSurah": 87,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2436,
        "text": "فَأَخرَجَ لَهُم عِجلًا جَسَدًا لَهُ خُوارٌ فَقالوا هٰذا إِلٰهُكُم وَإِلٰهُ موسىٰ فَنَسِيَ",
        "numberInSurah": 88,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 273,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2437,
        "text": "أَفَلا يَرَونَ أَلّا يَرجِعُ إِلَيهِم قَولًا وَلا يَملِكُ لَهُم ضَرًّا وَلا نَفعًا",
        "numberInSurah": 89,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 273,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2438,
        "text": "وَلَقَد قالَ لَهُم هارونُ مِن قَبلُ يا قَومِ إِنَّما فُتِنتُم بِهِ ۖ وَإِنَّ رَبَّكُمُ الرَّحمٰنُ فَاتَّبِعوني وَأَطيعوا أَمري",
        "numberInSurah": 90,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2439,
        "text": "قالوا لَن نَبرَحَ عَلَيهِ عاكِفينَ حَتّىٰ يَرجِعَ إِلَينا موسىٰ",
        "numberInSurah": 91,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2440,
        "text": "قالَ يا هارونُ ما مَنَعَكَ إِذ رَأَيتَهُم ضَلّوا",
        "numberInSurah": 92,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2441,
        "text": "أَلّا تَتَّبِعَنِ ۖ أَفَعَصَيتَ أَمري",
        "numberInSurah": 93,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2442,
        "text": "قالَ يَا ابنَ أُمَّ لا تَأخُذ بِلِحيَتي وَلا بِرَأسي ۖ إِنّي خَشيتُ أَن تَقولَ فَرَّقتَ بَينَ بَني إِسرائيلَ وَلَم تَرقُب قَولي",
        "numberInSurah": 94,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2443,
        "text": "قالَ فَما خَطبُكَ يا سامِرِيُّ",
        "numberInSurah": 95,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2444,
        "text": "قالَ بَصُرتُ بِما لَم يَبصُروا بِهِ فَقَبَضتُ قَبضَةً مِن أَثَرِ الرَّسولِ فَنَبَذتُها وَكَذٰلِكَ سَوَّلَت لي نَفسي",
        "numberInSurah": 96,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2445,
        "text": "قالَ فَاذهَب فَإِنَّ لَكَ فِي الحَياةِ أَن تَقولَ لا مِساسَ ۖ وَإِنَّ لَكَ مَوعِدًا لَن تُخلَفَهُ ۖ وَانظُر إِلىٰ إِلٰهِكَ الَّذي ظَلتَ عَلَيهِ عاكِفًا ۖ لَنُحَرِّقَنَّهُ ثُمَّ لَنَنسِفَنَّهُ فِي اليَمِّ نَسفًا",
        "numberInSurah": 97,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2446,
        "text": "إِنَّما إِلٰهُكُمُ اللَّهُ الَّذي لا إِلٰهَ إِلّا هُوَ ۚ وَسِعَ كُلَّ شَيءٍ عِلمًا",
        "numberInSurah": 98,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2447,
        "text": "كَذٰلِكَ نَقُصُّ عَلَيكَ مِن أَنباءِ ما قَد سَبَقَ ۚ وَقَد آتَيناكَ مِن لَدُنّا ذِكرًا",
        "numberInSurah": 99,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2448,
        "text": "مَن أَعرَضَ عَنهُ فَإِنَّهُ يَحمِلُ يَومَ القِيامَةِ وِزرًا",
        "numberInSurah": 100,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2449,
        "text": "خالِدينَ فيهِ ۖ وَساءَ لَهُم يَومَ القِيامَةِ حِملًا",
        "numberInSurah": 101,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2450,
        "text": "يَومَ يُنفَخُ فِي الصّورِ ۚ وَنَحشُرُ المُجرِمينَ يَومَئِذٍ زُرقًا",
        "numberInSurah": 102,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2451,
        "text": "يَتَخافَتونَ بَينَهُم إِن لَبِثتُم إِلّا عَشرًا",
        "numberInSurah": 103,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2452,
        "text": "نَحنُ أَعلَمُ بِما يَقولونَ إِذ يَقولُ أَمثَلُهُم طَريقَةً إِن لَبِثتُم إِلّا يَومًا",
        "numberInSurah": 104,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2453,
        "text": "وَيَسأَلونَكَ عَنِ الجِبالِ فَقُل يَنسِفُها رَبّي نَسفًا",
        "numberInSurah": 105,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 275,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2454,
        "text": "فَيَذَرُها قاعًا صَفصَفًا",
        "numberInSurah": 106,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 275,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2455,
        "text": "لا تَرىٰ فيها عِوَجًا وَلا أَمتًا",
        "numberInSurah": 107,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 275,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2456,
        "text": "يَومَئِذٍ يَتَّبِعونَ الدّاعِيَ لا عِوَجَ لَهُ ۖ وَخَشَعَتِ الأَصواتُ لِلرَّحمٰنِ فَلا تَسمَعُ إِلّا هَمسًا",
        "numberInSurah": 108,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 275,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2457,
        "text": "يَومَئِذٍ لا تَنفَعُ الشَّفاعَةُ إِلّا مَن أَذِنَ لَهُ الرَّحمٰنُ وَرَضِيَ لَهُ قَولًا",
        "numberInSurah": 109,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 275,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2458,
        "text": "يَعلَمُ ما بَينَ أَيديهِم وَما خَلفَهُم وَلا يُحيطونَ بِهِ عِلمًا",
        "numberInSurah": 110,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 275,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2459,
        "text": "۞ وَعَنَتِ الوُجوهُ لِلحَيِّ القَيّومِ ۖ وَقَد خابَ مَن حَمَلَ ظُلمًا",
        "numberInSurah": 111,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 275,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2460,
        "text": "وَمَن يَعمَل مِنَ الصّالِحاتِ وَهُوَ مُؤمِنٌ فَلا يَخافُ ظُلمًا وَلا هَضمًا",
        "numberInSurah": 112,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 275,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2461,
        "text": "وَكَذٰلِكَ أَنزَلناهُ قُرآنًا عَرَبِيًّا وَصَرَّفنا فيهِ مِنَ الوَعيدِ لَعَلَّهُم يَتَّقونَ أَو يُحدِثُ لَهُم ذِكرًا",
        "numberInSurah": 113,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 275,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2462,
        "text": "فَتَعالَى اللَّهُ المَلِكُ الحَقُّ ۗ وَلا تَعجَل بِالقُرآنِ مِن قَبلِ أَن يُقضىٰ إِلَيكَ وَحيُهُ ۖ وَقُل رَبِّ زِدني عِلمًا",
        "numberInSurah": 114,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 275,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2463,
        "text": "وَلَقَد عَهِدنا إِلىٰ آدَمَ مِن قَبلُ فَنَسِيَ وَلَم نَجِد لَهُ عَزمًا",
        "numberInSurah": 115,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 275,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2464,
        "text": "وَإِذ قُلنا لِلمَلائِكَةِ اسجُدوا لِآدَمَ فَسَجَدوا إِلّا إِبليسَ أَبىٰ",
        "numberInSurah": 116,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2465,
        "text": "فَقُلنا يا آدَمُ إِنَّ هٰذا عَدُوٌّ لَكَ وَلِزَوجِكَ فَلا يُخرِجَنَّكُما مِنَ الجَنَّةِ فَتَشقىٰ",
        "numberInSurah": 117,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2466,
        "text": "إِنَّ لَكَ أَلّا تَجوعَ فيها وَلا تَعرىٰ",
        "numberInSurah": 118,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2467,
        "text": "وَأَنَّكَ لا تَظمَأُ فيها وَلا تَضحىٰ",
        "numberInSurah": 119,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2468,
        "text": "فَوَسوَسَ إِلَيهِ الشَّيطانُ قالَ يا آدَمُ هَل أَدُلُّكَ عَلىٰ شَجَرَةِ الخُلدِ وَمُلكٍ لا يَبلىٰ",
        "numberInSurah": 120,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2469,
        "text": "فَأَكَلا مِنها فَبَدَت لَهُما سَوآتُهُما وَطَفِقا يَخصِفانِ عَلَيهِما مِن وَرَقِ الجَنَّةِ ۚ وَعَصىٰ آدَمُ رَبَّهُ فَغَوىٰ",
        "numberInSurah": 121,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2470,
        "text": "ثُمَّ اجتَباهُ رَبُّهُ فَتابَ عَلَيهِ وَهَدىٰ",
        "numberInSurah": 122,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2471,
        "text": "قالَ اهبِطا مِنها جَميعًا ۖ بَعضُكُم لِبَعضٍ عَدُوٌّ ۖ فَإِمّا يَأتِيَنَّكُم مِنّي هُدًى فَمَنِ اتَّبَعَ هُدايَ فَلا يَضِلُّ وَلا يَشقىٰ",
        "numberInSurah": 123,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2472,
        "text": "وَمَن أَعرَضَ عَن ذِكري فَإِنَّ لَهُ مَعيشَةً ضَنكًا وَنَحشُرُهُ يَومَ القِيامَةِ أَعمىٰ",
        "numberInSurah": 124,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2473,
        "text": "قالَ رَبِّ لِمَ حَشَرتَني أَعمىٰ وَقَد كُنتُ بَصيرًا",
        "numberInSurah": 125,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2474,
        "text": "قالَ كَذٰلِكَ أَتَتكَ آياتُنا فَنَسيتَها ۖ وَكَذٰلِكَ اليَومَ تُنسىٰ",
        "numberInSurah": 126,
        "juz": 16,
        "manzil": 4,
        "page": 321,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2475,
        "text": "وَكَذٰلِكَ نَجزي مَن أَسرَفَ وَلَم يُؤمِن بِآياتِ رَبِّهِ ۚ وَلَعَذابُ الآخِرَةِ أَشَدُّ وَأَبقىٰ",
        "numberInSurah": 127,
        "juz": 16,
        "manzil": 4,
        "page": 321,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2476,
        "text": "أَفَلَم يَهدِ لَهُم كَم أَهلَكنا قَبلَهُم مِنَ القُرونِ يَمشونَ في مَساكِنِهِم ۗ إِنَّ في ذٰلِكَ لَآياتٍ لِأُولِي النُّهىٰ",
        "numberInSurah": 128,
        "juz": 16,
        "manzil": 4,
        "page": 321,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2477,
        "text": "وَلَولا كَلِمَةٌ سَبَقَت مِن رَبِّكَ لَكانَ لِزامًا وَأَجَلٌ مُسَمًّى",
        "numberInSurah": 129,
        "juz": 16,
        "manzil": 4,
        "page": 321,
        "ruku": 277,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2478,
        "text": "فَاصبِر عَلىٰ ما يَقولونَ وَسَبِّح بِحَمدِ رَبِّكَ قَبلَ طُلوعِ الشَّمسِ وَقَبلَ غُروبِها ۖ وَمِن آناءِ اللَّيلِ فَسَبِّح وَأَطرافَ النَّهارِ لَعَلَّكَ تَرضىٰ",
        "numberInSurah": 130,
        "juz": 16,
        "manzil": 4,
        "page": 321,
        "ruku": 277,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2479,
        "text": "وَلا تَمُدَّنَّ عَينَيكَ إِلىٰ ما مَتَّعنا بِهِ أَزواجًا مِنهُم زَهرَةَ الحَياةِ الدُّنيا لِنَفتِنَهُم فيهِ ۚ وَرِزقُ رَبِّكَ خَيرٌ وَأَبقىٰ",
        "numberInSurah": 131,
        "juz": 16,
        "manzil": 4,
        "page": 321,
        "ruku": 277,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2480,
        "text": "وَأمُر أَهلَكَ بِالصَّلاةِ وَاصطَبِر عَلَيها ۖ لا نَسأَلُكَ رِزقًا ۖ نَحنُ نَرزُقُكَ ۗ وَالعاقِبَةُ لِلتَّقوىٰ",
        "numberInSurah": 132,
        "juz": 16,
        "manzil": 4,
        "page": 321,
        "ruku": 277,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2481,
        "text": "وَقالوا لَولا يَأتينا بِآيَةٍ مِن رَبِّهِ ۚ أَوَلَم تَأتِهِم بَيِّنَةُ ما فِي الصُّحُفِ الأولىٰ",
        "numberInSurah": 133,
        "juz": 16,
        "manzil": 4,
        "page": 321,
        "ruku": 277,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2482,
        "text": "وَلَو أَنّا أَهلَكناهُم بِعَذابٍ مِن قَبلِهِ لَقالوا رَبَّنا لَولا أَرسَلتَ إِلَينا رَسولًا فَنَتَّبِعَ آياتِكَ مِن قَبلِ أَن نَذِلَّ وَنَخزىٰ",
        "numberInSurah": 134,
        "juz": 16,
        "manzil": 4,
        "page": 321,
        "ruku": 277,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2483,
        "text": "قُل كُلٌّ مُتَرَبِّصٌ فَتَرَبَّصوا ۖ فَسَتَعلَمونَ مَن أَصحابُ الصِّراطِ السَّوِيِّ وَمَنِ اهتَدىٰ",
        "numberInSurah": 135,
