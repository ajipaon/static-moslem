        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4740,
        "text": "وَالسَّقفِ المَرفوعِ",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4741,
        "text": "وَالبَحرِ المَسجورِ",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4742,
        "text": "إِنَّ عَذابَ رَبِّكَ لَواقِعٌ",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4743,
        "text": "ما لَهُ مِن دافِعٍ",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4744,
        "text": "يَومَ تَمورُ السَّماءُ مَورًا",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4745,
        "text": "وَتَسيرُ الجِبالُ سَيرًا",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4746,
        "text": "فَوَيلٌ يَومَئِذٍ لِلمُكَذِّبينَ",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4747,
        "text": "الَّذينَ هُم في خَوضٍ يَلعَبونَ",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4748,
        "text": "يَومَ يُدَعّونَ إِلىٰ نارِ جَهَنَّمَ دَعًّا",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4749,
        "text": "هٰذِهِ النّارُ الَّتي كُنتُم بِها تُكَذِّبونَ",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4750,
        "text": "أَفَسِحرٌ هٰذا أَم أَنتُم لا تُبصِرونَ",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4751,
        "text": "اصلَوها فَاصبِروا أَو لا تَصبِروا سَواءٌ عَلَيكُم ۖ إِنَّما تُجزَونَ ما كُنتُم تَعمَلونَ",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4752,
        "text": "إِنَّ المُتَّقينَ في جَنّاتٍ وَنَعيمٍ",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4753,
        "text": "فاكِهينَ بِما آتاهُم رَبُّهُم وَوَقاهُم رَبُّهُم عَذابَ الجَحيمِ",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4754,
        "text": "كُلوا وَاشرَبوا هَنيئًا بِما كُنتُم تَعمَلونَ",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4755,
        "text": "مُتَّكِئينَ عَلىٰ سُرُرٍ مَصفوفَةٍ ۖ وَزَوَّجناهُم بِحورٍ عينٍ",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4756,
        "text": "وَالَّذينَ آمَنوا وَاتَّبَعَتهُم ذُرِّيَّتُهُم بِإيمانٍ أَلحَقنا بِهِم ذُرِّيَّتَهُم وَما أَلَتناهُم مِن عَمَلِهِم مِن شَيءٍ ۚ كُلُّ امرِئٍ بِما كَسَبَ رَهينٌ",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4757,
        "text": "وَأَمدَدناهُم بِفاكِهَةٍ وَلَحمٍ مِمّا يَشتَهونَ",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4758,
        "text": "يَتَنازَعونَ فيها كَأسًا لا لَغوٌ فيها وَلا تَأثيمٌ",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4759,
        "text": "۞ وَيَطوفُ عَلَيهِم غِلمانٌ لَهُم كَأَنَّهُم لُؤلُؤٌ مَكنونٌ",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4760,
        "text": "وَأَقبَلَ بَعضُهُم عَلىٰ بَعضٍ يَتَساءَلونَ",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4761,
        "text": "قالوا إِنّا كُنّا قَبلُ في أَهلِنا مُشفِقينَ",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4762,
        "text": "فَمَنَّ اللَّهُ عَلَينا وَوَقانا عَذابَ السَّمومِ",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4763,
        "text": "إِنّا كُنّا مِن قَبلُ نَدعوهُ ۖ إِنَّهُ هُوَ البَرُّ الرَّحيمُ",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4764,
        "text": "فَذَكِّر فَما أَنتَ بِنِعمَتِ رَبِّكَ بِكاهِنٍ وَلا مَجنونٍ",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4765,
        "text": "أَم يَقولونَ شاعِرٌ نَتَرَبَّصُ بِهِ رَيبَ المَنونِ",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4766,
        "text": "قُل تَرَبَّصوا فَإِنّي مَعَكُم مِنَ المُتَرَبِّصينَ",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4767,
        "text": "أَم تَأمُرُهُم أَحلامُهُم بِهٰذا ۚ أَم هُم قَومٌ طاغونَ",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4768,
        "text": "أَم يَقولونَ تَقَوَّلَهُ ۚ بَل لا يُؤمِنونَ",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4769,
        "text": "فَليَأتوا بِحَديثٍ مِثلِهِ إِن كانوا صادِقينَ",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4770,
        "text": "أَم خُلِقوا مِن غَيرِ شَيءٍ أَم هُمُ الخالِقونَ",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4771,
        "text": "أَم خَلَقُوا السَّماواتِ وَالأَرضَ ۚ بَل لا يوقِنونَ",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4772,
        "text": "أَم عِندَهُم خَزائِنُ رَبِّكَ أَم هُمُ المُصَيطِرونَ",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4773,
        "text": "أَم لَهُم سُلَّمٌ يَستَمِعونَ فيهِ ۖ فَليَأتِ مُستَمِعُهُم بِسُلطانٍ مُبينٍ",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4774,
        "text": "أَم لَهُ البَناتُ وَلَكُمُ البَنونَ",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4775,
        "text": "أَم تَسأَلُهُم أَجرًا فَهُم مِن مَغرَمٍ مُثقَلونَ",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4776,
        "text": "أَم عِندَهُمُ الغَيبُ فَهُم يَكتُبونَ",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4777,
        "text": "أَم يُريدونَ كَيدًا ۖ فَالَّذينَ كَفَروا هُمُ المَكيدونَ",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4778,
        "text": "أَم لَهُم إِلٰهٌ غَيرُ اللَّهِ ۚ سُبحانَ اللَّهِ عَمّا يُشرِكونَ",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4779,
        "text": "وَإِن يَرَوا كِسفًا مِنَ السَّماءِ ساقِطًا يَقولوا سَحابٌ مَركومٌ",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4780,
        "text": "فَذَرهُم حَتّىٰ يُلاقوا يَومَهُمُ الَّذي فيهِ يُصعَقونَ",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4781,
        "text": "يَومَ لا يُغني عَنهُم كَيدُهُم شَيئًا وَلا هُم يُنصَرونَ",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4782,
        "text": "وَإِنَّ لِلَّذينَ ظَلَموا عَذابًا دونَ ذٰلِكَ وَلٰكِنَّ أَكثَرَهُم لا يَعلَمونَ",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4783,
        "text": "وَاصبِر لِحُكمِ رَبِّكَ فَإِنَّكَ بِأَعيُنِنا ۖ وَسَبِّح بِحَمدِ رَبِّكَ حينَ تَقومُ",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4784,
