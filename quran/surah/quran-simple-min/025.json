  "data": {
    "number": 25,
    "name": "سُورَةُ الفُرۡقَانِ",
    "englishName": "Al-Furqaan",
    "englishNameTranslation": "The Criterion",
    "revelationType": "Meccan",
    "numberOfAyahs": 77,
    "ayahs": [
      {
        "number": 2856,
        "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ تَبارَكَ الَّذي نَزَّلَ الفُرقانَ عَلىٰ عَبدِهِ لِيَكونَ لِلعالَمينَ نَذيرًا",
        "numberInSurah": 1,
        "juz": 18,
        "manzil": 4,
        "page": 359,
        "ruku": 310,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2857,
        "text": "الَّذي لَهُ مُلكُ السَّماواتِ وَالأَرضِ وَلَم يَتَّخِذ وَلَدًا وَلَم يَكُن لَهُ شَريكٌ فِي المُلكِ وَخَلَقَ كُلَّ شَيءٍ فَقَدَّرَهُ تَقديرًا",
        "numberInSurah": 2,
        "juz": 18,
        "manzil": 4,
        "page": 359,
        "ruku": 310,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2858,
        "text": "وَاتَّخَذوا مِن دونِهِ آلِهَةً لا يَخلُقونَ شَيئًا وَهُم يُخلَقونَ وَلا يَملِكونَ لِأَنفُسِهِم ضَرًّا وَلا نَفعًا وَلا يَملِكونَ مَوتًا وَلا حَياةً وَلا نُشورًا",
        "numberInSurah": 3,
        "juz": 18,
        "manzil": 4,
        "page": 360,
        "ruku": 310,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2859,
        "text": "وَقالَ الَّذينَ كَفَروا إِن هٰذا إِلّا إِفكٌ افتَراهُ وَأَعانَهُ عَلَيهِ قَومٌ آخَرونَ ۖ فَقَد جاءوا ظُلمًا وَزورًا",
        "numberInSurah": 4,
        "juz": 18,
        "manzil": 4,
        "page": 360,
        "ruku": 310,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2860,
        "text": "وَقالوا أَساطيرُ الأَوَّلينَ اكتَتَبَها فَهِيَ تُملىٰ عَلَيهِ بُكرَةً وَأَصيلًا",
        "numberInSurah": 5,
        "juz": 18,
        "manzil": 4,
        "page": 360,
        "ruku": 310,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2861,
        "text": "قُل أَنزَلَهُ الَّذي يَعلَمُ السِّرَّ فِي السَّماواتِ وَالأَرضِ ۚ إِنَّهُ كانَ غَفورًا رَحيمًا",
        "numberInSurah": 6,
        "juz": 18,
        "manzil": 4,
        "page": 360,
        "ruku": 310,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2862,
        "text": "وَقالوا مالِ هٰذَا الرَّسولِ يَأكُلُ الطَّعامَ وَيَمشي فِي الأَسواقِ ۙ لَولا أُنزِلَ إِلَيهِ مَلَكٌ فَيَكونَ مَعَهُ نَذيرًا",
        "numberInSurah": 7,
        "juz": 18,
        "manzil": 4,
        "page": 360,
        "ruku": 310,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2863,
        "text": "أَو يُلقىٰ إِلَيهِ كَنزٌ أَو تَكونُ لَهُ جَنَّةٌ يَأكُلُ مِنها ۚ وَقالَ الظّالِمونَ إِن تَتَّبِعونَ إِلّا رَجُلًا مَسحورًا",
        "numberInSurah": 8,
        "juz": 18,
        "manzil": 4,
        "page": 360,
        "ruku": 310,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2864,
        "text": "انظُر كَيفَ ضَرَبوا لَكَ الأَمثالَ فَضَلّوا فَلا يَستَطيعونَ سَبيلًا",
        "numberInSurah": 9,
        "juz": 18,
        "manzil": 4,
        "page": 360,
        "ruku": 310,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2865,
        "text": "تَبارَكَ الَّذي إِن شاءَ جَعَلَ لَكَ خَيرًا مِن ذٰلِكَ جَنّاتٍ تَجري مِن تَحتِهَا الأَنهارُ وَيَجعَل لَكَ قُصورًا",
        "numberInSurah": 10,
        "juz": 18,
        "manzil": 4,
        "page": 360,
        "ruku": 311,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2866,
        "text": "بَل كَذَّبوا بِالسّاعَةِ ۖ وَأَعتَدنا لِمَن كَذَّبَ بِالسّاعَةِ سَعيرًا",
        "numberInSurah": 11,
        "juz": 18,
        "manzil": 4,
        "page": 360,
        "ruku": 311,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2867,
        "text": "إِذا رَأَتهُم مِن مَكانٍ بَعيدٍ سَمِعوا لَها تَغَيُّظًا وَزَفيرًا",
        "numberInSurah": 12,
        "juz": 18,
        "manzil": 4,
        "page": 361,
        "ruku": 311,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2868,
        "text": "وَإِذا أُلقوا مِنها مَكانًا ضَيِّقًا مُقَرَّنينَ دَعَوا هُنالِكَ ثُبورًا",
        "numberInSurah": 13,
        "juz": 18,
        "manzil": 4,
        "page": 361,
        "ruku": 311,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2869,
        "text": "لا تَدعُوا اليَومَ ثُبورًا واحِدًا وَادعوا ثُبورًا كَثيرًا",
        "numberInSurah": 14,
        "juz": 18,
        "manzil": 4,
        "page": 361,
        "ruku": 311,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2870,
        "text": "قُل أَذٰلِكَ خَيرٌ أَم جَنَّةُ الخُلدِ الَّتي وُعِدَ المُتَّقونَ ۚ كانَت لَهُم جَزاءً وَمَصيرًا",
        "numberInSurah": 15,
        "juz": 18,
        "manzil": 4,
        "page": 361,
        "ruku": 311,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2871,
        "text": "لَهُم فيها ما يَشاءونَ خالِدينَ ۚ كانَ عَلىٰ رَبِّكَ وَعدًا مَسئولًا",
        "numberInSurah": 16,
        "juz": 18,
        "manzil": 4,
        "page": 361,
        "ruku": 311,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2872,
        "text": "وَيَومَ يَحشُرُهُم وَما يَعبُدونَ مِن دونِ اللَّهِ فَيَقولُ أَأَنتُم أَضلَلتُم عِبادي هٰؤُلاءِ أَم هُم ضَلُّوا السَّبيلَ",
        "numberInSurah": 17,
        "juz": 18,
        "manzil": 4,
        "page": 361,
        "ruku": 311,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2873,
        "text": "قالوا سُبحانَكَ ما كانَ يَنبَغي لَنا أَن نَتَّخِذَ مِن دونِكَ مِن أَولِياءَ وَلٰكِن مَتَّعتَهُم وَآباءَهُم حَتّىٰ نَسُوا الذِّكرَ وَكانوا قَومًا بورًا",
        "numberInSurah": 18,
        "juz": 18,
        "manzil": 4,
        "page": 361,
        "ruku": 311,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2874,
        "text": "فَقَد كَذَّبوكُم بِما تَقولونَ فَما تَستَطيعونَ صَرفًا وَلا نَصرًا ۚ وَمَن يَظلِم مِنكُم نُذِقهُ عَذابًا كَبيرًا",
        "numberInSurah": 19,
        "juz": 18,
        "manzil": 4,
        "page": 361,
        "ruku": 311,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2875,
        "text": "وَما أَرسَلنا قَبلَكَ مِنَ المُرسَلينَ إِلّا إِنَّهُم لَيَأكُلونَ الطَّعامَ وَيَمشونَ فِي الأَسواقِ ۗ وَجَعَلنا بَعضَكُم لِبَعضٍ فِتنَةً أَتَصبِرونَ ۗ وَكانَ رَبُّكَ بَصيرًا",
        "numberInSurah": 20,
        "juz": 18,
        "manzil": 4,
        "page": 361,
        "ruku": 311,
        "hizbQuarter": 144,
        "sajda": false
      },
      {
        "number": 2876,
        "text": "۞ وَقالَ الَّذينَ لا يَرجونَ لِقاءَنا لَولا أُنزِلَ عَلَينَا المَلائِكَةُ أَو نَرىٰ رَبَّنا ۗ لَقَدِ استَكبَروا في أَنفُسِهِم وَعَتَوا عُتُوًّا كَبيرًا",
        "numberInSurah": 21,
        "juz": 19,
        "manzil": 4,
        "page": 362,
        "ruku": 312,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2877,
        "text": "يَومَ يَرَونَ المَلائِكَةَ لا بُشرىٰ يَومَئِذٍ لِلمُجرِمينَ وَيَقولونَ حِجرًا مَحجورًا",
        "numberInSurah": 22,
        "juz": 19,
        "manzil": 4,
        "page": 362,
        "ruku": 312,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2878,
        "text": "وَقَدِمنا إِلىٰ ما عَمِلوا مِن عَمَلٍ فَجَعَلناهُ هَباءً مَنثورًا",
        "numberInSurah": 23,
        "juz": 19,
        "manzil": 4,
        "page": 362,
        "ruku": 312,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2879,
        "text": "أَصحابُ الجَنَّةِ يَومَئِذٍ خَيرٌ مُستَقَرًّا وَأَحسَنُ مَقيلًا",
        "numberInSurah": 24,
        "juz": 19,
        "manzil": 4,
        "page": 362,
        "ruku": 312,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2880,
        "text": "وَيَومَ تَشَقَّقُ السَّماءُ بِالغَمامِ وَنُزِّلَ المَلائِكَةُ تَنزيلًا",
        "numberInSurah": 25,
        "juz": 19,
        "manzil": 4,
        "page": 362,
        "ruku": 312,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2881,
        "text": "المُلكُ يَومَئِذٍ الحَقُّ لِلرَّحمٰنِ ۚ وَكانَ يَومًا عَلَى الكافِرينَ عَسيرًا",
        "numberInSurah": 26,
        "juz": 19,
        "manzil": 4,
        "page": 362,
        "ruku": 312,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2882,
        "text": "وَيَومَ يَعَضُّ الظّالِمُ عَلىٰ يَدَيهِ يَقولُ يا لَيتَنِي اتَّخَذتُ مَعَ الرَّسولِ سَبيلًا",
        "numberInSurah": 27,
        "juz": 19,
        "manzil": 4,
        "page": 362,
        "ruku": 312,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2883,
        "text": "يا وَيلَتىٰ لَيتَني لَم أَتَّخِذ فُلانًا خَليلًا",
        "numberInSurah": 28,
        "juz": 19,
        "manzil": 4,
        "page": 362,
        "ruku": 312,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2884,
        "text": "لَقَد أَضَلَّني عَنِ الذِّكرِ بَعدَ إِذ جاءَني ۗ وَكانَ الشَّيطانُ لِلإِنسانِ خَذولًا",
        "numberInSurah": 29,
        "juz": 19,
        "manzil": 4,
        "page": 362,
        "ruku": 312,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2885,
        "text": "وَقالَ الرَّسولُ يا رَبِّ إِنَّ قَومِي اتَّخَذوا هٰذَا القُرآنَ مَهجورًا",
        "numberInSurah": 30,
        "juz": 19,
        "manzil": 4,
        "page": 362,
        "ruku": 312,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2886,
        "text": "وَكَذٰلِكَ جَعَلنا لِكُلِّ نَبِيٍّ عَدُوًّا مِنَ المُجرِمينَ ۗ وَكَفىٰ بِرَبِّكَ هادِيًا وَنَصيرًا",
        "numberInSurah": 31,
        "juz": 19,
        "manzil": 4,
        "page": 362,
        "ruku": 312,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2887,
        "text": "وَقالَ الَّذينَ كَفَروا لَولا نُزِّلَ عَلَيهِ القُرآنُ جُملَةً واحِدَةً ۚ كَذٰلِكَ لِنُثَبِّتَ بِهِ فُؤادَكَ ۖ وَرَتَّلناهُ تَرتيلًا",
        "numberInSurah": 32,
        "juz": 19,
        "manzil": 4,
        "page": 362,
        "ruku": 312,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2888,
        "text": "وَلا يَأتونَكَ بِمَثَلٍ إِلّا جِئناكَ بِالحَقِّ وَأَحسَنَ تَفسيرًا",
        "numberInSurah": 33,
        "juz": 19,
        "manzil": 4,
        "page": 363,
        "ruku": 312,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2889,
        "text": "الَّذينَ يُحشَرونَ عَلىٰ وُجوهِهِم إِلىٰ جَهَنَّمَ أُولٰئِكَ شَرٌّ مَكانًا وَأَضَلُّ سَبيلًا",
        "numberInSurah": 34,
        "juz": 19,
        "manzil": 4,
        "page": 363,
        "ruku": 312,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2890,
        "text": "وَلَقَد آتَينا موسَى الكِتابَ وَجَعَلنا مَعَهُ أَخاهُ هارونَ وَزيرًا",
        "numberInSurah": 35,
        "juz": 19,
        "manzil": 4,
        "page": 363,
        "ruku": 313,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2891,
        "text": "فَقُلنَا اذهَبا إِلَى القَومِ الَّذينَ كَذَّبوا بِآياتِنا فَدَمَّرناهُم تَدميرًا",
        "numberInSurah": 36,
        "juz": 19,
        "manzil": 4,
        "page": 363,
        "ruku": 313,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2892,
        "text": "وَقَومَ نوحٍ لَمّا كَذَّبُوا الرُّسُلَ أَغرَقناهُم وَجَعَلناهُم لِلنّاسِ آيَةً ۖ وَأَعتَدنا لِلظّالِمينَ عَذابًا أَليمًا",
        "numberInSurah": 37,
        "juz": 19,
        "manzil": 4,
        "page": 363,
        "ruku": 313,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2893,
        "text": "وَعادًا وَثَمودَ وَأَصحابَ الرَّسِّ وَقُرونًا بَينَ ذٰلِكَ كَثيرًا",
        "numberInSurah": 38,
        "juz": 19,
        "manzil": 4,
        "page": 363,
        "ruku": 313,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2894,
        "text": "وَكُلًّا ضَرَبنا لَهُ الأَمثالَ ۖ وَكُلًّا تَبَّرنا تَتبيرًا",
        "numberInSurah": 39,
        "juz": 19,
        "manzil": 4,
        "page": 363,
        "ruku": 313,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2895,
        "text": "وَلَقَد أَتَوا عَلَى القَريَةِ الَّتي أُمطِرَت مَطَرَ السَّوءِ ۚ أَفَلَم يَكونوا يَرَونَها ۚ بَل كانوا لا يَرجونَ نُشورًا",
        "numberInSurah": 40,
        "juz": 19,
        "manzil": 4,
        "page": 363,
        "ruku": 313,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2896,
        "text": "وَإِذا رَأَوكَ إِن يَتَّخِذونَكَ إِلّا هُزُوًا أَهٰذَا الَّذي بَعَثَ اللَّهُ رَسولًا",
        "numberInSurah": 41,
        "juz": 19,
        "manzil": 4,
        "page": 363,
        "ruku": 313,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2897,
        "text": "إِن كادَ لَيُضِلُّنا عَن آلِهَتِنا لَولا أَن صَبَرنا عَلَيها ۚ وَسَوفَ يَعلَمونَ حينَ يَرَونَ العَذابَ مَن أَضَلُّ سَبيلًا",
        "numberInSurah": 42,
        "juz": 19,
        "manzil": 4,
        "page": 363,
        "ruku": 313,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2898,
        "text": "أَرَأَيتَ مَنِ اتَّخَذَ إِلٰهَهُ هَواهُ أَفَأَنتَ تَكونُ عَلَيهِ وَكيلًا",
        "numberInSurah": 43,
        "juz": 19,
        "manzil": 4,
        "page": 363,
        "ruku": 313,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2899,
        "text": "أَم تَحسَبُ أَنَّ أَكثَرَهُم يَسمَعونَ أَو يَعقِلونَ ۚ إِن هُم إِلّا كَالأَنعامِ ۖ بَل هُم أَضَلُّ سَبيلًا",
        "numberInSurah": 44,
        "juz": 19,
        "manzil": 4,
        "page": 364,
        "ruku": 313,
        "hizbQuarter": 145,
