  "data": {
    "number": 54,
    "name": "سُورَةُ القَمَرِ",
    "englishName": "Al-Qamar",
    "englishNameTranslation": "The Moon",
    "revelationType": "Meccan",
    "numberOfAyahs": 55,
    "ayahs": [
      {
        "number": 4847,
        "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ اقتَرَبَتِ السّاعَةُ وَانشَقَّ القَمَرُ",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4848,
        "text": "وَإِن يَرَوا آيَةً يُعرِضوا وَيَقولوا سِحرٌ مُستَمِرٌّ",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4849,
        "text": "وَكَذَّبوا وَاتَّبَعوا أَهواءَهُم ۚ وَكُلُّ أَمرٍ مُستَقِرٌّ",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4850,
        "text": "وَلَقَد جاءَهُم مِنَ الأَنباءِ ما فيهِ مُزدَجَرٌ",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4851,
        "text": "حِكمَةٌ بالِغَةٌ ۖ فَما تُغنِ النُّذُرُ",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4852,
        "text": "فَتَوَلَّ عَنهُم ۘ يَومَ يَدعُ الدّاعِ إِلىٰ شَيءٍ نُكُرٍ",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4853,
        "text": "خُشَّعًا أَبصارُهُم يَخرُجونَ مِنَ الأَجداثِ كَأَنَّهُم جَرادٌ مُنتَشِرٌ",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4854,
        "text": "مُهطِعينَ إِلَى الدّاعِ ۖ يَقولُ الكافِرونَ هٰذا يَومٌ عَسِرٌ",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4855,
        "text": "۞ كَذَّبَت قَبلَهُم قَومُ نوحٍ فَكَذَّبوا عَبدَنا وَقالوا مَجنونٌ وَازدُجِرَ",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4856,
        "text": "فَدَعا رَبَّهُ أَنّي مَغلوبٌ فَانتَصِر",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4857,
        "text": "فَفَتَحنا أَبوابَ السَّماءِ بِماءٍ مُنهَمِرٍ",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4858,
        "text": "وَفَجَّرنَا الأَرضَ عُيونًا فَالتَقَى الماءُ عَلىٰ أَمرٍ قَد قُدِرَ",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4859,
        "text": "وَحَمَلناهُ عَلىٰ ذاتِ أَلواحٍ وَدُسُرٍ",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4860,
        "text": "تَجري بِأَعيُنِنا جَزاءً لِمَن كانَ كُفِرَ",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4861,
        "text": "وَلَقَد تَرَكناها آيَةً فَهَل مِن مُدَّكِرٍ",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4862,
        "text": "فَكَيفَ كانَ عَذابي وَنُذُرِ",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4863,
        "text": "وَلَقَد يَسَّرنَا القُرآنَ لِلذِّكرِ فَهَل مِن مُدَّكِرٍ",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4864,
        "text": "كَذَّبَت عادٌ فَكَيفَ كانَ عَذابي وَنُذُرِ",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4865,
        "text": "إِنّا أَرسَلنا عَلَيهِم ريحًا صَرصَرًا في يَومِ نَحسٍ مُستَمِرٍّ",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4866,
        "text": "تَنزِعُ النّاسَ كَأَنَّهُم أَعجازُ نَخلٍ مُنقَعِرٍ",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4867,
        "text": "فَكَيفَ كانَ عَذابي وَنُذُرِ",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4868,
        "text": "وَلَقَد يَسَّرنَا القُرآنَ لِلذِّكرِ فَهَل مِن مُدَّكِرٍ",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4869,
        "text": "كَذَّبَت ثَمودُ بِالنُّذُرِ",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4870,
        "text": "فَقالوا أَبَشَرًا مِنّا واحِدًا نَتَّبِعُهُ إِنّا إِذًا لَفي ضَلالٍ وَسُعُرٍ",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4871,
        "text": "أَأُلقِيَ الذِّكرُ عَلَيهِ مِن بَينِنا بَل هُوَ كَذّابٌ أَشِرٌ",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4872,
        "text": "سَيَعلَمونَ غَدًا مَنِ الكَذّابُ الأَشِرُ",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4873,
        "text": "إِنّا مُرسِلُو النّاقَةِ فِتنَةً لَهُم فَارتَقِبهُم وَاصطَبِر",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4874,
        "text": "وَنَبِّئهُم أَنَّ الماءَ قِسمَةٌ بَينَهُم ۖ كُلُّ شِربٍ مُحتَضَرٌ",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4875,
        "text": "فَنادَوا صاحِبَهُم فَتَعاطىٰ فَعَقَرَ",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4876,
        "text": "فَكَيفَ كانَ عَذابي وَنُذُرِ",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4877,
        "text": "إِنّا أَرسَلنا عَلَيهِم صَيحَةً واحِدَةً فَكانوا كَهَشيمِ المُحتَظِرِ",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4878,
        "text": "وَلَقَد يَسَّرنَا القُرآنَ لِلذِّكرِ فَهَل مِن مُدَّكِرٍ",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4879,
        "text": "كَذَّبَت قَومُ لوطٍ بِالنُّذُرِ",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4880,
        "text": "إِنّا أَرسَلنا عَلَيهِم حاصِبًا إِلّا آلَ لوطٍ ۖ نَجَّيناهُم بِسَحَرٍ",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4881,
        "text": "نِعمَةً مِن عِندِنا ۚ كَذٰلِكَ نَجزي مَن شَكَرَ",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4882,
        "text": "وَلَقَد أَنذَرَهُم بَطشَتَنا فَتَمارَوا بِالنُّذُرِ",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4883,
        "text": "وَلَقَد راوَدوهُ عَن ضَيفِهِ فَطَمَسنا أَعيُنَهُم فَذوقوا عَذابي وَنُذُرِ",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4884,
        "text": "وَلَقَد صَبَّحَهُم بُكرَةً عَذابٌ مُستَقِرٌّ",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4885,
        "text": "فَذوقوا عَذابي وَنُذُرِ",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4886,
        "text": "وَلَقَد يَسَّرنَا القُرآنَ لِلذِّكرِ فَهَل مِن مُدَّكِرٍ",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4887,
        "text": "وَلَقَد جاءَ آلَ فِرعَونَ النُّذُرُ",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4888,
        "text": "كَذَّبوا بِآياتِنا كُلِّها فَأَخَذناهُم أَخذَ عَزيزٍ مُقتَدِرٍ",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4889,
        "text": "أَكُفّارُكُم خَيرٌ مِن أُولٰئِكُم أَم لَكُم بَراءَةٌ فِي الزُّبُرِ",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4890,
        "text": "أَم يَقولونَ نَحنُ جَميعٌ مُنتَصِرٌ",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4891,
        "text": "سَيُهزَمُ الجَمعُ وَيُوَلّونَ الدُّبُرَ",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4892,
        "text": "بَلِ السّاعَةُ مَوعِدُهُم وَالسّاعَةُ أَدهىٰ وَأَمَرُّ",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4893,
        "text": "إِنَّ المُجرِمينَ في ضَلالٍ وَسُعُرٍ",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4894,
        "text": "يَومَ يُسحَبونَ فِي النّارِ عَلىٰ وُجوهِهِم ذوقوا مَسَّ سَقَرَ",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4895,
        "text": "إِنّا كُلَّ شَيءٍ خَلَقناهُ بِقَدَرٍ",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4896,
