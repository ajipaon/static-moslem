        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5396,
        "text": "وَإِذا مَسَّهُ الخَيرُ مَنوعًا",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5397,
