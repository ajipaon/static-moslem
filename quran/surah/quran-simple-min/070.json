        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5402,
        "text": "وَالَّذينَ هُم مِن عَذابِ رَبِّهِم مُشفِقونَ",
