{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 51,
    "name": "سُورَةُ الذَّارِيَاتِ",
    "englishName": "Adh-Dhaariyat",
    "englishNameTranslation": "The Winnowing Winds",
    "revelationType": "Meccan",
    "numberOfAyahs": 60,
    "ayahs": [
      {
        "number": 4676,
        "text": "Při ROZPTYLUJICÍCH rozptylováním",
        "numberInSurah": 1,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4677,
        "text": "a břímě nesoucích;",
        "numberInSurah": 2,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4678,
        "text": "a lehce plynoucích",
        "numberInSurah": 3,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4679,
        "text": "a dle rozkazu rozdělujících,",
        "numberInSurah": 4,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4680,
        "text": "(přisahám, že) co slibováno jest vám, zajisté jest pravdivo:",
        "numberInSurah": 5,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4681,
        "text": "a že (den) súčtování přijde nezvratně.",
        "numberInSurah": 6,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4682,
        "text": "Při nebi plném (hvězdných) drah,",
        "numberInSurah": 7,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4683,
        "text": "věru řeč vedete rozličnou:",
        "numberInSurah": 8,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4684,
        "text": "odvrácen od ní bude ten, jenž byl odvrácen.",
        "numberInSurah": 9,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4685,
        "text": "Nechť pobiti jsou lháři,",
        "numberInSurah": 10,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4686,
        "text": "kdož v hloubi (nevědomosti) své jsou lhostejni,",
        "numberInSurah": 11,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4687,
        "text": "a táží se: „Kdy přijde ten den súčtování?“",
        "numberInSurah": 12,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4688,
        "text": "V den ten na ohni budou zkoušeni:",
        "numberInSurah": 13,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4689,
        "text": "„Okuste této zkoušky své, již uspíšiti chtěli jste!“",
        "numberInSurah": 14,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4690,
        "text": "Však bohabojní v zahradách budou a u pramenů.",
        "numberInSurah": 15,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4691,
        "text": "při-jímajíce to, co dal jim Pán jejich za to, že předtím dobře činili:",
        "numberInSurah": 16,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4692,
        "text": "jen málo v noci spávali:",
        "numberInSurah": 17,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4693,
        "text": "z jitra za odpuštění prosili:",
        "numberInSurah": 18,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4694,
        "text": "ze statků svých patřičný podíl prosícím a odvrženým dávali.",
        "numberInSurah": 19,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4695,
        "text": "Na zemi znamení jsou pevně věřícím,",
        "numberInSurah": 20,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4696,
        "text": "i ve vás samých: což neprohlédnete?",
        "numberInSurah": 21,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4697,
        "text": "A na nebi výživa jest vaše a to, co slíbeno jest vám;",
        "numberInSurah": 22,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4698,
        "text": "a při Pánu nebes a země přisahám: pravda jest to, jako že mluvíte!",
        "numberInSurah": 23,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4699,
        "text": "Zdaž došla tě pověst o hostech Abrahama poctěných,",
        "numberInSurah": 24,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4700,
        "text": "když přišli k němu a řekli: „Pokoj tobě,“ a řekl: „Pokoj vám! — Jsou lidé to neznámí.“",
        "numberInSurah": 25,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4701,
        "text": "I poodešel k rodině své a přinesl tele krmené:",
        "numberInSurah": 26,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4702,
        "text": "a předložil jim je, řka: „Zdaž nepojíte snad?“",
        "numberInSurah": 27,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4703,
        "text": "A zachvěl se před nimi bázní; oni pak řekli: „Neboj se“ a oznámili mu narození mládce moudrého.",
        "numberInSurah": 28,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4704,
        "text": "I přišla žena jeho; vzkřikla a udeřivši se v tvář svou, řekla: „Stařena (jsem již) neplodná.“",
        "numberInSurah": 29,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4705,
        "text": "Řekli, „Tak řekl Pán tvůj; on zajisté vševědoucí jest a moudrý.“",
        "numberInSurah": 30,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4706,
        "text": "Řekl (Abraham): „A jaký jest cíl váš, ó poslové?“",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4707,
        "text": "Řekli: „Zajistéť posláni jsme k lidu provinilému,",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4708,
        "text": "abychom seslali naň kamení z hlíny (pálené),",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4709,
        "text": "u Pána tvého jmény výstředníků znamenané.“",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4710,
        "text": "A vyvedli jsme ven z města věřící:",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4711,
        "text": "však nalezli jsme v něm jen jeden dům, kde (lidé) odevzdáni byli do vůle boží;",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4712,
        "text": "a zanechali jsme v něm znamení pro ty, kdož bojí se trestu bolestného.",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4713,
        "text": "A (též) v Mojžíšovi, když poslali jsme jej k Faraonovi s mocí zjevnou:",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4714,
        "text": "však odvrátil se on i přední lidu jeho, řka: „Kouzelník to, neb (duchem) posedlý!“",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4715,
        "text": "I zachvátili jsme jej i vojska jeho a vmetli jsme je v moře; neb byl hoden pohany.",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4716,
        "text": "A též v (lidu) 'Ád, když poslali jsme naň vichr ničivý,",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4717,
        "text": "jenž neponechal ničeho v cestě své, aniž by nebyl v prach to obrátil.",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4718,
        "text": "A v (lidu) Tsemúd, když řečeno mu bylo: „Těšte se z požitků svých dočasně!“",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4719,
        "text": "Však protivili se rozkazu Pána svého a zachvátil je vichr bouřný, ani naň hleděli.",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4720,
        "text": "A nebyli s to, udržeti se v stoje, aniž si pomoci.",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4721,
        "text": "A v (lidu) Noemově, před nimi, neb lidem byl prostopášným.",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4722,
        "text": "A nebesa zbudovali jsme vlastnoručně a dali velký rozsah (jim):",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4723,
        "text": "a zemi (jak koberec) jsme rozprostřeli: jak krásně byla námi rozložena!",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4724,
        "text": "A všechny věci stvořili jsme v párech, abyste si toho byli pamětlivi.",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4725,
        "text": "Utíkejte se tedy k Bohu: jáť pak zajisté jsem vám od něho varovatelem zjevným!",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4726,
        "text": "A nečiňte (si) s Bohem bohů jiných: jáť zajisté jsem vám od něho varovatelem zjevným!",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4727,
        "text": "Stejně tak, kdykoli přišel k těm, již před nimi byli, prorok, řekli jen: „Kouzelník to, neb (duchem) posedlý.“",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4728,
        "text": "Dávají si to navzájem dědictvím? Ano, lid jest to svévolný.",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4729,
        "text": "I odvrať se od nich, nebude ti to zazlíváno:",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4730,
        "text": "však napomínej, neb zajisté napomenutí prospěje věřícím.",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4731,
        "text": "A stvořil jsem Džinny a lidi jen proto, aby mne uctívali.",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4732,
        "text": "Nežádám si od nich výživy, aniž žádám si, aby mně jídlo dávali.",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4733,
        "text": "Zajisté Bůh jest Živitel silný, pevný!",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4734,
        "text": "Nepravostným pak zajisté připadne stejný úděl, jaký připadl rovným jim předtím: nechť tedy na mne nepospíchají!",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4735,
        "text": "A běda těm, kdož neuvěřili, přede dnem soudu jejich, jenž slíben jest jim!",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      }
    ],
