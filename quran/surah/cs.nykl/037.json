        "numberInSurah": 42,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3831,
        "text": "v zahradách (rajských) rozkošných,",
        "numberInSurah": 43,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3832,
