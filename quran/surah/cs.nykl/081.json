  "data": {
    "number": 81,
    "name": "سُورَةُ التَّكۡوِيرِ",
    "englishName": "At-Takwir",
    "englishNameTranslation": "The Overthrowing",
    "revelationType": "Meccan",
    "numberOfAyahs": 29,
    "ayahs": [
      {
        "number": 5801,
        "text": "Když slunce bude SLOŽENO;",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5802,
        "text": "a hvězdy budou padati;",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5803,
        "text": "a pohoří se hýbati;",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5804,
        "text": "a velbloudice budou opuštěny;",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5805,
        "text": "a divá zvěř bude shromážděna;",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5806,
        "text": "a moře budou varem vřít;",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5807,
        "text": "a duše (ke svým tělům) budou pojeny;",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5808,
        "text": "a (dcerka) za živa zahrabaná bude tázána,",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5809,
        "text": "pro jakou vinu byla zabita;",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5810,
        "text": "když listy (knih) budou rozbaleny;",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5811,
        "text": "a nebesa stranou stažena;",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5812,
        "text": "a pekla plamen rozdmýchán;",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5813,
        "text": "zahrada ráje pak přiblížena:",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5814,
        "text": "tu (každá) duše zví, co vykonala.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5815,
        "text": "Aniž přisáhnout (nutno mi) při hvězdách zpátečných",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5816,
        "text": "jež běží rychle a skrývají se;",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5817,
        "text": "aniž při noci, když temno nadchází,",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5818,
        "text": "anebo jitru, když dechu nabírá —",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5819,
        "text": "že (Korán) jest rčením, jež pronesl posel vzácný,",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5820,
        "text": "mocný před Pánem trůnu, pevný,",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5821,
        "text": "jenž poslouchán jest a věrný.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5822,
        "text": "Neníť soudruh váš Džinny posedlý,",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5823,
        "text": "nýbrž viděl jej na obzoru jasném;",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5824,
        "text": "aniž pro sebe chce podržeti věci skryté,",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5825,
        "text": "aniž slova opakuje, jež vyřkl Satan kamenovaný.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5826,
        "text": "Kam tedy odcházíte?",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5827,
        "text": "(Korán) jest pouze Napomenutím veškerenstvu;",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5828,
        "text": "těm z vás, kdož přímou chtějí stezkou jít;",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5829,
        "text": "však nebudete toho chtít, leda by Bůh chtěl tomu, Pán světů veškerých.",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      }
    ],
