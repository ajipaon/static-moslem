  "data": {
    "number": 91,
    "name": "سُورَةُ الشَّمۡسِ",
    "englishName": "Ash-Shams",
    "englishNameTranslation": "The Sun",
    "revelationType": "Meccan",
    "numberOfAyahs": 15,
    "ayahs": [
      {
        "number": 6044,
        "text": "Při SLUNCI a polední záři jeho,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6045,
        "text": "a při měsíci, když je následuje;",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6046,
        "text": "a při dnu, když je odhaluje;",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6047,
        "text": "a při noci, když je zahaluje;",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6048,
        "text": "při nebi a tom, jenž zbudoval je;",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6049,
        "text": "a při zemi a tom, jenž rozprostřel ji",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6050,
        "text": "při duši a tom, jenž urovnal ji,",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6051,
        "text": "a hříšnost i bohabojnost vnukl jí:",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6052,
        "text": "dobře povede se tomu, jenž čistou udržuje si ji;",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6053,
        "text": "a ztroskotá ten, jenž kazí si ji.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6054,
        "text": "(Kmen) Tsemúd lhářem nazval (proroka) ve svévoli své;",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6055,
        "text": "když nejbídnější vyvstal z nich;",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6056,
        "text": "tu řekl prorok boží jim: „Velbloudice boží to; nechte ji píti!“",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6057,
        "text": "Však lhářem nazvali jej a žíly podřízli jí; i zahladil je Pán jejich za jejich vinu, rovným způsobem",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6058,
        "text": "aniž se bojí (toho) následků.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      }
    ],
