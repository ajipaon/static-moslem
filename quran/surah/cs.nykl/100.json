    "ayahs": [
      {
        "number": 6147,
        "text": "Při OŘÍCH supících,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6148,
        "text": "a jiskry sršících:",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6149,
        "text": "při z rána útočících",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6150,
        "text": "a prach tím vydeptávajících,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6151,
        "text": "a střed voje tím prorážejících!",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6152,
        "text": "Zajisté člověk vůči Pánu svému jest nevděčný",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6153,
        "text": "a zajisté sám toho jest svědkem:",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6154,
        "text": "neb v pravdě statků (světa) velkým jest milovníkem.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6155,
        "text": "Což neví, že když rozrušeno bude to, co v hrobech jest;",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6156,
        "text": "a kdy zjeveno bude, co nitra skrývají,",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 542,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6157,
        "text": "že Pán jejich v den onen bude dobře o nich zpraven?",
