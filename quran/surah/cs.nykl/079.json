{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 79,
    "name": "سُورَةُ النَّازِعَاتِ",
    "englishName": "An-Naazi'aat",
    "englishNameTranslation": "Those who drag forth",
    "revelationType": "Meccan",
    "numberOfAyahs": 46,
    "ayahs": [
      {
        "number": 5713,
        "text": "Při silou VYTRHUJÍCÍCH,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5714,
        "text": "při volně uvolňujících,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5715,
        "text": "při lehce plujících,",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5716,
        "text": "při předem předcházejících,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5717,
        "text": "při osudy všeho rovnajících (přisám):",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5718,
        "text": "v den, kdy zazvučí (roh) zvučící,",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5719,
        "text": "druhé přijde po něm (troubení):",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5720,
        "text": "v den ten srdce chvěti budou se (bázní);",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5721,
        "text": "zraky jich budou sklopeny.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5722,
        "text": "Říkati budou (nevěřící): „Budeme jistě, když zakopáni jsme, nazpět vráceni?",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5723,
        "text": "Nyní, když kosti jsme (jen) zpuchřelé?“",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5724,
        "text": "Řeknou: „Toť návrat byl by záhubný.“",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5725,
        "text": "Však pouze jediné se ozve (varovné) zavřesknutí",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5726,
        "text": "a již budou na země povrchu!",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5727,
        "text": "Zdaž děj Mojžíšův tě došel?",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5728,
        "text": "Když Pán jeho v posvátném údolí Tuwá naň volal:",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5729,
        "text": "„Jdi k Faraonovi, neb svévolným se stal“",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5730,
        "text": "a rci: „Zdaž očistiti bys se chtěl,",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5731,
        "text": "tak abych vedl tě k Pánu tvému a ty se (jej) bál!“",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5732,
        "text": "Pak znamení převelké mu ukázal —",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5733,
        "text": "však neposlechl, lhářem jej nazval:",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5734,
        "text": "poté pak odvrátil se, nemeškal:",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5735,
        "text": "(lid) shromáždil a (takto) zavolal,",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5736,
        "text": "řka: „Jáť Pánem vaším jsem nejvyšším.“",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5737,
        "text": "I zachvátil jej Bůh trestem života budoucí ho i tohoto.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5738,
        "text": "V tom zajisté naučení jest pro toho, kdož bojí se.",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5739,
        "text": "Zdaž těžším bylo stvořit vás neb nebe, jež Bůh zbudoval?",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5740,
        "text": "Do výše pozdvihl je, pak je urovnal:",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5741,
        "text": "a ztemnil jeho noc, a jeho půldne vznítil;",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5742,
        "text": "poté pak zemi rozprostřel",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5743,
        "text": "a vyvříti dal vodě z ní, a pastvám vzrůsti na ní dal:",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5744,
        "text": "a hory na ní upevnil:",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5745,
        "text": "k užitku vám a dobytčatům vašim.",
        "numberInSurah": 33,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5746,
        "text": "Když pak přijde rozvrácení převelké;",
        "numberInSurah": 34,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5747,
        "text": "v den, kdy rozpomene se člověk na to, co byl (usilovně) konal;",
        "numberInSurah": 35,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5748,
        "text": "a předveden bude plam pekla všem vidícím —",
        "numberInSurah": 36,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5749,
        "text": "tehdy ten, kdo svévolný byl,",
        "numberInSurah": 37,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5750,
        "text": "a život pozemský radši měl,",
        "numberInSurah": 38,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5751,
        "text": "plam pekla bytem dostane;",
        "numberInSurah": 39,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5752,
        "text": "však ten, jenž Pána svého veleby se bál a duši svou před choutkami chránil:",
        "numberInSurah": 40,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5753,
        "text": "zahradu ráje bytem dostane.",
        "numberInSurah": 41,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5754,
        "text": "Dotazovati se budou tě na hodinu (soudu) : na kdy že určena jest?",
        "numberInSurah": 42,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5755,
        "text": "Však jakou vědomost máš o ní?",
        "numberInSurah": 43,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5756,
        "text": "Konečná lhůta její závisí na Pánu tvém;",
        "numberInSurah": 44,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5757,
        "text": "tys varovatelem jen tomu; kdož bojí se jí.",
        "numberInSurah": 45,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5758,
        "text": "V den, kdy uzří ji, (připadne jim) jako by byli v hrobě dleli jen večer neb rána dne onoho.",
        "numberInSurah": 46,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      }
    ],
