        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5057,
        "text": "ve knize skryté (napsaný),",
        "numberInSurah": 78,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
