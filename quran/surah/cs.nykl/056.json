        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5060,
        "text": "zdaž tímto hovorem opovrhnete?",
        "numberInSurah": 81,
        "juz": 27,
