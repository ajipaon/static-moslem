  "data": {
    "number": 82,
    "name": "سُورَةُ الانفِطَارِ",
    "englishName": "Al-Infitaar",
    "englishNameTranslation": "The Cleaving",
    "revelationType": "Meccan",
    "numberOfAyahs": 19,
    "ayahs": [
      {
        "number": 5830,
        "text": "جب آسمان پھٹ پڑے،",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5831,
        "text": "اور جب تارے جھڑ پڑیں،",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5832,
        "text": "اور جب سمندر بہادیے جائیں",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5833,
        "text": "اور جب قبریں کریدی جائیں",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5834,
        "text": "ہر جان، جان لے گی جو اس نے آگے بھیجا اور جو پیچھے",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5835,
        "text": "اے آدمی! تجھے کس چیز نے فریب دیا اپنے کرم والے رب سے",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5836,
        "text": "جس نے تجھے پیدا کیا پھر ٹھیک بنایا پھر ہموار فرمایا",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5837,
        "text": "جس صورت میں چاہا تجھے ترکیب دیا",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5838,
        "text": "کوئی نہیں بلکہ تم انصاف ہونے کو جھٹلانتے ہو",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5839,
        "text": "اور بیشک تم پر کچھ نگہبان ہیں",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5840,
        "text": "معزز لکھنے والے",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5841,
        "text": "جانتے ہیں جو کچھ تم کرو",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5842,
        "text": "بیشک نِکو کار ضرور چین میں ہیں",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5843,
        "text": "اور بیشک بدکار ضرور دوزخ میں ہیں،",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5844,
        "text": "انصاف کے دن اس میں جائیں گے،",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5845,
        "text": "اور اس سے کہیں چھپ نہ سکیں گے،",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5846,
        "text": "اور تو کیا جانیں کیسا انصاف کا دن،",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5847,
        "text": "پھر تو کیا جانے کیسا انصاف کا دن،",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5848,
        "text": "جس دن کوئی جان کسی جان کا کچھ اختیار نہ رکھے گی اور سارا حکم اس دن اللہ کا ہے،",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      }
    ],
