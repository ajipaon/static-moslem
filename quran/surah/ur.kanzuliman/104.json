  "data": {
    "number": 104,
    "name": "سُورَةُ الهُمَزَةِ",
    "englishName": "Al-Humaza",
    "englishNameTranslation": "The Traducer",
    "revelationType": "Meccan",
    "numberOfAyahs": 9,
    "ayahs": [
      {
        "number": 6180,
        "text": "خرابی ہے اس کے لیے جو لوگوں کے منہ پر عیب کرے پیٹھ پیچھے بدی کرے",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6181,
        "text": "جس نے مال جوڑا اور گن گن کر رکھا،",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6182,
        "text": "کیا یہ سمجھتا ہے کہ اس کا مال اسے دنیا میں ہمیشہ رکھے گا",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6183,
        "text": "ہر گز نہیں ضرور وہ روندنے والی میں پھینکا جائے گا",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6184,
        "text": "اور تو نے کیا جانا کیا روندنے والی،",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6185,
        "text": "اللہ کی آگ کہ بھڑک رہی ہے",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6186,
        "text": "وہ جو دلوں پر چڑھ جائے گی",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6187,
        "text": "بیشک وہ ان پر بند کردی جائے گی",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6188,
        "text": "لمبے لمبے ستونوں میں",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      }
    ],
