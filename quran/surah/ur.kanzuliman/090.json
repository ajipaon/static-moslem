  "data": {
    "number": 90,
    "name": "سُورَةُ البَلَدِ",
    "englishName": "Al-Balad",
    "englishNameTranslation": "The City",
    "revelationType": "Meccan",
    "numberOfAyahs": 20,
    "ayahs": [
      {
        "number": 6024,
        "text": "مجھے اس شہر کی قسم",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6025,
        "text": "کہ اے محبوب! تم اس شہر میں تشریف فرما ہو",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6026,
        "text": "اور تمہارے باپ ابراہیم کی قسم اور اس کی اولاد کی کہ تم ہو",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6027,
        "text": "بیشک ہم نے آدمی کو مشقت میں رہتا پیدا کیا",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6028,
        "text": "کیا آدمی یہ سمجھتا ہے کہ ہرگز اس پر کوئی قدرت نہیں پائے گا",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6029,
        "text": "کہتا ہے میں نے ڈھیروں مال فنا کردیا",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6030,
        "text": "کیا آدمی یہ سمجھتا ہے کہ اسے کسی نے نہ دیکھا",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6031,
        "text": "کیا ہم نے اس کی دو آنکھیں نہ بنائیں",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6032,
        "text": "اور زبان اور دو ہونٹ",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6033,
        "text": "اور اسے دو ابھری چیزوں کی راہ بتائی",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6034,
        "text": "پھر بے تامل گھاٹی میں نہ کودا",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6035,
        "text": "اور تو نے کیا جانا وہ گھاٹی کیا ہے",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6036,
        "text": "کسی بندے کی گردن چھڑانا",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6037,
        "text": "یا بھوک کے دن کھانا دینا",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6038,
        "text": "رشتہ دار یتیم کو،",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6039,
        "text": "یا خاک نشین مسکین کو",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6040,
        "text": "پھر ہو ان سے جو ایمان لائے اور انہوں نے آپس میں صبر کی وصیتیں کیں اور آپس میں مہربانی کی وصیتیں کیں",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6041,
        "text": "یہ دہنی طرف والے ہیں",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6042,
        "text": "اور جنہوں نے ہماری آیتوں سے کفر کیا وہ بائیں طرف والے",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6043,
        "text": "ان پر آگ ہے کہ اس میں ڈال کر اوپر سے بند کردی گئی",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      }
    ],
