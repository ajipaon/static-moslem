    "name": "سُورَةُ القَارِعَةِ",
    "englishName": "Al-Qaari'a",
    "englishNameTranslation": "The Calamity",
    "revelationType": "Meccan",
    "numberOfAyahs": 11,
    "ayahs": [
      {
        "number": 6158,
        "text": "大难,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6159,
        "text": "大难是什么？",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6160,
        "text": "你怎能知道大难是什么？",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6161,
        "text": "在那日, 众人将似分散的飞蛾,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6162,
        "text": "山岳将似疏松的采绒。",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6163,
        "text": "至於善功的份量较重者,",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6164,
        "text": "将在满意的生活中;",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6165,
        "text": "至於善功的份量较轻者,",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6166,
        "text": "他的归宿是深坑。",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6167,
        "text": "你怎能知道深坑里有什么？",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6168,
        "text": "有烈火。",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      }
