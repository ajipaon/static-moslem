  "status": "OK",
  "data": {
    "number": 55,
    "name": "سُورَةُ الرَّحۡمَٰن",
    "englishName": "Ar-Rahmaan",
    "englishNameTranslation": "The Beneficent",
    "revelationType": "Medinan",
    "numberOfAyahs": 78,
    "ayahs": [
      {
        "number": 4902,
        "text": "خدائے رحمٰن۔",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4903,
        "text": "نے قرآن کی تعلیم دی۔",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4904,
        "text": "اسی نے انسان کو پیدا کیا۔",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4905,
        "text": "اسی نے اسے بولنا (مافی الضمیر بیان کرنا) سکھایا۔",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4906,
        "text": "سورج اور چاند ایک (خاص) حساب کے پابند ہیں۔",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4907,
        "text": "اور سبزیاں، بیلیں اور درخت (اسی کو) سجدہ کرتے ہیں۔",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4908,
        "text": "اس نے آسمان کو بلند کیا اور میزان (عدل) رکھ دی۔",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4909,
        "text": "تاکہ تم میزان (تولنے) میں زیادتی نہ کرو۔",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4910,
        "text": "اور انصاف کے ساتھ ٹھیک طریقہ پر تولو اور وزن (تولنے) میں کمی نہ کرو۔",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4911,
        "text": "اور اس نے زمین کو تمام مخلوق کیلئے بنایا۔",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4912,
        "text": "اس میں (ہر طرح کے) میوے ہیں اور کھجور کے غلافوں والے درخت ہیں۔",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4913,
        "text": "اور بھوسہ والا اناج بھی اور خوشبودار پھول بھی۔",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4914,
        "text": "سو (اے جن و انس) تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤ گے؟",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4915,
        "text": "اس نے انسان کو ٹھیکرے کی طرح کھنکھناتی ہوئی مٹی سے پیدا کیا۔",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4916,
        "text": "اور جنات کو پیدا کیا آگ کے شعلہ سے۔",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4917,
        "text": "سو (اے جن و انس) تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤ گے؟",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4918,
        "text": "وہی دونوں مشرقوں کا پروردگار ہے اور وہی دونوں مغربوں کا مالک و پروردگار ہے۔",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4919,
        "text": "سو (اے جن و انس) تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤ گے؟",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4920,
        "text": "اسی نے دو دریا جاری کئے جو آپس میں مل رہے ہیں۔",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4921,
        "text": "ان دونوں کے درمیان ایک فاصلہ ہے جس سے وہ تجاوز نہیں کرتے۔",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4922,
        "text": "سو(اے جن و انس) تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤ گے؟",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4923,
        "text": "ان دونوں دریاؤں سے موتی اور مونگے نکلتے ہیں۔",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4924,
        "text": "سو (اے جن و انس) تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤگے؟",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4925,
        "text": "اور اسی کے لئے وہ (جہاز) ہیں جو سمندروں میں پہاڑوں کی طرح اٹھے ہوئے ہیں۔",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4926,
        "text": "سو (اے جن و انس) تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤ گے؟",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4927,
        "text": "جو بھی روئے زمین پر ہیں وہ سب فنا ہونے والے ہیں۔",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4928,
        "text": "اور آپ(ص) کے پروردگار کی ذات باقی رہے گی جو عظمت و اکرام والی ہے۔",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4929,
        "text": "تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤ گے؟",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4930,
        "text": "آسمانوں اور زمین میں جو بھی ہیں اسی سے (اپنی حاجتیں) مانگتے ہیں وہ ہر روز (بلکہ ہر آن) ایک نئی شان میں ہے۔",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4931,
        "text": "پس تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤ گے؟",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4932,
        "text": "اے جن وانس ہم عنقریب (تمہاری خبر لینے کیلئے) تمہاری طرف متوجہ ہو نے والے ہیں۔",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4933,
        "text": "تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤ گے؟",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4934,
        "text": "اے گروہِ جن و انس! اگر تم سے ہو سکے تو تم آسمانوں اور زمین کی حدود سے باہر نکل جاؤ (لیکن) تم طاقت اور زور کے بغیر نہیں نکل سکتے۔",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4935,
        "text": "تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤ گے؟",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4936,
        "text": "تم دونوں پر آگ کاشعلہ اور دھواں چھوڑا جائے گا پھر تم اپنا بچاؤ نہ کر سکو گے۔",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4937,
        "text": "تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤ گے؟",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4938,
        "text": "(اس وقت کیا حالت ہوگی) جب آسمان پھٹ جائے گا اور (رنگے ہوئے) سرخ چمڑے کی طرح لال ہو جائے گا۔",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4939,
        "text": "تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤ گے؟",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4940,
        "text": "پس اس دن کسی انسان یا جن سے اس کے گناہ کے متعلق سوال نہیں کیا جائے گا۔",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4941,
        "text": "تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤ گے؟",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4942,
        "text": "مجرم اپنے چہروں (اپنی علامتوں) سے پہچان لئے جائیں گے اور پھر پیشانیوں اور پاؤں سے پکڑے جائیں گے (اور جہنم میں پھینک دئیے جائیں گے)۔",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4943,
        "text": "سو تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤ گے؟",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4944,
        "text": "یہی وہ دوزخ ہے جسے مجرم لوگ جھٹلایا کرتے تھے۔",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4945,
        "text": "وہ (مجرم) اس (دوزخ) اور اس کے انتہائی کھولتے ہوئے پانی کے درمیان گردش کرتے رہیں گے۔",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4946,
        "text": "پس اے جن و انس! سو تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤ گے؟",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4947,
        "text": "اور جو شخص اپنے پروردگار کی بارگاہ میں حاضری سے ڈرتا ہے اس کے لئے دو باغ ہیں۔",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4948,
        "text": "پس تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤ گے؟ ۔",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4949,
        "text": "دونوں باغ بہت سی شاخوں والے۔",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4950,
        "text": "پس تم اپنے پروردگار کی کن کن نعمتوں جھٹلاؤگے؟",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4951,
        "text": "ان دونوں باغوں میں دو چشمے جاری ہوں گے۔",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4952,
        "text": "پس تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤ گے؟",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4953,
        "text": "ان دونوں باغوں میں ہر پھل کی دو قِسمیں ہوں گی۔",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4954,
        "text": "پس تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤگے؟",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4955,
        "text": "وہ ایسے بچھونوں پر تکیہ لگائے بیٹھے ہوں گے جن کے استر دبیز ریشم کے ہوں گے۔",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4956,
        "text": "پس تم اپنے پروردگار کی کن کن نعمتوں کو جھٹلاؤ گے؟",
