  "data": {
    "number": 84,
    "name": "سُورَةُ الانشِقَاقِ",
    "englishName": "Al-Inshiqaaq",
    "englishNameTranslation": "The Splitting Open",
    "revelationType": "Meccan",
    "numberOfAyahs": 25,
    "ayahs": [
      {
        "number": 5885,
        "text": "اور جب آسمان پھٹ جائے گا۔",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5886,
        "text": "اور وہ اپنے پروردگار کا حکم سن لے گا (اور اس کی تعمیل کرے گا) اور اس پر لازم بھی یہی ہے۔",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5887,
        "text": "اور جب زمین پھیلا دی جائے گی۔",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5888,
        "text": "اور جو کچھ اس کے اندر ہے وہ اسے باہر پھینک دے گی اور خالی ہو جائے گی۔",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5889,
        "text": "اور اپنے پروردگار کا حکم سنے گی اور (اس کی تعمیل کرے گی) اور اس پر لازم بھی یہی ہے۔",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5890,
        "text": "اے انسان! تو کشاں کشاں اپنے پروردگار کی طرف (کھنچا چلا) جا رہا ہے اور اس کی بارگاہ میں حاضر ہو نے والا ہے۔",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5891,
        "text": "پس جس کا نامۂ اعمال اس کے دائیں ہاتھ میں دیا جائے گا۔",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5892,
        "text": "تو اس سے آسان حساب لیا جائے گا۔",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5893,
        "text": "اور وہ اپنے لوگوں کی طرف خوش و خرم لوٹے گا۔",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5894,
        "text": "اور جس کا نامۂ اعمال اس کے پس پشت دیا جائے گا۔",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5895,
        "text": "تو وہ موت (اور تباہی) کو پکارے گا۔",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5896,
        "text": "اور (دوزخ کی) بھڑکتی ہوئی آگ میں داخل ہوگا۔",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5897,
        "text": "یہ شخص (دنیا میں) اپنے لوگوں میں خوش خوش رہتا تھا۔",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5898,
        "text": "اس کا خیال تھا کہ وہ کبھی (اپنے خدا کے پاس) لوٹ کر نہیں جائے گا۔",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5899,
        "text": "کیوں نہیں! بےشک اس کا پروردگار اسے خوب دیکھ رہا تھا۔",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5900,
        "text": "پس نہیں! میں قَسم کھاتا ہوں شفق کی۔",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5901,
        "text": "اور رات کی اور ان چیزوں کی (قَسم کھاتا ہوں) جن کو وہ (رات) سمیٹ لیتی ہے۔",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5902,
        "text": "اور چاند کی (قَسم کھاتا ہوں) جب وہ پورا ہو جائے۔",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5903,
        "text": "تمہیں یونہی (تدریجاً) زینہ بہ زینہ چڑھنا ہے (اور ایک ایک منزل طے کرنی ہے)۔",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5904,
        "text": "تو انہیں کیا ہوگیا ہے کہ ایمان نہیں لاتے؟",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5905,
        "text": "اور جب ان کے سامنے قرآن پڑھا جاتا ہے تویہ سجدہ نہیں کرتے؟",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": {
          "id": 14,
          "recommended": true,
          "obligatory": false
        }
      },
      {
        "number": 5906,
        "text": "بلکہ کافر لوگ تو الٹا (اسے) جھٹلاتے ہیں۔",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5907,
        "text": "اور اللہ بہتر جانتا ہے جو کچھ وہ (اپنے دلوں میں) جمع کر رہے ہیں۔",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5908,
        "text": "آپ(ص) انہیں دردناک عذاب کی خبر دے دیجئے۔",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5909,
        "text": "ہاں البتہ جو لوگ ایمان لائے اور نیک اعمال کئے ان کیلئے کبھی ختم نہ ہو نے والا اجر و ثواب ہے۔",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      }
    ],
