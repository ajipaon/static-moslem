  "data": {
    "number": 102,
    "name": "سُورَةُ التَّكَاثُرِ",
    "englishName": "At-Takaathur",
    "englishNameTranslation": "Competition",
    "revelationType": "Meccan",
    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6169,
        "text": "تمہیں کثرت (مال و اولاد) کی مسابقت نے غفلت میں رکھا۔",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6170,
        "text": "یہاں تک کہ تم قبروں میں جا پہنچے۔",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6171,
        "text": "ہرگز نہیں! عنقریب تمہیں معلوم ہو جائے گا۔",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6172,
        "text": "پھر ہرگز نہیں! عنقریب تمہیں معلوم ہو جائے گا۔",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6173,
        "text": "ہرگز نہیں! اگر تم یقینی طور پر (اس روش کے انجام کو) جانتے (تو ہرگز ایسا نہ کرتے)۔",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6174,
        "text": "تم ضرور دوزخ کو دیکھو گے۔",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6175,
        "text": "پھر تم لوگ (آخرت میں) ضرور یقین کی آنکھ سے اسے (یقینی دیکھنا) دیکھو گے۔",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6176,
        "text": "پھر اس دن تم سے نعمتوں کے بارے میں ضرور بازپُرس کی جائے گی۔",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      }
