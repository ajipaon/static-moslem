    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6091,
        "text": "(اے رسول(ص)) کیا ہم نے آپ(ص) کے سینے کو کشادہ نہیں کر دیا؟",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6092,
        "text": "اور وہ بَوجھ آپ سے اتار دیا۔",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6093,
        "text": "جو آپ(ص) کی کمر کو توڑ رہا تھا۔",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6094,
        "text": "اور آپ(ص) کے ذکر (آوازہ) کو بلند کیا۔",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6095,
        "text": "یقیناً ہر دشواری کے ساتھ آسانی ہے۔",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6096,
        "text": "بےشک ہر تنگی کے ساتھ فراخی ہے۔",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6097,
        "text": "جب (کارِ رسا لت سے) فارغ ہوں تو (عبادت و ریا ضت کرنے میں) محنت کیجئے۔",
        "numberInSurah": 7,
        "juz": 30,
