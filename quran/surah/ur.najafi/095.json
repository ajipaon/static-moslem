        "sajda": false
      },
      {
        "number": 6101,
        "text": "اور پُرامن شہر (مکہ) کی۔",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6102,
        "text": "ہم نے انسان کو بہترین ساخت و انداز کے ساتھ پیدا کیا ہے۔",
        "numberInSurah": 4,
        "juz": 30,
