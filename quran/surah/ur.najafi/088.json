  "data": {
    "number": 88,
    "name": "سُورَةُ الغَاشِيَةِ",
    "englishName": "Al-Ghaashiya",
    "englishNameTranslation": "The Overwhelming",
    "revelationType": "Meccan",
    "numberOfAyahs": 26,
    "ayahs": [
      {
        "number": 5968,
        "text": "کیا تمہیں (کائنات پر) چھا جانے والی (مصیبت یعنی قیامت) کی خبر پہنچی ہے؟",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5969,
        "text": "اس دن کچھ چہرے ذلیل (اترے ہوئے) ہوں گے۔",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5970,
        "text": "سخت محنت کرنے والے (نڈھال اور) تھکے ماندے ہوں گے۔",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5971,
        "text": "وہ دہکتی ہوئی آگ میں پڑیں گے (اور جھلسیں گے)۔",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5972,
        "text": "انہیں کھولتے ہوئے چشمہ کا پانی پلایا جائے گا۔",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5973,
        "text": "(وہاں) ان کیلئے کوئی کھانا نہ ہوگا سوائے خاردار جھاڑ کے۔",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5974,
        "text": "جو نہ (انہیں) موٹا کرے گا اور نہ بھوک دور کرے گا۔",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5975,
        "text": "(اور) کچھ چہرے اس دن تر و تازہ (اور با رونق) ہوں گے۔",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5976,
        "text": "اپنی کاوش و کوشش پر خوش اور مطمئن ہوں گے۔",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5977,
        "text": "(اور) عالیشان جنت میں ہوں گے۔",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5978,
        "text": "جہاں وہ کوئی بےہودہ بات نہیں سنیں گے۔",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5979,
        "text": "اس میں چشمہ رواں دواں ہوگا۔",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5980,
        "text": "اس میں اونچے اونچے تخت (بچھے ہوئے) ہوں گے۔",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5981,
        "text": "اور جام و ساغر رکھے ہوں گے۔",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5982,
        "text": "اور گاؤ تکیے قطار در قطار لگے ہوئے ہوں گے۔",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5983,
        "text": "اور طرح طرح کے نفیس فرش و فروش بچھے ہوئے ہوں گے۔",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5984,
        "text": "کیا یہ لوگ اونٹ کو (غور سے) نہیں دیکھتے کہ وہ کیونکر پیدا کیا گیا ہے؟",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5985,
        "text": "اور آسمان کی طرف نہیں دیکھتے کہ کیسے بلند کیا گیا ہے۔",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5986,
        "text": "اور پہاڑوں کی طرف نہیں دیکھتے کہ کیونکر کھڑے کئے گئے ہیں؟",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5987,
        "text": "اور زمین کی طرف نہیں دیکھتے کہ کیسے بچھائی گئی ہے۔",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5988,
        "text": "پس(اے رسول(ص)) آپ(ص) نصیحت کیجئے (اور سمجھائیے) کہ آپ(ص) نصیحت کرنے (اور سمجھانے) والے ہیں۔",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5989,
        "text": "آپ(ص) ان پر داروغہ نہیں ہیں۔",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5990,
        "text": "ہاں البتہ جو شخص رُوگردانی کرے گا اور کفر کرے گا۔",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5991,
        "text": "تو اللہ اس کو بڑا عذاب دے گا۔",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5992,
        "text": "یقیناً ان کی بازگشت ہماری طرف ہے۔",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5993,
        "text": "پھر بیشک ان کا حساب کتاب ہمارے ذمہ ہے۔",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      }
    ],
