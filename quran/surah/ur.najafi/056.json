{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 56,
    "name": "سُورَةُ الوَاقِعَةِ",
    "englishName": "Al-Waaqia",
    "englishNameTranslation": "The Inevitable",
    "revelationType": "Meccan",
    "numberOfAyahs": 96,
    "ayahs": [
      {
        "number": 4980,
        "text": "جب واقعہ ہو نے والی (قیامت) واقع ہو جائے گی۔",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4981,
        "text": "جس کے واقع ہو نے میں کوئی جھوٹ نہیں ہے۔",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4982,
        "text": "وہ (کسی کو) پست کرنے والی اور (کسی کو) بلند کرنے والی ہوگی۔",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4983,
        "text": "جب زمین بالکل ہلا ڈالی جائے گی۔",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4984,
        "text": "اور پہاڑ ٹوٹ کر ریزہ ریزہ ہو جائیں گے۔",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4985,
        "text": "(اور) پراگندہ غبار کی طرح ہو جائیں گے۔",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4986,
        "text": "اورتم لوگ تین قسم کے ہو جاؤ گے۔",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4987,
        "text": "پس (ایک قِسم) دائیں ہاتھ والوں کی ہوگی وہ دائیں ہاتھ والے کیا اچھے ہیں؟",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4988,
        "text": "اور(دوسری قِسم) بائیں ہاتھ والوں کی ہوگی اور بائیں ہاتھ والے کیا برے ہیں؟",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4989,
        "text": "اور (تیسری قِسم) سبقت کرنے والوں کی ہوگی وہ تو سبقت کرنے والے ہی ہیں۔",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4990,
        "text": "وہی لوگ خاص مقرب (بارگاہ) ہیں۔",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4991,
        "text": "(یہ لوگ) عیش و آرام کے باغوں میں ہوں گے۔",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4992,
        "text": "ان کی بڑی جماعت اگلوں میں سے ہوگی۔",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4993,
        "text": "اور بہت تھوڑے پچھلوں میں سے ہوں گے۔",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4994,
        "text": "مرصع تختوں پر تکیہ لگائے۔",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4995,
        "text": "آمنے سامنے بیٹھے ہوں گے۔",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4996,
        "text": "ان کے پاس ایسے غلمان (نوخیز لڑکے) ہوں گے جو ہمیشہ غلمان ہی رہیں گے۔",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4997,
        "text": "(شرابِ طہور سے بھرے ہو ئے) آبخورے، آفتابے اور پاک و صاف شراب کے پیالے (یعنی جام، صراحی سبو و ساغر) لئے گردش کر رہے ہوں گے۔",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4998,
        "text": "جس سے نہ دردِ سر ہوگا اور نہ عقل میں فتور آئے گا۔",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4999,
        "text": "اور وہ (غلمان) طرح طرح کے پھل پیش کریں گے وہ جسے چاہیں چن لیں۔",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5000,
        "text": "اور پرندوں پرندوں کے گوشت بھی جس کی وہ خواہش کریں گے۔",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5001,
        "text": "(اور ان کیلئے) گوری رنگت والی غزال چشم حوریں بھی ہوں گی۔",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5002,
        "text": "جو چھپا کر رکھے ہوئے موتیوں کی طرح (حسین) ہوں گی۔",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5003,
        "text": "یہ سب کچھ ان کے ان اعمال کی جزا ہے جو وہ کیا کرتے تھے۔",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5004,
        "text": "وہ اس میں کوئی فضول اور گناہ والی بات نہیں سنیں گے۔",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5005,
        "text": "مگر صرف سلام و دعا کی آواز آئے گی۔",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5006,
        "text": "اور وہ دائیں ہاتھ والے اور وہ دائیں والے کیا اچھے ہیں۔",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5007,
        "text": "وہ ایسی بیری کے درختوں میں ہوں گے جن میں کانٹے نہیں ہوں گے۔",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5008,
        "text": "اورتہہ بہ تہہ کیلوں میں۔",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5009,
        "text": "اور (دور تک) پھیلے ہوئے سایوں میں۔",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5010,
        "text": "اور بہتے ہوئے پانی میں۔",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5011,
        "text": "اور بہت سے پھلوں میں۔",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5012,
        "text": "جو نہ کبھی ختم ہوں گے اور نہ کوئی روک ٹوک ہوگی۔",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5013,
        "text": "اور اونچے بچھونوں پر ہوں گے۔",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5014,
        "text": "ہم نے (حوروں) کو خاص نئے سرے سے پیدا کیا ہے۔",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5015,
        "text": "پھر ان کو کنواریاں بنایا ہے۔",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5016,
        "text": "(اور) پیار کرنے والیاں اور ہم عمر ہیں۔",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5017,
        "text": "یہ سب نعمتیں دائیں ہاتھ والوں کے لئے ہیں۔",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5018,
        "text": "ایک بڑی جماعت اگلوں میں سے۔",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5019,
        "text": "اور ایک بڑی جماعت پچھلوں میں سے ہے۔",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5020,
        "text": "اور بائیں ہاتھ والے اور کیا برے ہیں بائیں ہاتھ والے۔",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5021,
        "text": "وہ لو کی لپٹ (سخت تپش) اور کھولتے ہوئے پانی۔",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5022,
        "text": "اور سیاہ دھوئیں کے سایہ میں ہوں گے۔",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5023,
        "text": "جو نہ ٹھنڈا ہوگااور نہ نفع بخش۔",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5024,
        "text": "(کیونکہ) وہ اس سے پہلے (دنیا میں) خوشحال (اور عیش و عشرت میں) تھے۔",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5025,
        "text": "اور وہ بڑے گناہ پر اصرار کرتے تھے۔",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5026,
        "text": "اور کہتے تھے کہ جب ہم مر جائیں گے اور مٹی اور ہڈیاں ہو جائیں گے تو کیا ہم دوبارہ اٹھائے جائیں گے؟",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5027,
        "text": "اور کیا ہمارے پہلے باپ دادا بھی (اٹھائے جائیں گے؟)",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5028,
        "text": "آپ(ص) کہہ دیجئے! کہ بےشک اگلے اور پچھلے۔",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5029,
        "text": "سب ایک دن کے مقرروقت پر اکٹھے کئے جائیں گے۔",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5030,
        "text": "پھر تم اے گمراہو اور جھٹلانے والو۔",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5031,
        "text": "تم (تلخ ترین درخت) زقوم سے کھاؤ گے۔",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5032,
        "text": "اور اسی سے (اپنے) پیٹ بھرو گے۔",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5033,
        "text": "اور اوپر سے کھولتا ہوا پانی پیو گے۔",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5034,
        "text": "جس طرح سخت پیاسے اونٹ (بےتحاشا) پانی پیتے ہیں۔",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5035,
        "text": "جزا و سزا کے دن یہ ان کی مہمانی ہوگی۔",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5036,
        "text": "ہم نے ہی تمہیں پیدا کیا ہے پھر تم (قیامت کی) تصدیق کیوں نہیں کرتے؟",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5037,
        "text": "کیا تم نے کبھی غور کیا ہے کہ جو نطفہ تم ٹپکاتے ہو۔",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5038,
        "text": "کیا تم اسے(آدمی بنا کر) پیدا کرتے ہو یا ہم پیدا کرنے و الے ہیں۔",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5039,
        "text": "ہم نے ہی تمہارے درمیان موت (کا نظام) مقرر کیا ہے اور ہم اس سے عاجز نہیں ہیں۔",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5040,
        "text": "کہ ہم تمہاری جگہ تم جیسے اور لوگ پیدا کر دیں اور تم کو ایسی صورت میں (یا ایسے عالَم میں) پیدا کر دیں جس کو تم نہیں جانتے۔",
        "numberInSurah": 61,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5041,
        "text": "اور تم (اپنی) پہلی پیدائش کو تو جانتے ہی ہو پھر نصیحت کیوں نہیں حاصل کرتے؟",
        "numberInSurah": 62,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5042,
        "text": "کیا تم نے کبھی غور کیا ہے کہ تم جو کچھ (بیج) بوتے ہو۔",
        "numberInSurah": 63,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5043,
        "text": "کیا تم اس کو اگاتے ہو یا ہم اگانے والے ہیں۔",
        "numberInSurah": 64,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5044,
        "text": "اگر ہم چاہیں تو اس (پیداوار) کو (خشک کر کے) چُورا چُورا کر دیں تو تم باتیں بناتے رہ جاؤ۔",
        "numberInSurah": 65,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5045,
        "text": "کہ ہم پر تاوان پڑگیا۔",
        "numberInSurah": 66,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5046,
        "text": "بلکہ ہم بالکل محروم ہو گئے۔",
        "numberInSurah": 67,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5047,
        "text": "کیا تم نے کبھی غور کیا ہے کہ وہ پانی جو تم پیتے ہو۔",
        "numberInSurah": 68,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5048,
        "text": "کیا تم نے اسے بادل سے اتارا ہے یا ہم (اس کے) اتارنے والے ہیں؟",
        "numberInSurah": 69,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5049,
        "text": "اگر ہم چاہتے تو اسے سخت بھاری بنا دیتے پھر تم شکر کیوں نہیں کرتے؟",
        "numberInSurah": 70,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5050,
        "text": "کیا تم نے کبھی غور کیا ہے کہ وہ آگ جو تم سلگاتے ہو۔",
        "numberInSurah": 71,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5051,
        "text": "آیا تم نے اس کا درخت پیدا کیا ہے یا ہم پیدا کرنے والے ہیں۔",
        "numberInSurah": 72,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5052,
        "text": "ہم نے ہی اسے یاددہانی کا ذریعہ اور مسافروں کیلئے فائدہ کی چیز بنایا ہے۔",
        "numberInSurah": 73,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5053,
        "text": "پس تم اپنے عظیم پروردگار کے نام کی تسبیح کرو۔",
        "numberInSurah": 74,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5054,
        "text": "پس میں ستاروں کے (ڈو بنے کے) مقامات کی قَسم کھاتا ہوں۔",
        "numberInSurah": 75,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5055,
        "text": "اور اگر تم سمجھو تو یہ بہت بڑی قَسم ہے۔",
        "numberInSurah": 76,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5056,
        "text": "بےشک یہ قرآن بڑی عزت والا ہے۔",
        "numberInSurah": 77,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5057,
        "text": "نگاہوں سے پوشیدہ ایک کتاب کے اندر ہے۔",
        "numberInSurah": 78,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5058,
        "text": "اسے پاک لوگوں کے سوا کوئی نہیں چھو سکتا۔",
        "numberInSurah": 79,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5059,
        "text": "یہ تمام جہانوں کے پروردگار کی طرف سے نازل کردہ ہے۔",
        "numberInSurah": 80,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5060,
        "text": "کیا تم اس کتاب سے بےاعتنائی کرتے ہو؟",
        "numberInSurah": 81,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5061,
        "text": "اور تم نے اپنے حصہ کی روزی یہی قرار دی ہے کہ اسے جھٹلاتے ہو۔",
        "numberInSurah": 82,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5062,
        "text": "(اگر تم کسی کے محکوم نہیں) تو جب (مرنے والے کی) روح حلق تک پہنچ جاتی ہے۔",
        "numberInSurah": 83,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5063,
        "text": "اور تم اس وقت دیکھ رہے ہوتے ہو۔",
        "numberInSurah": 84,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5064,
        "text": "اور اس وقت ہم تم سے زیادہ مرنے والے کے قریب ہوتے ہیں مگر تم دیکھتے نہیں۔",
        "numberInSurah": 85,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5065,
        "text": "پس اگر تمہیں کوئی جزا و سزا ملنے والی نہیں ہے۔",
        "numberInSurah": 86,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5066,
        "text": "تو پھر اس (روح) کو کیوں واپس لوٹا نہیں لیتے اگر تم (اس انکار میں) سچے ہو۔",
        "numberInSurah": 87,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5067,
        "text": "پس اگر وہ (مرنے والا) مقربین میں سے ہے۔",
        "numberInSurah": 88,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5068,
        "text": "تو اس کے لئے راحت، خوشبودار عذاب اور نعمت بھری جنت ہے۔",
        "numberInSurah": 89,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5069,
        "text": "اور اگر وہ اصحاب الیمین میں سے ہے (تواس سے کہا جاتا ہے)۔",
        "numberInSurah": 90,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5070,
        "text": "تمہارے لئے سلامتی ہو تو اصحاب الیمین میں سے ہے۔",
        "numberInSurah": 91,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5071,
        "text": "اور اگر وہ جھٹلانے والے گمراہوں میں سے ہے۔",
        "numberInSurah": 92,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5072,
        "text": "تو پھر اس کی مہمانی کھولتے ہوئے پانی سے ہوگی۔",
        "numberInSurah": 93,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5073,
        "text": "اور دوزخ کی تپش (اور) اس میں داخلہ ہے۔",
        "numberInSurah": 94,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5074,
        "text": "(جو کچھ بیان ہوا) بےشک یہ حق الیقین (قطعی حق) ہے۔",
        "numberInSurah": 95,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5075,
        "text": "پس اپنے عظیم پروردگار کے نام کی تسبیح کرو۔",
        "numberInSurah": 96,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      }
