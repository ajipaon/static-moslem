    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6091,
        "text": "آیا سینه‌ات را برایت نگشادیم‌؟",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 596,
