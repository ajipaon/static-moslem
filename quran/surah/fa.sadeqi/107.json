    "ayahs": [
      {
        "number": 6198,
        "text": "آیا کسی را دیدی که با دین (همین را) تکذیب می‌کند؟",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6199,
        "text": "پس این همان کس است که یتیم را به سختی می‌راند.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6200,
        "text": "و بر خوراک بینوا ترغیب نمی‌کند.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6201,
        "text": "پس وای بر نمازگزاران!",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6202,
        "text": "کسانی که از (انجام به وقت و درست) نمازشان سهل‌انگارند.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6203,
        "text": "کسانی که ایشان ریا می‌کنند.",
