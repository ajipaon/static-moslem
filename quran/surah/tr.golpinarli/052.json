{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 52,
    "name": "سُورَةُ الطُّورِ",
    "englishName": "At-Tur",
    "englishNameTranslation": "The Mount",
    "revelationType": "Meccan",
    "numberOfAyahs": 49,
    "ayahs": [
      {
        "number": 4736,
        "text": "Andolsun Tur'a.",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4737,
        "text": "Ve yazılmış kitaba.",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4738,
        "text": "Yayılmış kağıtta.",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4739,
        "text": "Ve mamur eve.",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4740,
        "text": "Ve yüceltilmiş tavana.",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4741,
        "text": "Ve taşkın, coşkun, dalgalanıp duran denize.",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4742,
        "text": "Şüphe yok ki Rabbinin azabı, yerine gelip olacak.",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4743,
        "text": "Onu bir defedip gideren bulunmayacak.",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4744,
        "text": "O gün gök, bir çalkantıya düşüp döner.",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4745,
        "text": "Ve dağlar, yerlerinden oynayıp yürür.",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4746,
        "text": "Artık yazıklar olsun o gün yalanlayanlara.",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4747,
        "text": "Öyle kişilerdir onlar ki daldıkları batakta oynayıp dururlar.",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4748,
        "text": "O gün itilip kakılarak cehenneme atılırlar.",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4749,
        "text": "İşte budur yalanladığınız ateş.",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4750,
        "text": "Bir büyü mü bu, yoksa görmüyor musunuz?",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4751,
        "text": "Girin ona da artık sabredin, yahut etmeyin, birdir size; ancak yaptığınızın karşılığı olarak cezalanacaksınız.",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4752,
        "text": "Şüphe yok ki çekinenler, cennetlerdedir ve nimetler içinde.",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4753,
        "text": "Nimetlenirler orada Rablerinin verdiği nimetlerle ve Rableri korur onları koca cehennemin azabından.",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4754,
        "text": "Yiyin ve için, afiyetler olsun, yaptığınız şeylere karşılık.",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4755,
        "text": "Safsaf dizilmiş tahtlara dayanarak ve onları, iri gözlü hurilerle evlendiririz.",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4756,
        "text": "Ve inananlarla soylarından, inanarak onlara uyanları, soylarından gelenlerle birleştirir, buluştururuz ve yaptıklarının mükafatından hiçbir şeyi eksiltmeyiz; herkes, kazancına bağlıdır.",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4757,
        "text": "Ve onlara meyve ve gönüllerinin tam istediği et vereceğiz.",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4758,
        "text": "Ve birbirlerine öyle bir kadeh sunarlar ki içtikleri şaRabın sonucunda ne boş şeylerden bahsediş var, ne günaha giriş.",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4759,
        "text": "Ve öylesine genç hizmetçiler, etraflarında dönerdurur ki sanki onlar, haznelerde saklanmış inciler.",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4760,
        "text": "Ve birbirlerine dönüp sorarlar, konuşurlar.",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4761,
        "text": "Derler ki: Gerçekten de daha önce ehlimizin içinde, ilimizde, yurdumuzda, korku içindeydik biz.",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4762,
        "text": "Derken Allah lutfetti bize ve korudu bizi ta iliklere işleyen sam yelinin azabından.",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4763,
        "text": "Gerçekten de önceden onu çağırırdık; şüphe yok ki o, şanı yüce bir lütuf sahibidir, rahimdir.",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4764,
        "text": "Artık öğüt ver, gerçekten de Rabbinin nimeti sayesinde sen, ne kahinsin, ne deli.",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4765,
        "text": "Yoksa onlar, bir şair ki ölmesini, zamanın kötülüklerine uğramasını gözetiyoruz mu diyorlar?",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4766,
        "text": "De ki: Gözetin bakalım, gerçekten ben de sizinle beraber gözetmedeyim.",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4767,
        "text": "Yoksa bu sözleri akılları mı emrediyor onlara, yoksa azgın bir topluluk mu onlar?",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4768,
        "text": "Yoksa onu kendisi uyduruyor mu diyorlar? Hayır, inanmamışlardır onlar.",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4769,
        "text": "Artık buna benzer bir söz getirin meydana sözünüz doğruysa.",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4770,
        "text": "Yoksa boşuboşuna mı yaratıldı onlar, yoksa onlar mı yaratıcılar?",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4771,
        "text": "Yoksa gökleri ve yeryüzünü mü yarattı onlar? Hayır, iyideniyiye inanmamışlardır onlar.",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4772,
        "text": "Yoksa onların yanında mı Rabbinin hazneleri, yoksa onlar sorumsuz bir saltanata mı sahip?",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4773,
        "text": "Yoksa merdivenleri var da gökten mi duyuyorlar? Öyleyse duyanları, apaçık bir delil göstersin.",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4774,
        "text": "Yoksa kızlar onların da erkek evlatları sizin mi?",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4775,
        "text": "Yoksa onlardan ücret istiyorsun da bu yüzden ağır bir borca mı giriyorlar?",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4776,
        "text": "Yoksa gizli şey, yanlarında da yazıyorlar mı?",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4777,
        "text": "Yoksa bir düzen mi kurmak istiyorlar? Asıl düzene uğrayıp cezalanacaklar, kafir olanlar.",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4778,
        "text": "Yoksa Allah'tan başka bir mabutları mı var? Şanı yücedir, münezzehtir Allah, şirk koşanların şirk koştukları şeylerden.",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4779,
        "text": "Gökten bir parçanın düştüğünü görseler, birbiri üstüne yığılmış bulut derler.",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4780,
        "text": "Artık bırak onları helak olacakları güne dek.",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4781,
        "text": "Bir gündür o gün ki düzenleri, onlardan hiçbir şeyi gideremez ve onlara yardım da edilmez.",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4782,
        "text": "Ve şüphe yok ki zulmedenlere, bundan başka azap da var ve fakat çoğu bilmez.",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4783,
        "text": "Ve sabret Rabbinin hükmüne, gerçekten de gözümüzün altındasın sen ve Rabbine hamdederek tenzih et onu kalkınca.",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4784,
        "text": "Ve geceleyin de onu tenzih et ve yıldızların batacağı sırada da.",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "tr.golpinarli",
      "language": "tr",
      "name": "Abdulbakî Gölpınarlı",
      "englishName": "Abdulbaki Golpinarli",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}