{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 44,
    "name": "سُورَةُ الدُّخَانِ",
    "englishName": "Ad-Dukhaan",
    "englishNameTranslation": "The Smoke",
    "revelationType": "Meccan",
    "numberOfAyahs": 59,
    "ayahs": [
      {
        "number": 4415,
        "text": "Ha mim.",
        "numberInSurah": 1,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4416,
        "text": "Andolsun her şeyi açıklayan Kur'an'a.",
        "numberInSurah": 2,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4417,
        "text": "Şüphe yok ki biz onu, kutlu bir gecede indirdik, şüphe yok ki biz, insanları korkuturuz.",
        "numberInSurah": 3,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4418,
        "text": "O gecede ayrılır, takdir edilir her hükmolunan iş.",
        "numberInSurah": 4,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4419,
        "text": "Bir iş ki katımızdan hükmolunur, şüphe yok ki biz göndermişizdir.",
        "numberInSurah": 5,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4420,
        "text": "Rahmet olarak Rabbinden; şüphe yok ki o, duyar, bilir.",
        "numberInSurah": 6,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4421,
        "text": "Rabbidir göklerin ve yeryüzünün ve ikisinin arasındakilerin. Adamakıllı inanır, iyice bilirseniz.",
        "numberInSurah": 7,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4422,
        "text": "Yoktur ondan başka tapacak, diriltir ve öldürür; Rabbinizdir ve Rabbidir gelip geçen atalarınızın.",
        "numberInSurah": 8,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4423,
        "text": "Hayır, onlar şüphe içindedir, alay edip dururlar.",
        "numberInSurah": 9,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4424,
        "text": "Artık gözetle gökyüzünden apaçık, gözle görünür bir dumanın geleceği günü.",
        "numberInSurah": 10,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4425,
        "text": "Bütün insanlara yayılır, budur elemli azap.",
        "numberInSurah": 11,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4426,
        "text": "Rabbimiz, bizden azabı, gider, şüphe yok ki inandık biz.",
        "numberInSurah": 12,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4427,
        "text": "Siz neredesiniz, öğüt alma nerede ve andolsun ki onlara, her şeyi açıklayan bir Peygamber geldi de.",
        "numberInSurah": 13,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4428,
        "text": "Sonra yüz çevirdiler ondan ve kendisine birşeyler öğretilmiş delinin biri dediler.",
        "numberInSurah": 14,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4429,
        "text": "Şüphe yok ki birazcık gidereceğiz azabı, fakat gene şüphe yok ki kafirliğe döneceksiniz.",
        "numberInSurah": 15,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4430,
        "text": "O gün pek şiddetli bir surette tutar, cezalandırırız, şüphe yok ki öc alırız biz.",
        "numberInSurah": 16,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4431,
        "text": "Ve andolsun ki onlardan önce Firavun'un kavmini de sınamıştık ve onlara güzel huylu bir peygamber gelmişti de.",
        "numberInSurah": 17,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4432,
        "text": "Allah'ın kullarını demişti, bana teslim edin, şüphe yok ki ben, emin bir peygamberim size.",
        "numberInSurah": 18,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4433,
        "text": "Ve Allah'a karşı yücelik satmaya kalkışmayın; şüphe yok ki ben size, apaçık bir delil getirdim.",
        "numberInSurah": 19,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4434,
        "text": "Ve şüphe yok ki ben Rabbime ve Rabbinize sığınırım beni taşlayıp öldürmenizden.",
        "numberInSurah": 20,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4435,
        "text": "Bana inanmıyorsanız bırakın tek başıma beni.",
        "numberInSurah": 21,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4436,
        "text": "Derken Rabbine dua edip şüphe yok ki bunlar demişti, mücrim bir topluluk.",
        "numberInSurah": 22,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4437,
        "text": "Artık kullarımla geceleyin yola düş, şüphe yok ki ardınızdan geleceklerdir.",
        "numberInSurah": 23,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4438,
        "text": "Deniz açılmışken öylece bırak, şüphe yok, onlar bir ordudur ki boğulacak.",
        "numberInSurah": 24,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4439,
        "text": "Nice bahçeler terkettiler ve nice akarsular.",
        "numberInSurah": 25,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4440,
        "text": "Ve tarlalar ve güzelim meclisler.",
        "numberInSurah": 26,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4441,
        "text": "Ve bolbol yeyip geçindikleri nice nimetler.",
        "numberInSurah": 27,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4442,
        "text": "Böyle işte ve onları miras verdik bir başka topluluğa.",
        "numberInSurah": 28,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4443,
        "text": "Derken ne gök ağladı onlara, ne yer ve mühlet de verilmedi onlara.",
        "numberInSurah": 29,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4444,
        "text": "Ve andolsun ki İsrailoğullarını aşağılatıcı bir azaptan kurtardık.",
        "numberInSurah": 30,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4445,
        "text": "Firavun'dan; şüphe yok ki o haddi aşanlardan yücelik satan, ululanan biriydi.",
        "numberInSurah": 31,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4446,
        "text": "Ve andolsun ki İsrailoğullarını, bilerek bütün alemlerden üstün olmak üzere seçtik.",
        "numberInSurah": 32,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4447,
        "text": "Ve onlara, apaçık nimetleri muhtevi deliller verdik.",
        "numberInSurah": 33,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4448,
        "text": "Gerçekten de şunlar elbette derler ki.",
        "numberInSurah": 34,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4449,
        "text": "İlk ölümümüzden başka ölüm yok bize ve biz, tekrar dirilmeyiz de.",
        "numberInSurah": 35,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4450,
        "text": "Doğru söylüyorsanız getirin babalarımızı bize.",
        "numberInSurah": 36,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4451,
        "text": "Bunlar mı daha hayırlıdır, yoksa Tubba' kavmiyle onlardan öncekiler mi? Helak ettik onları, şüphe yok ki mücrimlerdi onlar.",
        "numberInSurah": 37,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4452,
        "text": "Ve biz gökleri ve yeryüzünü ve ikisinin arasındakileri eğlence için, boşuboşuna yaratmadık.",
        "numberInSurah": 38,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4453,
        "text": "Biz onları, ancak gerçek olarak yarattık ve fakat çoğu bilmez.",
        "numberInSurah": 39,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4454,
        "text": "Ayrılma günü, gerçekten de hepsinin muayyen bir günüdür.",
        "numberInSurah": 40,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4455,
        "text": "O gün dostun dosta faydası olmaz ve onlar, bir yardım da görmezler.",
        "numberInSurah": 41,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4456,
        "text": "Ancak Allah kime acırsa o başka; şüphe yok ki odur üstün ve rahim.",
        "numberInSurah": 42,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4457,
        "text": "Şüphe yok ki zakkum ağacı.",
        "numberInSurah": 43,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4458,
        "text": "Suçluların yemeğidir.",
        "numberInSurah": 44,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4459,
        "text": "Erimiş bakıra, kurşuna benzer, karınlarda kaynar.",
        "numberInSurah": 45,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4460,
        "text": "Kaynar su gibi.",
        "numberInSurah": 46,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4461,
        "text": "Tutun onu da sürüyün koca cehennemin ta ortasına.",
        "numberInSurah": 47,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4462,
        "text": "Sonra da dökün kaynar suyu azab olarak tepesine.",
        "numberInSurah": 48,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4463,
        "text": "Tat, şüphe yok ki sen üstündün, kerem sahibiydin.",
        "numberInSurah": 49,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4464,
        "text": "Gerçekten de buydu şüphe ettiğiniz.",
        "numberInSurah": 50,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4465,
        "text": "Şüphe yok ki çekinenler, emin bir makamdadır.",
        "numberInSurah": 51,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4466,
        "text": "Cennetlerde ve akarsuların kıyılarında.",
        "numberInSurah": 52,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4467,
        "text": "İnce ve kalın ipekliler giyerler, karşıkarşıya otururlar.",
        "numberInSurah": 53,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4468,
        "text": "Böyle işte ve onları evlendiririz iri gözlü hurilerle.",
        "numberInSurah": 54,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4469,
        "text": "Orada emin bir halde her çeşit meyvelar isterler.",
        "numberInSurah": 55,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4470,
        "text": "İlk ölümden başka ölüm tatmazlar orada ve onları korur koca cehennemin azabından.",
        "numberInSurah": 56,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4471,
        "text": "Rabbinden bir lütuf ve ihsan olarak; budur o büyük kurtuluşun, murada erişin ta kendisi.",
        "numberInSurah": 57,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4472,
        "text": "Gerçekten de öğüt alsınlar diye Kur'an'ı senin dilinle indirdik, okuyuşunu da kolaylaştırdık.",
        "numberInSurah": 58,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4473,
        "text": "Artık gözetle, bekle; şüphe yok ki onlar da gözetlemedeler, beklemedeler.",
        "numberInSurah": 59,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "tr.golpinarli",
      "language": "tr",
      "name": "Abdulbakî Gölpınarlı",
      "englishName": "Abdulbaki Golpinarli",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}