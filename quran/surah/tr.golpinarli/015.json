{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 15,
    "name": "سُورَةُ الحِجۡرِ",
    "englishName": "Al-Hijr",
    "englishNameTranslation": "The Rock",
    "revelationType": "Meccan",
    "numberOfAyahs": 99,
    "ayahs": [
      {
        "number": 1803,
        "text": "Elif lam ra, budur kitabın ve her şeyi açıklayan Kur'an'ın ayetleri.",
        "numberInSurah": 1,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1804,
        "text": "Nice demler gelecek ki kafirler, ne olur keşke biz de Müslüman olsaydık diyecekler.",
        "numberInSurah": 2,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1805,
        "text": "Bırak onları, yesinler, geçinsinler ve isteklere düşüp oyalansınlar, yakında bilecekler.",
        "numberInSurah": 3,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1806,
        "text": "Ve biz hiçbir şehri helak etmedik ki helak edeceğimiz zaman, malum ve mukadder olmasın.",
        "numberInSurah": 4,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1807,
        "text": "Hiçbir ümmet, ne helak edileceği zamanı mukadder vaktinden öne alabilir, ne de onu geciktirebilir.",
        "numberInSurah": 5,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1808,
        "text": "Ve derler ki: Ey kendisine Kur'an indirilen sen gerçekten de delisin.",
        "numberInSurah": 6,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1809,
        "text": "Gerçeklerdensen neden meleklerle gelmiyorsun bize?",
        "numberInSurah": 7,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1810,
        "text": "Biz melekleri, ancak hak ve gerçek olarak indiririz, indiririz ama o vakit de mühlet vermeyiz, göz açtırmayız kafirlere.",
        "numberInSurah": 8,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1811,
        "text": "Şüphe yok ki Kur'an'ı biz indirdik ve şüphe yok ki onu mutlaka koruyacağız.",
        "numberInSurah": 9,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1812,
        "text": "Andolsun ki senden önce, evvelki ümmetlere de peygamberler göndermiştik.",
        "numberInSurah": 10,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1813,
        "text": "Hiçbir peygamber göndermedik ki alay etmesinler onunla.",
        "numberInSurah": 11,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1814,
        "text": "Biz böylece, Kur'an'ı, yüreklerine kadar sokarız da.",
        "numberInSurah": 12,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1815,
        "text": "Gene ona inanmazlar ve gerçekten, eskilerin yoluyoradamı da böylece olup bitmiş, onlar da bu yüzden azaba uğrayıp gitmiştir.",
        "numberInSurah": 13,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1816,
        "text": "Onlara gökten bir kapı açsak da melekler, o kapıdan inip çıksalar.",
        "numberInSurah": 14,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1817,
        "text": "Bunu görürler de gene ancak derler, gözlerimiz bağlandı bizim, hatta büyülenmiş bir topluluğuz biz.",
        "numberInSurah": 15,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1818,
        "text": "Andolsun ki gökte burçlar halkettik ve göğü, seyredenlere bezedik.",
        "numberInSurah": 16,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1819,
        "text": "Ve onu, bütün taşlanmış Şeytanlardan koruduk.",
        "numberInSurah": 17,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1820,
        "text": "Ancak hırsızlama bir şey duymaya kalkışan olursa onun da ardından apaçık görünen bir ateş yalımıdır gönderdik.",
        "numberInSurah": 18,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1821,
        "text": "Yeryüzünü, enine boyuna döşedik ve orada metin dağlar yarattık ve oradan, taktirimize göre, her şeyi bitirdik.",
        "numberInSurah": 19,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1822,
        "text": "Orada sizin için de, sizin rızıklandırmadığınız mahlukat için de geçim sebepleri halkettik.",
        "numberInSurah": 20,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1823,
        "text": "Hiçbir şey yoktur ki hazineleri, katımızda olmasın ve biz onu ancak malum bir miktarda indiririz.",
        "numberInSurah": 21,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1824,
        "text": "Yüklü rüzgarlar gönderdik de gökten yağmur yağdırdık, suya kandırdık sizi ve onu koruyup saklayan siz değilsiniz.",
        "numberInSurah": 22,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1825,
        "text": "Ve şüphe yok ki ancak biz diriltiriz, biz öldürürüz ve biziz her şeye varis olan.",
        "numberInSurah": 23,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1826,
        "text": "Ve andolsun ki önce geçip gidenlerinizi de biliriz, sonraya kalanlarınızı da.",
        "numberInSurah": 24,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1827,
        "text": "Ve şüphe yok ki Rabbin, hepsini de haşreder; şüphe yok ki o, hüküm ve hikmet sahibidir ve her şeyi bilir.",
        "numberInSurah": 25,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1828,
        "text": "Andolsun ki biz Âdem'i, kuru, kokmuş, şekil ve suret verilmiş balçıktan yarattık.",
        "numberInSurah": 26,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1829,
        "text": "Şeytan'ıysa daha önce, yakıp öldürücü bir harareti olan ateşten yarattık.",
        "numberInSurah": 27,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1830,
        "text": "An o zamanı ki Rabbin, meleklere demişti: Gerçekten de ben, kuru, kokmuş, şekil ve suret verilmiş balçıktan bir insan yaratacağım.",
        "numberInSurah": 28,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1831,
        "text": "Onun yaratılışını tamamlayıp kemale getirerek ruhumdan ruh üfürünce derhal ona karşı secdeye kapanın.",
        "numberInSurah": 29,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1832,
        "text": "Meleklerin hepsi birden secde ettiler.",
        "numberInSurah": 30,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1833,
        "text": "Ancak İblis secde etmedi, secde edenlere katılmaktan çekindi.",
        "numberInSurah": 31,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1834,
        "text": "Ey İblis dedi, sana ne oldu da secde edenlere katılmaktan çekindin?",
        "numberInSurah": 32,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1835,
        "text": "Kuru, kokmuş, şekil ve suret verilmiş balçıktan yarattığın insana dedi, ben secde etmem.",
        "numberInSurah": 33,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1836,
        "text": "Çık buradan dedi, şüphe yok ki taşlanmış, kovulmuşsun sen.",
        "numberInSurah": 34,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1837,
        "text": "Ve gerçekten de din gününe dek lanet sana.",
        "numberInSurah": 35,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1838,
        "text": "Rabbim dedi, onların tekrar dirilecekleri güne dek mühlet ver, yaşat beni.",
        "numberInSurah": 36,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1839,
        "text": "Şüphe yok ki dedi, sen, mühlet verilmişlerdensin.",
        "numberInSurah": 37,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1840,
        "text": "Malum vaktin gelip çatacağı güne dek.",
        "numberInSurah": 38,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1841,
        "text": "Rabbim dedi, beni rahmetinden mahrum ettiğin gibi bende kötülükleri, yeryüzünde onlara bezeyecek, onları isyan ettirerek hepsini de rahmetinden mahrum edeceğim.",
        "numberInSurah": 39,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1842,
        "text": "Ancak ihlasa sahip edilmiş kulların müstesna.",
        "numberInSurah": 40,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1843,
        "text": "Tanrı, işte bu yol dedi, dosdoğru bana varan yol.",
        "numberInSurah": 41,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1844,
        "text": "Şüphe yok ki kullarıma hiçbir suretle gücün yetmez, ancak sana uyan azgınlara yeter senin gücün.",
        "numberInSurah": 42,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1845,
        "text": "Ve şüphe yok ki onların hepsine de vaadedilen yer, cehennemdir.",
        "numberInSurah": 43,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1846,
        "text": "Orasının yedi kapısı var, her kapıya da onlardan bir kısmı ayrılmıştır.",
        "numberInSurah": 44,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1847,
        "text": "Şüphe yok ki çekinenler, cennetlerde ve ırmak başlarındadır.",
        "numberInSurah": 45,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1848,
        "text": "Esenlikle emin olarak girin cennetlere.",
        "numberInSurah": 46,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1849,
        "text": "Gönüllerindeki kini, hasedi, ta kökünden söküp attık onların, kardeşlerdir, birbirlerine karşı tahtlar üstünde otururlar.",
        "numberInSurah": 47,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1850,
        "text": "Orada ne bir yorgunluk duyarlar, ne de oradan çıkarılırlar.",
        "numberInSurah": 48,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1851,
        "text": "Haber ver kullarıma, şüphe yok ki ben suçları örterim, rahimim.",
        "numberInSurah": 49,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1852,
        "text": "Ve şüphe yok ki azabım da pek elemli bir azaptır.",
        "numberInSurah": 50,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1853,
        "text": "Onları, İbrahim'e gelen misafirlerden de haberdar et.",
        "numberInSurah": 51,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1854,
        "text": "Hani, huzuruna girmişler de esenlik sana demişlerdi; o da, biz gerçekten de sizden korkuyoruz demişti.",
        "numberInSurah": 52,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1855,
        "text": "Korkma demişlerdi, biz sana, bilgi sahibi bir erkek evlat müjdeliyoruz.",
        "numberInSurah": 53,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1856,
        "text": "İhtiyarlık çağımda mı demişti, bana müjde veriyorsunuz? Neye istinaden müjde vermektesiniz bana?",
        "numberInSurah": 54,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1857,
        "text": "Sana öyle bir müjde veriyoruz ki gerçektir bu, sakın ümidini kesenlerden olma demişlerdi.",
        "numberInSurah": 55,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1858,
        "text": "O da Rabbinin rahmetinden demişti, ancak doğru yoldan sapanlardan başka kim ümit keser?",
        "numberInSurah": 56,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1859,
        "text": "Ey elçiler demişti, başka ne memuriyetiniz var?",
        "numberInSurah": 57,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1860,
        "text": "Biz demişlerdi, şüphe yok ki mücrim bir topluluğa gönderildik.",
        "numberInSurah": 58,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1861,
        "text": "Ancak Lut ve soyu müstesna; onların hepsini de mutlaka kurtaracağız.",
        "numberInSurah": 59,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1862,
        "text": "Yalnız karısını kurtarmayacağız, onun, helak olanlarla beraber şehirde kalmasını takdir ettik.",
        "numberInSurah": 60,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1863,
        "text": "Elçiler Lut ailesine geldikleri zaman.",
        "numberInSurah": 61,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1864,
        "text": "O, siz dedi tanınmamış kimselersiniz.",
        "numberInSurah": 62,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1865,
        "text": "Onlar, biz dediler, onların şüphe ettikleri şeyi getirdik.",
        "numberInSurah": 63,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1866,
        "text": "O gerçek haberle geldik sana ve biz doğru sözlüyüz.",
        "numberInSurah": 64,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1867,
        "text": "Gece yarısından sonra aileni yola çıkar, sen de artlarına düş, hiçbiriniz arkanıza bakmayın, emrolunacak yere geçingidin.",
        "numberInSurah": 65,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1868,
        "text": "Ve bu işi ona vahyettik de hiç şüphe yok ki dedik, sabah çağı bunların kökleri kesilir.",
        "numberInSurah": 66,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1869,
        "text": "Şehir halkı, birbirlerini müjdeleyerek misafirlerin yanına geldi.",
        "numberInSurah": 67,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1870,
        "text": "Lut, bunlar benim konuklarım dedi, onlara karşı utandırmayın beni.",
        "numberInSurah": 68,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1871,
        "text": "Allah'tan çekinin de mahzun etmeyin beni.",
        "numberInSurah": 69,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1872,
        "text": "Seni konuk kabul etmekten menetmedik miydi dediler.",
        "numberInSurah": 70,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1873,
        "text": "Lut, evlenecekseniz işte kızlarım, onları alın dedi.",
        "numberInSurah": 71,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1874,
        "text": "Ömrün hakkı için onlar, gafletten adeta sarhoştular, gaflet içinde şaşkın bir haldeydiler.",
        "numberInSurah": 72,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1875,
        "text": "Güneş doğduktan sonra onları bir bağırış, helak ediverdi.",
        "numberInSurah": 73,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1876,
        "text": "Ülkelerinin altını üstüne getirdik, üstlerine balçıktan meydana gelmiş taşlar yağdırdık.",
        "numberInSurah": 74,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1877,
        "text": "Şüphe yok ki bunda düşünenlere ibretler var.",
        "numberInSurah": 75,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1878,
        "text": "Ve şüphe yok ki o şehir, hala herkesin yol uğrağı olan bir yerde.",
        "numberInSurah": 76,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1879,
        "text": "Şüphe yok ki bunda, inananlar için bir delil var.",
        "numberInSurah": 77,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1880,
        "text": "Ashab-ı Eyke de gerçekten zalimdi.",
        "numberInSurah": 78,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1881,
        "text": "Öcaldık onlardan; iki şehir de apaçık görünmede, yol uğrağında hala.",
        "numberInSurah": 79,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1882,
        "text": "Ashabı Hicr de peygamberini inkar etti.",
        "numberInSurah": 80,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1883,
        "text": "Delillerimizi göstermiştik onlara, fakat onlardan yüz çevirmişlerdi.",
        "numberInSurah": 81,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1884,
        "text": "Ve evlerini dağlarda oyarlar, emin bir halde yaşarlardı.",
        "numberInSurah": 82,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1885,
        "text": "Sabah çağına erdikleri gibi bir bağırış yüzünden helak olup gittiler.",
        "numberInSurah": 83,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1886,
        "text": "Kazandıkları mal ve servet, azabı defedemedi onlardan.",
        "numberInSurah": 84,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1887,
        "text": "Ve biz, gökleri ve yeryüzünü abes olarak halketmedik ve kıyamet, mutlaka gelecektir, aldırış bile etme, bir hoşça vaz geç onlardan şimdilik.",
        "numberInSurah": 85,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1888,
        "text": "Şüphe yok ki Rabbin, her şeyi yaratandır ve her şeyi bilir.",
        "numberInSurah": 86,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1889,
        "text": "Andolsun ki biz sana, tekrarlanan yedi ayeti ve pek büyük olan Kur'an'ı verdik.",
        "numberInSurah": 87,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1890,
        "text": "Onlara verdiğimiz mala, evlada göz dikme, onlar için tasalanıp gam yeme, inananlara karşı kanadını indir, onları koru, onlara karşı mütevazı ol.",
        "numberInSurah": 88,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1891,
        "text": "Ve de ki: Hiç şüphe yok ki ben, gerçekten de bir korkutucuyum.",
        "numberInSurah": 89,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1892,
        "text": "Nitekim bölükbölük olanlara da indirmiştik.",
        "numberInSurah": 90,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1893,
        "text": "Öyle kişilerdi onlar ki Kuran'ı parçaparça ettiler; bir kısmına inandılar da bir kısmına inanmadılar.",
        "numberInSurah": 91,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1894,
        "text": "Andolsun Rabbine ki onların hepsine soracağız.",
        "numberInSurah": 92,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1895,
        "text": "Yaptıkları şeyleri.",
        "numberInSurah": 93,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1896,
        "text": "Artık sen emredildiğin şeyi açıkla ve şirk koşanlardan yüz çevir.",
        "numberInSurah": 94,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1897,
        "text": "O alaycılara karşı biz yeteriz sana.",
        "numberInSurah": 95,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1898,
        "text": "Onlar, Allah'tan başka tanrılar da kabul etmişlerdir; yakında bilip anlayacaklar.",
        "numberInSurah": 96,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1899,
        "text": "Ve andolsun biliriz ki şüphe yok, söyledikleri sözlerden yüreğin sıkılır.",
        "numberInSurah": 97,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1900,
        "text": "Artık Rabbine hamd ederek tenzih et ve secde edenlerden ol.",
        "numberInSurah": 98,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1901,
        "text": "Ve ölüm gelip çatıncaya dek Rabbine ibadet et.",
        "numberInSurah": 99,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      }
    ],
