{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 78,
    "name": "سُورَةُ النَّبَإِ",
    "englishName": "An-Naba",
    "englishNameTranslation": "The Announcement",
    "revelationType": "Meccan",
    "numberOfAyahs": 40,
    "ayahs": [
      {
        "number": 5673,
        "text": "Neyi birbirlerine sorup dururlar?",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5674,
        "text": "Pek ulu haberi.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5675,
        "text": "Öylesine haber ki onlar, bu hususta aykırılığa düşmüşlerdir.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5676,
        "text": "Hayır, bilirler yakında.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5677,
        "text": "Gene de hayır, bilirler yakında.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5678,
        "text": "Yeryüzünü, hazır bir yaygı olarak yaymadık mı?",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5679,
        "text": "Ve dağları, çiviler gibi çaktık.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5680,
        "text": "Ve sizi, çiftçift yarattık.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5681,
        "text": "Ve uykunuzu, vakitli bir istirahat zamanı kıldık.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5682,
        "text": "Ve geceyi, her şeyi örten bir örtü yaptık.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5683,
        "text": "Ve gündüzü de geçim zamanı.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5684,
        "text": "Ve üstünüzde, yedi sağlam yapı kurduk.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5685,
        "text": "Ve yalımyalım yanan bir kandil yarattık.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5686,
        "text": "Ve sıkılan bulutlardan şarılşarıl sular akıttık.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5687,
        "text": "Akıttık da o sayede tohumları, otları.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5688,
        "text": "Ve birbirine sarmaşdolaş bahçeleri, bağları meydana getirdik.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5689,
        "text": "Şüphe yok ki ayırma gününün vakti de tayin edilmiştir.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5690,
        "text": "O gün Sur üfürülür de gelirsiniz bölükbölük.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5691,
        "text": "Ve gök açılmış, kapılar haline gelmiştir.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5692,
        "text": "ve dağlar yürütülmüş, seraba dönmüştür.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5693,
        "text": "Şüphe yok ki cehennem pusudadır.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5694,
        "text": "Azanlara dönüp varılacak son yerdir.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5695,
        "text": "Yıllar boyunca kalırlar orada.",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5696,
        "text": "Ne bir serinlik tadarlar, ne içilecek bir şey.",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5697,
        "text": "Ancak bir kaynar su, ancak bir kan ve irin.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5698,
        "text": "Bir cezadır ki tam uygun.",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5699,
        "text": "Şüphe yok ki onlar, hiçbir soru ummazlardı.",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5700,
        "text": "Ve delillerimizi boyuna yalanlarlardı.",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5701,
        "text": "Ve biz her şeyi birbir sayıp yazdık.",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5702,
        "text": "Artık tadın, ancak azabınızı arttırırız sizin.",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5703,
        "text": "Şüphe yok ki çekinenlere bir kurtuluş, bir kutluluk ve murada eriş yeri var.",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5704,
        "text": "Bahçeler, üzümler.",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5705,
        "text": "Ve memeleri yeni sertleşmiş yaşıt kızlar.",
        "numberInSurah": 33,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5706,
        "text": "Ve dopdolu kadeh.",
        "numberInSurah": 34,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5707,
        "text": "Ne boş bir söz duyarlar orada, ne birbirlerini yalanlama.",
        "numberInSurah": 35,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5708,
        "text": "Rabbinden, fazlasıyle bir lütuf ve ihsan.",
        "numberInSurah": 36,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5709,
        "text": "Göklerin ve yeryüzünün ve ikisinin arasındakilerin Rabbidir rahman, onun hitabına nail olmazlar.",
        "numberInSurah": 37,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5710,
        "text": "O gün, Ruh ve melekler, saf saf dururlar; konuşamazlar, ancak rahmanın izin verdiği konuşur ve gerçek söyler.",
        "numberInSurah": 38,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5711,
        "text": "Bugün, gerçektir, artık dileyen, dönüp Rabbinin tapısına varmaya bir vesile edinir.",
        "numberInSurah": 39,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5712,
        "text": "Şüphe yok ki biz sizi, yakın bir azapla korkutmadayız; o gün kişi, elleriyle hazırladığına bakar ve kafir de ne olurdu der, keşke toprak olaydım.",
        "numberInSurah": 40,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      }
    ],
    "edition": {
