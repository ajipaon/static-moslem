{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 54,
    "name": "سُورَةُ القَمَرِ",
    "englishName": "Al-Qamar",
    "englishNameTranslation": "The Moon",
    "revelationType": "Meccan",
    "numberOfAyahs": 55,
    "ayahs": [
      {
        "number": 4847,
        "text": "Yaklaştı kıyamet ve yarıldı ay.",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4848,
        "text": "Ve onlar, bir delil gördüler mi yüz çevirirler de sürüp giden bir büyü derler.",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4849,
        "text": "Ve yalanlarlar ve dileklerine uyarlar ve her iş, kararlaştırılmıştır.",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4850,
        "text": "Ve andolsun, öyle haberler geldi onlara ki o haberlerde onları vazgeçirecek, onlara öğüt verecek şeyler vardı.",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4851,
        "text": "Yüksek hikmet vardı, derken korkutuşlar fayda vermedi gitti.",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4852,
        "text": "Artık yüz çevir onlardan; o gün çağıran, hoşlanılmayan birşeye çağırır.",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4853,
        "text": "Gözleri yerde, kabirlerden çıkarlar, sanki onlar, dağılmış çekirgelerdir.",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4854,
        "text": "Yönelirler çağırana; kafirler, bugün derler, ne de zorlu gün.",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4855,
        "text": "Onlardan önce Nuh kavmi de kulumuzu yalanlamıştı ve delil dediler ona, pek fena incittiler onu.",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4856,
        "text": "Derken Rabbine dua etti: Şüphe yok ki altoldum ben, artık sen yardım et bana.",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4857,
        "text": "Derken açtık göklerin kapılarını da şarıl şarıl ardı gelmez yağmurlar yağdırdık.",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4858,
        "text": "Ve yerden de sular fışkırttık, derken sular, mukadder bir emre göre birleşti.",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4859,
        "text": "Ve onu, tahtalardan yapılmış ve mıhlarla kenetlenmiş bir gemide taşıdık.",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4860,
        "text": "Gözümüzün önünde akıp giderdi; bir mükafattı nankörlük görene.",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4861,
        "text": "Ve andolsun ki bir delil olarak bıraktık onu, fakat bir ibret alan mı var?",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4862,
        "text": "Derken nasıldı azabım benim ve korkutuşlarım?",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4863,
        "text": "Ve andolsun öğüt ve ibret için Kur'an'ı kolaylaştırdık, fakat bir ibret alan mı var?",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4864,
        "text": "Âd da yalanlamıştı, derken nasıldı azabım benim ve korkutuşlarım?",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4865,
        "text": "Şüphe yok ki sürüp giden uğursuz bir günde onlara bir kasırgadır yolladık.",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4866,
        "text": "Onları kökünden koparmadaydı, sanki köklerinden kopup baş aşağı devrilen hurma kütükleriydi onlar.",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4867,
        "text": "Derken nasıldı azabım benim ve korkutuşlarım?",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4868,
        "text": "Ve andolsun ki öğüt ve ibret için Kur'an'ı kolaylaştırdık, fakat bir ibret alan mı var?",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4869,
        "text": "Semud da korkutucuları yalanladı.",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4870,
        "text": "Derken bizden bir adama mı uyacağız dediler, gerçekten de o zaman elbette sapıklığa düşeriz, ateşlere yanarkavruluruz.",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4871,
        "text": "Vahiy, içimizden gelegele ona mı geliyor? Hayır, o, yalancı kendini beğenmiş birisi.",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4872,
        "text": "Yarın bilirler kimmiş yalancı kendini beğenmiş.",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4873,
        "text": "Şüphe yok ki onları sınamak için dişi deveyi gönderiyoruz, artık gözetle onları ve dayan.",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4874,
        "text": "Ve haber ver onlara, su, aralarında paylaştırılmıştır, her bölük, nöbetinde hazır olur, su alır.",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4875,
        "text": "Derken arkadaşlarına seslendiler, derken kılıcını çekti de devenin ayaklarını kesti, öldürdü.",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4876,
        "text": "Derken nasıldı azabım benim ve korkutuşlarım?",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4877,
        "text": "Gerçekten de bir bağırış gönderdik onlara, derken hayvan ağılına konan çalıya çırpıya döndüler.",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4878,
        "text": "Ve andolsun ki öğüt ve ibret için Kur'an'ı kolaylaştırdık, fakat bir ibret alan mı var?",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4879,
        "text": "Lut kavmi de korkutucuları yalanladılar.",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4880,
        "text": "Gerçekten de, Lut'un ailesi müstesna, onlara taş yağdıran bir yel gönderdik, Lut'un ailesini de bir seher çağı kurtardık.",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4881,
        "text": "Katımızdan bir nimet olarak; işte böyle mükafatlandırırız şükredeni.",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4882,
        "text": "Ve andolsun ki o, bizim helakimizle korkutmuştu onları da onlar, bu korkutuşlardan şüpheye düşmüşlerdi.",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4883,
        "text": "Ve gerçekten de onun konuklarını istemişlerdi de biz, kör edivermiştik gözlerini, artık tadın azabımı ve korkutuşlarımın sonucunu.",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4884,
        "text": "Ve andolsun ki bir sabah çağı üstlerine bir azap çöküvermişti onların.",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4885,
        "text": "Artık tadın azabımı ve korkutuşlarımı.",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4886,
        "text": "Ve andolsun ki öğüt ve ibret için Kur'an'ı kolaylaştırdık, fakat bir ibret alan mı var?",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4887,
        "text": "Ve andolsun ki Firavun soyuna da korkutucular gelmişti.",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4888,
        "text": "Bütün delillerimizi yalanladılar, derken onları üstün ve mutlak kudretli bir helak edişle helak ediverdik.",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4889,
        "text": "Sizin kafirleriniz, onlardan hayırlı mı, yoksa kitaplarda bir kurtuluş mu var size?",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4890,
        "text": "Yoksa biz, birbirine yardım eden bir topluluğuz mu derler?",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4891,
        "text": "O topluluk, yakında bozguna uğrayacak ve ardını dönüp kaçacak.",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4892,
        "text": "Onlara vaadedilen azabın mukadder zamanı kıyamettir ve kıyametin azabı, daha da zararlıdır ve daha da acı.",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4893,
        "text": "Şüphe yok ki suçlular, sapıklık içinde ve yakıp kavuran ateşlerdedir.",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4894,
        "text": "O gün, yüzüstü ateşe sürüklenip atılırlar; tadın bakalım, cehennemin yakışını.",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4895,
        "text": "Şüphe yok ki biz; her şeyi, bilgimizde mukadder olduğu gibi ve zamanında yarattık.",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4896,
        "text": "Ve bizim emrimiz, birdir, ancak bir göz kırpış, bir göz yumup açış gibi tezdir.",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4897,
        "text": "Ve andolsun ki taraftarlarınızı da helak ettik, fakat bir ibret alan mı var?",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4898,
        "text": "Ve işledikleri her şey, kitaplardadır.",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4899,
        "text": "Ve küçük, büyük, hepsi de yazılıdır.",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4900,
        "text": "Şüphe yok ki çekinenler, cennetlerdedir, ırmakların başlarında.",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4901,
        "text": "Gerçeklik makamında, çok kudretli bir büyük padişah katında.",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      }
    ],
