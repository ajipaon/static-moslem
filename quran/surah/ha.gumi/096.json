  "data": {
    "number": 96,
    "name": "سُورَةُ العَلَقِ",
    "englishName": "Al-Alaq",
    "englishNameTranslation": "The Clot",
    "revelationType": "Meccan",
    "numberOfAyahs": 19,
    "ayahs": [
      {
        "number": 6107,
        "text": "Ka yi karatu da sunan Ubangijinka, wanda Ya yi halitta.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6108,
        "text": "Ya hahitta mutum daga gudan jini.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6109,
        "text": "Ka yi karatu, kuma UbangiJinka shi ne Mafi karimci.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6110,
        "text": "Wanda Ya sanar (da mutum) game da alƙalami.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6111,
        "text": "Ya sanar da mutum abin da bai sani ba.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6112,
        "text": "A'aha! Lalle, ne mutum haƙĩƙa, yana girman kai (ya ƙi karatu).",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6113,
        "text": "Dõmin yã ga kansa, yã wadãta.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6114,
        "text": "Lalle ne zuwa ga Ubangijinka makoma take.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6115,
        "text": "Shin, kã ga wanda ke hana.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6116,
        "text": "Bãwã idan yã yi salla?",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6117,
        "text": "Ashe, kã gani, idan (shi bãwan) ya kasance a kan shiriya?",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6118,
        "text": "Ko ya yi umurni da taƙawa?",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6119,
        "text": "Ashe, kã gani, idan (shi mai hanin) ya ƙaryata, kuma ya jũya bãya?",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6120,
        "text": "Ashe, bai sani ba cewa Allah Yanã gani?",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6121,
        "text": "A'aha! Lalle ne, idan bai hanu ba, lalle ne zã Mu ja gãshin makwarkwaɗa.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6122,
        "text": "Makwarkwaɗa maƙaryaciya, mai laifi.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6123,
        "text": "Sai ya kirayi ƙungiyarsa.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6124,
        "text": "Zã Mu kirayi zabaniyãwa (mala'iku mãsu girma).",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6125,
        "text": "A'aha kada ka bĩ shi. Kuma ka yi tawali'u, kuma ka nemi kusanta (zuwa ga Ubangijinka).",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": {
          "id": 15,
          "recommended": false,
          "obligatory": true
        }
      }
    ],
