  "data": {
    "number": 84,
    "name": "سُورَةُ الانشِقَاقِ",
    "englishName": "Al-Inshiqaaq",
    "englishNameTranslation": "The Splitting Open",
    "revelationType": "Meccan",
    "numberOfAyahs": 25,
    "ayahs": [
      {
        "number": 5885,
        "text": "Idan sama ta kẽce,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5886,
        "text": "Ta saurari Ubangijinta, kuma aka wajabta mata yin saurãron,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5887,
        "text": "Kuma idan ƙasã aka mĩƙe ta,",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5888,
        "text": "Kuma ta jẽfar da abin da yake a cikinta, tã wõfinta daga kõme.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5889,
        "text": "Kuma ta saurãri Ubangijinta, aka wajabta mata yin saurãren,",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5890,
        "text": "Ya kai mutum! Lalle ne kai mai aikin wahal da kai ne zuwa ga Ubangijinka, wahala mai tsanani, To, kai mai haɗuwa da Shi ne.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5891,
        "text": "To, amma wanda aka bai wa littãfinsa da damansa.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5892,
        "text": "To, za a yi masa hisãbi, hisãbi mai sauƙi.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5893,
        "text": "Kuma ya jũya zuwa ga iyãlinsa (a cikin Aljanna), yanã mai raha.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5894,
        "text": "Kuma amma wanda aka bai wa littãfinsa, daga wajen bãyansa.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5895,
        "text": "To, zã shi dinga kiran halaka!",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5896,
        "text": "Kuma ya shiga sa'ĩr.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5897,
        "text": "Lal1e ne shi, yã kasance (a dũniya) cikin iyãlinsa yanã mai raha.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5898,
        "text": "Lalle ne yã yi zaton bã zai kõmo ba.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5899,
        "text": "Na'am! Lalle ne, Ubangijinsa Ya kasance Mai gani gare shi.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5900,
        "text": "To, ba sai Nã rantse da shafaƙi ba.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5901,
        "text": "Da dare, da abin da ya ƙunsa.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5902,
        "text": "Da watã idan (haskensa) ya cika.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5903,
        "text": "Lalle ne kunã hawan wani hãli daga wani hãli.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5904,
        "text": "To, mẽ ya sãme su, ba su yin ĩmãni?",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5905,
        "text": "Kuma idan an karanta Alkur'ãni a kansu, bã su yin tawãli'u?",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": {
          "id": 14,
          "recommended": true,
          "obligatory": false
        }
      },
      {
        "number": 5906,
        "text": "Ba haka ba! waɗanda suka kãfirta, sai ƙaryatãwa suke yi.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5907,
        "text": "Alhãli Allah Shĩ ne Mafi sani ga abin a suke tãrãwa.",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5908,
        "text": "Saboda haka, ka yi musu bushãra da azãba mai raɗaɗi.",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5909,
        "text": "Fãce waɗanda suka yi ĩmãni, suka aikata ayyukan ƙwarai, sunã da wani sakamako wanda bã ya yankẽwa.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      }
    ],
