{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 15,
    "name": "سُورَةُ الحِجۡرِ",
    "englishName": "Al-Hijr",
    "englishNameTranslation": "The Rock",
    "revelationType": "Meccan",
    "numberOfAyahs": 99,
    "ayahs": [
      {
        "number": 1803,
        "text": "A. L̃.R. Waɗancan ãyoyin littãfi ne da abin karantãwa mai bayyanãwa.",
        "numberInSurah": 1,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1804,
        "text": "Da yawa waɗanda suka kãfirta suke gũrin dã dai sun kasance Musulmi.",
        "numberInSurah": 2,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1805,
        "text": "Ka bar su su ci kuma su ji dãdi, kuma gũri ya shagaltar da su, sa'an nan da sannu zã su sani.",
        "numberInSurah": 3,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1806,
        "text": "Kuma ba Mu halakar da wata alƙarya ba fãce tanã da littafi sananne.",
        "numberInSurah": 4,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1807,
        "text": "Wata al'umma bã ta gabãtar ajalinta, kuma bã zã su jinkirta ba.",
        "numberInSurah": 5,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1808,
        "text": "Suka ce: \"Yã kai wanda aka saukar da Ambato (Alƙur'ãni) a kansa! Lalle ne kai, haƙĩƙa, mahaukaci ne.\"",
        "numberInSurah": 6,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1809,
        "text": "\"Dõmin me bã zã ka zo mana da malã'ĩku ba idan ka kasance daga mãsu gaskiya?\"",
        "numberInSurah": 7,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1810,
        "text": "Bã Mu sassaukar da malã'iku fãce da gaskiya, bã zã su kasance, a wannan lõkacin, waɗanda ake yi wa jinkĩri ba.",
        "numberInSurah": 8,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1811,
        "text": "Lalle Mũ ne, Muka saukar da Ambato (Alƙur'ãni), kuma lal1e Mũ, haƙĩƙa, Mãsu kiyayẽwane gare shi.",
        "numberInSurah": 9,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1812,
        "text": "Kuma lalle ne, haƙĩƙa, Mun aika Manzanni a cikin ƙungiyõyin farko, gabãninka.",
        "numberInSurah": 10,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1813,
        "text": "Kuma wani Manzo bã ya zuwa gare su fãce sun kasance sunã mãsu, izgili a gare shi.",
        "numberInSurah": 11,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1814,
        "text": "Kamar wancan ne Muke shigar da shi a cikin zukãtan mãsu laifi.",
        "numberInSurah": 12,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1815,
        "text": "Bã su yin ĩmãni da shi, kuma haƙĩƙa, hanyar mutãnen farko ta shige.",
        "numberInSurah": 13,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1816,
        "text": "Kuma dã Mun bũɗe wata ƙõfa daga sama a kansu har suka wuni a ciki sunã tãkãwa.",
        "numberInSurah": 14,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1817,
        "text": "Lalle ne dã sun ce: \"Abin sani kawai, an rufe idãnuwanmu ne. Ã'a, mũ mutãne ne waɗanda aka sihirce.\"",
        "numberInSurah": 15,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1818,
        "text": "Kuma lalle ne, haƙĩƙa Mun sanya waɗansu masaukai a, cikin sama, kuma Muka ƙawãta ta ga masu kallo.",
        "numberInSurah": 16,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1819,
        "text": "Kuma Muka kiyãye ta daga dukan Shaiɗani wanda ake jĩfa.",
        "numberInSurah": 17,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1820,
        "text": "Fãce wanda ya sãci saurãre sai wutar yũlã bayyananniya ta bĩ shi.",
        "numberInSurah": 18,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1821,
        "text": "Kuma ƙasa Mun mĩkẽ ta kuma Mun jẽfa duwãtsu tabbatattu a cikinta kuma Mun tsirar a cikinta, daga dukan abu wanda ake aunãwa da sikẽli.",
        "numberInSurah": 19,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1822,
        "text": "Kuma Muka sanyã muku, a cikinta, abũbuwan rãyuwã da wanda ba ku zamã mãsu ciyarwa gare shi ba.",
        "numberInSurah": 20,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1823,
        "text": "Kuma bãbu wani abu fãce a wurinMu, akwai taskõkĩnsa kuma ba Mu saukar da shi ba fãce kan gwargwado sananne.",
        "numberInSurah": 21,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1824,
        "text": "Kuma Muka aika iskõki mãsu barbarar jũna sa'an nan Muka saukar da ruwa daga sama, sa'an nan Muka shãyar da ku shi, kuma ba ku zama mãsu taskacẽwa a gare shi ba.",
        "numberInSurah": 22,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1825,
        "text": "Kuma lalle ne Mu Muke rãyarwa, kuma Muke kashẽwa kuma Mũ ne magada.",
        "numberInSurah": 23,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1826,
        "text": "Kuma lalle ne, haƙĩƙa, Mun san mãsu gabãta daga cikinku, kuma Mun san mãsu jinkiri.",
        "numberInSurah": 24,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1827,
        "text": "Kuma lalle ne Ubangijinka Shĩ ne Yake tãra su, lalle Shĩ ne Mai hikima, Masani.",
        "numberInSurah": 25,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1828,
        "text": "Kuma lalle ne Mun halicci mutum daga ƙeƙasasshiyar lãka, daga baƙin yumɓu wanda ya canja.",
        "numberInSurah": 26,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1829,
        "text": "Kuma Aljani Mun haliccẽ shi daga gabãni, daga wutar iskar zafi.",
        "numberInSurah": 27,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1830,
        "text": "Kuma a lõkacin da Ubangijinka ya ce wa malã'iku: \"Lalle Nĩ mai halittar wani jiki ne daga ƙẽƙasasshen yumɓu wanda ya canja.\"",
        "numberInSurah": 28,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1831,
        "text": "\"To idan Na daidaitã shi kuma Na hũra daga RũhĩNa a cikinsa, to, ku fãɗi a gare shi, kunã mãsu yin sujada.\"",
        "numberInSurah": 29,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1832,
        "text": "Sai malã'iku suka yi sujada dukkansu gaba ɗaya.",
        "numberInSurah": 30,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1833,
        "text": "Fãce Iblĩs, ya ƙi kasancẽwa daga mãsu yin sujadar.",
        "numberInSurah": 31,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1834,
        "text": "Ya ce: \"Yã Iblĩs mẽne ne a gare ka, ba ka kasance tãre da mãsu yin sujuda ba?\"",
        "numberInSurah": 32,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1835,
        "text": "Ya ce: \"Ban kasance inã yin sujada ba ga mutum wanda Ka halicce shi daga bũsasshen yumɓun lãka wadda ta canja.\"",
        "numberInSurah": 33,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1836,
        "text": "Ya ce: \"To, ka fita daga gare ta, dõmin lalle kai abin jĩfa ne.\"",
        "numberInSurah": 34,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1837,
        "text": "\"Kuma lalle ne akwai la'ana a kanka har ya zuwa rãnar sakamako.\"",
        "numberInSurah": 35,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1838,
        "text": "Ya ce: \"Yã Ubangjĩna! Sai Ka yi mini jinkiri zuwa rãnar da ake tãshin su.\"",
        "numberInSurah": 36,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1839,
        "text": "Ya ce: \"To, lalle ne kanã daga waɗanda ake yi wa jinkiri.\"",
        "numberInSurah": 37,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1840,
        "text": "\"Zuwa ga Yinin Lõkacin nan sananne.\"",
        "numberInSurah": 38,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1841,
        "text": "Ya ce: \"Yã Ubangijina! Inã rantsuwa da abin da Ka ɓatar da ni da shi, haƙĩƙa inã ƙawãta musu (rãyuwa) a cikin ƙasã kuma haƙĩƙa inã ɓatar da su gabã ɗaya.\"",
        "numberInSurah": 39,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1842,
        "text": "\"Fãce bãyinKa daga gare su, waɗanda Ka tsarkake.\"",
        "numberInSurah": 40,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1843,
        "text": "Ya ce: \"Wannan tafarki ne a gare Ni, madaidaici.\"",
        "numberInSurah": 41,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1844,
        "text": "\"Lalle ne bãyĩNa, bã ka da ĩkõ a kansu, fãce wanda ya bĩ ka daga ɓatattu.\"",
        "numberInSurah": 42,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1845,
        "text": "Kuma lalle Jahannama ce haƙĩƙa, ma'a1kawartarsu gabã ɗaya.",
        "numberInSurah": 43,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1846,
        "text": "Tanã da ƙõfõfi bakwai, ga kõwace ƙõfa akwai wani juz'i daga gare su rababbe.",
        "numberInSurah": 44,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1847,
        "text": "Lalle mãsu taƙawa sunã a cikin gidãjen Aljanna mai idandunan ruwa.",
        "numberInSurah": 45,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1848,
        "text": "\"Ku shigẽ ta da aminci, kunã amintattu.\"",
        "numberInSurah": 46,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1849,
        "text": "Kuma Muka ɗẽbe abinda ke a cikin zukãtansu na daga ƙullin zũci, suka zama 'yan'uwa a kan gadãje, sunã mãsu fuskantar junã.",
        "numberInSurah": 47,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1850,
        "text": "Wata wahala bã zã ta shãfe su bã a cikinta kuma ba su zama mãsu fita daga, cikinta ba.",
        "numberInSurah": 48,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1851,
        "text": "Ka bai wa bãyiNa lãbari cẽwa lalle ne Ni, Mai gãfarane, Mai jin ƙai.",
        "numberInSurah": 49,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1852,
        "text": "Kuma azãbãTa ita ce azãba mai raɗaɗi.",
        "numberInSurah": 50,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1853,
        "text": "Kuma ka bã su lãbãrin bãƙin Ibrãhĩm.",
        "numberInSurah": 51,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1854,
        "text": "A lõkacin da suka shiga gunsa, sai suka ce: \"Sallama.\" Ya ce: \"Lalle mũ, daga gare ku, mãsu firgita ne.\"",
        "numberInSurah": 52,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1855,
        "text": "Suka ce: \"Kada ka firgita. Lalle ne mũ, munã yi makabushãra game da wani yãro masani.\"",
        "numberInSurah": 53,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1856,
        "text": "Ya ce: \"Shin kun bã ni bushãra ne a kan tsũfa yã shãfe ni? To, da me kuke bã ni bushara?\"",
        "numberInSurah": 54,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1857,
        "text": "Suka ce: \"Munã yi maka bushãra da gaskiya ne, sabõda haka, kada da kasance daga mãsu yanke tsammãni.\"",
        "numberInSurah": 55,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1858,
        "text": "Ya ce: \"Kuma wãne ne yake yanke tsammãni daga rahamar Ubangijinsa, fãce ɓatattu?\"",
        "numberInSurah": 56,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1859,
        "text": "Ya ce: \"To, mẽne ne babban al'amarinku? Yã kũ manzanni!\"",
        "numberInSurah": 57,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1860,
        "text": "Suka ce: \"Lalle ne mũ, an aika mu zuwa ga wasu mutãne mãsu laifi.\"",
        "numberInSurah": 58,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1861,
        "text": "\"Fãce mutãnen Lũɗu, lalle ne mũ, haƙĩƙa, mãsu tsĩrar dasu ne gabã ɗaya.\"",
        "numberInSurah": 59,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1862,
        "text": "\"Fãce mãtarsa mun ƙaddara cẽwa lalle ne ita, haƙĩƙa, tanã daga mãsu halaka.\"",
        "numberInSurah": 60,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1863,
        "text": "To, a lõkacin da mazannin suka jẽ wa mutãnen Lũɗu,",
        "numberInSurah": 61,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1864,
        "text": "Ya ce: \"Lalle ne ku mutãne ne waɗanda ba a sani ba.\"",
        "numberInSurah": 62,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1865,
        "text": "Suka ce: \"Ã'a, mun zo maka sabõda abin da suka kasance sunã shakka a cikinsa.\"",
        "numberInSurah": 63,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1866,
        "text": "\"Kuma mun zo maka da gaskiya. Kuma lalle ne mũ, haƙĩƙa, mãsu gaskiya ne.\"",
        "numberInSurah": 64,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1867,
        "text": "\"Sai ka yi tafiya da iyãlinka, a wani yanki na dare, kuma ka bi bãyansu, kuma kada wani daga cikinku ya yi waiwaye, kuma ku bi ta inda aka aumurce ku.\"",
        "numberInSurah": 65,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1868,
        "text": "Kuma Muka hukunta wancan al'amarin zuwa gare shi cẽwa lalle ne ƙarshen waɗancan abin yankẽwa ne a lõkacin da suke mãsu shiga asuba.",
        "numberInSurah": 66,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1869,
        "text": "Kuma mutãnen alƙaryar suka je sunã mãsu bushãra.",
        "numberInSurah": 67,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1870,
        "text": "Ya ce: \"Lalle ne waɗannan bãƙĩna ne, sabõda haka kada ku kunyata ni.\"",
        "numberInSurah": 68,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1871,
        "text": "\"Kuma ku bi Allah da taƙawa, kuma kada ku sanya ni a baƙin ciki.\"",
        "numberInSurah": 69,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1872,
        "text": "Suka ce: \"Ashe ba mu hana ka daga tãlikai ba?\"",
        "numberInSurah": 70,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1873,
        "text": "Ya ce: \"Ga waɗannan, 'ya'yãna idan kun kasance mãsu aikatãwa ne.\"",
        "numberInSurah": 71,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1874,
        "text": "Rantsuwa da rãyuwarka! Lalle ne sũ a cikin mãyensu sunã ta ɗĩmuwa.",
        "numberInSurah": 72,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1875,
        "text": "Sa'an nan tsãwa ta kãma su, sunã mãsu shiga lõkacin hũdõwar rãnã.",
        "numberInSurah": 73,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1876,
        "text": "Sa'an nan Muka sanya samanta ya kõma ƙasanta, kuma Muka yi ruwan duwãtsu na lãkar wuta a kansu.",
        "numberInSurah": 74,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1877,
        "text": "Lalle ne a cikin wancan akwai ãyõyi ga mãsu tsõkaci da hankali.",
        "numberInSurah": 75,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1878,
        "text": "Kuma lalle ne ita, haƙĩƙa sunã a gẽfen wata hanyã tabbatacciya.",
        "numberInSurah": 76,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1879,
        "text": "Lalle ne a cikin wancan akwai ãyã ga mãsu ĩmãni.",
        "numberInSurah": 77,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1880,
        "text": "Kuma lalle ne ma'abũta Al'aika sun kasance, haƙĩƙa, mãsu zãlunci!",
        "numberInSurah": 78,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1881,
        "text": "Sai Muka yi azãbar rãmuwã a gare su, kuma lalle sũ biyun, haƙĩƙa, sunã a gẽfen wani tafarki mabayyani.",
        "numberInSurah": 79,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1882,
        "text": "Kuma lalle ne haƙĩƙa ma'abũta Hijiri sun ƙaryata Manzanni.",
        "numberInSurah": 80,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1883,
        "text": "Kuma Kuka kai musu ãyõyinMu, sai suka kasance mãsu bijirẽwa daga gare su.",
        "numberInSurah": 81,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1884,
        "text": "Kuma sun kasance sunã sassaƙa gidãje daga duwãtsu, alhãli sunã amintattu.",
        "numberInSurah": 82,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1885,
        "text": "Sai tsãwa ta kãma su sunã mãsu shiga asuba.",
        "numberInSurah": 83,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1886,
        "text": "Sa'an nan abin da suka kasance sunã tsirfantãwa bai wadãtar ga barinsu ba.",
        "numberInSurah": 84,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1887,
        "text": "Kuma ba Mu halicci sammai da ƙasa da abin da yake a tsakãninsu ba fãce da gaskiya. Kuma lalle ne Sã'a (Rãnar Alƙiyãma) haƙĩƙa mai zuwa ce. Sabõdahaka ka yi rangwame, rangwame mai kyau.",
        "numberInSurah": 85,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1888,
        "text": "Lalle ne Ubangijinka, shi ne Mai yawan halitta, Masani.",
        "numberInSurah": 86,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1889,
        "text": "Kuma lalle ne, haƙĩƙa, Mun bã ka bakwai waɗanda ake maimaita karatunsu da Alƙur'ãni mai girma.",
        "numberInSurah": 87,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1890,
        "text": "Kada lalle kaƙĩƙa idãnunka biyu zuwa ga abin da Muka jiyar da su dãɗi game da shi, nau'i-nau'i a gare su, kuma kada ka yi baƙin ciki a kansu, kuma sassauta fikãfikanka ga mãsu ĩmãni.",
        "numberInSurah": 88,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1891,
        "text": "Kuma ka ce: \"Lalle nĩ nĩ ne mai gargaɗi bayyananne.\"",
        "numberInSurah": 89,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1892,
        "text": "Kamar yadda Muka saukar a kan mãsu yin rantsuwa,",
        "numberInSurah": 90,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1893,
        "text": "Waɗanda suka sanya Alƙur'ãni tãtsuniyõyi.",
        "numberInSurah": 91,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1894,
        "text": "To, rantsuwa da Ubangijinka! Haƙĩƙa, Munã tambayarsugabã ɗaya.",
        "numberInSurah": 92,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1895,
        "text": "Daga abin da suka kasance sunã aikatãwa.",
        "numberInSurah": 93,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1896,
        "text": "Sai ka tsage gaskiya game da abin da ake umurnin ka kuma ka kau da kai daga masu shirki.",
        "numberInSurah": 94,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1897,
        "text": "Lalle ne Mũ Mun isar maka daga mãsu izgili.",
        "numberInSurah": 95,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1898,
        "text": "Waɗanda suke sanyãwar wani abin bautãwa na dabam tare da Allah, sa'an nan da sannu zã su sani.",
        "numberInSurah": 96,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1899,
        "text": "Kuma lalle ne haƙĩƙa Munã sanin cẽwa lalle kai, ƙirjinka yanã yin ƙunci game da abin da suke faɗã (na izgili).",
        "numberInSurah": 97,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1900,
        "text": "Sabõda haka ka yi tasbĩhi game da gõde wa Ubangijinka, kuma ka kasance daga mãsu sujada.",
        "numberInSurah": 98,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1901,
        "text": "Kuma ka bauta wa Ubangijinka, har mutuwa ta zo maka.",
        "numberInSurah": 99,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      }
    ],
