  "code": 200,
  "status": "OK",
  "data": {
    "number": 96,
    "name": "سُورَةُ العَلَقِ",
    "englishName": "Al-Alaq",
    "englishNameTranslation": "The Clot",
    "revelationType": "Meccan",
    "numberOfAyahs": 19,
    "ayahs": [
      {
        "number": 6107,
        "text": "بخوان به نام پروردگارت كه بيافريد- همه آفريدگان را-.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6108,
        "text": "آدمى را از خونى بسته آفريد.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6109,
        "text": "بخوان، و پروردگار تو بزرگوارترين [بخشندگان‌] است،",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6110,
        "text": "آن كه [نوشتن‌] با قلم بياموخت.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6111,
        "text": "آدمى را آنچه نمى‌دانست بياموخت.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6112,
        "text": "آرى، هر آينه آدمى سركشى مى‌كند و از حد مى‌گذرد،",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6113,
        "text": "از آن رو كه خود را بى‌نياز و توانگر بيند.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6114,
        "text": "همانا بازگشت [همه‌] به سوى پروردگار توست.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6115,
        "text": "مرا بگو كه آيا آن [كافر سركش‌] كه باز مى‌دارد،",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6116,
        "text": "بنده‌اى [گزين و بهين‌] را آنگاه كه نماز گزارد [آيا نمى‌داند كه خدا او را مى‌بيند]؟",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6117,
        "text": "مرا بگو كه اگر [آن بنده‌] بر راه راست باشد،",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6118,
        "text": "يا به پرهيزگارى فرمان دهد [حال آن كافر چه خواهد بود]؟",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6119,
        "text": "مرا بگو [از حال آن كافر] اگر [حق را] تكذيب كند و [از ايمان و كار نيك‌] روى گرداند [سزاوار چه كيفرى است؟]",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6120,
        "text": "آيا ندانسته است كه خدا مى‌بيند [و او را كيفر مى‌دهد]؟",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6121,
        "text": "نه، اگر باز نايستد هر آينه موى پيشانى او را بگيريم و بكشيمش [به دوزخ‌].",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6122,
        "text": "موى پيشانى دروغزنى بزهكار را.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6123,
        "text": "پس او انجمنش را بخواند.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6124,
        "text": "ما نيز آن دوزخبانان را خواهيم خواند.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6125,
        "text": "نه، او را فرمان مبر، و سجده كن و [به پروردگار خويش‌] نزديكى جوى.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": {
          "id": 15,
          "recommended": false,
          "obligatory": true
        }
      }
    ],
