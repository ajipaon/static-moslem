  "data": {
    "number": 69,
    "name": "سُورَةُ الحَاقَّةِ",
    "englishName": "Al-Haaqqa",
    "englishNameTranslation": "The Reality",
    "revelationType": "Meccan",
    "numberOfAyahs": 52,
    "ayahs": [
      {
        "number": 5324,
        "text": "elḥâḳḳah.",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5325,
        "text": "me-lḥâḳḳah.",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5326,
        "text": "vemâ edrâke me-lḥâḳḳah.",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5327,
        "text": "keẕẕebet ŝemûdü ve`âdüm bilḳâri`ah.",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5328,
        "text": "feemmâ ŝemûdü feühlikû biṭṭâgiyeh.",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5329,
        "text": "veemmâ `âdün feühlikû birîḥin ṣarṣarin `âtiyeh.",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5330,
        "text": "seḫḫarahâ `aleyhim seb`a leyâliv veŝemâniyete eyyâmin ḥusûmen fetere-lḳavme fîhâ ṣar`â keennehüm a`câzü naḫlin ḫâviyeh.",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5331,
        "text": "fehel terâ lehüm mim bâḳiyeh.",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5332,
        "text": "vecâe fir`avnü vemen ḳablehû velmü'tefikâtü bilḫâṭieh.",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5333,
        "text": "fe`aṣav rasûle rabbihim feeḫaẕehüm aḫẕeter râbiyetâ.",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5334,
        "text": "innâ lemmâ ṭaga-lmâü ḥamelnâküm fi-lcâriyeh.",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5335,
        "text": "linec`alehâ leküm teẕkiratev vete`iyehâ üẕünüv vâ`iyeh.",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5336,
        "text": "feiẕâ nüfiḫa fi-ṣṣûri nefḫatüv vâḥideh.",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5337,
        "text": "veḥumileti-l'arḍu velcibâlü fedükketâ dekketev vâḥidetâ.",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5338,
        "text": "feyevmeiẕiv veḳa`ati-lvâḳi`ah.",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5339,
        "text": "venşeḳḳati-ssemâü fehiye yevmeiẕiv vâhiyeh.",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5340,
        "text": "velmelekü `alâ ercâihâ. veyaḥmilü `arşe rabbike fevḳahüm yevmeiẕin ŝemâniyeh.",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5341,
        "text": "yevmeiẕin tü`raḍûne lâ taḫfâ minküm ḫâfiyeh.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5342,
        "text": "feemmâ men ûtiye kitâbehû biyemînihî feyeḳûlü hâümu-ḳraû kitâbiyeh.",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5343,
        "text": "innî żanentü ennî mülâḳin ḥisâbiyeh.",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5344,
        "text": "fehüve fî `îşetir râḍiyeh.",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5345,
        "text": "fî cennetin `âliyeh.",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5346,
        "text": "ḳuṭûfühâ dâniyeh.",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5347,
        "text": "külû veşrabû henîem bimâ esleftüm fi-l'eyyâmi-lḫâliyeh.",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5348,
        "text": "veemmâ men ûtiye kitâbehû bişimâlihî feyeḳûlü yâ leytenî lem ûte kitâbiyeh.",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5349,
        "text": "velem edri mâ ḥisâbiyeh.",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5350,
        "text": "yâ leytehâ kâneti-lḳâḍiyeh.",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5351,
        "text": "mâ agnâ `annî mâliyeh.",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5352,
        "text": "heleke `annî sülṭâniyeh.",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5353,
        "text": "ḫuẕûhü fegullûh.",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5354,
        "text": "ŝümme-lceḥîme ṣallûh.",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5355,
        "text": "ŝümme fî silsiletin ẕer`uhâ seb`ûne ẕirâ`an feslükûh.",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5356,
        "text": "innehû kâne lâ yü'minü billâhi-l`ażîm.",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5357,
        "text": "velâ yeḥuḍḍu `alâ ṭa`âmi-lmiskîn.",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5358,
        "text": "feleyse lehü-lyevme hâhünâ ḥamîm.",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5359,
        "text": "velâ ṭa`âmün illâ min gislîn.",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5360,
        "text": "lâ ye'külühû ille-lḫâṭiûn.",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5361,
        "text": "felâ uḳsimü bimâ tübṣirûn.",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5362,
        "text": "vemâ lâ tübṣirûn.",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5363,
        "text": "innehû leḳavlü rasûlin kerîm.",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5364,
        "text": "vemâ hüve biḳavli şâ`ir. ḳalîlem mâ tü'minûn.",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5365,
        "text": "velâ biḳavli kâhin. ḳalîlem mâ teẕekkerûn.",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5366,
        "text": "tenzîlüm mir rabbi-l`âlemîn.",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5367,
        "text": "velev teḳavvele `aleynâ ba`ḍa-l'eḳâvîl.",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5368,
        "text": "leeḫaẕnâ minhü bilyemîn.",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5369,
        "text": "ŝümme leḳaṭa`nâ minhü-lvetîn.",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5370,
        "text": "femâ minküm min eḥadin `anhü ḥâcizîn.",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5371,
        "text": "veinnehû leteẕkiratül lilmütteḳîn.",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5372,
        "text": "veinnâ lena`lemü enne minküm mükeẕẕibîn.",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5373,
        "text": "veinnehû leḥasratün `ale-lkâfirîn.",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5374,
        "text": "veinnehû leḥaḳḳu-lyeḳîn.",
        "numberInSurah": 51,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5375,
        "text": "fesebbiḥ bismi rabbike-l`ażîm.",
        "numberInSurah": 52,
        "juz": 29,
