{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 32,
    "name": "سُورَةُ السَّجۡدَةِ",
    "englishName": "As-Sajda",
    "englishNameTranslation": "The Prostration",
    "revelationType": "Meccan",
    "numberOfAyahs": 30,
    "ayahs": [
      {
        "number": 3504,
        "text": "elif-lâm-mîm.",
        "numberInSurah": 1,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3505,
        "text": "tenzîlü-lkitâbi lâ raybe fîhi mir rabbi-l`âlemîn.",
        "numberInSurah": 2,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3506,
        "text": "em yeḳûlûne-fterâh. bel hüve-lḥaḳḳu mir rabbike litünẕira ḳavmem mâ etâhüm min neẕîrim min ḳablike le`allehüm yehtedûn.",
        "numberInSurah": 3,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3507,
        "text": "allâhü-lleẕî ḫaleḳa-ssemâvâti vel'arḍa vemâ beynehümâ fî sitteti eyyâmin ŝümme-stevâ `ale-l`arş. mâ leküm min dûnihî miv veliyyiv velâ şefî`. efelâ teteẕekkerûn.",
        "numberInSurah": 4,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3508,
        "text": "yüdebbiru-l'emra mine-ssemâi ile-l'arḍi ŝümme ya`rucü ileyhi fî yevmin kâne miḳdâruhû elfe senetim mimmâ te`uddûn.",
        "numberInSurah": 5,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3509,
        "text": "ẕâlike `âlimü-lgaybi veşşehâdeti-l`azîzü-rraḥîm.",
        "numberInSurah": 6,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3510,
        "text": "-lleẕî aḥsene külle şey'in ḫaleḳahû vebedee ḫalḳa-l'insâni min ṭîn.",
        "numberInSurah": 7,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3511,
        "text": "ŝümme ce`ale neslehû min sülâletim mim mâim mehîn.",
        "numberInSurah": 8,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3512,
        "text": "ŝümme sevvâhü venefeḫa fîhi mir rûḥihî vece`ale lekümü-ssem`a vel'ebṣâra vel'ef'ideh. ḳalîlem mâ teşkürûn.",
        "numberInSurah": 9,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3513,
        "text": "veḳâlû eiẕâ ḍalelnâ fi-l'arḍi einnâ lefî ḫalḳin cedîd. bel hüm biliḳâi rabbihim kâfirûn.",
        "numberInSurah": 10,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3514,
        "text": "ḳul yeteveffâküm melekü-lmevti-lleẕî vukkile biküm ŝümme ilâ rabbiküm türce`ûn.",
        "numberInSurah": 11,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3515,
        "text": "velev terâ iẕi-lmücrimûne nâkisû ruûsihim `inde rabbihim. rabbenâ ebṣarnâ vesemi`nâ ferci`nâ na`mel ṣâliḥan innâ mûḳinûn.",
        "numberInSurah": 12,
        "juz": 21,
        "manzil": 5,
        "page": 416,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3516,
        "text": "velev şi'nâ leâteynâ külle nefsin hüdâhâ velâkin ḥaḳḳa-lḳavlü minnî leemleenne cehenneme mine-lcinneti vennâsi ecme`în.",
        "numberInSurah": 13,
        "juz": 21,
        "manzil": 5,
        "page": 416,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3517,
        "text": "feẕûḳû bimâ nesîtüm liḳâe yevmiküm hâẕâ. innâ nesînâküm veẕûḳû `aẕâbe-lḫuldi bimâ küntüm ta`melûn.",
        "numberInSurah": 14,
        "juz": 21,
        "manzil": 5,
        "page": 416,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3518,
        "text": "innemâ yü'minü biâyâtine-lleẕîne iẕâ ẕükkirû bihâ ḫarrû süccedev vesebbeḥû biḥamdi rabbihim vehüm lâ yestekbirûn.",
        "numberInSurah": 15,
        "juz": 21,
        "manzil": 5,
        "page": 416,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": {
          "id": 10,
          "recommended": false,
          "obligatory": true
        }
      },
      {
        "number": 3519,
        "text": "tetecâfâ cünûbühüm `ani-lmeḍâci`i yed`ûne rabbehüm ḫavfev veṭame`â. vemimmâ razaḳnâhüm yünfiḳûn.",
        "numberInSurah": 16,
        "juz": 21,
        "manzil": 5,
        "page": 416,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3520,
        "text": "felâ ta`lemü nefsüm mâ uḫfiye lehüm min ḳurrati a`yün. cezâem bimâ kânû ya`melûn.",
        "numberInSurah": 17,
        "juz": 21,
        "manzil": 5,
        "page": 416,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3521,
        "text": "efemen kâne mü'minen kemen kâne fâsiḳâ. lâ yestevûn.",
        "numberInSurah": 18,
        "juz": 21,
        "manzil": 5,
        "page": 416,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3522,
        "text": "emme-lleẕîne âmenû ve`amilu-ṣṣâliḥâti felehüm cennâtü-lme'vâ. nüzülem bimâ kânû ya`melûn.",
        "numberInSurah": 19,
        "juz": 21,
        "manzil": 5,
        "page": 416,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3523,
        "text": "veemme-lleẕîne feseḳû feme'vâhümü-nnâr. küllemâ erâdû ey yaḫrucû minhâ ü`îdû fîhâ veḳîle lehüm ẕûḳû `aẕâbe-nnâri-lleẕî küntüm bihî tükeẕẕibûn.",
        "numberInSurah": 20,
        "juz": 21,
        "manzil": 5,
        "page": 416,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3524,
        "text": "velenüẕîḳannehüm mine-l`aẕâbi-l'ednâ dûne-l`aẕâbi-l'ekberi le`allehüm yerci`ûn.",
        "numberInSurah": 21,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3525,
        "text": "vemen ażlemü mimmen ẕükkira biâyâti rabbihî ŝümme a`raḍa `anhâ. innâ mine-lmücrimîne münteḳimûn.",
        "numberInSurah": 22,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3526,
        "text": "veleḳad âteynâ mûse-lkitâbe felâ tekün fî miryetim mil liḳâehi vece`alnâhü hüdel libenî isrâîl.",
        "numberInSurah": 23,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 360,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3527,
        "text": "vece`alnâ minhüm eimmetey yehdûne biemrinâ lemmâ ṣaberû. vekânû biâyâtinâ yûḳinûn.",
        "numberInSurah": 24,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 360,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3528,
        "text": "inne rabbeke hüve yefṣilü beynehüm yevme-lḳiyâmeti fîmâ kânû fîhi yaḫtelifûn.",
        "numberInSurah": 25,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 360,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3529,
        "text": "evelem yehdi lehüm kem ehleknâ min ḳablihim mine-lḳurûni yemşûne fî mesâkinihim. inne fî ẕâlike leâyât. efelâ yesme`ûn.",
        "numberInSurah": 26,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 360,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3530,
        "text": "evelem yerav ennâ nesûḳu-lmâe ile-l'arḍi-lcüruzi fenuḫricü bihî zer`an te'külü minhü en`âmühüm veenfüsühüm. efelâ yübṣirûn.",
        "numberInSurah": 27,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 360,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3531,
        "text": "veyeḳûlûne metâ hâẕe-lfetḥu in küntüm ṣâdiḳîn.",
        "numberInSurah": 28,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 360,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3532,
        "text": "ḳul yevme-lfetḥi lâ yenfe`u-lleẕîne keferû îmânühüm velâ hüm yünżarûn.",
        "numberInSurah": 29,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 360,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3533,
        "text": "fea`riḍ `anhüm venteżir innehüm münteżirûn.",
        "numberInSurah": 30,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 360,
        "hizbQuarter": 166,
        "sajda": false
      }
    ],
