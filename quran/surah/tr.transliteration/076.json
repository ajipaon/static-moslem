{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 76,
    "name": "سُورَةُ الإِنسَانِ",
    "englishName": "Al-Insaan",
    "englishNameTranslation": "Man",
    "revelationType": "Medinan",
    "numberOfAyahs": 31,
    "ayahs": [
      {
        "number": 5592,
        "text": "hel etâ `ale-l'insâni ḥînüm mine-ddehri lem yekün şey'em meẕkûrâ.",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5593,
        "text": "innâ ḫalaḳne-l'insâne min nuṭfetin emşâc. nebtelîhi fece`alnâhü semî`am beṣîrâ.",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5594,
        "text": "innâ hedeynâhü-ssebîle immâ şâkirav veimmâ kefûrâ.",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5595,
        "text": "innâ a`tednâ lilkâfirîne selâsile veaglâlev vese`îrâ.",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5596,
        "text": "inne-l'ebrâra yeşrabûne min ke'sin kâne mizâcühâ kâfûrâ.",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5597,
        "text": "`ayney yeşrabü bihâ `ibâdü-llâhi yüfeccirûnehâ tefcîrâ.",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5598,
        "text": "yûfûne binneẕri veyeḫâfûne yevmen kâne şerruhû müsteṭîrâ.",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5599,
        "text": "veyuṭ`imûne-ṭṭa`âme `alâ ḥubbihî miskînev veyetîmev veesîrâ.",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5600,
        "text": "innemâ nuṭ`imüküm livechi-llâhi lâ nürîdü minküm cezâev velâ şükûrâ.",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5601,
        "text": "innâ neḫâfü mir rabbinâ yevmen `abûsen ḳamṭarîrâ.",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5602,
        "text": "feveḳâhümü-llâhü şerra ẕâlike-lyevmi veleḳḳâhüm naḍratev vesürûrâ.",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5603,
        "text": "vecezâhüm bimâ ṣaberû cennetev veḥarîrâ.",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5604,
        "text": "müttekiîne fîhâ `ale-l'erâik. lâ yeravne fîhâ şemsev velâ zemherîrâ.",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5605,
        "text": "vedâniyeten `aleyhim żilâlühâ veẕüllilet ḳuṭûfühâ teẕlîlâ.",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5606,
        "text": "veyüṭâfü `aleyhim biâniyetim min fiḍḍativ veekvâbin kânet ḳavârîrâ.",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5607,
        "text": "ḳavârîrae min fiḍḍatin ḳadderûhâ taḳdîrâ.",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5608,
        "text": "veyüsḳavne fîhâ ke'sen kâne mizâcühâ zencebîlâ.",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5609,
        "text": "`aynen fîhâ tüsemmâ selsebîlâ.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5610,
        "text": "veyeṭûfü `aleyhim vildânüm müḫalledûn. iẕâ raeytehüm ḥasibtehüm lü'lüem menŝûrâ.",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5611,
        "text": "veiẕâ raeyte ŝemme raeyte ne`îmev vemülken kebîrâ.",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5612,
        "text": "`âliyehüm ŝiyâbü sündüsin ḫuḍruv veistebraḳ. veḥullû esâvira min fiḍḍah. veseḳâhüm rabbühüm şerâben ṭahûrâ.",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5613,
        "text": "inne hâẕâ kâne leküm cezâev vekâne sa`yüküm meşkûrâ.",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5614,
        "text": "innâ naḥnü nezzelnâ `aleyke-lḳur'âne tenzîlâ.",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 515,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5615,
        "text": "faṣbir liḥukmi rabbike velâ tüṭi` minhüm âŝimen ev kefûrâ.",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 515,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5616,
        "text": "veẕküri-sme rabbike bükratev veeṣîlâ.",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 515,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5617,
        "text": "vemine-lleyli fescüd lehû vesebbiḥhü leylen ṭavîlâ.",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 515,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5618,
        "text": "inne hâülâi yüḥibbûne-l`âcilete veyeẕerûne verâehüm yevmen ŝeḳîlâ.",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 515,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5619,
        "text": "naḥnü ḫalaḳnâhüm veşedednâ esrahüm. veiẕâ şi'nâ beddelnâ emŝâlehüm tebdîlâ.",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 515,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5620,
        "text": "inne hâẕihî teẕkirah. femen şâe-tteḫaẕe ilâ rabbihî sebîlâ.",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 515,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5621,
        "text": "vemâ teşâûne illâ ey yeşâe-llâh. inne-llâhe kâne `alîmen ḥakîmâ.",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 515,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5622,
        "text": "yüdḫilü mey yeşâü fî raḥmetih. veżżâlimîne e`adde lehüm `aẕâben elîmâ.",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 515,
        "hizbQuarter": 232,
        "sajda": false
      }
    ],
