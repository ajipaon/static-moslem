{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 15,
    "name": "سُورَةُ الحِجۡرِ",
    "englishName": "Al-Hijr",
    "englishNameTranslation": "The Rock",
    "revelationType": "Meccan",
    "numberOfAyahs": 99,
    "ayahs": [
      {
        "number": 1803,
        "text": "elif-lâm-râ. tilke âyâtü-lkitâbi veḳur'ânim mübîn.",
        "numberInSurah": 1,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1804,
        "text": "rubemâ yeveddü-lleẕîne keferû lev kânû müslimîn.",
        "numberInSurah": 2,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1805,
        "text": "ẕerhüm ye'külû veyetemette`û veyülhihimü-l'emelü fesevfe ya`lemûn.",
        "numberInSurah": 3,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1806,
        "text": "vemâ ehleknâ min ḳaryetin illâ velehâ kitâbüm ma`lûm.",
        "numberInSurah": 4,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1807,
        "text": "mâ tesbiḳu min ümmetin ecelehâ vemâ yeste'ḫirûn.",
        "numberInSurah": 5,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1808,
        "text": "veḳâlû yâ eyyühe-lleẕî nüzzile `aleyhi-ẕẕikru inneke lemecnûn.",
        "numberInSurah": 6,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1809,
        "text": "lev mâ te'tînâ bilmelâiketi in künte mine-ṣṣâdiḳîn.",
        "numberInSurah": 7,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1810,
        "text": "mâ nünezzilü-lmelâikete illâ bilḥaḳḳi vemâ kânû iẕem münżarîn.",
        "numberInSurah": 8,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1811,
        "text": "innâ naḥnü nezzelne-ẕẕikra veinnâ lehû leḥâfiżûn.",
        "numberInSurah": 9,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1812,
        "text": "veleḳad erselnâ min ḳablike fî şiye`i-l'evvelîn.",
        "numberInSurah": 10,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1813,
        "text": "vemâ ye'tîhim mir rasûlin illâ kânû bihî yestehziûn.",
        "numberInSurah": 11,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1814,
        "text": "keẕâlike neslükühû fî ḳulûbi-lmücrimîn.",
        "numberInSurah": 12,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1815,
        "text": "lâ yü'minûne bihî veḳad ḫalet sünnetü-l'evvelîn.",
        "numberInSurah": 13,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1816,
        "text": "velev fetaḥnâ `aleyhim bâbem mine-ssemâi feżallû fîhi ya`rucûn.",
        "numberInSurah": 14,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1817,
        "text": "leḳâlû innemâ sükkirat ebṣârunâ bel naḥnü ḳavmüm mesḥûrûn.",
        "numberInSurah": 15,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1818,
        "text": "veleḳad ce`alnâ fi-ssemâi bürûcev vezeyyennâhâ linnâżirîn.",
        "numberInSurah": 16,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1819,
        "text": "veḥafiżnâhâ min külli şeyṭânir racîm.",
        "numberInSurah": 17,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1820,
        "text": "illâ meni-steraḳa-ssem`a feetbe`ahû şihâbüm mübîn.",
        "numberInSurah": 18,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1821,
        "text": "vel'arḍa medednâhâ veelḳaynâ fîhâ ravâsiye veembetnâ fîhâ min külli şey'im mevzûn.",
        "numberInSurah": 19,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1822,
        "text": "vece`alnâ leküm fîhâ me`âyişe vemel lestüm lehû birâziḳîn.",
        "numberInSurah": 20,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1823,
        "text": "veim min şey'in illâ `indenâ ḫazâinüh. vemâ nünezzilühû illâ biḳaderim ma`lûm.",
        "numberInSurah": 21,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1824,
        "text": "veerselne-rriyâḥa levâḳiḥa feenzelnâ mine-ssemâi mâen feesḳaynâkümûh. vemâ entüm lehû biḫâzinîn.",
        "numberInSurah": 22,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1825,
        "text": "veinnâ lenaḥnü nuḥyî venümîtü venaḥnü-lvâriŝûn.",
        "numberInSurah": 23,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1826,
        "text": "veleḳad `alimne-lmüstaḳdimîne minküm veleḳad `alimne-lmüste'ḫirîn.",
        "numberInSurah": 24,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1827,
        "text": "veinne rabbeke hüve yaḥşüruhüm. innehû ḥakîmün `alîm.",
        "numberInSurah": 25,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1828,
        "text": "veleḳad ḫalaḳne-l'insâne min ṣalṣâlim min ḥameim mesnûn.",
        "numberInSurah": 26,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1829,
        "text": "velcânne ḫalaḳnâhü min ḳablü min nâri-ssemûm.",
        "numberInSurah": 27,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1830,
        "text": "veiẕ ḳâle rabbüke lilmelâiketi innî ḫâliḳum beşeram min ṣalṣâlim min ḥameim mesnûn.",
        "numberInSurah": 28,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1831,
        "text": "feiẕâ sevveytühû venefaḫtü fîhi mir rûḥî feḳa`û lehû sâcidîn.",
        "numberInSurah": 29,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1832,
        "text": "fesecede-lmelâiketü küllühüm ecme`ûn.",
        "numberInSurah": 30,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1833,
        "text": "illâ iblîs. ebâ ey yekûne me`a-ssâcidîn.",
        "numberInSurah": 31,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1834,
        "text": "ḳâle yâ iblîsü mâ leke ellâ tekûne me`a-ssâcidîn.",
        "numberInSurah": 32,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1835,
        "text": "ḳâle lem ekül liescüde libeşerin ḫalaḳtehû min ṣalṣâlim min ḥameim mesnûn.",
        "numberInSurah": 33,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1836,
        "text": "ḳâle faḫruc minhâ feinneke racîm.",
        "numberInSurah": 34,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1837,
        "text": "veinne `aleyke-lla`nete ilâ yevmi-ddîn.",
        "numberInSurah": 35,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1838,
        "text": "ḳâle rabbi feenżirnî ilâ yevmi yüb`aŝûn.",
        "numberInSurah": 36,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1839,
        "text": "ḳâle feinneke mine-lmünżarîn.",
        "numberInSurah": 37,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1840,
        "text": "ilâ yevmi-lvaḳti-lma`lûm.",
        "numberInSurah": 38,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1841,
        "text": "ḳâle rabbi bimâ agveytenî leüzeyyinenne lehüm fi-l'arḍi veleugviyennehüm ecme`în.",
        "numberInSurah": 39,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1842,
        "text": "illâ `ibâdeke minhümü-lmuḫleṣîn.",
        "numberInSurah": 40,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1843,
        "text": "ḳâle hâẕâ ṣirâṭun `aleyye müsteḳîm.",
        "numberInSurah": 41,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1844,
        "text": "inne `ibâdî leyse leke `aleyhim sülṭânün illâ meni-ttebe`ake mine-lgâvîn.",
        "numberInSurah": 42,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1845,
        "text": "veinne cehenneme lemev`idühüm ecme`în.",
        "numberInSurah": 43,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1846,
        "text": "lehâ seb`atü ebvâb. likülli bâbim minhüm cüz'üm maḳsûm.",
        "numberInSurah": 44,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1847,
        "text": "inne-lmütteḳîne fî cennâtiv ve`uyûn.",
        "numberInSurah": 45,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1848,
        "text": "üdḫulûhâ biselâmin âminîn.",
        "numberInSurah": 46,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1849,
        "text": "veneza`nâ mâ fî ṣudûrihim min gillin iḫvânen `alâ sürurim müteḳâbilîn.",
        "numberInSurah": 47,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1850,
        "text": "lâ yemessühüm fîhâ neṣabüv vemâ hüm minhâ bimuḫracîn.",
        "numberInSurah": 48,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1851,
        "text": "nebbi' `ibâdî ennî ene-lgafûru-rraḥîm.",
        "numberInSurah": 49,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1852,
        "text": "veenne `aẕâbî hüve-l`aẕâbü-l'elîm.",
        "numberInSurah": 50,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1853,
        "text": "venebbi'hüm `an ḍayfi ibrâhîm.",
        "numberInSurah": 51,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1854,
        "text": "iẕ deḫalû `aleyhi feḳâlû selâmâ. ḳâle innâ minküm vecilûn.",
        "numberInSurah": 52,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1855,
        "text": "ḳâlû lâ tevcel innâ nübeşşiruke bigulâmin `alîm.",
        "numberInSurah": 53,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1856,
        "text": "ḳâle ebeşşertümûnî `alâ em messeniye-lkiberu febime tübeşşirûn.",
        "numberInSurah": 54,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1857,
        "text": "ḳâlû beşşernâke bilḥaḳḳi felâ teküm mine-lḳâniṭîn.",
        "numberInSurah": 55,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1858,
        "text": "ḳâle vemey yaḳneṭu mir raḥmeti rabbihî ille-ḍḍâllûn.",
        "numberInSurah": 56,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1859,
        "text": "ḳâle femâ ḫaṭbüküm eyyühe-lmürselûn.",
        "numberInSurah": 57,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1860,
        "text": "ḳâlû innâ ürsilnâ ilâ ḳavmim mücrimîn.",
        "numberInSurah": 58,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1861,
        "text": "illâ âle lûṭ. innâ lemüneccûhüm ecme`în.",
        "numberInSurah": 59,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1862,
        "text": "ille-mraetehû ḳaddernâ innehâ lemine-lgâbirîn.",
        "numberInSurah": 60,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1863,
        "text": "felemmâ câe âle lûṭini-lmürselûn.",
        "numberInSurah": 61,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1864,
        "text": "ḳâle inneküm ḳavmüm münkerûn.",
        "numberInSurah": 62,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1865,
        "text": "ḳâlû bel ci'nâke bimâ kânû fîhi yemterûn.",
        "numberInSurah": 63,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1866,
        "text": "veeteynâke bilḥaḳḳi veinnâ leṣâdiḳûn.",
        "numberInSurah": 64,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1867,
        "text": "feesri biehlike biḳiṭ`im mine-lleyli vettebi` edbârahüm velâ yeltefit minküm eḥadüv vemḍû ḥayŝü tü'merûn.",
        "numberInSurah": 65,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
