{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 68,
    "name": "سُورَةُ القَلَمِ",
    "englishName": "Al-Qalam",
    "englishNameTranslation": "The Pen",
    "revelationType": "Meccan",
    "numberOfAyahs": 52,
    "ayahs": [
      {
        "number": 5272,
        "text": "nûn. velḳalemi vemâ yesṭurûn.",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5273,
        "text": "mâ ente bini`meti rabbike bimecnûn.",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5274,
        "text": "veinne leke leecran gayra memnûn.",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5275,
        "text": "veinneke le`alâ ḫulüḳin `ażîm.",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5276,
        "text": "fesetübṣiru veyübṣirûn.",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5277,
        "text": "bieyyikümü-lmeftûn.",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5278,
        "text": "inne rabbeke hüve a`lemü bimen ḍalle `an sebîlih. vehüve a`lemü bilmühtedîn.",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5279,
        "text": "felâ tüṭi`i-lmükeẕẕibîn.",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5280,
        "text": "veddû lev tüdhinü feyüdhinûn.",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5281,
        "text": "velâ tüṭi` külle ḥallâfim mehîn.",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5282,
        "text": "hemmâzim meşşâim binemîm.",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5283,
        "text": "mennâ`il lilḫayri mü`tedin eŝîm.",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5284,
        "text": "`utüllim ba`de ẕâlike zenîm.",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5285,
        "text": "en kâne ẕâ mâliv vebenîn.",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5286,
        "text": "iẕâ tütlâ `aleyhi âyâtünâ ḳâle esâṭîru-l'evvelîn.",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5287,
        "text": "senesimühû `ale-lḫurṭûm.",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5288,
        "text": "innâ belevnâhüm kemâ belevnâ aṣḥâbe-lcenneh. iẕ aḳsemû leyaṣrimünnehâ muṣbiḥîn.",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5289,
        "text": "velâ yesteŝnûn.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5290,
        "text": "feṭâfe `aleyhâ ṭâifüm mir rabbike vehüm nâimûn.",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5291,
        "text": "feaṣbeḥat keṣṣarîm.",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5292,
        "text": "fetenâdev muṣbiḥîn.",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5293,
        "text": "eni-gdû `alâ ḥarŝiküm in küntüm ṣârimîn.",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5294,
        "text": "fenṭaleḳû vehüm yeteḫâfetûn.",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5295,
        "text": "el lâ yedḫulennehe-lyevme `aleyküm miskîn.",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5296,
        "text": "vegadev `alâ ḥardin ḳâdirîn.",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5297,
        "text": "felemmâ raevhâ ḳâlû innâ leḍâllûn.",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5298,
        "text": "bel naḥnü maḥrûmûn.",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5299,
        "text": "ḳâle evseṭuhüm elem eḳul leküm levlâ tüsebbiḥûn.",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5300,
        "text": "ḳâlû sübḥâne rabbinâ innâ künnâ żâlimîn.",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5301,
        "text": "feaḳbele ba`ḍuhüm `alâ ba`ḍiy yetelâvemûn.",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5302,
        "text": "ḳâlû yâ veylenâ innâ künnâ ṭâgîn.",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5303,
        "text": "`asâ rabbünâ ey yübdilenâ ḫayram minhâ innâ ilâ rabbinâ râgibûn.",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5304,
        "text": "keẕâlike-l`aẕâb. vele`aẕâbü-l'âḫirati ekber. lev kânû ya`lemûn.",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5305,
        "text": "inne lilmütteḳîne `inde rabbihim cennâti-nne`îm.",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5306,
        "text": "efenec`alü-lmüslimîne kelmücrimîn.",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5307,
        "text": "mâ leküm. keyfe taḥkümûn.",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5308,
        "text": "em leküm kitâbün fîhi tedrusûn.",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5309,
        "text": "inne leküm fîhi lemâ teḫayyerûn.",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5310,
        "text": "em leküm eymânün `aleynâ bâligatün ilâ yevmi-lḳiyâmeti inne leküm lemâ taḥkümûn.",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5311,
        "text": "selhüm eyyühüm biẕâlike za`îm.",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5312,
        "text": "em lehüm şürakâ'. felye'tû bişürakâihim in kânû ṣâdiḳîn.",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5313,
        "text": "yevme yükşefü `an sâḳiv veyüd`avne ile-ssücûdi felâ yesteṭî`ûn.",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5314,
        "text": "ḫâşi`aten ebṣâruhüm terheḳuhüm ẕilleh. veḳad kânû yüd`avne ile-ssücûdi vehüm sâlimûn.",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5315,
        "text": "feẕernî vemey yükeẕẕibü bihâẕe-lḥadîŝ. senestedricühüm min ḥayŝü lâ ya`lemûn.",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5316,
        "text": "veümlî lehüm. inne keydî metîn.",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5317,
        "text": "em tes'elühüm ecran fehüm mim magramim müŝḳalûn.",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5318,
        "text": "em `indehümü-lgaybü fehüm yektübûn.",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5319,
        "text": "faṣbir liḥukmi rabbike velâ tekün keṣâḥibi-lḥût. iẕ nâdâ vehüve mekżûm.",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5320,
        "text": "levlâ en tedârakehû ni`metüm mir rabbihî lenübiẕe bil`arâi vehüve meẕmûm.",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5321,
        "text": "fectebâhü rabbühû fece`alehû mine-ṣṣâliḥîn.",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5322,
        "text": "veiy yekâdü-lleẕîne keferû leyüzliḳûneke biebṣârihim lemmâ semi`ü-ẕẕikra veyeḳûlûne innehû lemecnûn.",
        "numberInSurah": 51,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5323,
        "text": "vemâ hüve illâ ẕikrul lil`âlemîn.",
        "numberInSurah": 52,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      }
    ],
    "edition": {
