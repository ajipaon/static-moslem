{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 23,
    "name": "سُورَةُ المُؤۡمِنُونَ",
    "englishName": "Al-Muminoon",
    "englishNameTranslation": "The Believers",
    "revelationType": "Meccan",
    "numberOfAyahs": 118,
    "ayahs": [
      {
        "number": 2674,
        "text": "ḳad efleḥa-lmü'minûn.",
        "numberInSurah": 1,
        "juz": 18,
        "manzil": 4,
        "page": 342,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2675,
        "text": "elleẕîne hüm fî ṣalâtihim ḫâşi`ûn.",
        "numberInSurah": 2,
        "juz": 18,
        "manzil": 4,
        "page": 342,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2676,
        "text": "velleẕîne hüm `ani-llagvi mü`riḍûn.",
        "numberInSurah": 3,
        "juz": 18,
        "manzil": 4,
        "page": 342,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2677,
        "text": "velleẕîne hüm lilzekâti fâ`ilûn.",
        "numberInSurah": 4,
        "juz": 18,
        "manzil": 4,
        "page": 342,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2678,
        "text": "velleẕîne hüm lifürûcihim ḥâfiżûn.",
        "numberInSurah": 5,
        "juz": 18,
        "manzil": 4,
        "page": 342,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2679,
        "text": "illâ `alâ ezvâcihim ev mâ meleket eymânühüm feinnehüm gayru melûmîn.",
        "numberInSurah": 6,
        "juz": 18,
        "manzil": 4,
        "page": 342,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2680,
        "text": "femeni-btegâ verâe ẕâlike feülâike hümü-l`âdûn.",
        "numberInSurah": 7,
        "juz": 18,
        "manzil": 4,
        "page": 342,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2681,
        "text": "velleẕîne hüm liemânâtihim ve`ahdihim râ`ûn.",
        "numberInSurah": 8,
        "juz": 18,
        "manzil": 4,
        "page": 342,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2682,
        "text": "velleẕîne hüm `alâ ṣalevâtihim yüḥâfiżûn.",
        "numberInSurah": 9,
        "juz": 18,
        "manzil": 4,
        "page": 342,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2683,
        "text": "ülâike hümü-lvâriŝûn.",
        "numberInSurah": 10,
        "juz": 18,
        "manzil": 4,
        "page": 342,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2684,
        "text": "elleẕîne yeriŝûne-lfirdevs. hüm fîhâ ḫâlidûn.",
        "numberInSurah": 11,
        "juz": 18,
        "manzil": 4,
        "page": 342,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2685,
        "text": "veleḳad ḫalaḳne-l'insâne min sülâletim min ṭîn.",
        "numberInSurah": 12,
        "juz": 18,
        "manzil": 4,
        "page": 342,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2686,
        "text": "ŝümme ce`alnâhü nuṭfeten fî ḳarârim mekîn.",
        "numberInSurah": 13,
        "juz": 18,
        "manzil": 4,
        "page": 342,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2687,
        "text": "ŝümme ḫalaḳne-nnuṭfete `aleḳaten feḫalaḳne-l`aleḳate muḍgaten feḫalaḳne-lmuḍgate `iżâmen fekesevne-l`iżâme laḥmâ. ŝümme enşe'nâhü ḫalḳan âḫar. fetebârake-llâhü aḥsenü-lḫâliḳîn.",
        "numberInSurah": 14,
        "juz": 18,
        "manzil": 4,
        "page": 342,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2688,
        "text": "ŝümme inneküm ba`de ẕâlike lemeyyitûn.",
        "numberInSurah": 15,
        "juz": 18,
        "manzil": 4,
        "page": 342,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2689,
        "text": "ŝümme inneküm yevme-lḳiyâmeti tüb`aŝûn.",
        "numberInSurah": 16,
        "juz": 18,
        "manzil": 4,
        "page": 342,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2690,
        "text": "veleḳad ḫalaḳnâ fevḳaküm seb`a ṭarâiḳ. vemâ künnâ `ani-lḫalḳi gâfilîn.",
        "numberInSurah": 17,
        "juz": 18,
        "manzil": 4,
        "page": 342,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2691,
        "text": "veenzelnâ mine-ssemâi mâem biḳaderin feeskennâhü fi-l'arḍ. veinnâ `alâ ẕehâbim bihî leḳâdirûn.",
        "numberInSurah": 18,
        "juz": 18,
        "manzil": 4,
        "page": 343,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2692,
        "text": "feenşe'nâ leküm bihî cennâtim min neḫîliv vea`nâb. leküm fîhâ fevâkihü keŝîratüv veminhâ te'külûn.",
        "numberInSurah": 19,
        "juz": 18,
        "manzil": 4,
        "page": 343,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2693,
        "text": "veşeceraten taḫrucü min ṭûri seynâe tembütü biddühni veṣibgil lil'âkilîn.",
        "numberInSurah": 20,
        "juz": 18,
        "manzil": 4,
        "page": 343,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2694,
        "text": "veinne leküm fi-l'en`âmi le`ibrah. nüsḳîküm mimmâ fî büṭûnihâ veleküm fîhâ menâfi`u keŝîratüv veminhâ te'külûn.",
        "numberInSurah": 21,
        "juz": 18,
        "manzil": 4,
        "page": 343,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2695,
        "text": "ve`aleyhâ ve`ale-lfülki tuḥmelûn.",
        "numberInSurah": 22,
        "juz": 18,
        "manzil": 4,
        "page": 343,
        "ruku": 295,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2696,
        "text": "veleḳad erselnâ nûḥan ilâ ḳavmihî feḳâle yâ ḳavmi-`büdü-llâhe mâ leküm min ilâhin gayruh. efelâ tetteḳûn.",
        "numberInSurah": 23,
        "juz": 18,
        "manzil": 4,
        "page": 343,
        "ruku": 296,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2697,
        "text": "feḳâle-lmeleü-lleẕîne keferû min ḳavmihî mâ hâẕâ illâ beşerum miŝlüküm yürîdü ey yetefeḍḍale `aleyküm. velev şâe-llâhü leenzele melâikeh. mâ semi`nâ bihâẕâ fî âbâine-l'evvelîn.",
        "numberInSurah": 24,
        "juz": 18,
        "manzil": 4,
        "page": 343,
        "ruku": 296,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2698,
        "text": "in hüve illâ racülüm bihî cinnetün feterabbeṣû bihî ḥattâ ḥîn.",
        "numberInSurah": 25,
        "juz": 18,
        "manzil": 4,
        "page": 343,
        "ruku": 296,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2699,
        "text": "ḳâle rabbi-nṣurnî bimâ keẕẕebûn.",
        "numberInSurah": 26,
        "juz": 18,
        "manzil": 4,
        "page": 343,
        "ruku": 296,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2700,
        "text": "feevḥaynâ ileyhi eni-ṣne`i-lfülke bia`yüninâ vevaḥyinâ feiẕâ câe emrunâ vefâra-ttennûru feslük fîhâ min küllin zevceyni-ŝneyni veehleke illâ men sebeḳa `aleyhi-lḳavlü minhüm. velâ tüḫâṭibnî fi-lleẕîne żalemû. innehüm mugraḳûn.",
        "numberInSurah": 27,
        "juz": 18,
        "manzil": 4,
        "page": 343,
        "ruku": 296,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2701,
        "text": "feiẕe-steveyte ente vemem me`ake `ale-lfülki feḳuli-lḥamdü lillâhi-lleẕî neccânâ mine-lḳavmi-żżâlimîn.",
        "numberInSurah": 28,
        "juz": 18,
        "manzil": 4,
        "page": 344,
        "ruku": 296,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2702,
        "text": "veḳur rabbi enzilnî münzelem mübârakev veente ḫayru-lmünzilîn.",
        "numberInSurah": 29,
        "juz": 18,
        "manzil": 4,
        "page": 344,
        "ruku": 296,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2703,
        "text": "inne fî ẕâlike leâyâtiv vein künnâ lemübtelîn.",
        "numberInSurah": 30,
        "juz": 18,
        "manzil": 4,
        "page": 344,
        "ruku": 296,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2704,
        "text": "ŝümme enşe'nâ mim ba`dihim ḳarnen âḫarîn.",
        "numberInSurah": 31,
        "juz": 18,
        "manzil": 4,
        "page": 344,
        "ruku": 296,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2705,
        "text": "feerselnâ fîhim rasûlem minhüm eni-`büdü-llâhe mâ leküm min ilâhin gayruh. efelâ tetteḳûn.",
        "numberInSurah": 32,
        "juz": 18,
        "manzil": 4,
        "page": 344,
        "ruku": 296,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2706,
        "text": "veḳâle-lmeleü min ḳavmihi-lleẕîne keferû vekeẕẕebû biliḳâi-l'âḫirati veetrafnâhüm fi-lḥayâti-ddünyâ mâ hâẕâ illâ beşerum miŝlüküm ye'külü mimmâ te'külûne minhü veyeşrabü mimmâ teşrabûn.",
        "numberInSurah": 33,
        "juz": 18,
        "manzil": 4,
        "page": 344,
        "ruku": 297,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2707,
        "text": "velein eṭa`tüm beşeram miŝleküm inneküm iẕel leḫâsirûn.",
        "numberInSurah": 34,
        "juz": 18,
        "manzil": 4,
        "page": 344,
        "ruku": 297,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2708,
        "text": "eye`idüküm enneküm iẕâ mittüm veküntüm türâbev ve`iżâmen enneküm muḫracûn.",
        "numberInSurah": 35,
        "juz": 18,
        "manzil": 4,
        "page": 344,
        "ruku": 297,
        "hizbQuarter": 137,
        "sajda": false
      },
      {
        "number": 2709,
        "text": "heyhâte heyhâte limâ tû`adûn.",
        "numberInSurah": 36,
        "juz": 18,
        "manzil": 4,
        "page": 344,
        "ruku": 297,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2710,
        "text": "in hiye illâ ḥayâtüne-ddünyâ nemûtü venaḥyâ vemâ naḥnü bimeb`ûŝîn.",
        "numberInSurah": 37,
        "juz": 18,
        "manzil": 4,
        "page": 344,
        "ruku": 297,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2711,
        "text": "in hüve illâ racülün-fterâ `ale-llâhi keẕibev vemâ naḥnü lehû bimü'minîn.",
        "numberInSurah": 38,
        "juz": 18,
        "manzil": 4,
        "page": 344,
        "ruku": 297,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2712,
        "text": "ḳâle rabbi-nṣurnî bimâ keẕẕebûn.",
        "numberInSurah": 39,
        "juz": 18,
        "manzil": 4,
        "page": 344,
        "ruku": 297,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2713,
        "text": "ḳâle `ammâ ḳalîlil leyuṣbiḥunne nâdimîn.",
        "numberInSurah": 40,
        "juz": 18,
        "manzil": 4,
        "page": 344,
        "ruku": 297,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2714,
        "text": "feeḫaẕethümu-ṣṣayḥatü bilḥaḳḳi fece`alnâhüm guŝââ. febü`del lilḳavmi-żżâlimîn.",
        "numberInSurah": 41,
        "juz": 18,
        "manzil": 4,
        "page": 344,
        "ruku": 297,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2715,
        "text": "ŝümme enşe'nâ mim ba`dihim ḳurûnen âḫarîn.",
        "numberInSurah": 42,
        "juz": 18,
        "manzil": 4,
        "page": 344,
        "ruku": 297,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2716,
        "text": "mâ tesbiḳu min ümmetin ecelehâ vemâ yeste'ḫirûn.",
        "numberInSurah": 43,
        "juz": 18,
        "manzil": 4,
        "page": 345,
        "ruku": 297,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2717,
        "text": "ŝümme erselnâ rusülenâ tetrâ. küllemâ câe ümmeter rasûlühâ keẕẕebûhü feetba`nâ ba`ḍahüm ba`ḍav vece`alnâhüm eḥâdîŝ. febü`del liḳavmil lâ yü'minûn.",
        "numberInSurah": 44,
        "juz": 18,
        "manzil": 4,
        "page": 345,
        "ruku": 297,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2718,
        "text": "ŝümme erselnâ mûsâ veeḫâhü hârûne biâyâtinâ vesülṭânim mübîn.",
        "numberInSurah": 45,
        "juz": 18,
        "manzil": 4,
        "page": 345,
        "ruku": 297,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2719,
        "text": "ilâ fir`avne vemeleihî festekberû vekânû ḳavmen `âlîn.",
        "numberInSurah": 46,
        "juz": 18,
        "manzil": 4,
        "page": 345,
        "ruku": 297,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2720,
        "text": "feḳâlû enü'minü libeşerayni miŝlinâ veḳavmühümâ lenâ `âbidûn.",
        "numberInSurah": 47,
        "juz": 18,
        "manzil": 4,
        "page": 345,
        "ruku": 297,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2721,
        "text": "fekeẕẕebûhümâ fekânû mine-lmühlekîn.",
        "numberInSurah": 48,
        "juz": 18,
        "manzil": 4,
        "page": 345,
        "ruku": 297,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2722,
        "text": "veleḳad âteynâ mûse-lkitâbe le`allehüm yehtedûn.",
        "numberInSurah": 49,
        "juz": 18,
        "manzil": 4,
        "page": 345,
        "ruku": 297,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2723,
        "text": "vece`alne-bne meryeme veümmehû âyetev veâveynâhümâ ilâ rabvetin ẕâti ḳarâriv veme`în.",
        "numberInSurah": 50,
        "juz": 18,
        "manzil": 4,
        "page": 345,
        "ruku": 297,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2724,
        "text": "yâ eyyühe-rrusülü külû mine-ṭṭayyibâti va`melû ṣâliḥâ. innî bimâ ta`melûne `alîm.",
        "numberInSurah": 51,
        "juz": 18,
        "manzil": 4,
        "page": 345,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2725,
        "text": "veinne hâẕihî ümmetüküm ümmetev vâḥidetev veenâ rabbüküm fetteḳûn.",
        "numberInSurah": 52,
        "juz": 18,
        "manzil": 4,
        "page": 345,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2726,
        "text": "feteḳaṭṭa`û emrahüm beynehüm zübürâ. küllü ḥizbim bimâ ledeyhim feriḥûn.",
        "numberInSurah": 53,
        "juz": 18,
        "manzil": 4,
        "page": 345,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2727,
        "text": "feẕerhüm fî gamratihim ḥattâ ḥîn.",
        "numberInSurah": 54,
        "juz": 18,
        "manzil": 4,
        "page": 345,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2728,
        "text": "eyaḥsebûne ennemâ nümiddühüm bihî mim mâliv vebenîn.",
        "numberInSurah": 55,
        "juz": 18,
        "manzil": 4,
        "page": 345,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2729,
        "text": "nüsâri`u lehüm fi-lḫayrât. bel lâ yeş`urûn.",
        "numberInSurah": 56,
        "juz": 18,
        "manzil": 4,
        "page": 345,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2730,
        "text": "inne-lleẕîne hüm min ḫaşyeti rabbihim müşfiḳûn.",
        "numberInSurah": 57,
        "juz": 18,
        "manzil": 4,
        "page": 345,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2731,
        "text": "velleẕîne hüm biâyâti rabbihim yü'minûn.",
        "numberInSurah": 58,
        "juz": 18,
        "manzil": 4,
        "page": 345,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2732,
        "text": "velleẕîne hüm birabbihim lâ yüşrikûn.",
        "numberInSurah": 59,
        "juz": 18,
        "manzil": 4,
        "page": 345,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2733,
        "text": "velleẕîne yü'tûne mâ âtev veḳulûbühüm veciletün ennehüm ilâ rabbihim râci`ûn.",
        "numberInSurah": 60,
        "juz": 18,
        "manzil": 4,
        "page": 346,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2734,
        "text": "ülâike yüsâri`ûne fi-lḫayrâti vehüm lehâ sâbiḳûn.",
        "numberInSurah": 61,
        "juz": 18,
        "manzil": 4,
        "page": 346,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2735,
        "text": "velâ nükellifü nefsen illâ vus`ahâ veledeynâ kitâbüy yenṭiḳu bilḥaḳḳi vehüm lâ yużlemûn.",
        "numberInSurah": 62,
        "juz": 18,
        "manzil": 4,
        "page": 346,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2736,
        "text": "bel ḳulûbühüm fî gamratim min hâẕâ velehüm a`mâlüm min dûni ẕâlike hüm lehâ `âmilûn.",
        "numberInSurah": 63,
        "juz": 18,
        "manzil": 4,
        "page": 346,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2737,
        "text": "ḥattâ iẕâ eḫaẕnâ mütrafîhim bil`aẕâbi iẕâ hüm yec'erûn.",
        "numberInSurah": 64,
        "juz": 18,
        "manzil": 4,
        "page": 346,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2738,
        "text": "lâ tec'erü-lyevme inneküm minnâ lâ tünṣarûn.",
        "numberInSurah": 65,
        "juz": 18,
        "manzil": 4,
        "page": 346,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2739,
        "text": "ḳad kânet âyâtî tütlâ `aleyküm feküntüm `alâ a`ḳâbiküm tenkisûn.",
        "numberInSurah": 66,
        "juz": 18,
        "manzil": 4,
        "page": 346,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2740,
        "text": "müstekbirîne bih. sâmiran tehcürûn.",
        "numberInSurah": 67,
        "juz": 18,
        "manzil": 4,
        "page": 346,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2741,
        "text": "efelem yeddebberü-lḳavle em câehüm mâ lem ye'ti âbâehümü-l'evvelîn.",
        "numberInSurah": 68,
        "juz": 18,
        "manzil": 4,
        "page": 346,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2742,
        "text": "em lem ya`rifû rasûlehüm fehüm lehû münkirûn.",
        "numberInSurah": 69,
        "juz": 18,
        "manzil": 4,
        "page": 346,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2743,
        "text": "em yeḳûlûne bihî cinneh. bel câehüm bilḥaḳḳi veekŝeruhüm lilḥaḳḳi kârihûn.",
        "numberInSurah": 70,
        "juz": 18,
        "manzil": 4,
        "page": 346,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2744,
        "text": "velevi-ttebe`a-lḥaḳḳu ehvâehüm lefesedeti-ssemâvâtü vel'arḍu vemen fîhinn. bel eteynâhüm biẕikrihim fehüm `an ẕikrihim mü`riḍûn.",
        "numberInSurah": 71,
        "juz": 18,
        "manzil": 4,
        "page": 346,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2745,
        "text": "em tes'elühüm ḫarcen feḫarâcü rabbike ḫayr. vehüve ḫayru-rrâziḳîn.",
        "numberInSurah": 72,
        "juz": 18,
        "manzil": 4,
        "page": 346,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2746,
        "text": "veinneke leted`ûhüm ilâ ṣirâṭim müsteḳîm.",
        "numberInSurah": 73,
        "juz": 18,
        "manzil": 4,
        "page": 346,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2747,
        "text": "veinne-lleẕîne lâ yü'minûne bil'âḫirati `ani-ṣṣirâṭi lenâkibûn.",
        "numberInSurah": 74,
        "juz": 18,
        "manzil": 4,
        "page": 346,
        "ruku": 298,
        "hizbQuarter": 138,
        "sajda": false
      },
      {
        "number": 2748,
        "text": "velev raḥimnâhüm vekeşefnâ mâ bihim min ḍurril leleccû fî ṭugyânihim ya`mehûn.",
        "numberInSurah": 75,
        "juz": 18,
        "manzil": 4,
        "page": 347,
        "ruku": 298,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2749,
        "text": "veleḳad eḫaẕnâhüm bil`aẕâbi feme-stekânû lirabbihim vemâ yeteḍarra`ûn.",
        "numberInSurah": 76,
        "juz": 18,
        "manzil": 4,
        "page": 347,
        "ruku": 298,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2750,
        "text": "ḥattâ iẕâ fetaḥnâ `aleyhim bâben ẕâ `aẕâbin şedîdin iẕâ hüm fîhi müblisûn.",
        "numberInSurah": 77,
        "juz": 18,
        "manzil": 4,
        "page": 347,
        "ruku": 298,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2751,
        "text": "vehüve-lleẕî enşee lekümü-ssem`a vel'ebṣâra vel'ef'ideh. ḳalîlem mâ teşkürûn.",
        "numberInSurah": 78,
        "juz": 18,
        "manzil": 4,
        "page": 347,
        "ruku": 299,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2752,
        "text": "vehüve-lleẕî ẕera'eküm fi-l'arḍi veileyhi tuḥşerûn.",
        "numberInSurah": 79,
        "juz": 18,
        "manzil": 4,
        "page": 347,
        "ruku": 299,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2753,
        "text": "vehüve-lleẕî yuḥyî veyümîtü velehu-ḫtilâfü-lleyli vennehâr. efelâ ta`ḳilûn.",
        "numberInSurah": 80,
        "juz": 18,
        "manzil": 4,
        "page": 347,
        "ruku": 299,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2754,
        "text": "bel ḳâlû miŝle mâ ḳâle-l'evvelûn.",
        "numberInSurah": 81,
        "juz": 18,
        "manzil": 4,
        "page": 347,
        "ruku": 299,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2755,
        "text": "ḳâlû eiẕâ mitnâ vekünnâ türâbev ve`iżâmen einnâ lemeb`ûŝûn.",
        "numberInSurah": 82,
        "juz": 18,
        "manzil": 4,
        "page": 347,
        "ruku": 299,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2756,
        "text": "leḳad vu`idnâ naḥnü veâbâünâ hâẕâ min ḳablü in hâẕâ illâ esâṭîru-l'evvelîn.",
        "numberInSurah": 83,
        "juz": 18,
        "manzil": 4,
        "page": 347,
        "ruku": 299,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2757,
        "text": "ḳul limeni-l'arḍu vemen fîhâ in küntüm ta`lemûn.",
        "numberInSurah": 84,
        "juz": 18,
        "manzil": 4,
        "page": 347,
        "ruku": 299,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2758,
        "text": "seyeḳûlûne lillâh. ḳul efelâ teẕekkerûn.",
        "numberInSurah": 85,
        "juz": 18,
        "manzil": 4,
        "page": 347,
        "ruku": 299,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2759,
        "text": "ḳul mer rabbü-ssemâvâti-sseb`i verabbü-l`arşi-l`ażîm.",
        "numberInSurah": 86,
        "juz": 18,
        "manzil": 4,
        "page": 347,
        "ruku": 299,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2760,
        "text": "seyeḳûlûne lillâh. ḳul efelâ tetteḳûn.",
        "numberInSurah": 87,
        "juz": 18,
        "manzil": 4,
        "page": 347,
        "ruku": 299,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2761,
        "text": "ḳul mem biyedihî melekûtü külli şey'iv vehüve yücîru velâ yücâru `aleyhi in küntüm ta`lemûn.",
        "numberInSurah": 88,
        "juz": 18,
        "manzil": 4,
        "page": 347,
        "ruku": 299,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2762,
        "text": "seyeḳûlûne lillâh. ḳul feennâ tüsḥarûn.",
        "numberInSurah": 89,
        "juz": 18,
        "manzil": 4,
        "page": 347,
        "ruku": 299,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2763,
        "text": "bel eteynâhüm bilḥaḳḳi veinnehüm lekâẕibûn.",
        "numberInSurah": 90,
        "juz": 18,
        "manzil": 4,
        "page": 348,
        "ruku": 299,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2764,
        "text": "me-tteḫaẕe-llâhü miv velediv vemâ kâne me`ahû min ilâhin iẕel leẕehebe küllü ilâhim bimâ ḫaleḳa vele`alâ ba`ḍuhüm `alâ ba`ḍ. sübḥâne-llâhi `ammâ yeṣifûn.",
        "numberInSurah": 91,
        "juz": 18,
        "manzil": 4,
        "page": 348,
        "ruku": 299,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2765,
        "text": "`âlimi-lgaybi veşşehâdeti fete`âlâ `ammâ yüşrikûn.",
        "numberInSurah": 92,
        "juz": 18,
        "manzil": 4,
        "page": 348,
        "ruku": 299,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2766,
        "text": "ḳur rabbi immâ türiyennî mâ yû`adûn.",
        "numberInSurah": 93,
        "juz": 18,
        "manzil": 4,
        "page": 348,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2767,
        "text": "rabbi felâ tec`alnî fi-lḳavmi-żżâlimîn.",
        "numberInSurah": 94,
        "juz": 18,
        "manzil": 4,
        "page": 348,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2768,
        "text": "veinnâ `alâ en nüriyeke mâ ne`idühüm leḳâdirûn.",
        "numberInSurah": 95,
        "juz": 18,
        "manzil": 4,
        "page": 348,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2769,
        "text": "idfa` billetî hiye aḥsenü-sseyyieh. naḥnü a`lemü bimâ yeṣifûn.",
        "numberInSurah": 96,
        "juz": 18,
        "manzil": 4,
        "page": 348,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2770,
        "text": "veḳur rabbi e`ûẕü bike min hemezâti-şşeyâṭîn.",
        "numberInSurah": 97,
        "juz": 18,
        "manzil": 4,
        "page": 348,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2771,
        "text": "vee`ûẕü bike rabbi ey yaḥḍurûn.",
        "numberInSurah": 98,
        "juz": 18,
        "manzil": 4,
        "page": 348,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2772,
        "text": "ḥattâ iẕâ câe eḥadehümü-lmevtü ḳâle rabbi-rci`ûn.",
        "numberInSurah": 99,
        "juz": 18,
        "manzil": 4,
        "page": 348,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2773,
        "text": "le`allî a`melü ṣâliḥan fîmâ teraktü kellâ. innehâ kelimetün hüve ḳâilühâ. vemiv verâihim berzeḫun ilâ yevmi yüb`aŝûn.",
        "numberInSurah": 100,
        "juz": 18,
        "manzil": 4,
        "page": 348,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2774,
        "text": "feiẕâ nüfiḫa fi-ṣṣûri felâ ensâbe beynehüm yevmeiẕiv velâ yetesâelûn.",
        "numberInSurah": 101,
        "juz": 18,
        "manzil": 4,
        "page": 348,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2775,
        "text": "femen ŝeḳulet mevâzînühû feülâike hümü-lmüfliḥûn.",
        "numberInSurah": 102,
        "juz": 18,
        "manzil": 4,
        "page": 348,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2776,
        "text": "vemen ḫaffet mevâzînühû feülâike-lleẕîne ḫasirû enfüsehüm fî cehenneme ḫâlidûn.",
        "numberInSurah": 103,
        "juz": 18,
        "manzil": 4,
        "page": 348,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2777,
        "text": "telfeḥu vucûhehümü-nnâru vehüm fîhâ kâliḥûn.",
        "numberInSurah": 104,
        "juz": 18,
        "manzil": 4,
        "page": 348,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2778,
        "text": "elem tekün âyâtî tütlâ `aleyküm feküntüm bihâ tükeẕẕibûn.",
        "numberInSurah": 105,
        "juz": 18,
        "manzil": 4,
        "page": 349,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2779,
        "text": "ḳâlû rabbenâ galebet `aleynâ şiḳvetünâ vekünnâ ḳavmen ḍâllîn.",
        "numberInSurah": 106,
        "juz": 18,
        "manzil": 4,
        "page": 349,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2780,
        "text": "rabbenâ aḫricnâ minhâ fein `udnâ feinnâ żâlimûn.",
        "numberInSurah": 107,
        "juz": 18,
        "manzil": 4,
        "page": 349,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2781,
        "text": "ḳâle-ḫseû fîhâ velâ tükellimûn.",
        "numberInSurah": 108,
        "juz": 18,
        "manzil": 4,
        "page": 349,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2782,
        "text": "innehû kâne ferîḳum min `ibâdî yeḳûlûne rabbenâ âmennâ fagfir lenâ verḥamnâ veente ḫayru-rrâḥimîn.",
        "numberInSurah": 109,
        "juz": 18,
        "manzil": 4,
        "page": 349,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2783,
        "text": "fetteḫaẕtümûhüm siḫriyyen ḥattâ ensevküm ẕikrî veküntüm minhüm taḍḥakûn.",
        "numberInSurah": 110,
        "juz": 18,
        "manzil": 4,
        "page": 349,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2784,
        "text": "innî cezeytühümü-lyevme bimâ ṣaberû ennehüm hümü-lfâizûn.",
        "numberInSurah": 111,
        "juz": 18,
        "manzil": 4,
        "page": 349,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2785,
        "text": "ḳâle kem lebiŝtüm fi-l'arḍi `adede sinîn.",
        "numberInSurah": 112,
        "juz": 18,
        "manzil": 4,
        "page": 349,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2786,
        "text": "ḳâlû lebiŝnâ yevmen ev ba`ḍa yevmin fes'eli-l`âddîn.",
        "numberInSurah": 113,
        "juz": 18,
        "manzil": 4,
        "page": 349,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2787,
        "text": "ḳâle il lebiŝtüm illâ ḳalîlel lev enneküm küntüm ta`lemûn.",
        "numberInSurah": 114,
        "juz": 18,
        "manzil": 4,
        "page": 349,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2788,
        "text": "efeḥasibtüm ennemâ ḫalaḳnâküm `abeŝev veenneküm ileynâ lâ türce`ûn.",
        "numberInSurah": 115,
        "juz": 18,
        "manzil": 4,
        "page": 349,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2789,
        "text": "fete`âle-llâhü-lmelikü-lḥaḳḳ. lâ ilâhe illâ hû. rabbü-l`arşi-lkerîm.",
        "numberInSurah": 116,
        "juz": 18,
        "manzil": 4,
        "page": 349,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2790,
        "text": "vemey yed`u me`a-llâhi ilâhen âḫara lâ bürhâne lehû bihî feinnemâ ḥisâbühû `inde rabbih. innehû lâ yüfliḥu-lkâfirûn.",
        "numberInSurah": 117,
        "juz": 18,
        "manzil": 4,
        "page": 349,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2791,
        "text": "veḳur rabbi-gfir verḥam veente ḫayru-rrâḥimîn.",
        "numberInSurah": 118,
        "juz": 18,
        "manzil": 4,
        "page": 349,
        "ruku": 300,
        "hizbQuarter": 139,
        "sajda": false
      }
