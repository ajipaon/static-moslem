{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 77,
    "name": "سُورَةُ المُرۡسَلَاتِ",
    "englishName": "Al-Mursalaat",
    "englishNameTranslation": "The Emissaries",
    "revelationType": "Meccan",
    "numberOfAyahs": 50,
    "ayahs": [
      {
        "number": 5623,
        "text": "Ved den som sendes, den ene etter den andre,",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5624,
        "text": "som stormer frem,",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5625,
        "text": "som brer ut",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5626,
        "text": "og skiller ut,",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5627,
        "text": "og kaster ut en formaning",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5628,
        "text": "til unnskyldning eller advarsel!",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5629,
        "text": "Det dere er stilt i utsikt, vil visselig inntreffe.",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5630,
        "text": "Når stjernene slukkes,",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5631,
        "text": "når himmelen revner,",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5632,
        "text": "når fjellene pulveriseres",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5633,
        "text": "og sendebudenes tid fastsettes,",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5634,
        "text": "– til hvilken dag er det berammet?",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5635,
        "text": "Til dommens dag!",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5636,
        "text": "Hvordan kan du vite hva dommens dag er?",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5637,
        "text": "Ve dem som holder sannhet for løgn på denne dag!",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5638,
        "text": "Har Vi ikke utslettet de tidligere?",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5639,
        "text": "Så lar Vi de senere følge dem.",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5640,
        "text": "Slik gjør Vi med synderne.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5641,
        "text": "Ve dem som holder sannhet for løgn på denne dag!",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5642,
        "text": "Har Vi ikke skapt dere av en skrøpelig væske?",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5643,
        "text": "Og plassert den i sikker forvaring,",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5644,
        "text": "til en bestemt tid?",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5645,
        "text": "Vi bestemte, og Vi bestemmer godt!",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5646,
        "text": "Ve dem som holder sannhet for løgn på denne dag!",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5647,
        "text": "Har Vi ikke gjort jorden til et samlingssted",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5648,
        "text": "for levende og døde?",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5649,
        "text": "Vi satte bergmassiver på den. Vi har gitt dere ferskt vann å drikke.",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5650,
        "text": "Ve dem som holder sannhet for løgn på denne dag!",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5651,
        "text": "«Gå hen til det dere holdt for løgn!",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5652,
        "text": "Gå hen til den trefoldige skygge,",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5653,
        "text": "som ikke svaler eller hjelper mot flammen,",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5654,
        "text": "som kaster gnister store som vedkubber,",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5655,
        "text": "som gule kameler.»",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5656,
        "text": "Ve dem som holder sannhet for løgn på denne dag!",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5657,
        "text": "Dette er dagen der de ikke snakker,",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5658,
        "text": "og det ikke blir tillatt dem å bringe unnskyldninger.",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5659,
        "text": "Ve dem som holder sannhet for løgn på denne dag!",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5660,
        "text": "Dette er dommens dag! Vi har samlet dere og de tidligere henfarne.",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5661,
        "text": "Om dere har en list, så prøv å overliste meg!",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5662,
        "text": "Ve dem som holder sannhet for løgn på denne dag!",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5663,
        "text": "De gudfryktige skal være blant skygger og kilder,",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5664,
        "text": "og frukt de ønsker seg.",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5665,
        "text": "«Spis og drikk og la det smake, for den innsats dere har gjort!»",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5666,
        "text": "Slik lønner Vi dem som gjør vel.",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5667,
        "text": "Ve dem som holder sannhet for løgn på denne dag!",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5668,
        "text": "Spis og nyt livet en kort tid! Dere er visselig syndere.",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5669,
        "text": "Ve dem som holder sannhet for løgn på denne dag!",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5670,
        "text": "Om man sier til dem: «Bøy dere i bønn,» så bøyer de seg ikke.",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5671,
        "text": "Ve dem som holder sannhet for løgn på denne dag!",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5672,
        "text": "På hvilket budskap vil de vel tro etter dette?",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      }
    ],
