  "data": {
    "number": 69,
    "name": "سُورَةُ الحَاقَّةِ",
    "englishName": "Al-Haaqqa",
    "englishNameTranslation": "The Reality",
    "revelationType": "Meccan",
    "numberOfAyahs": 52,
    "ayahs": [
      {
        "number": 5324,
        "text": "یقینا پیش آنے والی قیامت",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5325,
        "text": "اور کیسی پیش آنے والی",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5326,
        "text": "اور تم کیا جانو کہ یہ یقینا پیش آنے والی شے کیا ہے",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5327,
        "text": "قوم ثمود و عاد نے اس کھڑ کھڑانے والی کا انکار کیا تھا",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5328,
        "text": "تو ثمود ایک چنگھاڑ کے ذریعہ ہلاک کردیئے گئے",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5329,
        "text": "اور عاد کو انتہائی تیز و تند آندھی سے برباد کردیا گیا",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5330,
        "text": "جسے ان کے اوپر سات رات اور آٹھ دن کے لئے مسلسل مسخّرکردیا گیا تو تم دیکھتے ہو کہ قوم بالکل مفِدہ پڑی ہوئی ہے جیسے کھوکھلے کھجور کے درخت کے تنے",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5331,
        "text": "تو کیا تم ان کا کوئی باقی رہنے والا حصہّ دیکھ رہے ہو",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5332,
        "text": "اور فرعون اور اس سے پہلے اور الٹی بستیوں والے سب نے غلطیوں کا ارتکاب کیا ہے",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5333,
        "text": "کہ پروردگار کے نمائندہ کی نافرمانی کی تو پروردگار نے انہیں بڑی سختی سے پکڑ لیا",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5334,
        "text": "ہم نے تم کو اس وقت کشتی میں اٹھالیا تھا جب پانی سر سے چڑھ رہا تھا",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5335,
        "text": "تاکہ اسے تمہارے لئے نصیحت بنائیں اور محفوظ رکھنے والے کان سن لیں",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5336,
        "text": "پھر جب صور میں پہلی مرتبہ پھونکا جائے گا",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5337,
        "text": "اور زمین اور پہاڑوں کو اکھاڑ کر ٹکرا کر ریزہ ریزہ کردیا جائے گا",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5338,
        "text": "تو اس دن قیامت واقع ہوجائے گی",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5339,
        "text": "اور آسمان شق ہوکر بالکل پھس پھسے ہوجائیں گے",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5340,
        "text": "اور فرشتے اس کے اطراف پر ہوں گے اور عرش الہٰی کو اس دن آٹھ فرشتے اٹھائے ہوں گے",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5341,
        "text": "اس دن تم کو منظر عام پر لایا جائے گا اور تمہاری کوئی بات پوشیدہ نہ رہے گی",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5342,
        "text": "پھر جس کو نامئہ اعمال داہنے ہاتھ میں دیا جائے گا وہ سب سے کہے گا کہ ذرا میرا نامئہ اعمال تو پڑھو",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5343,
        "text": "مجھے پہلے ہی معلوم تھا کہ میرا حساب مجھے ملنے والا ہے",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5344,
        "text": "پھر وہ پسندیدہ زندگی میں ہوگا",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5345,
        "text": "بلند ترین باغات میں",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5346,
        "text": "اس کے میوے قریب قریب ہوں گے",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5347,
        "text": "اب آرام سے کھاؤ پیو کہ تم نے گزشتہ دنوں میں ان نعمتوں کا انتظام کیا ہے",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5348,
        "text": "لیکن جس کو نامئہ اعمال بائیں ہاتھ میں دیا جائے گا وہ کہے گا اے کاش یہ نامہ اعمال مجھے نہ دیا جاتا",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5349,
        "text": "اور مجھے اپنا حساب نہ معلوم ہوتا",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5350,
        "text": "اے کاش اس موت ہی نے میرا فیصلہ کردیا ہوتا",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5351,
        "text": "میرا مال بھی میرے کام نہ آیا",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5352,
        "text": "اور میری حکومت بھی برباد ہوگئی",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5353,
        "text": "اب اسے پکڑو اور گرفتار کرلو",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5354,
        "text": "پھر اسے جہنم ّمیں جھونک دو",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5355,
        "text": "پھر ایک ستر گز کی رسی میں اسے جکڑ لو",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5356,
        "text": "یہ خدائے عظیم پر ایمان نہیں رکھتا تھا",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5357,
        "text": "اور لوگوں کو مسکینوں کو کھلانے پر آمادہ نہیں کرتا تھا",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5358,
        "text": "تو آج اس کا یہاں کوئی غمخوار نہیں ہے",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5359,
        "text": "اور نہ پیپ کے علاوہ کوئی غذا ہے",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5360,
        "text": "جسے گنہگاروں کے علاوہ کوئی نہیں کھاسکتا",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5361,
        "text": "میں اس کی بھی قسم کھاتا ہوں جسے تم دیکھ رہے ہو",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5362,
        "text": "اور اس کی بھی جس کو نہیں دیکھ رہے ہو",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5363,
        "text": "کہ یہ ایک محترم فرشتے کا بیان ہے",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5364,
        "text": "اور یہ کسی شاعر کا قول نہیں ہے ہاں تم بہت کم ایمان لاتے ہو",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5365,
        "text": "اور یہ کسی کاہن کا کلام نہیںہے جس پر تم بہت کم غور کرتے ہو",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5366,
        "text": "یہ رب العالمین کا نازل کردہ ہے",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5367,
        "text": "اور اگر یہ پیغمبر ہماری طرف سے کوئی بات گڑھ لیتا",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5368,
        "text": "تو ہم اس کے ہاتھ کو پکڑ لیتے",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5369,
        "text": "اور پھر اس کی گردن اڑادیتے",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5370,
        "text": "پھر تم میں سے کوئی مجھے روکنے والا نہ ہوتا",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5371,
        "text": "اور یہ قرآن صاحبانِ تقویٰ کے لئے نصیحت ہے",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5372,
        "text": "اور ہم جانتے ہیں کہ تم میں سے جھٹلانے والے بھی ہیں",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5373,
        "text": "اور یہ کافرین کے لئے باعث هحسرت ہے",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5374,
        "text": "اور یہ بالکل یقینی چیز ہے",
        "numberInSurah": 51,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5375,
        "text": "لہذا آپ اپنے عظیم پروردگار کے نام کی تسبیح کریں",
        "numberInSurah": 52,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      }
