    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6099,
        "text": "അത്തിയും ഒലീവും സാക്ഷി.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 597,
