  "status": "OK",
  "data": {
    "number": 56,
    "name": "سُورَةُ الوَاقِعَةِ",
    "englishName": "Al-Waaqia",
    "englishNameTranslation": "The Inevitable",
    "revelationType": "Meccan",
    "numberOfAyahs": 96,
    "ayahs": [
      {
        "number": 4980,
        "text": "'Иҙӓ Ұа қ а`ати А л-Ұӓ қ и`ату \r",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4981,
        "text": "Ла й са Лиұа қ `атиһӓ Кӓҙибату н \r",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4982,
        "text": "Ҳ ӓфиđату н Рӓ фи`ату н \r",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4983,
        "text": "'Иҙӓ Ру җҗати А л-'Арđу Ра җҗӓа н \r",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4984,
        "text": "Ұа Буссати А л-Җиб ӓ лу Бассӓа н \r",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4985,
        "text": "Факӓнат Һаб ӓ 'а н Му н баććӓа н \r",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4986,
        "text": "Ұа Ку н ту м 'Азұӓҗӓа н Ćалӓćата н \r",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4987,
        "text": "Фа'а ҫ х ӓ бу А л-Майманати М ӓ 'А ҫ х ӓ бу А л-Майманати \r",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4988,
        "text": "Ұа 'А ҫ х ӓ бу А л-Маш'амати М ӓ 'А ҫ х ӓ бу А л-Маш'амати \r",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4989,
        "text": "Ұа А с-Сӓби қ ӱ на А с-Сӓби қ ӱ на \r",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4990,
        "text": "'Ӱл ӓ 'ика А л-Му қ ар ра б ӱ на \r",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4991,
        "text": "Фӥ Җа нн ӓ ти А н -На` ӥ ми \r",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4992,
        "text": "Ćуллату н Мина А л-'Аұұал ӥ на \r",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4993,
        "text": "Ұа Қ ал ӥ лу н Мина А л-'Ӓ ҳ и р ӥ на \r",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4994,
        "text": "`Алá Су ру р и н Маұđӱнати н \r",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4995,
        "text": "Муттаки' ӥ на `Алайһӓ Мута қ ӓбил ӥ на \r",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4996,
        "text": "Йа ҭ ӱ фу `Алайһи м Ұилд ӓ ну н Му ҳ аллад ӱ на \r",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4997,
        "text": "Би'акұ ӓ би н Ұа 'Абӓ р ӥ қ а Ұа Ка'си н Ми н Ма` ӥ ни н \r",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4998,
        "text": "Лӓ Йу ҫ адда` ӱ на `Анһӓ Ұа Лӓ Йу н зиф ӱ на \r",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4999,
        "text": "Ұа Фӓкиһати н Ми мм ӓ Йата ҳ аййар ӱ на \r",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5000,
        "text": "Ұа Лахми Ҭ а й р и н Ми мм ӓ Йаштаһ ӱ на \r",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5001,
        "text": "Ұа Х ӱ ру н ` Ӥ ну н \r",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5002,
        "text": "Ка'а м ć ӓ ли А л-Лу'улу'уи А л-Макн ӱ ни \r",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5003,
        "text": "Җаз ӓ 'а н Бимӓ Кӓнӱ Йа`мал ӱ на \r",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5004,
        "text": "Лӓ Йасма` ӱ на Фӥһӓ Ла ғ ұа н Ұа Лӓ Та'ćӥмӓа н \r",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5005,
        "text": "'Иллӓ Қ ӥлӓа н Салӓмӓа н Салӓмӓа н \r",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5006,
        "text": "Ұа 'А ҫ х ӓ бу А л-Йам ӥ ни М ӓ 'А ҫ х ӓ бу А л-Йам ӥ ни \r",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5007,
        "text": "Фӥ Си д р и н Ма ҳ đ ӱ д и н \r",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5008,
        "text": "Ұа Ҭ алхи н Ма н đ ӱ д и н \r",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5009,
        "text": "Ұа Ž илли н Ма м д ӱ д и н \r",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5010,
        "text": "Ұа М ӓ 'и н Маск ӱ б и н \r",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5011,
        "text": "Ұа Фӓкиһати н Каćӥ ра ти н \r",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5012,
        "text": "Лӓ Ма қ ҭ ӱ`ати н Ұа Лӓ Ма м нӱ`ати н \r",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5013,
        "text": "Ұа Фу ру ши н Марфӱ`ати н \r",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5014,
        "text": "'И нн ӓ 'А н ша'нӓһу нн а 'И н ш ӓ 'а н \r",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5015,
        "text": "Фаҗа`алнӓһу нн а 'А б кӓ рӓ а н \r",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5016,
        "text": "`У ру бӓан 'Ат рӓ бӓа н \r",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5017,
        "text": "Ли'а ҫ х ӓ би А л-Йам ӥ ни \r",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5018,
        "text": "Ćуллату н Мина А л-'Аұұал ӥ на \r",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5019,
        "text": "Ұа Ćуллату н Мина А л-'Ӓ ҳ и р ӥ на \r",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5020,
        "text": "Ұа 'А ҫ х ӓ бу А ш-Шим ӓ ли М ӓ 'А ҫ х ӓ бу А ш-Шим ӓ ли \r",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5021,
        "text": "Фӥ Сам ӱ ми н Ұа Хам ӥ ми н \r",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5022,
        "text": "Ұа Ž илли н Ми н Йахм ӱ ми н \r",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5023,
        "text": "Лӓ Бӓ р иди н Ұа Лӓ Ка р ӥ ми н \r",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5024,
        "text": "'И нн аһу м Кӓнӱ Қ а б ла Ҙӓлика Мут ра ф ӥ на \r",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5025,
        "text": "Ұа Кӓнӱ Йу ҫ и р р ӱ на `Алá А л-Хи н ćи А л-`Аž ӥ ми \r",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5026,
        "text": "Ұа Кӓнӱ Йа қ ӱл ӱ на 'А'иҙӓ Митнӓ Ұа Ку нн ӓ Ту рӓ бӓа н Ұа `Иžӓмӓан 'А'и нн ӓ Лама б `ӱć ӱ на \r",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5027,
        "text": "'Аұа 'Ӓб ӓ 'уунӓ А л-'Аұұал ӱ на \r",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5028,
        "text": "Қ ул 'И нн а А л-'Аұұал ӥ на Ұа А л-'Ӓ ҳ и р ӥ на \r",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5029,
        "text": "Лама җ мӱ` ӱ на 'Илá Мӥ қ ӓ ти Йа ұ ми н Ма`л ӱ ми н \r",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5030,
        "text": "Ćу мм а 'И нн аку м 'Аййуһӓ А đ- Đ ӓ лл ӱ на А л-Мукаҙҙиб ӱ на \r",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5031,
        "text": "Ла' ӓ кил ӱ на Ми н Шаҗа р и н Ми н За қ қ ӱ ми н \r",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5032,
        "text": "Фамӓли' ӱ на Минһӓ А л-Бу ҭ ӱ на \r",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5033,
        "text": "Фашӓ р иб ӱ на `Ала й һи Мина А л-Хам ӥ ми \r",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5034,
        "text": "Фашӓ р иб ӱ на Шурба А л-Һ ӥ ми \r",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5035,
        "text": "Һӓҙӓ Нузулуһу м Йа ұ ма А д-Д ӥ ни \r",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5036,
        "text": "Нахну Ҳ ала қ нӓку м Фалаұлӓ Ту ҫ адди қ ӱ на \r",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5037,
        "text": "'Афа ра 'айту м Мӓ Ту м н ӱ на \r",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5038,
        "text": "'А'а н ту м Та ҳ лу қ ӱнаһ у~ 'А м Нахну А л- Ҳ ӓли қ ӱ на \r",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5039,
        "text": "Нахну Қ аддарнӓ Байнакуму А л-Ма ұ та Ұа Мӓ Нахну Бимасбӱ қ ӥ на \r",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5040,
        "text": "`Ал á 'А н Нубаддила 'А м ćӓлаку м Ұа Ну н ши'аку м Фӥ Мӓ Лӓ Та`лам ӱ на \r",
        "numberInSurah": 61,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5041,
        "text": "Ұа Ла қ а д `Али м туму А н -Наш'ата А л-'Ӱлá Фалаұлӓ Таҙккар ӱ на \r",
        "numberInSurah": 62,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5042,
        "text": "'Афа ра 'айту м Мӓ Тах ру ć ӱ на \r",
        "numberInSurah": 63,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5043,
        "text": "'А'а н ту м Таз ра `ӱнаһ у~ 'А м Нахну А з-Зӓ р и` ӱ на \r",
        "numberInSurah": 64,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5044,
        "text": "Лаұ Наш ӓ 'у Лаҗа`алн ӓ һу Ху ҭ ӓмӓа н Фаžалалту м Тафаккаһ ӱ на \r",
        "numberInSurah": 65,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5045,
        "text": "'И нн ӓ Ламу ғ ра м ӱ на \r",
        "numberInSurah": 66,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5046,
        "text": "Бал Нахну Махрӱм ӱ на \r",
        "numberInSurah": 67,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5047,
        "text": "'Афа ра 'айтуму А л-М ӓ 'а А л-Лаҙӥ Таш ра б ӱ на \r",
        "numberInSurah": 68,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5048,
        "text": "'А'а н ту м 'А н залтум ӱ һу Мина А л-Музни 'А м Нахну А л-Му н зил ӱ на \r",
        "numberInSurah": 69,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5049,
        "text": "Лаұ Наш ӓ 'у Җа`алн ӓ һу 'Уҗӓҗӓа н Фалаұлӓ Ташкур ӱ на \r",
        "numberInSurah": 70,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5050,
        "text": "'Афа ра 'айтуму А н -Н ӓ ра А ллатӥ Тӱр ӱ на \r",
        "numberInSurah": 71,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5051,
        "text": "'А'а н ту м 'А н ша'ту м Шаҗа ра таһ ӓ 'А м Нахну А л-Му н ши' ӱ на \r",
        "numberInSurah": 72,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5052,
        "text": "Нахну Җа`алнӓһӓ Таҙки ра та н Ұа Матӓ`ӓа н Лилму қ ұ ӥ на \r",
        "numberInSurah": 73,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5053,
        "text": "Фасаббих Биӓсми Ра ббика А л-`Аž ӥ ми \r",
        "numberInSurah": 74,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5054,
        "text": "Фал ӓ 'У қ симу Бимаұӓ қ и`и А н -Нуҗ ӱ ми \r",
        "numberInSurah": 75,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5055,
        "text": "Ұа 'И нн аһ у Ла қ асаму н Лаұ Та`лам ӱ на `Аž ӥ му н \r",
        "numberInSurah": 76,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5056,
        "text": "'И нн аһ у Ла қ ур' ӓ ну н Ка р ӥ му н \r",
        "numberInSurah": 77,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5057,
        "text": "Фӥ Кит ӓ би н Макн ӱ ни н \r",
        "numberInSurah": 78,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5058,
        "text": "Лӓ Йамассуһ у~ 'Иллӓ А л-Му ҭ аһһар ӱ на \r",
        "numberInSurah": 79,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5059,
        "text": "Та н з ӥ лу н Ми н Ра бби А л-`Ӓлам ӥ на \r",
        "numberInSurah": 80,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5060,
        "text": "'Афабиһаҙӓ А л-Хадӥćи 'А н ту м Му д /һин ӱ на \r",
        "numberInSurah": 81,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5061,
        "text": "Ұа Та җ `ал ӱ на Р из қ аку м 'А нн аку м Тукаҙҙиб ӱ на \r",
        "numberInSurah": 82,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5062,
        "text": "Фалаұл ӓ 'Иҙӓ Бала ғ ати А л-Хул қ ӱ ма \r",
        "numberInSurah": 83,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5063,
        "text": "Ұа 'А н ту м Хӥна'иҙи н Та н žур ӱ на \r",
        "numberInSurah": 84,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5064,
        "text": "Ұа Нахну 'А қ ра бу 'Ила й һи Ми н ку м Ұа Лаки н Лӓ Ту б ҫ ир ӱ на \r",
        "numberInSurah": 85,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5065,
        "text": "Фалаұл ӓ 'И н Ку н ту м Ғ а й ра Мадӥн ӥ на \r",
        "numberInSurah": 86,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5066,
        "text": "Тарҗи`ӱнаһ ӓ 'И н Ку н ту м Ҫ ӓди қ ӥ на \r",
        "numberInSurah": 87,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5067,
        "text": "Фа'а мм ӓ 'И н К ӓ на Мина А л-Му қ ар ра б ӥ на \r",
        "numberInSurah": 88,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5068,
        "text": "Фа ра ұху н Ұа Ра йх ӓ ну н Ұа Җа нн ату На` ӥ ми н \r",
        "numberInSurah": 89,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5069,
        "text": "Ұа 'А мм ӓ 'И н К ӓ на Мин 'А ҫ х ӓ би А л-Йам ӥ ни \r",
        "numberInSurah": 90,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5070,
        "text": "Фасал ӓ му н Лака Мин 'А ҫ х ӓ би А л-Йам ӥ ни \r",
        "numberInSurah": 91,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5071,
        "text": "Ұа 'А мм ӓ 'И н К ӓ на Мина А л-Мукаҙҙиб ӥ на А đ- Đ ӓ лл ӥ на \r",
        "numberInSurah": 92,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5072,
        "text": "Фанузулу н Мин Хам ӥ ми н \r",
        "numberInSurah": 93,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5073,
        "text": "Ұа Та ҫ лийату Җах ӥ ми н \r",
        "numberInSurah": 94,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5074,
        "text": "'И нн а Һӓҙӓ Лаһуұа Ха қ қ у А л-Йа қ ӥ ни \r",
        "numberInSurah": 95,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5075,
        "text": "Фасаббих Биӓсми Ра ббика А л-`Аž ӥ ми \r",
        "numberInSurah": 96,
        "juz": 27,
