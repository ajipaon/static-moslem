  "status": "OK",
  "data": {
    "number": 44,
    "name": "سُورَةُ الدُّخَانِ",
    "englishName": "Ad-Dukhaan",
    "englishNameTranslation": "The Smoke",
    "revelationType": "Meccan",
    "numberOfAyahs": 59,
    "ayahs": [
      {
        "number": 4415,
        "text": "Хӓ-Мӥ м \r",
        "numberInSurah": 1,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4416,
        "text": "Ұа А л-Кит ӓ би А л-Муб ӥ ни \r",
        "numberInSurah": 2,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4417,
        "text": "'И нн ӓ 'А н залн ӓ һу Фӥ Лайлати н Мубӓ ра катин ۚ 'И нн ӓ Ку нн ӓ Му н ҙи р ӥ на \r",
        "numberInSurah": 3,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4418,
        "text": "Фӥһӓ Йуф ра қ у Куллу 'А м р ин Хак ӥ ми н \r",
        "numberInSurah": 4,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4419,
        "text": "'А м рӓ а н Мин `И н дин ӓ ۚ 'И нн ӓ Ку нн ӓ Мурсил ӥ на \r",
        "numberInSurah": 5,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4420,
        "text": "Ра хмата н Ми н Ра ббика ۚ 'И нн аһ у Һуұа А с-Сам ӥ `у А л-`Ал ӥ му \r",
        "numberInSurah": 6,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4421,
        "text": "Ра бби А с-Самӓұ ӓ ти Ұа А л-'Арđи Ұа Мӓ Байнаһум ӓ ۖ 'И н Ку н ту м Мӱ қ ин ӥ на \r",
        "numberInSurah": 7,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4422,
        "text": "Л ӓ 'Ил ӓ һа 'Иллӓ Һуұа Йухйӥ Ұа Йум ӥ ту ۖ Ра ббуку м Ұа Ра ббу 'Ӓб ӓ 'икуму А л-'Аұұал ӥ на \r",
        "numberInSurah": 8,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4423,
        "text": "Бал Һу м Фӥ Шакки н Йал`аб ӱ на \r",
        "numberInSurah": 9,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4424,
        "text": "Фӓрта қ и б Йа ұ ма Та'тӥ А с-Сам ӓ 'у Биду ҳ ӓ ни н Муб ӥ ни н \r",
        "numberInSurah": 10,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4425,
        "text": "Йа ғ шá А н -Н ӓ са ۖ Һӓҙӓ `Аҙ ӓ бун 'Ал ӥ му н \r",
        "numberInSurah": 11,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4426,
        "text": "Ра ббанӓ А кшиф `А нн ӓ А л-`Аҙ ӓ ба 'И нн ӓ Му'умин ӱ на \r",
        "numberInSurah": 12,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4427,
        "text": "'А нн á Лаһуму А ҙ-Ҙикрá Ұа Қ а д Җ ӓ 'аһу м Ра с ӱ лу н Муб ӥ ну н \r",
        "numberInSurah": 13,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4428,
        "text": "Ćу мм а Таұаллаұ `Анһу Ұа Қ ӓлӱ Му`алламу н Ма җ н ӱ ну н \r",
        "numberInSurah": 14,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4429,
        "text": "'И нн ӓ Кӓшифӱ А л-`Аҙ ӓ би Қ алӥлӓан ۚ 'И нн аку м ` Ӓ 'ид ӱ на \r",
        "numberInSurah": 15,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4430,
        "text": "Йа ұ ма На б ҭ ишу А л-Ба ҭ шата А л-Ку б р á 'И нн ӓ Му н та қ им ӱ на \r",
        "numberInSurah": 16,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4431,
        "text": "Ұа Ла қ а д Фата нн ӓ Қ а б лаһу м Қ а ұ ма Фи р `а ұ на Ұа Җ ӓ 'аһу м Ра с ӱ лу н Ка р ӥ му н \r",
        "numberInSurah": 17,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4432,
        "text": "'Ан 'Адд ӱ 'Ила й йа `Иб ӓ да А лл ӓһи ۖ 'И нн ӥ Лаку м Ра с ӱ лун 'Ам ӥ ну н \r",
        "numberInSurah": 18,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4433,
        "text": "Ұа 'А н Лӓ Та`лӱ `Алá А лл ӓһи ۖ 'И нн ӥ 'Ӓтӥку м Бисул ҭ ӓ ни н Муб ӥ ни н \r",
        "numberInSurah": 19,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4434,
        "text": "Ұа 'И нн ӥ `Уҙту Би ра ббӥ Ұа Ра ббику м 'А н Тарҗум ӱ ни \r",
        "numberInSurah": 20,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4435,
        "text": "Ұа 'И н Ла м Ту'уминӱ Лӥ Фӓ`тазил ӱ ни \r",
        "numberInSurah": 21,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4436,
        "text": "Фада`ӓ Ра ббаһ у~ 'А нн а Һ ӓ 'уул ӓ ' Қ а ұ му н Му җ р им ӱ на \r",
        "numberInSurah": 22,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4437,
        "text": "Фа'ас р и Би`ибӓдӥ Лайлӓан 'И нн аку м Муттаба` ӱ на \r",
        "numberInSurah": 23,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4438,
        "text": "Ұа А т ру ки А л-Бах ра Ра һұан ۖ 'И нн аһу м Җу н ду н Му ғ ра қ ӱ на \r",
        "numberInSurah": 24,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4439,
        "text": "Ка м Та ра кӱ Ми н Җа нн ӓ ти н Ұа `Уй ӱ ни н \r",
        "numberInSurah": 25,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4440,
        "text": "Ұа Зур ӱ `и н Ұа Ма қ ӓ ми н Ка р ӥ ми н \r",
        "numberInSurah": 26,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4441,
        "text": "Ұа На`мати н Кӓнӱ Фӥһӓ Фӓкиһ ӥ на \r",
        "numberInSurah": 27,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4442,
        "text": "Каҙӓлика ۖ Ұа 'Аұ ра ćнӓһӓ Қ аұмӓан 'Ӓ ҳ а р ӥ на \r",
        "numberInSurah": 28,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4443,
        "text": "Фамӓ Бакат `Алайһиму А с-Сам ӓ 'у Ұа А л-'Арđу Ұа Мӓ Кӓнӱ Му н žа р ӥ на \r",
        "numberInSurah": 29,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4444,
        "text": "Ұа Ла қ а д Наҗҗайнӓ Бан ӥ 'Иср ӓ ' ӥ ла Мина А л-`Аҙ ӓ би А л-Муһ ӥ ни \r",
        "numberInSurah": 30,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4445,
        "text": "Ми н Фи р `а ұ на ۚ 'И нн аһ у К ӓ на `Ӓлӥӓа н Мина А л-Мус р иф ӥ на \r",
        "numberInSurah": 31,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4446,
        "text": "Ұа Ла қ ади А ҳ тарнӓһу м `Алá `Илмин `Алá А л-`Ӓлам ӥ на \r",
        "numberInSurah": 32,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4447,
        "text": "Ұа 'Ӓтайнӓһу м Мина А л-'Ӓй ӓ ти Мӓ Ф ӥ һи Бал ӓ 'у н Муб ӥ ну н \r",
        "numberInSurah": 33,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4448,
        "text": "'И нн а Һ ӓ 'уул ӓ ' Лайа қ ӱл ӱ на \r",
        "numberInSurah": 34,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4449,
        "text": "'Ин Һийа 'Иллӓ Маұтатунӓ А л-'Ӱлá Ұа Мӓ Нахну Биму н ша р ӥ на \r",
        "numberInSurah": 35,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4450,
        "text": "Фа'тӱ Би'ӓб ӓ 'ин ӓ 'И н Ку н ту м Ҫ ӓди қ ӥ на \r",
        "numberInSurah": 36,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4451,
        "text": "'Аһу м Ҳ а й ру н 'А м Қ а ұ му Тубба`и н Ұа А л-Лаҙ ӥ на Ми н Қ а б лиһи м ۚ 'Аһлакнӓһу м ۖ 'И нн аһу м Кӓнӱ Му җ р им ӥ на \r",
        "numberInSurah": 37,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4452,
        "text": "Ұа Мӓ Ҳ ала қ нӓ А с-Самӓұ ӓ ти Ұа А л-'Арđа Ұа Мӓ Байнаһумӓ Лӓ`иб ӥ на \r",
        "numberInSurah": 38,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4453,
        "text": "Мӓ Ҳ ала қ нӓһум ӓ 'Иллӓ Бил-Ха қ қ и Ұа Лаки нн а 'Акćа ра һу м Лӓ Йа`лам ӱ на \r",
        "numberInSurah": 39,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4454,
        "text": "'И нн а Йа ұ ма А л-Фа ҫ ли Мӥ қ ӓтуһу м 'А җ ма` ӥ на \r",
        "numberInSurah": 40,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4455,
        "text": "Йа ұ ма Лӓ Йу ғ нӥ Ма ұ лан `А н Ма ұ ла н Шай' ӓ а н Ұа Лӓ Һу м Йу н ҫ ар ӱ на \r",
        "numberInSurah": 41,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4456,
        "text": "'Иллӓ Ма н Ра хима А лл ӓһу ۚ 'И нн аһ у Һуұа А л-`Аз ӥ зу А р- Ра х ӥ му \r",
        "numberInSurah": 42,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4457,
        "text": "'И нн а Шаҗа ра та А з-За қ қ ӱ ми \r",
        "numberInSurah": 43,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4458,
        "text": "Ҭ а` ӓ му А л-'Аć ӥ ми \r",
        "numberInSurah": 44,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4459,
        "text": "Кӓлмуһли Йа ғ лӥ Фӥ А л-Бу ҭ ӱ ни \r",
        "numberInSurah": 45,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4460,
        "text": "Ка ғ алйи А л-Хам ӥ ми \r",
        "numberInSurah": 46,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4461,
        "text": "Ҳ уҙ ӱ һу Фӓ`тил ӱ һу 'Илá Саұ ӓ 'и А л-Җах ӥ ми \r",
        "numberInSurah": 47,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4462,
        "text": "Ćу мм а Ҫ уббӱ Фа ұ қ а Ра 'сиһ и Мин `Аҙ ӓ би А л-Хам ӥ ми \r",
        "numberInSurah": 48,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4463,
        "text": "Ҙу қ 'И нн ака 'А н та А л-`Аз ӥ зу А л-Ка р ӥ му \r",
        "numberInSurah": 49,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4464,
        "text": "'И нн а Һӓҙӓ Мӓ Ку н ту м Биһ и Та м тар ӱ на \r",
        "numberInSurah": 50,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4465,
        "text": "'И нн а А л-Мутта қ ӥ на Фӥ Ма қ ӓ мин 'Ам ӥ ни н \r",
        "numberInSurah": 51,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4466,
        "text": "Фӥ Җа нн ӓ ти н Ұа `Уй ӱ ни н \r",
        "numberInSurah": 52,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4467,
        "text": "Йалбас ӱ на Ми н Су н дуси н Ұа 'Иста б ра қ и н Мута қ ӓбил ӥ на \r",
        "numberInSurah": 53,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4468,
        "text": "Каҙӓлика Ұа Заұұа җ нӓһу м Бих ӱ р ин ` Ӥ ни н \r",
        "numberInSurah": 54,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4469,
        "text": "Йа д ` ӱ на Фӥһӓ Бикулли Фӓкиһатин 'Ӓмин ӥ на \r",
        "numberInSurah": 55,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
