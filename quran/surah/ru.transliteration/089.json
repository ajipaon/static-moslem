  "data": {
    "number": 89,
    "name": "سُورَةُ الفَجۡرِ",
    "englishName": "Al-Fajr",
    "englishNameTranslation": "The Dawn",
    "revelationType": "Meccan",
    "numberOfAyahs": 30,
    "ayahs": [
      {
        "number": 5994,
        "text": "Ұа А л-Фа җ р и \r",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5995,
        "text": "Ұа Лай ӓ лин `Аш р и н \r",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5996,
        "text": "Ұа А ш-Шаф`и Ұа А л-Ұат р и \r",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5997,
        "text": "Ұа А л-Ла й ли 'Иҙӓ Йас р и \r",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5998,
        "text": "Һал Фӥ Ҙӓлика Қ асаму н Лиҙӥ Хи җ р и н \r",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5999,
        "text": "'Ала м Та ра Ка й фа Фа`ала Ра ббука Би` ӓ д и н \r",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6000,
        "text": "'И ра ма Ҙ ӓ ти А л-`Им ӓ ди \r",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6001,
        "text": "Аллатӥ Ла м Йу ҳ ла қ Миćлуһӓ Фӥ А л-Бил ӓ ди \r",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6002,
        "text": "Ұа Ćам ӱ да А л-Лаҙ ӥ на Җӓбӱ А ҫ - Ҫ а ҳ ра Бил-Ұ ӓ ди \r",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6003,
        "text": "Ұа Фи р `а ұ на Ҙӥ А л-'Аұт ӓ ди \r",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6004,
        "text": "Ал-Лаҙ ӥ на Ҭ а ғ аұ Фӥ А л-Бил ӓ ди \r",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6005,
        "text": "Фа'акćарӱ Фӥһӓ А л-Фас ӓ да \r",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6006,
        "text": "Фа ҫ абба `Алайһи м Ра ббука Саұ ҭ а `Аҙ ӓ б и н \r",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6007,
        "text": "'И нн а Ра ббака Лабиӓлми р ҫ ӓ ди \r",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6008,
        "text": "Фа'а мм ӓ А л-'И н с ӓ ну 'Иҙӓ Мӓ А б тал ӓ һу Ра ббуһ у Фа'ак ра маһ у Ұа На``амаһ у Файа қ ӱ лу Ра бб ӥ 'Ак ра мани \r",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6009,
        "text": "Ұа 'А мм ӓ 'Иҙӓ Мӓ А б тал ӓ һу Фа қ ада ра `Ала й һи Р из қ аһ у Файа қ ӱ лу Ра бб ӥ 'Аһӓнани \r",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6010,
        "text": "Каллӓ ۖ Ба л Лӓ Тук р им ӱ на А л-Йат ӥ ма \r",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6011,
        "text": "Ұа Лӓ Тах ӓ đđ ӱ на `Алá Ҭ а` ӓ ми А л-Миск ӥ ни \r",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6012,
        "text": "Ұа Та'кул ӱ на А т-Ту рӓ ćа 'Аклӓа н Ла мм ӓа н \r",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6013,
        "text": "Ұа Тухибб ӱ на А л-М ӓ ла Хуббӓа н Җа мм ӓа н \r",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6014,
        "text": "Калл ӓ 'Иҙӓ Дуккати А л-'Арđу Даккӓа н Даккӓа н \r",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6015,
        "text": "Ұа Җ ӓ 'а Ра ббука Ұа А л-Малаку Ҫ аффӓа н Ҫ аффӓа н \r",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6016,
        "text": "Ұа Җ ӥ 'а Йаұма'иҙи н Биҗаһа нн ама ۚ Йаұма'иҙи н Йатаҙакка ру А л-'И н с ӓ ну Ұа 'А нн á Лаһу А ҙ-Ҙикрá \r",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6017,
        "text": "Йа қ ӱ лу Йӓ Лайтанӥ Қ адда м ту Лихайӓтӥ \r",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6018,
        "text": "Файаұма'иҙи н Лӓ Йу`аҙҙибу `Аҙӓбаһ у~ 'Аха д у н \r",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6019,
        "text": "Ұа Лӓ Йӱćи қ у Ұаćӓ қ аһ у~ 'Аха д у н \r",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6020,
        "text": "Й ӓ 'Аййатуһӓ А н -Нафсу А л-Му ҭ ма'и нн ату \r",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6021,
        "text": "А рҗи` ӥ 'Илá Ра ббики Рӓ đийата н Марđӥйата н \r",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6022,
        "text": "Фӓ д ҳ улӥ Фӥ `Ибӓдӥ \r",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6023,
        "text": "Ұа А д ҳ улӥ Җа нн атӥ \r",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
