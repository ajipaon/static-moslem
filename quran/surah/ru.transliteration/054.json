  "data": {
    "number": 54,
    "name": "سُورَةُ القَمَرِ",
    "englishName": "Al-Qamar",
    "englishNameTranslation": "The Moon",
    "revelationType": "Meccan",
    "numberOfAyahs": 55,
    "ayahs": [
      {
        "number": 4847,
        "text": "А қ та ра бати А с-Сӓ`ату Ұа А н ша қ қ а А л- Қ ама ру \r",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4848,
        "text": "Ұа 'И н Йа ра ұ 'Ӓйата н Йу` р иđӱ Ұа Йа қ ӱлӱ Сих ру н Мустами р ру н \r",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4849,
        "text": "Ұа Каҙҙабӱ Ұа А ттаба` ӱ 'Аһұ ӓ 'аһу м ۚ Ұа Куллу 'А м р и н Муста қ и р ру н \r",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4850,
        "text": "Ұа Ла қ а д Җ ӓ 'аһу м Мина А л-'А н б ӓ 'и Мӓ Ф ӥ һи Муздаҗа ру н \r",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4851,
        "text": "Хикмату н Бӓли ғ ату н ۖ Фамӓ Ту ғ ни А н -Нуҙу ру \r",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4852,
        "text": "Фатаұалла `Анһу м ۘ Йа ұ ма Йа д `у А д-Д ӓ `и 'Илá Ша й 'и н Нуку р и н \r",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4853,
        "text": "Ҳ ушша`ӓан 'А б ҫ ӓ ру һу м Йа ҳ ру җ ӱ на Мина А л-'А җ дӓćи Ка'а нн аһу м Җа р ӓ ду н Му н таши р у н \r",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4854,
        "text": "Муһ ҭ и` ӥ на 'Илá А д-Д ӓ `и ۖ Йа қ ӱ лу А л-Кӓфир ӱ на Һӓҙӓ Йа ұ мун `Аси р у н \r",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4855,
        "text": "Каҙҙабат Қ а б лаһу м Қ а ұ му Нӱхи н Факаҙҙабӱ `А б данӓ Ұа Қ ӓлӱ Ма җ н ӱ ну н Ұа А здуҗи р а \r",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4856,
        "text": "Фада`ӓ Ра ббаһ у~ 'А нн ӥ Ма ғ л ӱ бу н Фӓ н та ҫ и р \r",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4857,
        "text": "Фафатахн ӓ 'А б ұ ӓ ба А с-Сам ӓ 'и Бим ӓ 'и н Мунһами р и н \r",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4858,
        "text": "Ұа Фаҗҗарнӓ А л-'Арđа `Уйӱнӓа н Фӓлта қ á А л-М ӓ 'у `Ал á 'А м р и н Қ а д Қ уди ра \r",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4859,
        "text": "Ұа Хамалн ӓ һу `Алá Ҙ ӓ ти 'Алұӓхи н Ұа Дусу р и н \r",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4860,
        "text": "Та җ р ӥ Би'а`йунинӓ Җаз ӓ 'а н Лима н К ӓ на Куфи р а \r",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4861,
        "text": "Ұа Ла қ а д Та ра кнӓһ ӓ 'Ӓйата н Фаһал Ми н Муддаки р и н \r",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4862,
        "text": "Фака й фа К ӓ на `Аҙӓбӥ Ұа Нуҙу р и \r",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4863,
        "text": "Ұа Ла қ а д Йассарнӓ А л- Қ ур' ӓ на Лилҙҙик р и Фаһал Ми н Муддаки р и н \r",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4864,
        "text": "Каҙҙабат ` Ӓ ду н Фака й фа К ӓ на `Аҙӓбӥ Ұа Нуҙу р и \r",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4865,
        "text": "'И нн ӓ 'Арсалнӓ `Алайһи м Р ӥхӓа н Ҫ ар ҫ а рӓ а н Фӥ Йа ұ ми Нахси н Мустами р р и н \r",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4866,
        "text": "Та н зи`у А н -Н ӓ са Ка'а нн аһу м 'А`җ ӓ зу На ҳ ли н Му н қ а`и р и н \r",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4867,
        "text": "Фака й фа К ӓ на `Аҙӓбӥ Ұа Нуҙу р и \r",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4868,
        "text": "Ұа Ла қ а д Йассарнӓ А л- Қ ур' ӓ на Лилҙҙик р и Фаһал Ми н Муддаки р и н \r",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4869,
        "text": "Каҙҙабат Ćам ӱ ду Би н -Нуҙу р и \r",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4870,
        "text": "Фа қ ӓл ӱ 'Абаша рӓ а н Ми нн ӓ Ұӓхидӓа н Наттаби`уһ у~ 'И нн ӓ 'Иҙӓа н Лафӥ Đ ал ӓ ли н Ұа Су`у р и н \r",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4871,
        "text": "'А'уул қ ийа А ҙ-Ҙик ру `Ала й һи Ми н Байнинӓ Бал Һуұа Каҙҙ ӓ бун 'Аши р у н \r",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4872,
        "text": "Сайа`лам ӱ на Ғ адӓа н Мани А л-Каҙҙ ӓ бу А л-'Аши р у \r",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4873,
        "text": "'И нн ӓ Мурсилӱ А н -Нӓ қ ати Фитната н Лаһу м Фӓрта қ и б һу м Ұа А ҫ ҭ а б и р \r",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4874,
        "text": "Ұа Набби'һу м 'А нн а А л-М ӓ 'а Қ исмату н Байнаһу м ۖ Куллу Ши р би н Мухтаđа ру н \r",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4875,
        "text": "Фанӓдаұ Ҫ ӓхибаһу м Фата`ӓ ҭ á Фа`а қ а ра \r",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4876,
        "text": "Фака й фа К ӓ на `Аҙӓбӥ Ұа Нуҙу р и \r",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4877,
        "text": "'И нн ӓ 'Арсалнӓ `Алайһи м Ҫ айхата н Ұӓхидата н Факӓнӱ Каһаш ӥ ми А л-Мухтаžи р и \r",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4878,
        "text": "Ұа Ла қ а д Йассарнӓ А л- Қ ур' ӓ на Лилҙҙик р и Фаһал Ми н Муддаки р и н \r",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4879,
        "text": "Каҙҙабат Қ а ұ му Лӱ ҭ и н Би н -Нуҙу р и \r",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4880,
        "text": "'И нн ӓ 'Арсалнӓ `Алайһи м Хӓ ҫ ибӓан 'Илл ӓ ' Ӓ ла Лӱ ҭ и н ۖ Наҗҗайнӓһу м Бисаха р и н \r",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4881,
        "text": "Ни`мата н Мин `И н динӓ ۚ Каҙӓлика На җ зӥ Ма н Шака ра \r",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4882,
        "text": "Ұа Ла қ а д 'А н ҙа ра һу м Ба ҭ шатанӓ Фатамӓ ра ұ Би н -Нуҙу р и \r",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4883,
        "text": "Ұа Ла қ а д Рӓ ұад ӱ һу `А н Đ айфиһ и Фа ҭ амасн ӓ 'А`йунаһу м Фаҙӱ қ ӱ `Аҙӓбӥ Ұа Нуҙу р и \r",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4884,
        "text": "Ұа Ла қ а д Ҫ аббахаһу м Бук ра тан `Аҙ ӓ бу н Муста қ и р ру н \r",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4885,
        "text": "Фаҙӱ қ ӱ `Аҙӓбӥ Ұа Нуҙу р и \r",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4886,
        "text": "Ұа Ла қ а д Йассарнӓ А л- Қ ур' ӓ на Лилҙҙик р и Фаһал Ми н Муддаки р и н \r",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4887,
        "text": "Ұа Ла қ а д Җ ӓ 'а ' Ӓ ла Фи р `а ұ на А н -Нуҙу ру \r",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4888,
        "text": "Каҙҙабӱ Би'ӓйӓтинӓ Куллиһӓ Фа'а ҳ аҙнӓһу м 'А ҳ ҙа `Аз ӥ зи н Му қ тади р и н \r",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4889,
        "text": "'Акуффӓ ру ку м Ҳ а й ру н Мин 'Ӱла'ику м 'А м Лаку м Бар ӓ 'ату н Фӥ А з-Зубу р и \r",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4890,
        "text": "'А м Йа қ ӱл ӱ на Нахну Җам ӥ `у н Му н та ҫ и ру н \r",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4891,
        "text": "Сайуһзаму А л-Җа м `у Ұа Йуұалл ӱ на А д-Дубу ра \r",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4892,
        "text": "Бали А с-Сӓ`ату Маұ`идуһу м Ұа А с-Сӓ`ату 'А д /һá Ұа 'Амар ру \r",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4893,
        "text": "'И нн а А л-Му җ р им ӥ на Фӥ Đ ал ӓ ли н Ұа Су`у р и н \r",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4894,
        "text": "Йа ұ ма Йусхаб ӱ на Фӥ А н -Н ӓ р и `Алá Ұуҗӱһиһи м Ҙӱ қ ӱ Масса Са қ а ра \r",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4895,
        "text": "'И нн ӓ Кулла Ша й 'ин Ҳ ала қ н ӓ һу Би қ ада р и н \r",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4896,
        "text": "Ұа М ӓ 'А м ру н ӓ 'Иллӓ Ұӓхидату н Кала м хи н Бил-Ба ҫ а р и \r",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4897,
        "text": "Ұа Ла қ а д 'Аһлакн ӓ 'Ашйӓ`аку м Фаһал Ми н Муддаки р и н \r",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4898,
        "text": "Ұа Куллу Ша й 'и н Фа`ал ӱ һу Фӥ А з-Зубу р и \r",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4899,
        "text": "Ұа Куллу Ҫ а ғ ӥ р и н Ұа Каб ӥ р и н Муста ҭ а ру н \r",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4900,
        "text": "'И нн а А л-Мутта қ ӥ на Фӥ Җа нн ӓ ти н Ұа Наһа р и н \r",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
