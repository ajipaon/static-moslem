        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6045,
        "text": "Ұа А л- Қ ама р и 'Иҙӓ Талӓһӓ \r",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6046,
        "text": "Ұа А н -Наһ ӓ р и 'Иҙӓ Җаллӓһӓ \r",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6047,
        "text": "Ұа А л-Ла й ли 'Иҙӓ Йа ғ шӓһӓ \r",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6048,
        "text": "Ұа А с-Сам ӓ 'и Ұа Мӓ Банӓһӓ \r",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6049,
        "text": "Ұа А л-'Арđи Ұа Мӓ Ҭ ахӓһӓ \r",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6050,
        "text": "Ұа Нафси н Ұа Мӓ Саұұӓһӓ \r",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6051,
        "text": "Фа'алһамаһӓ Фуҗӱ ра һӓ Ұа Та қ ұӓһӓ \r",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6052,
        "text": "Қ а д 'Афлаха Ма н Заккӓһӓ \r",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6053,
        "text": "Ұа Қ а д Ҳ ӓ ба Ма н Дассӓһӓ \r",
