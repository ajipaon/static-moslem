  "data": {
    "number": 88,
    "name": "سُورَةُ الغَاشِيَةِ",
    "englishName": "Al-Ghaashiya",
    "englishNameTranslation": "The Overwhelming",
    "revelationType": "Meccan",
    "numberOfAyahs": 26,
    "ayahs": [
      {
        "number": 5968,
        "text": "Һал 'Ат ӓ ка Хадӥćу А л- Ғ ӓшийати \r",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5969,
        "text": "Ұуҗ ӱ һу н Йаұма'иҙин Ҳ ӓши`ату н \r",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5970,
        "text": "`Ӓмилату н Нӓ ҫ ибату н \r",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5971,
        "text": "Та ҫ лá Нӓ рӓ ан Хӓмийата н \r",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5972,
        "text": "Тус қ á Мин `А й нин 'Ӓнийати н \r",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5973,
        "text": "Ла й са Лаһу м Ҭ а` ӓ мун 'Иллӓ Ми н Đ а р ӥ `и н \r",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5974,
        "text": "Лӓ Йусмину Ұа Лӓ Йу ғ нӥ Ми н Җ ӱ `и н \r",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5975,
        "text": "Ұуҗ ӱ һу н Йаұма'иҙи н Нӓ`имату н \r",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5976,
        "text": "Лиса`йиһӓ Рӓ đийату н \r",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5977,
        "text": "Фӥ Җа нн атин `Ӓлийати н \r",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5978,
        "text": "Лӓ Тасма`у Фӥһӓ Лӓ ғ ийата н \r",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5979,
        "text": "Фӥһӓ `А й ну н Җӓ р ийату н \r",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5980,
        "text": "Фӥһӓ Су ру ру н Марфӱ`ату н \r",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5981,
        "text": "Ұа 'Акұ ӓ бу н Маұđӱ`ату н \r",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5982,
        "text": "Ұа Намӓ р и қ у Ма ҫ фӱфату н \r",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5983,
        "text": "Ұа За рӓ б ӥ йу Ма б ćӱćату н \r",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5984,
        "text": "'Афалӓ Йа н žур ӱ на 'Илá А л-'Ибили Ка й фа Ҳ ули қ ат \r",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5985,
        "text": "Ұа 'Илá А с-Сам ӓ 'и Ка й фа Ру фи`ат \r",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5986,
        "text": "Ұа 'Илá А л-Җиб ӓ ли Ка й фа Ну ҫ и б ат \r",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5987,
        "text": "Ұа 'Илá А л-'Арđи Ка й фа Су ҭ ихат \r",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5988,
        "text": "Фаҙакки р 'И нн ам ӓ 'А н та Муҙакки р у н \r",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5989,
        "text": "Ласта `Алайһи м Бимусай ҭ и р и н \r",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5990,
        "text": "'Иллӓ Ма н Таұаллá Ұа Кафа ра \r",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5991,
        "text": "Файу`аҙҙибуһу А лл ӓһу А л-`Аҙ ӓ ба А л-'Акба ра \r",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5992,
        "text": "'И нн а 'Илайн ӓ 'Ӥӓбаһу м \r",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5993,
        "text": "Ćу мм а 'И нн а `Алайнӓ Хисӓбаһу м \r",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      }
    ],
