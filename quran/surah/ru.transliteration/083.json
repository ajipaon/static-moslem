  "data": {
    "number": 83,
    "name": "سُورَةُ المُطَفِّفِينَ",
    "englishName": "Al-Mutaffifin",
    "englishNameTranslation": "Defrauding",
    "revelationType": "Meccan",
    "numberOfAyahs": 36,
    "ayahs": [
      {
        "number": 5849,
        "text": "Ұа й лу н Лилму ҭ аффиф ӥ на \r",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5850,
        "text": "Ал-Лаҙ ӥ на 'Иҙӓ А ктӓлӱ `Алá А н -Н ӓ си Йастаұф ӱ на \r",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5851,
        "text": "Ұа 'Иҙӓ Кӓлӱһу м 'Аұ Ұазанӱһу м Йу ҳ сир ӱ на \r",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5852,
        "text": "'Алӓ Йаžу нн у 'Ӱла'ика 'А нн аһу м Ма б `ӱć ӱ на \r",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5853,
        "text": "Лийа ұ мин `Аž ӥ ми н \r",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5854,
        "text": "Йа ұ ма Йа қ ӱ му А н -Н ӓ су Ли ра бби А л-`Ӓлам ӥ на \r",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5855,
        "text": "Калл ӓ 'И нн а Кит ӓ ба А л-Фуҗҗ ӓ р и Лафӥ Сиҗҗ ӥ ни н \r",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5856,
        "text": "Ұа М ӓ 'А д р ӓ ка Мӓ Сиҗҗ ӥ ну н \r",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5857,
        "text": "Кит ӓ бу н Мар қ ӱ му н \r",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5858,
        "text": "Ұа й лу н Йаұма'иҙи н Лилмукаҙҙиб ӥ на \r",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5859,
        "text": "Ал-Лаҙ ӥ на Йукаҙҙиб ӱ на Бийа ұ ми А д-Д ӥ ни \r",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5860,
        "text": "Ұа Мӓ Йукаҙҙибу Биһ и~ 'Иллӓ Куллу Му`тадин 'Аć ӥ ми н \r",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5861,
        "text": "'Иҙӓ Тутлá `Ала й һи 'Ӓйӓтунӓ Қ ӓ ла 'Асӓ ҭ ӥ р у А л-'Аұұал ӥ на \r",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5862,
        "text": "Каллӓ ۖ Ба л ۜ Р ӓ на `Алá Қ улӱбиһи м Мӓ Кӓнӱ Йаксиб ӱ на \r",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5863,
        "text": "Калл ӓ 'И нн аһу м `А н Ра ббиһи м Йаұма'иҙи н Ламахҗӱб ӱ на \r",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5864,
        "text": "Ćу мм а 'И нн аһу м Ла ҫ ӓлӱ А л-Җах ӥ ми \r",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5865,
        "text": "Ćу мм а Йу қ ӓ лу Һӓҙӓ А л-Лаҙӥ Ку н ту м Биһ и Тукаҙҙиб ӱ на \r",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5866,
        "text": "Калл ӓ 'И нн а Кит ӓ ба А л-'А б р ӓ р и Лафӥ `Иллӥй ӥ на \r",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5867,
        "text": "Ұа М ӓ 'А д р ӓ ка Мӓ `Иллӥй ӱ на \r",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5868,
        "text": "Кит ӓ бу н Мар қ ӱ му н \r",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5869,
        "text": "Йаш/һадуһу А л-Му қ ар ра б ӱ на \r",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5870,
        "text": "'И нн а А л-'А б р ӓ ра Лафӥ На` ӥ ми н \r",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5871,
        "text": "`Алá А л-'Ар ӓ 'ики Йа н žур ӱ на \r",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5872,
        "text": "Та` р ифу Фӥ Ұуҗӱһиһи м Наđ ра та А н -На` ӥ ми \r",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5873,
        "text": "Йус қ а ұ на Ми н Ра х ӥ қ и н Ма ҳ т ӱ ми н \r",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5874,
        "text": "Ҳ итӓмуһ у Миску н ۚ Ұа Фӥ Ҙӓлика Фалйатанӓфаси А л-Мутанӓфис ӱ на \r",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5875,
        "text": "Ұа Мизӓҗуһ у Ми н Тасн ӥ ми н \r",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5876,
        "text": "`Айнӓа н Йаш ра бу Биһӓ А л-Му қ ар ра б ӱ на \r",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5877,
        "text": "'И нн а А л-Лаҙ ӥ на 'А җ ра мӱ Кӓнӱ Мина А л-Лаҙ ӥ на 'Ӓманӱ Йаđхак ӱ на \r",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5878,
        "text": "Ұа 'Иҙӓ Маррӱ Биһи м Йата ғ ӓмаз ӱ на \r",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5879,
        "text": "Ұа 'Иҙӓ А н қ алаб ӱ 'Ил á 'Аһлиһиму А н қ алабӱ Факиһ ӥ на \r",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5880,
        "text": "Ұа 'Иҙӓ Ра 'аұһу м Қ ӓл ӱ 'И нн а Һ ӓ 'уул ӓ ' Лаđ ӓ лл ӱ на \r",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5881,
        "text": "Ұа М ӓ 'Урсилӱ `Алайһи м Хӓфиž ӥ на \r",
        "numberInSurah": 33,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5882,
        "text": "Фӓлйа ұ ма А л-Лаҙ ӥ на 'Ӓманӱ Мина А л-Куфф ӓ р и Йаđхак ӱ на \r",
        "numberInSurah": 34,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5883,
        "text": "`Алá А л-'Ар ӓ 'ики Йа н žур ӱ на \r",
        "numberInSurah": 35,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5884,
        "text": "Һал Ćӱұиба А л-Куфф ӓ ру Мӓ Кӓнӱ Йаф`ал ӱ на \r",
        "numberInSurah": 36,
        "juz": 30,
        "manzil": 7,
