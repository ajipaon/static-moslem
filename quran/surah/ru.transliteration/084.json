    "numberOfAyahs": 25,
    "ayahs": [
      {
        "number": 5885,
        "text": "'Иҙӓ А с-Сам ӓ 'у А н ша қ қ ат \r",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5886,
        "text": "Ұа 'Аҙинат Ли ра ббиһӓ Ұа Ху қ қ ат \r",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5887,
        "text": "Ұа 'Иҙӓ А л-'Арđу Муддат \r",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5888,
        "text": "Ұа 'Ал қ ат Мӓ Фӥһӓ Ұа Та ҳ аллат \r",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5889,
        "text": "Ұа 'Аҙинат Ли ра ббиһӓ Ұа Ху қ қ ат \r",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5890,
        "text": "Й ӓ 'Аййуһӓ А л-'И н с ӓ ну 'И нн ака Кӓдихун 'Илá Ра ббика Ка д хӓа н Фамулӓ қ ӥ һи \r",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5891,
        "text": "Фа'а мм ӓ Ман 'Ӱтийа Китӓбаһ у Бийамӥниһ и \r",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5892,
        "text": "Фаса ұ фа Йухӓсабу Хисӓбӓа н Йасӥ р ӓа н \r",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5893,
        "text": "Ұа Йа н қ алибу 'Ил á 'Аһлиһ и Масрӱ рӓ а н \r",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5894,
        "text": "Ұа 'А мм ӓ Ман 'Ӱтийа Китӓбаһ у Ұар ӓ 'а Ž аһ р иһ и \r",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5895,
        "text": "Фаса ұ фа Йа д `ӱ Ćубӱ рӓ а н \r",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5896,
        "text": "Ұа Йа ҫ лá Са`ӥ р ӓа н \r",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5897,
        "text": "'И нн аһ у К ӓ на Ф ӥ 'Аһлиһ и Масрӱ рӓ а н \r",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5898,
        "text": "'И нн аһу Ž а нн а 'А н Ла н Йах ӱ ра \r",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5899,
        "text": "Бал á 'И нн а Ра ббаһ у К ӓ на Биһ и Ба ҫ ӥ р ӓа н \r",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5900,
        "text": "Фал ӓ 'У қ симу Биш-Шафа қ и \r",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5901,
        "text": "Ұа А л-Ла й ли Ұа Мӓ Ұаса қ а \r",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5902,
        "text": "Ұа А л- Қ ама р и 'Иҙӓ А ттаса қ а \r",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5903,
        "text": "Латаркабу нн а Ҭ аба қ ӓан `А н Ҭ аба қ и н \r",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5904,
        "text": "Фамӓ Лаһу м Лӓ Йу'умин ӱ на \r",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5905,
        "text": "Ұа 'Иҙӓ Қ у р и'а `Алайһиму А л- Қ ур' ӓ ну Лӓ Йасҗуд ӱ на \r",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": {
          "id": 14,
          "recommended": true,
          "obligatory": false
        }
      },
      {
        "number": 5906,
        "text": "Бали А л-Лаҙ ӥ на Кафарӱ Йукаҙҙиб ӱ на \r",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5907,
        "text": "Ұа А лл ӓһу 'А`ламу Бимӓ Йӱ` ӱ на \r",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5908,
        "text": "Фабашши р һу м Би`аҙ ӓ бин 'Ал ӥ ми н \r",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
