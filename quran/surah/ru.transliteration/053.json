{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 53,
    "name": "سُورَةُ النَّجۡمِ",
    "englishName": "An-Najm",
    "englishNameTranslation": "The Star",
    "revelationType": "Meccan",
    "numberOfAyahs": 62,
    "ayahs": [
      {
        "number": 4785,
        "text": "Ұа А н -На җ ми 'Иҙӓ Һаұá \r",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4786,
        "text": "Мӓ Đ алла Ҫ ӓхибуку м Ұа Мӓ Ғ аұá \r",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4787,
        "text": "Ұа Мӓ Йа н ҭ и қ у `Ани А л-Һаұá \r",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4788,
        "text": "'Ин Һуұа 'Иллӓ Ұахйу н Йӱхá \r",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4789,
        "text": "`Алламаһу Шад ӥ ду А л- Қ уұá \r",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4790,
        "text": "Ҙӱ Ми р ра ти н Фӓстаұá \r",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4791,
        "text": "Ұа Һуұа Бил-'Уфу қ и А л-'А`лá \r",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4792,
        "text": "Ćу мм а Данӓ Фатадаллá \r",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4793,
        "text": "Фак ӓ на Қ ӓ ба Қ аұса й ни 'Аұ 'А д нá \r",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4794,
        "text": "Фа'аұх á 'Илá `А б диһ и М ӓ 'Аұхá \r",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4795,
        "text": "Мӓ Каҙаба А л-Фу'у ӓ ду Мӓ Ра ' á \r",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4796,
        "text": "'Афатумӓрӱнаһ у `Алá Мӓ Йарá \r",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4797,
        "text": "Ұа Ла қ а д Ра ' ӓ һу Назлатан 'У ҳ рá \r",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4798,
        "text": "`И н да Си д ра ти А л-Му н таһá \r",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4799,
        "text": "`И н даһӓ Җа нн ату А л-Ма'ұá \r",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4800,
        "text": "'Иҙ Йа ғ шá А с-Си д ра та Мӓ Йа ғ шá \r",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4801,
        "text": "Мӓ Зӓ ғ а А л-Ба ҫ а ру Ұа Мӓ Ҭ а ғ á \r",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4802,
        "text": "Ла қ а д Ра ' á Мин 'Ӓй ӓ ти Ра ббиһи А л-Ку б рá \r",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4803,
        "text": "'Афа ра 'айтуму А л-Л ӓ та Ұа А л-`Уззá \r",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4804,
        "text": "Ұа Манӓата А ć-Ćӓлиćата А л-'У ҳ рá \r",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4805,
        "text": "'Алакуму А ҙ-Ҙака ру Ұа Лаһу А л-'У н ćá \r",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4806,
        "text": "Тилка 'Иҙӓа н Қ исмату н Đ ӥзá \r",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4807,
        "text": "'Ин Һийа 'Илл ӓ 'Асм ӓ 'у н Са мм айтумӱһ ӓ 'А н ту м Ұа 'Ӓб ӓ 'ууку м М ӓ 'А н зала А лл ӓһу Биһӓ Ми н Сул ҭ ӓ нин ۚ 'И н Йаттаби` ӱ на 'Иллӓ А ž- Ž а нн а Ұа Мӓ Таһұá А л-'А н фусу ۖ Ұа Ла қ а д Җ ӓ 'аһу м Ми н Ра ббиһиму А л-Һудá \r",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4808,
        "text": "'А м Лил'и н с ӓ ни Мӓ Тама нн á \r",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4809,
        "text": "Фали лл ӓ һи А л-'Ӓ ҳ и ра ту Ұа А л-'Ӱлá \r",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4810,
        "text": "Ұа Ка м Ми н Малаки н Фӥ А с-Самӓұ ӓ ти Лӓ Ту ғ нӥ Шафӓ`атуһу м Шай' ӓ ан 'Иллӓ Ми н Ба`ди 'А н Йа'ҙана А лл ӓһу Лима н Йаш ӓ 'у Ұа Йарđá \r",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4811,
        "text": "'И нн а А л-Лаҙ ӥ на Лӓ Йу'умин ӱ на Бил-'Ӓ ҳ и ра ти Лайуса мм ӱ на А л-Мал ӓ 'иката Тасмийата А л-'У н ćá \r",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4812,
        "text": "Ұа Мӓ Лаһу м Биһ и Мин `Илмин ۖ 'И н Йаттаби` ӱ на 'Иллӓ А ž- Ž а нн а ۖ Ұа 'И нн а А ž- Ž а нн а Лӓ Йу ғ нӥ Мина А л-Ха қ қ и Шай' ӓ а н \r",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4813,
        "text": "Фа'а` р иđ `А н Ма н Таұаллá `А н Ҙик р инӓ Ұа Ла м Йу р и д 'Иллӓ А л-Хайӓата А д-Ду н йӓ \r",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4814,
        "text": "Ҙӓлика Ма б ла ғ уһу м Мина А л-`Илми ۚ 'И нн а Ра ббака Һуұа 'А`ламу Бима н Đ алла `А н Сабӥлиһ и Ұа Һуұа 'А`ламу Бимани А һтадá \r",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4815,
        "text": "Ұа Ли лл ӓ һ Мӓ Фӥ А с-Самӓұ ӓ ти Ұа Мӓ Фӥ А л-'Арđи Лийа җ зийа А л-Лаҙ ӥ на 'Ас ӓ ' ӱ Бимӓ `Амилӱ Ұа Йа җ зийа А л-Лаҙ ӥ на 'Ахсанӱ Бил-Хуснá \r",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4816,
        "text": "Ал-Лаҙ ӥ на Йа җ таниб ӱ на Каб ӓ 'и р а А л-'Иćми Ұа А л-Фаұӓхиша 'Иллӓ А л-Ламама ۚ 'И нн а Ра ббака Ұӓси`у А л-Ма ғ фи ра ти ۚ Һуұа 'А`ламу Бику м 'Иҙ 'А н ша'аку м Мина А л-'Арđи Ұа 'Иҙ 'А н ту м 'Аҗи нн ату н Фӥ Бу ҭ ӱ ни 'У мм аһӓтику м ۖ Фалӓ Тузакк ӱ 'А н фусаку м ۖ Һуұа 'А`ламу Бимани А тта қ á \r",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4817,
        "text": "'Афа ра 'а й та А л-Лаҙӥ Таұаллá \r",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4818,
        "text": "Ұа 'А` ҭ á Қ алӥлӓа н Ұа 'Акдá \r",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4819,
        "text": "'А`и н даһ у `Илму А л- Ғ а й би Фаһуұа Йарá \r",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4820,
        "text": "'А м Ла м Йунабба' Бимӓ Фӥ Ҫ ухуфи Мӱсá \r",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4821,
        "text": "Ұа 'И б рӓ һ ӥ ма А л-Лаҙӥ Ұа Ффá \r",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4822,
        "text": "'Аллӓ Тази р у Ұӓзи ра ту н Ұиз ра 'У ҳ рá \r",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4823,
        "text": "Ұа 'А н Ла й са Лил'и н с ӓ ни 'Иллӓ Мӓ Са`á \r",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4824,
        "text": "Ұа 'А нн а Са`йаһ у Са ұ фа Йурá \r",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4825,
        "text": "Ćу мм а Йу җ з ӓ һу А л-Җаз ӓ 'а А л-'Аұфá \r",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4826,
        "text": "Ұа 'А нн а 'Илá Ра ббика А л-Му н таһá \r",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4827,
        "text": "Ұа 'А нн аһ у Һуұа 'Аđхака Ұа 'А б кá \r",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4828,
        "text": "Ұа 'А нн аһ у Һуұа 'Ам ӓ та Ұа 'Ахйӓ \r",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4829,
        "text": "Ұа 'А нн аһу Ҳ ала қ а А з-Заұҗа й ни А ҙ-Ҙака ра Ұа А л-'У н ćá \r",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4830,
        "text": "Ми н Ну ҭ фатин 'Иҙӓ Ту м нá \r",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4831,
        "text": "Ұа 'А нн а `Ала й һи А н -Наш'ата А л-'У ҳ рá \r",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4832,
        "text": "Ұа 'А нн аһ у Һуұа 'А ғ нá Ұа 'А қ нá \r",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4833,
        "text": "Ұа 'А нн аһ у Һуұа Ра ббу А ш-Ши`рá \r",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4834,
        "text": "Ұа 'А нн аһ у~ 'Аһлака `Ӓдӓан А л-'Ӱлá \r",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4835,
        "text": "Ұа Ćам ӱ да Фам ӓ 'А б қ á \r",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4836,
        "text": "Ұа Қ а ұ ма Нӱхи н Ми н Қ а б лу ۖ 'И нн аһу м Кӓнӱ Һу м 'Аžлама Ұа 'А ҭ ғ á \r",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4837,
        "text": "Ұа А л-Му'утафиката 'Аһұá \r",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4838,
        "text": "Фа ғ ашшӓһӓ Мӓ Ғ ашшá \r",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4839,
        "text": "Фаби'а й йи 'Ӓл ӓ 'и Ра ббика Татамӓрá \r",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4840,
        "text": "Һӓҙӓ Наҙ ӥ р у н Мина А н -Нуҙу р и А л-'Ӱлá \r",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4841,
        "text": "'Азифати А л-'Ӓзифату \r",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
