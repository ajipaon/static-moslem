{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 79,
    "name": "سُورَةُ النَّازِعَاتِ",
    "englishName": "An-Naazi'aat",
    "englishNameTranslation": "Those who drag forth",
    "revelationType": "Meccan",
    "numberOfAyahs": 46,
    "ayahs": [
      {
        "number": 5713,
        "text": "Ұа А н -Нӓзи` ӓ ти Ғ ар қ ӓа н \r",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5714,
        "text": "Ұа А н -Нӓши ҭ ӓ ти Наш ҭ ӓа н \r",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5715,
        "text": "Ұа А с-Сӓбих ӓ ти Са б хӓа н \r",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5716,
        "text": "Фӓлссӓби қ ӓ ти Са б қ ӓа н \r",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5717,
        "text": "Фӓлмудабби р ӓ ти 'А м рӓ а н \r",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5718,
        "text": "Йа ұ ма Тарҗуфу А р- Рӓ җифату \r",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5719,
        "text": "Татба`уһӓ А р- Рӓ дифату \r",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5720,
        "text": "Қ ул ӱ бу н Йаұма'иҙи н Ұӓҗифату н \r",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5721,
        "text": "'А б ҫ ӓ ру һӓ Ҳ ӓши`ату н \r",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5722,
        "text": "Йа қ ӱл ӱ на 'А'и нн ӓ Ламардӱд ӱ на Фӥ А л-Хӓфи ра ти \r",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5723,
        "text": "'А'иҙӓ Ку нн ӓ `Иžӓмӓа н На ҳ и ра та н \r",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5724,
        "text": "Қ ӓлӱ Тилка 'Иҙӓа н Кар ра тун Ҳ ӓси ра ту н \r",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5725,
        "text": "Фа'и нн амӓ Һийа За җ ра ту н Ұӓхидату н \r",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5726,
        "text": "Фа'иҙӓ Һу м Бис-Сӓһи ра ти \r",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5727,
        "text": "Һал 'Т ӓ ка Хадӥćу Мӱсá \r",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5728,
        "text": "'Иҙ Нӓд ӓ һу Ра ббуһ у Бил-Ұ ӓ ди А л-Му қ аддаси Ҭ уұа н \r",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5729,
        "text": "А ҙ/һа б 'Илá Фи р `а ұ на 'И нн аһу Ҭ а ғ á \r",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5730,
        "text": "Фа қ ул Һа л Лака 'Ил á 'А н Тазаккá \r",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5731,
        "text": "Ұа 'Аһдийака 'Илá Ра ббика Фата ҳ шá \r",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5732,
        "text": "Фа'а р ӓ һу А л-'Ӓйата А л-Ку б рá \r",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5733,
        "text": "Факаҙҙаба Ұа `А ҫ á \r",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5734,
        "text": "Ćу мм а 'А д ба ра Йас`á \r",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5735,
        "text": "Фахаша ра Фанӓдá \r",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5736,
        "text": "Фа қ ӓ ла 'Анӓ Ра ббукуму А л-'А`лá \r",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5737,
        "text": "Фа'а ҳ аҙаһу А лл ӓһу Нак ӓ ла А л-'Ӓ ҳ и ра ти Ұа А л-'Ӱлá \r",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5738,
        "text": "'И нн а Фӥ Ҙӓлика Ла`и б ра та н Лима н Йа ҳ шá \r",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5739,
        "text": "'А'а н ту м 'Ашадду Ҳ ал қ ӓан 'Ами А с-Сам ӓ 'у ۚ Банӓһӓ \r",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5740,
        "text": "Ра фа`а Са м каһӓ Фасаұұӓһӓ \r",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5741,
        "text": "Ұа 'А ғ ҭ аша Лайлаһӓ Ұа 'А ҳ ра җа Đ ухӓһӓ \r",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5742,
        "text": "Ұа А л-'Арđа Ба`да Ҙӓлика Дахӓһӓ \r",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5743,
        "text": "'А ҳ ра җа Минһӓ М ӓ 'аһӓ Ұа Мар`ӓһӓ \r",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5744,
        "text": "Ұа А л-Җиб ӓ ла 'Арсӓһӓ \r",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5745,
        "text": "Матӓ`ӓа н Лаку м Ұа Ли'ан`ӓмику м \r",
        "numberInSurah": 33,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5746,
        "text": "Фа'иҙӓ Җ ӓ 'ати А ҭ - Ҭ ӓ мм ату А л-Ку б рá \r",
        "numberInSurah": 34,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5747,
        "text": "Йа ұ ма Йатаҙакка ру А л-'И н с ӓ ну Мӓ Са`á \r",
        "numberInSurah": 35,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5748,
        "text": "Ұа Бур р изати А л-Җах ӥ му Лима н Йарá \r",
        "numberInSurah": 36,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5749,
        "text": "Фа'а мм ӓ Ма н Ҭ а ғ á \r",
        "numberInSurah": 37,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5750,
        "text": "Ұа 'Ӓćа ра А л-Хайӓата А д-Ду н йӓ \r",
        "numberInSurah": 38,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5751,
        "text": "Фа'и нн а А л-Җах ӥ ма Һийа А л-Ма'ұá \r",
        "numberInSurah": 39,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5752,
        "text": "Ұа 'А мм ӓ Ман Ҳ ӓ фа Ма қ ӓ ма Ра ббиһ и Ұа Наһá А н -Нафса `Ани А л-Һаұá \r",
        "numberInSurah": 40,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5753,
        "text": "Фа'и нн а А л-Җа нн ата Һийа А л-Ма'ұá \r",
        "numberInSurah": 41,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5754,
        "text": "Йас'алӱнака `Ани А с-Сӓ`ати 'Айй ӓ на Мурсӓһӓ \r",
        "numberInSurah": 42,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5755,
        "text": "Ф ӥ ма 'А н та Ми н Ҙик рӓ һӓ \r",
        "numberInSurah": 43,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5756,
        "text": "'Илá Ра ббика Му н таһӓһӓ \r",
        "numberInSurah": 44,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5757,
        "text": "'И нн ам ӓ 'А н та Му н ҙи р у Ма н Йа ҳ шӓһӓ \r",
        "numberInSurah": 45,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5758,
        "text": "Ка'а нн аһу м Йа ұ ма Йа ра ұнаһӓ Ла м Йалбаć ӱ 'Иллӓ `Ашӥйатан 'Аұ Đ ухӓһӓ \r",
        "numberInSurah": 46,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      }
    ],
