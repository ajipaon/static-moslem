    "numberOfAyahs": 22,
    "ayahs": [
      {
        "number": 5910,
        "text": "Ұа А с-Сам ӓ 'и Ҙ ӓ ти А л-Бур ӱ җи \r",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5911,
        "text": "Ұа А л-Йа ұ ми А л-Маұ` ӱ ди \r",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5912,
        "text": "Ұа Шӓһиди н Ұа Маш/һ ӱ д и н \r",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5913,
        "text": "Қ утила 'А ҫ х ӓ бу А л-'У ҳ д ӱ ди \r",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5914,
        "text": "А н -Н ӓ р и Ҙ ӓ ти А л-Ұа қ ӱ ди \r",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5915,
        "text": "'Иҙ Һу м `Алайһӓ Қ у` ӱ д у н \r",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5916,
        "text": "Ұа Һу м `Алá Мӓ Йаф`ал ӱ на Бил-Му'умин ӥ на Шуһ ӱ д у н \r",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5917,
        "text": "Ұа Мӓ На қ амӱ Минһу м 'Илл ӓ 'А н Йу'уминӱ Би лл ӓһи А л-`Аз ӥ зи А л-Хам ӥ ди \r",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5918,
        "text": "Ал-Лаҙӥ Лаһ у Мулку А с-Самӓұ ӓ ти Ұа А л-'Арđи Ұа ۚ А лл ӓһу `Алá Кулли Ша й 'и н Шаһ ӥ д у н \r",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5919,
        "text": "'И нн а А л-Лаҙ ӥ на Фатанӱ А л-Му'умин ӥ на Ұа А л-Му'умин ӓ ти Ćу мм а Ла м Йатӱбӱ Фалаһу м `Аҙ ӓ бу Җаһа нн ама Ұа Лаһу м `Аҙ ӓ бу А л-Ха р ӥ қ и \r",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5920,
        "text": "'И нн а А л-Лаҙ ӥ на 'Ӓманӱ Ұа `Амилӱ А ҫ - Ҫ ӓлих ӓ ти Лаһу м Җа нн ӓ ту н Та җ р ӥ Ми н Тахтиһӓ А л-'Анһ ӓ ру ۚ Ҙӓлика А л-Фа ұ зу А л-Каб ӥ р у \r",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5921,
        "text": "'И нн а Ба ҭ ша Ра ббика Лашад ӥ д у н \r",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5922,
        "text": "'И нн аһ у Һуұа Йу б ди'у Ұа Йу` ӥ ду \r",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5923,
        "text": "Ұа Һуұа А л- Ғ аф ӱ ру А л-Ұад ӱ ду \r",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5924,
        "text": "Ҙӱ А л-`Арши А л-Маҗ ӥ ду \r",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5925,
        "text": "Фа`` ӓ лу н Лимӓ Йу р ӥ ду \r",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5926,
        "text": "Һал 'Ат ӓ ка Хадӥćу А л-Җун ӱ ди \r",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5927,
        "text": "Фи р `а ұ на Ұа Ćам ӱ да \r",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5928,
        "text": "Бали А л-Лаҙ ӥ на Кафарӱ Фӥ Такҙ ӥ б и н \r",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5929,
        "text": "Ұа А лл ӓһу Ми н Ұар ӓ 'иһи м Мухӥ ҭ у н \r",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
