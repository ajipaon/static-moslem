  "data": {
    "number": 81,
    "name": "سُورَةُ التَّكۡوِيرِ",
    "englishName": "At-Takwir",
    "englishNameTranslation": "The Overthrowing",
    "revelationType": "Meccan",
    "numberOfAyahs": 29,
    "ayahs": [
      {
        "number": 5801,
        "text": "'Иҙӓ А ш-Ша м су Кӱұи ра т \r",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5802,
        "text": "Ұа 'Иҙӓ А н -Нуҗ ӱ му А н када ра т \r",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5803,
        "text": "Ұа 'Иҙӓ А л-Җиб ӓ лу Суййи ра т \r",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5804,
        "text": "Ұа 'Иҙӓ А л-`Иш ӓ ру `У ҭ ҭ илат \r",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5805,
        "text": "Ұа 'Иҙӓ А л-Ұухӱшу Хуши ра т \r",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5806,
        "text": "Ұа 'Иҙӓ А л-Бих ӓ ру Суҗҗи ра т \r",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5807,
        "text": "Ұа 'Иҙӓ А н -Нуф ӱ су Зӱұи җ ат \r",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5808,
        "text": "Ұа 'Иҙӓ А л-Маұ' ӱ дату Су'илат \r",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5809,
        "text": "Би'а й йи Ҙа н би н Қ утилат \r",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5810,
        "text": "Ұа 'Иҙӓ А ҫ - Ҫ ухуфу Нуши ра т \r",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5811,
        "text": "Ұа 'Иҙӓ А с-Сам ӓ 'у Куши ҭ ат \r",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5812,
        "text": "Ұа 'Иҙӓ А л-Җах ӥ му Су``и ра т \r",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5813,
        "text": "Ұа 'Иҙӓ А л-Җа нн ату 'Узлифат \r",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5814,
        "text": "`Алимат Нафсу н М ӓ 'Ахđа ра т \r",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5815,
        "text": "Фал ӓ 'У қ симу Бил- Ҳ у нн аси \r",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5816,
        "text": "Ал-Җаұ ӓ р и А л-Ку нн аси \r",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5817,
        "text": "Ұа А л-Ла й ли 'Иҙӓ `Ас`аса \r",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5818,
        "text": "Ұа А ҫ - Ҫ у б хи 'Иҙӓ Танаффаса \r",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5819,
        "text": "'И нн аһ у Ла қ а ұ лу Ра с ӱ ли н Ка р ӥ ми н \r",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5820,
        "text": "Ҙӥ Қ ӱұатин `И н да Ҙӥ А л-`Арши Мак ӥ ни н \r",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5821,
        "text": "Му ҭ ӓ `и н Ćа мм а 'Ам ӥ ни н \r",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5822,
        "text": "Ұа Мӓ Ҫ ӓхибуку м Бима җ н ӱ ни н \r",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5823,
        "text": "Ұа Ла қ а д Ра ' ӓ һу Бил-'Уфу қ и А л-Муб ӥ ни \r",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5824,
        "text": "Ұа Мӓ Һуұа `Алá А л- Ғ а й би Биđан ӥ ни н \r",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5825,
        "text": "Ұа Мӓ Һуұа Би қ а ұ ли Шай ҭ ӓ ни н Ра җ ӥ ми н \r",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5826,
        "text": "Фа'а й на Таҙ/һаб ӱ на \r",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5827,
        "text": "'Ин Һуұа 'Иллӓ Ҙик ру н Лил`ӓлам ӥ на \r",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5828,
        "text": "Лима н Ш ӓ 'а Ми н ку м 'А н Йаста қ ӥ ма \r",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5829,
        "text": "Ұа Мӓ Таш ӓ ' ӱ на 'Илл ӓ 'А н Йаш ӓ 'а А лл ӓһу Ра ббу А л-`Ӓлам ӥ на \r",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      }
