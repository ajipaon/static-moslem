    "revelationType": "Meccan",
    "numberOfAyahs": 52,
    "ayahs": [
      {
        "number": 5324,
        "text": "Ал-Х ӓ қ қ ату \r",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5325,
        "text": "Мӓ А л-Х ӓ қ қ ату \r",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5326,
        "text": "Ұа М ӓ 'А д р ӓ ка Мӓ А л-Х ӓ қ қ ату \r",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5327,
        "text": "Каҙҙабат Ćам ӱ ду Ұа ` Ӓ ду н Бил- Қ ӓ р и`ати \r",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5328,
        "text": "Фа'а мм ӓ Ćам ӱ ду Фа'уһликӱ Би ҭ - Ҭ ӓ ғ ийати \r",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5329,
        "text": "Ұа 'А мм ӓ ` Ӓ ду н Фа'уһликӱ Би р ӥхи н Ҫ ар ҫ а р ин `Ӓтийати н \r",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5330,
        "text": "Са ҳ ҳ а ра һӓ `Алайһи м Са б `а Лай ӓ ли н Ұа Ćамӓнийата 'Айй ӓ мин Хусӱмӓа н Фатарá А л- Қ а ұ ма Фӥһӓ Ҫ ар`á Ка'а нн аһу м 'А`җ ӓ зу На ҳ лин Ҳ ӓұийати н \r",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5331,
        "text": "Фаһал Тарá Лаһу м Ми н Бӓ қ ийати н \r",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5332,
        "text": "Ұа Җ ӓ 'а Фи р `а ұ ну Ұа Ма н Қ а б лаһ у Ұа А л-Му'утафик ӓ ту Бил- Ҳ ӓ ҭ и'ати \r",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5333,
        "text": "Фа`а ҫ аұ Ра с ӱ ла Ра ббиһи м Фа'а ҳ аҙаһу м 'А ҳ ҙата н Рӓ бийата н \r",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5334,
        "text": "'И нн ӓ Ла мм ӓ Ҭ а ғ á А л-М ӓ 'у Хамалнӓку м Фӥ А л-Җӓ р ийати \r",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5335,
        "text": "Лина җ `алаһӓ Лаку м Таҙки ра та н Ұа Та`ийаһ ӓ 'Уҙуну н Ұӓ`ийату н \r",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5336,
        "text": "Фа'иҙӓ Нуфи ҳ а Фӥ А ҫ - Ҫ ӱ р и Наф ҳ ату н Ұӓхидату н \r",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5337,
        "text": "Ұа Хумилати А л-'Арđу Ұа А л-Җиб ӓ лу Фадуккатӓ Дакката н Ұӓхидата н \r",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5338,
        "text": "Файаұма'иҙи н Ұа қ а`ати А л-Ұӓ қ и`ату \r",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5339,
        "text": "Ұа А н ша қ қ ати А с-Сам ӓ 'у Фаһийа Йаұма'иҙи н Ұӓһийату н \r",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5340,
        "text": "Ұа А л-Малаку `Ал á 'Арҗ ӓ 'иһӓ ۚ Ұа Йахмилу `Арша Ра ббика Фаұ қ аһу м Йаұма'иҙи н Ćамӓнийату н \r",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5341,
        "text": "Йаұма'иҙи н Ту` ра đ ӱ на Лӓ Та ҳ фá Ми н ку м Ҳ ӓфийату н \r",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5342,
        "text": "Фа'а мм ӓ Ман 'Ӱтийа Китӓбаһ у Бийамӥниһ и Файа қ ӱ лу Һ ӓ 'уу м А қ ра ' ӱ Китӓбӥ \r",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5343,
        "text": "'И нн ӥ Ž ана н ту 'А нн ӥ Мул ӓ қ ин Хисӓбийаһ \r",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5344,
        "text": "Фаһуұа Фӥ `Ӥшати н Рӓ đийати н \r",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5345,
        "text": "Фӥ Җа нн атин `Ӓлийати н \r",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5346,
        "text": "Қ у ҭ ӱфуһӓ Дӓнийату н \r",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5347,
        "text": "Кулӱ Ұа А ш ра бӱ Һан ӥ ' ӓ а н Бим ӓ 'Аслафту м Фӥ А л-'Айй ӓ ми А л- Ҳ ӓлийати \r",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5348,
        "text": "Ұа 'А мм ӓ Ман 'Ӱтийа Китӓбаһ у Бишимӓлиһ и Файа қ ӱ лу Йӓ Лайтанӥ Ла м ' Ӱ та Китӓбӥһ \r",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5349,
        "text": "Ұа Ла м 'А д р и Мӓ Хисӓбӥһ \r",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5350,
        "text": "Йӓ Лайтаһӓ Кӓнати А л- Қ ӓđийата \r",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5351,
        "text": "М ӓ 'А ғ нá `А нн ӥ Мӓлӥһ ۜ \r",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5352,
        "text": "Һалака `А нн ӥ Сул ҭ ӓнӥһ \r",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5353,
        "text": "Ҳ уҙ ӱ һу Фа ғ улл ӱ һу \r",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5354,
        "text": "Ćу мм а А л-Җах ӥ ма Ҫ алл ӱ һу \r",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5355,
        "text": "Ćу мм а Фӥ Силсилати н Ҙар`уһӓ Са б ` ӱ на Ҙи р ӓ`ӓа н Фӓслук ӱ һу \r",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5356,
        "text": "'И нн аһ у К ӓ на Лӓ Йу'умину Би лл ӓһи А л-`Аž ӥ ми \r",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5357,
        "text": "Ұа Лӓ Йахуđđу `Алá Ҭ а` ӓ ми А л-Миск ӥ ни \r",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5358,
        "text": "Фала й са Лаһу А л-Йа ұ ма Һӓһунӓ Хам ӥ му н \r",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5359,
        "text": "Ұа Лӓ Ҭ а` ӓ мун 'Иллӓ Мин Ғ исл ӥ ни н \r",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5360,
        "text": "Лӓ Йа'кулуһ у~ 'Иллӓ А л- Ҳ ӓ ҭ и' ӱ на \r",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5361,
        "text": "Фал ӓ 'У қ симу Бимӓ Ту б ҫ ир ӱ на \r",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5362,
        "text": "Ұа Мӓ Лӓ Ту б ҫ ир ӱ на \r",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5363,
        "text": "'И нн аһ у Ла қ а ұ лу Ра с ӱ ли н Ка р ӥ ми н \r",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5364,
        "text": "Ұа Мӓ Һуұа Би қ а ұ ли Шӓ`и р и н ۚ Қ алӥлӓа н Мӓ Ту'умин ӱ на \r",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5365,
        "text": "Ұа Лӓ Би қ а ұ ли Кӓһини н ۚ Қ алӥлӓа н Мӓ Таҙаккар ӱ на \r",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5366,
        "text": "Та н з ӥ лу н Ми н Ра бби А л-`Ӓлам ӥ на \r",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5367,
        "text": "Ұа Лаұ Та қ аұұала `Алайнӓ Ба`đа А л-'А қ ӓұ ӥ ли \r",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5368,
        "text": "Ла'а ҳ аҙнӓ Минһу Бил-Йам ӥ ни \r",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5369,
        "text": "Ćу мм а Ла қ а ҭ а`нӓ Минһу А л-Ұат ӥ на \r",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5370,
        "text": "Фамӓ Ми н ку м Мин 'Ахадин `Анһу Хӓҗиз ӥ на \r",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5371,
        "text": "Ұа 'И нн аһ у Латаҙки ра ту н Лилмутта қ ӥ на \r",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5372,
        "text": "Ұа 'И нн ӓ Лана`ламу 'А нн а Ми н ку м Мукаҙҙиб ӥ на \r",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5373,
        "text": "Ұа 'И нн аһ у Лахас ра тун `Алá А л-Кӓфи р ӥ на \r",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5374,
        "text": "Ұа 'И нн аһ у Лаха қ қ у А л-Йа қ ӥ ни \r",
        "numberInSurah": 51,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5375,
        "text": "Фасаббих Биӓсми Ра ббика А л-`Аž ӥ ми \r",
        "numberInSurah": 52,
        "juz": 29,
        "manzil": 7,
