    "numberOfAyahs": 17,
    "ayahs": [
      {
        "number": 5932,
        "text": "Ұа А с-Сам ӓ 'и Ұа А ҭ - Ҭ ӓ р и қ и \r",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 591,
