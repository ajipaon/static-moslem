    "revelationType": "Medinan",
    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6139,
        "text": "'Иҙӓ Зулзилати А л-'Арđу Зилзӓлаһӓ \r",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6140,
        "text": "Ұа 'А ҳ ра җати А л-'Арđу 'Аć қ ӓлаһӓ \r",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6141,
        "text": "Ұа Қ ӓ ла А л-'И н с ӓ ну Мӓ Лаһӓ \r",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6142,
        "text": "Йаұма'иҙи н Тухаддиćу 'А ҳ бӓ ра һӓ \r",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6143,
        "text": "Би'а нн а Ра ббака 'Аұхá Лаһӓ \r",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6144,
        "text": "Йаұма'иҙи н Йа ҫ ду ру А н -Н ӓ су 'Аштӓтӓа н Лийу ра ұ 'А`мӓлаһу м \r",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6145,
        "text": "Фама н Йа`мал Миć қ ӓ ла Ҙар ра тин Ҳ ай рӓ а н Йа ра һ у \r",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6146,
        "text": "Ұа Ма н Йа`мал Миć қ ӓ ла Ҙар ра ти н Шар рӓ а н Йа ра һ у \r",
