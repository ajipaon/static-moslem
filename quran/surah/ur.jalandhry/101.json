    "name": "سُورَةُ القَارِعَةِ",
    "englishName": "Al-Qaari'a",
    "englishNameTranslation": "The Calamity",
    "revelationType": "Meccan",
    "numberOfAyahs": 11,
    "ayahs": [
      {
        "number": 6158,
        "text": "کھڑ کھڑانے والی",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6159,
        "text": "کھڑ کھڑانے والی کیا ہے؟",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6160,
        "text": "اور تم کیا جانوں کھڑ کھڑانے والی کیا ہے؟",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6161,
        "text": "(وہ قیامت ہے) جس دن لوگ ایسے ہوں گے جیسے بکھرے ہوئے پتنگے",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6162,
        "text": "اور پہاڑ ایسے ہو جائیں گے جیسے دھنکی ہوئی رنگ برنگ کی اون",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6163,
        "text": "تو جس کے (اعمال کے) وزن بھاری نکلیں گے",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6164,
        "text": "وہ دل پسند عیش میں ہو گا",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6165,
        "text": "اور جس کے وزن ہلکے نکلیں گے",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6166,
        "text": "اس کا مرجع ہاویہ ہے",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6167,
        "text": "اور تم کیا سمجھے کہ ہاویہ کیا چیز ہے؟",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6168,
        "text": "دہکتی ہوئی آگ ہے",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      }
