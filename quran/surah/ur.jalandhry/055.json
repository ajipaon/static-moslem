  "data": {
    "number": 55,
    "name": "سُورَةُ الرَّحۡمَٰن",
    "englishName": "Ar-Rahmaan",
    "englishNameTranslation": "The Beneficent",
    "revelationType": "Medinan",
    "numberOfAyahs": 78,
    "ayahs": [
      {
        "number": 4902,
        "text": "(خدا جو) نہایت مہربان",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4903,
        "text": "اسی نے قرآن کی تعلیم فرمائی",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4904,
        "text": "اسی نے انسان کو پیدا کیا",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4905,
        "text": "اسی نے اس کو بولنا سکھایا",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4906,
        "text": "سورج اور چاند ایک حساب مقرر سے چل رہے ہیں",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4907,
        "text": "اور بوٹیاں اور درخت سجدہ کر رہے ہیں",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4908,
        "text": "اور اسی نے آسمان کو بلند کیا اور ترازو قائم کی",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4909,
        "text": "کہ ترازو (سے تولنے) میں حد سے تجاوز نہ کرو",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4910,
        "text": "اور انصاف کے ساتھ ٹھیک تولو۔ اور تول کم مت کرو",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4911,
        "text": "اور اسی نے خلقت کے لئے زمین بچھائی",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4912,
        "text": "اس میں میوے اور کھجور کے درخت ہیں جن کے خوشوں پر غلاف ہوتے ہیں",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4913,
        "text": "اور اناج جس کے ساتھ بھس ہوتا ہے اور خوشبودار پھول",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4914,
        "text": "تو (اے گروہ جن وانس) تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4915,
        "text": "اسی نے انسان کو ٹھیکرے کی طرح کھنکھناتی مٹی سے بنایا",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4916,
        "text": "اور جنات کو آگ کے شعلے سے پیدا کیا",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4917,
        "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4918,
        "text": "وہی دونوں مشرقوں اور دونوں مغربوں کا مالک (ہے)",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4919,
