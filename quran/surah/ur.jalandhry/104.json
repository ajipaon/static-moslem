  "data": {
    "number": 104,
    "name": "سُورَةُ الهُمَزَةِ",
    "englishName": "Al-Humaza",
    "englishNameTranslation": "The Traducer",
    "revelationType": "Meccan",
    "numberOfAyahs": 9,
    "ayahs": [
      {
        "number": 6180,
        "text": "ہر طعن آمیز اشارتیں کرنے والے چغل خور کی خرابی ہے",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6181,
        "text": "جو مال جمع کرتا اور اس کو گن گن کر رکھتا ہے",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6182,
        "text": "(اور) خیال کرتا ہے کہ اس کا مال اس کی ہمیشہ کی زندگی کا موجب ہو گا",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6183,
        "text": "ہر گز نہیں وہ ضرور حطمہ میں ڈالا جائے گا",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6184,
        "text": "اور تم کیا سمجھے حطمہ کیا ہے؟",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6185,
        "text": "وہ خدا کی بھڑکائی ہوئی آگ ہے",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6186,
        "text": "جو دلوں پر جا لپٹے گی",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6187,
        "text": "(اور) وہ اس میں بند کر دیئے جائیں گے",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6188,
        "text": "(یعنی آگ کے) لمبے لمبے ستونوں میں",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      }
    ],
