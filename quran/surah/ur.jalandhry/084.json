  "data": {
    "number": 84,
    "name": "سُورَةُ الانشِقَاقِ",
    "englishName": "Al-Inshiqaaq",
    "englishNameTranslation": "The Splitting Open",
    "revelationType": "Meccan",
    "numberOfAyahs": 25,
    "ayahs": [
      {
        "number": 5885,
        "text": "جب آسمان پھٹ جائے گا",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5886,
        "text": "اور اپنے پروردگار کا فرمان بجا لائے گا اور اسے واجب بھی یہ ہی ہے",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5887,
        "text": "اور جب زمین ہموار کر دی جائے گی",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5888,
        "text": "جو کچھ اس میں ہے اسے نکال کر باہر ڈال دے گی اور (بالکل) خالی ہو جائے گی",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5889,
        "text": "اور اپنے پروردگار کے ارشاد کی تعمیل کرے گی اور اس کو لازم بھی یہی ہے (تو قیامت قائم ہو جائے گی)",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5890,
        "text": "اے انسان! تو اپنے پروردگار کی طرف (پہنچنے میں) خوب کوشِش کرتا ہے سو اس سے جا ملے گا",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5891,
        "text": "تو جس کا نامہٴ (اعمال) اس کے داہنے ہاتھ میں دیا جائے گا",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5892,
        "text": "اس سے حساب آسان لیا جائے گا",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5893,
        "text": "اور وہ اپنے گھر والوں میں خوش خوش آئے گا",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5894,
        "text": "اور جس کا نامہٴ (اعمال) اس کی پیٹھ کے پیچھے سے دیا جائے گا",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5895,
        "text": "وہ موت کو پکارے گا",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5896,
        "text": "اور وہ دوزخ میں داخل ہو گا",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5897,
        "text": "یہ اپنے اہل (و عیال) میں مست رہتا تھا",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5898,
        "text": "اور خیال کرتا تھا کہ (خدا کی طرف) پھر کر نہ جائے گا",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5899,
        "text": "ہاں ہاں۔ اس کا پروردگار اس کو دیکھ رہا تھا",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5900,
        "text": "ہمیں شام کی سرخی کی قسم",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5901,
        "text": "اور رات کی اور جن چیزوں کو وہ اکٹھا کر لیتی ہے ان کی",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5902,
        "text": "اور چاند کی جب کامل ہو جائے",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5903,
        "text": "کہ تم درجہ بدرجہ (رتبہٴ اعلیٰ پر) چڑھو گے",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5904,
        "text": "تو ان لوگوں کو کیا ہوا ہے کہ ایمان نہیں لاتے",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5905,
        "text": "اور جب ان کے سامنے قرآن پڑھا جاتا ہے تو سجدہ نہیں کرتے",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": {
          "id": 14,
          "recommended": true,
          "obligatory": false
        }
      },
      {
        "number": 5906,
        "text": "بلکہ کافر جھٹلاتے ہیں",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5907,
        "text": "اور خدا ان باتوں کو جو یہ اپنے دلوں میں چھپاتے ہیں خوب جانتا ہے",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5908,
        "text": "تو ان کو دکھ دینے والے عذاب کی خبر سنا دو",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5909,
        "text": "ہاں جو لوگ ایمان لائے اور نیک عمل کرتے رہے ان کے لیے بےانتہا اجر ہے",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      }
    ],
