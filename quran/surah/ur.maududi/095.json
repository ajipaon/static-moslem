    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6099,
        "text": "قسم ہے انجیر اور زیتون کی",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 597,
