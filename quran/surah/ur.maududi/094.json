        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6095,
        "text": "پس حقیقت یہ ہے کہ تنگی کے ساتھ فراخی بھی ہے",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6096,
        "text": "بے شک تنگی کے ساتھ فراخی بھی ہے",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 596,
