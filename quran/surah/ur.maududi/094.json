        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6094,
        "text": "اور تمہاری خاطر تمہارے ذکر کا آوازہ بلند کر دیا",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
