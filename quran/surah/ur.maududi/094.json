        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6092,
        "text": "اور تم پر سے وہ بھاری بوجھ اتار دیا",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6093,
        "text": "جو تمہاری کمر توڑے ڈال رہا تھا",
