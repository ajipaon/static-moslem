        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6190,
        "text": "کیا اُس نے اُن کی تدبیر کو اکارت نہیں کر دیا؟",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
