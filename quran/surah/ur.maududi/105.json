        "ruku": 547,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6191,
        "text": "اور اُن پر پرندوں کے جھنڈ کے جھنڈ بھیج دیے",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 547,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6192,
        "text": "جو اُن پر پکی ہوئی مٹی کے پتھر پھینک رہے تھے",
        "numberInSurah": 4,
        "juz": 30,
