    "numberOfAyahs": 5,
    "ayahs": [
      {
        "number": 6189,
        "text": "تم نے دیکھا نہیں کہ تمہارے رب نے ہاتھی والوں کے ساتھ کیا کیا؟",
        "numberInSurah": 1,
