  "data": {
    "number": 85,
    "name": "سُورَةُ البُرُوجِ",
    "englishName": "Al-Burooj",
    "englishNameTranslation": "The Constellations",
    "revelationType": "Meccan",
    "numberOfAyahs": 22,
    "ayahs": [
      {
        "number": 5910,
        "text": "Par le ciel aux constellations!",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5911,
        "text": "et par le jour promis!",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5912,
        "text": "et par le témoin et ce dont on témoigne!",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5913,
        "text": "Périssent les gens de l'Uhdûd,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5914,
        "text": "par le feu plein de combustible,",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5915,
        "text": "cependant qu'ils étaient assis tout autour,",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5916,
        "text": "ils étaient ainsi témoins de ce qu'ils faisaient des croyants,",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5917,
        "text": "à qui ils ne leur reprochaient que d'avoir cru en Allah, le Puissant, le Digne de louange,",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5918,
        "text": "Auquel appartient la royauté des cieux et de la terre. Allah est témoin de toute chose.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5919,
        "text": "Ceux qui font subir des épreuves aux croyants et aux croyantes, puis ne se repentent pas, auront le châtiment de l'Enfer et le supplice du feu.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5920,
        "text": "Ceux qui croient et accomplissent les bonnes œuvres auront des Jardins sous lesquels coulent les ruisseaux. Cela est le grand succès.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5921,
        "text": "La riposte de ton Seigneur est redoutable.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5922,
        "text": "C'est Lui, certes, qui commence (la création) et la refait.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5923,
        "text": "Et c'est Lui le Pardonneur, le Tout-Affectueux,",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5924,
        "text": "Le Maître du Trône, le Tout-Glorieux,",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5925,
        "text": "Il réalise parfaitement tout ce qu'Il veut.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5926,
        "text": "T'est-il parvenu le récit des armées,",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5927,
        "text": "de Pharaon, et de Thamûd?",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5928,
        "text": "Mais ceux qui ne croient pas persistent à démentir,",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5929,
        "text": "alors qu'Allah, derrière eux, les cerne de toutes parts.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5930,
        "text": "Mais c'est plutôt un Coran glorifié",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5931,
        "text": "préservé sur une Tablette (auprès d'Allah).",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 590,
        "ruku": 527,
        "hizbQuarter": 236,
        "sajda": false
      }
    ],
