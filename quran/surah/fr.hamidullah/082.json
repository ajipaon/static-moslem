    "revelationType": "Meccan",
    "numberOfAyahs": 19,
    "ayahs": [
      {
        "number": 5830,
        "text": "Quand le ciel se rompra,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5831,
        "text": "et que les étoiles se disperseront,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5832,
        "text": "et que les mers confondront leurs eaux,",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5833,
        "text": "et que les tombeaux seront bouleversés,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5834,
        "text": "toute âme saura alors ce qu'elle a accompli et ce qu'elle a remis de faire à plus tard.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5835,
        "text": "O homme! Qu'est-ce qui t'a trompé au sujet de ton Seigneur, le Noble,",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5836,
        "text": "qui t'a créé, puis modelé et constitué harmonieusement?",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5837,
        "text": "Il t'a façonné dans la forme qu'Il a voulue.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5838,
        "text": "Non...! [malgré tout] vous traitez la Rétribution de mensonge;",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5839,
        "text": "alors que veillent sur vous des gardiens,",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5840,
        "text": "de nobles scribes,",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5841,
        "text": "qui savent ce que vous faites.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5842,
        "text": "Les bons seront, certes, dans un [jardin] de délice,",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5843,
        "text": "et les libertins seront, certes, dans une fournaise",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5844,
        "text": "où ils brûleront, le jour de la Rétribution",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5845,
        "text": "incapables de s'en échapper.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5846,
        "text": "Et qui te dira ce qu'est le jour de la Rétribution?",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5847,
        "text": "Encore une fois, qui te dira ce qu'est le jour de la Rétribution?",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5848,
        "text": "Le jour où aucune âme ne pourra rien en faveur d'une autre âme. Et ce jour-là, le commandement sera à Allah.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 587,
