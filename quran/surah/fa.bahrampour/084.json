  "data": {
    "number": 84,
    "name": "سُورَةُ الانشِقَاقِ",
    "englishName": "Al-Inshiqaaq",
    "englishNameTranslation": "The Splitting Open",
    "revelationType": "Meccan",
    "numberOfAyahs": 25,
    "ayahs": [
      {
        "number": 5885,
        "text": "آن‌گاه كه آسمان بشكافد",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5886,
        "text": "و فرمان پروردگار خويش برد و حق همين است",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5887,
        "text": "و آن‌گاه كه زمين كشيده شود",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5888,
        "text": "و آنچه را درون خود دارد بيرون افكند و تهى شود",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5889,
        "text": "و فرمان پروردگار خويش برد و حق همين باشد",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5890,
        "text": "اى انسان! مسلما تو با تلاش و رنجى فراوان به سوى پروردگارت مى‌روى، پس ديدارش خواهى كرد",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5891,
        "text": "پس آن كه كارنامه‌اش به دست راستش داده شود",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5892,
        "text": "پس زودا كه با حسابى آسان محاسبه شود",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5893,
        "text": "و خوشحال نزد كسان خويش بازگردد",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5894,
        "text": "و اما آن كه كارنامه‌اش از پشت سرش به او داده شود",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5895,
        "text": "پس زودا كه فريادش بلند شود كه واى بر من",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5896,
        "text": "و در آتش در آيد",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5897,
        "text": "كه وى [در دنيا] ميان كسان خويش [از گناه‌] شادمان بود",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5898,
        "text": "او مى‌پنداشت كه هرگز بازگشتى نخواهد داشت",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5899,
        "text": "چرا، به راستى پروردگارش به او بينا بود",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5900,
        "text": "پس سوگند به شفق",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5901,
        "text": "سوگند به شب و آنچه را در بر گيرد",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5902,
        "text": "سوگند به ماه آن‌گاه كه كامل شود",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5903,
        "text": "كه شما حالات و منازلى را پشت سر مى‌گذاريد [حيات و مرگ و قيامت و حساب‌]",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5904,
        "text": "پس آنها را چه شده است كه ايمان نمى‌آورند",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5905,
        "text": "و چون قرآن بر آنها خوانده شود سجده نمى‌كنند",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": {
          "id": 14,
          "recommended": true,
          "obligatory": false
        }
      },
      {
        "number": 5906,
        "text": "نه، بلكه كافران [همواره‌] تكذيب مى‌كنند",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5907,
        "text": "و خدا بهتر داند كه در خاطر چه دارند",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5908,
        "text": "پس به عذاب دردناكى بشارتشان ده",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5909,
        "text": "مگر كسانى كه ايمان آوردند و اعمال شايسته كردند كه ايشان را پاداشى بى‌پايان است",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      }
    ],
