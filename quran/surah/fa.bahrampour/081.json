  "code": 200,
  "status": "OK",
  "data": {
    "number": 81,
    "name": "سُورَةُ التَّكۡوِيرِ",
    "englishName": "At-Takwir",
    "englishNameTranslation": "The Overthrowing",
    "revelationType": "Meccan",
    "numberOfAyahs": 29,
    "ayahs": [
      {
        "number": 5801,
        "text": "آن‌گاه كه خورشيد درهم پيچيده شود",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5802,
        "text": "و آن‌گاه كه ستارگان تيره شوند [و فرو ريزند]",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5803,
        "text": "و آن‌گاه كه كوه‌ها به راه افتند [و خرد شوند]",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5804,
        "text": "و آن‌گاه كه شتران آبستن وا نهاده شوند",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5805,
        "text": "و آن‌گاه كه درندگان گرد آورده شوند",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5806,
        "text": "و آن‌گاه كه درياها بجوشند",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5807,
        "text": "و آن‌گاه كه جان‌ها جفت شوند [و با قرين خود همراه گردند]",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5808,
        "text": "و آن‌گاه كه از دختر زنده به گور پرسيده شود",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5809,
        "text": "كه به كدامين گناه كشته شده است",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5810,
        "text": "و آن‌گاه كه نامه‌هاى اعمال گشوده شود",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5811,
        "text": "و آن‌گاه كه آسمان بركنده شود",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5812,
        "text": "و آن‌گاه كه دوزخ شعله‌ور گردد",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5813,
        "text": "و آن‌گاه كه بهشت نزديك آورده شود",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5814,
        "text": "[در آن روز] هر كس داند چه حاضر كرده است",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5815,
        "text": "پس سوگند به اختران بازگردنده",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5816,
        "text": "كه مى‌روند و نهان مى‌شوند",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5817,
        "text": "و سوگند به شب، آن‌گاه كه پشت كند",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5818,
        "text": "و سوگند به صبح چون بردمد",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5819,
        "text": "كه قطعا اين قرآن سخن فرستاده‌اى ارجمند است (جبرئيل)",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5820,
        "text": "نيرومندى كه نزد خداوند عرش، منزلت والايى دارد",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5821,
        "text": "در آن جا فرمانروا و امين است",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5822,
        "text": "و اين رفيق شما ديوانه نيست",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5823,
        "text": "او [جبرئيل‌] را در افق روشن ديده است",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5824,
        "text": "و او در [ابلاغ‌] وحى بخيل نيست",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5825,
        "text": "و اين [قرآن‌] سخن شيطان مطرود نيست",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5826,
        "text": "پس به كجا مى‌رويد",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5827,
        "text": "اين جز تذكارى براى جهانيان نيست",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5828,
        "text": "براى هر كس از شما كه خواهد راست و مستقيم شود",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5829,
        "text": "و شما نمى‌خواهيد مگر آن كه خدا، پروردگار جهانيان، بخواهد",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      }
    ],
