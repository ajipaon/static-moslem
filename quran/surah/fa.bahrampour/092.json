  "data": {
    "number": 92,
    "name": "سُورَةُ اللَّيۡلِ",
    "englishName": "Al-Lail",
    "englishNameTranslation": "The Night",
    "revelationType": "Meccan",
    "numberOfAyahs": 21,
    "ayahs": [
      {
        "number": 6059,
        "text": "قسم به شب وقتى كه فراپوشد",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6060,
        "text": "و به روز آنگاه كه تجلى كند",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6061,
        "text": "و به آنكه نر و ماده را آفريد",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6062,
        "text": "كه سعى و تلاش شما بسى پراكنده است [و هر كس به راهى مى‌رود]",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6063,
        "text": "اما آنكه انفاق كرد و پارسائى نمود",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6064,
        "text": "و [وعده‌ى‌] نيكو را باور داشت",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6065,
        "text": "پس زودا كه راه آسانى پيش پاى او گذاريم",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6066,
        "text": "و اما آن كه بخل ورزيد و [از دين‌] بى‌نيازى نمود",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6067,
        "text": "و آن [وعده‌ى‌] نيكو را انكار كرد",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6068,
        "text": "پس زودا كه طريقه سختى پيش وى آريم",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6069,
        "text": "و آن‌گاه كه به هلاكت افتد، مالش به حال او سودى ندهد",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6070,
        "text": "همانا هدايت بر عهده‌ى ماست",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6071,
        "text": "و آخرت و دنيا هم متعلق به ماست",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6072,
        "text": "پس شما را به آتشى كه زبانه كشد بيم دادم",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6073,
        "text": "كه جز تيره بخت‌ترين مردم، بدان نسوزد",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6074,
        "text": "همان كه تكذيب كرد و رخ برتافت",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6075,
        "text": "زودا كه پرهيزكارترين مردم، از آن دور داشته شود",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6076,
        "text": "همان كه مال خود را مى‌دهد تا پاك شود",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6077,
        "text": "و هيچ كس را نزد او حق نعمتى نيست تا او را پاداش دهد",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6078,
        "text": "جز طلب رضاى پروردگار والايش",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6079,
        "text": "و به زودى خشنود گردد",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      }
    ],
