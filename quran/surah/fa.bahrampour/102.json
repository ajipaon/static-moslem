    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6169,
        "text": "تفاخر و افزون طلبى، شما را سرگرم ساخت",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6170,
        "text": "تا وقتى كه به ديدار گورها رفتيد",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6171,
        "text": "چنين نيست [كه مى‌پنداريد]، به زودى خواهيد دانست [نتيجه غفلت را",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6172,
        "text": "باز چنين نيست، [كه مى‌پنداريد] به زودى خواهيد دانست",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6173,
        "text": "چنين نيست [كه مى‌پنداريد] اگر شما به علم يقينى مى‌دانستيد [كه آخرتى هست‌]",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6174,
        "text": "به يقين جهنم را مى‌ديديد",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6175,
        "text": "سپس يقينا آن دوزخ را به ديده‌ى يقين [و لمس‌] مشاهده مى‌كرديد",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6176,
        "text": "و آنگاه آن روز از اين نعمت بازخواست مى‌شويد",
        "numberInSurah": 8,
