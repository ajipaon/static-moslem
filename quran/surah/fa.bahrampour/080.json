{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 80,
    "name": "سُورَةُ عَبَسَ",
    "englishName": "Abasa",
    "englishNameTranslation": "He frowned",
    "revelationType": "Meccan",
    "numberOfAyahs": 42,
    "ayahs": [
      {
        "number": 5759,
        "text": "رو ترش كرد و رخ برتافت",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5760,
        "text": "كه آن نابينا نزد وى آمد",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5761,
        "text": "و تو چه دانى؟ شايد او پاك شود",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5762,
        "text": "يا پند گيرد و آن پند سودش دهد",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5763,
        "text": "اما آن كه [از پند تو] اظهار بى‌نيازى مى‌كند",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5764,
        "text": "پس تو به او مى‌پردازى",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5765,
        "text": "و اگر هم پاك نشود، گناهى بر تو نيست",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5766,
        "text": "و اما آن كه شتابان به سويت آمده",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5767,
        "text": "در حالى كه از خدا مى‌ترسد",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5768,
        "text": "پس تو از او به ديگرى مى‌پردازى",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5769,
        "text": "چنين مپندار، كه اين [آيات‌] تذكارى است",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5770,
        "text": "پس هر كه خواهد آن را ياد كند",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5771,
        "text": "[كه‌] در صحيفه‌هايى ارجمند",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5772,
        "text": "والا و پاكيزه",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5773,
        "text": "به دست سفيرانى",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5774,
        "text": "گرامى و نيكوكار است",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5775,
        "text": "مرگ بر اين انسان، چقدر حق پوش و ناسپاس است",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5776,
        "text": "خدا او را از چه چيزى آفريده",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5777,
        "text": "او را از نطفه‌اى آفريد و موزونش ساخت",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5778,
        "text": "آن‌گاه راه [ايمان و اطاعت‌] را بر او آسان كرد",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5779,
        "text": "سپس وى را بميراند و به گورش برد",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5780,
        "text": "سپس هر وقت بخواهد زنده‌اش مى‌كند",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5781,
        "text": "ولى نه! هنوز [انسان‌] آنچه را كه خدايش فرمان داده به جا نياورده است",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5782,
        "text": "پس آدمى بايد طعام خويش را بنگرد",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5783,
        "text": "به تحقيق ما آب فراوانى [از آسمان‌] فرو ريختيم",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5784,
        "text": "سپس زمين را از هم شكافتيم، شكافتنى",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5785,
        "text": "و در آن دانه‌ها برويانديم",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5786,
        "text": "و انگور و سبزى",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5787,
        "text": "و زيتون و خرما",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5788,
        "text": "و باغ‌هاى پردرخت",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5789,
        "text": "و ميوه و علفزار",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5790,
        "text": "تا متاعى براى شما و چهارپايانتان باشد",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5791,
        "text": "پس چون آن صيحه گوش خراش در رسد",
        "numberInSurah": 33,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5792,
        "text": "روزى كه آدمى از برادرش بگريزد",
        "numberInSurah": 34,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5793,
        "text": "و از مادر و پدرش",
        "numberInSurah": 35,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5794,
        "text": "و از همسر و فرزندانش",
        "numberInSurah": 36,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5795,
        "text": "در آن روز هر كسى از مردم وضعى دارد كه به خود مشغولش سازد",
        "numberInSurah": 37,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5796,
        "text": "آن روز بعضى چهره‌ها باز و روشن‌اند",
        "numberInSurah": 38,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5797,
        "text": "خندان و شادمانند",
        "numberInSurah": 39,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5798,
        "text": "و برخى چهره‌ها آن روز [گرفته‌اند و] بر آنها غبار [اندوه‌] نشسته است",
        "numberInSurah": 40,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5799,
        "text": "تيرگى و سياهى آنها را پوشانده است",
        "numberInSurah": 41,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5800,
        "text": "آنان همان كافران بدكارند",
        "numberInSurah": 42,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      }
    ],
