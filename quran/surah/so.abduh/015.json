{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 15,
    "name": "سُورَةُ الحِجۡرِ",
    "englishName": "Al-Hijr",
    "englishNameTranslation": "The Rock",
    "revelationType": "Meccan",
    "numberOfAyahs": 99,
    "ayahs": [
      {
        "number": 1803,
        "text": "Xarfaha hore waa la soo sheegay Macnahooda, Kuwana waa Aayadaha Kitaabka iyo Quraanka cad.",
        "numberInSurah": 1,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1804,
        "text": "Wax badanbay tamanin (Jeclaan) kuwii Gaaloobay (Aakhiro) inay Muslimiin ahaadaan (adduunkii).",
        "numberInSurah": 2,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1805,
        "text": "ka tag ha cuneen hana raaxasyteene Hana Shuqliso yididiilo way ogaan doonaane,",
        "numberInSurah": 3,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1806,
        "text": "Magaalo la halaago oo kasta waxaa u sugnaaday Wakhti la yaqaanno.",
        "numberInSurah": 4,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1807,
        "text": "Kamana hor marto umadi Ajasheeda (wakhtigeeda) Kamana dib marto.",
        "numberInSurah": 5,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1808,
        "text": "Waxay dhaheen kan Quraanka lagu soo dejiyayow Adugu waad waalantahay.",
        "numberInSurah": 6,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1809,
        "text": "Maad malaa'ig noo keentid haddaad run Sheegi.",
        "numberInSurah": 7,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1810,
        "text": "Umana soo Dehinno Malaa'igta xaq mooyee lamana sugeen Gaalada (markaas).",
        "numberInSurah": 8,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1811,
        "text": "Annagaa soo dejinnay Quraanka annagaana Dhawri.",
        "numberInSurah": 9,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1812,
        "text": "waxaan u dirray hortaa (rasuullo) Ummadihii hore.",
        "numberInSurah": 10,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1813,
        "text": "Rasuul kasta oo u yimaaddana waxay ahaayeen kuwo ku jeesjeesa.",
        "numberInSurah": 11,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1814,
        "text": "saasaana u galinaa (Gaalnimada) quluubta dambiilayaasha.",
        "numberInSurah": 12,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1815,
        "text": "mana rumeeyaan Quraanka waxaana tegay (hor maray) sumaddii «Jidkii» kuwii horreeyey.",
        "numberInSurah": 13,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1816,
        "text": "haddaan uga furro korkooda irrid samada ooy ahaadaan kuwo kora",
        "numberInSurah": 14,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1817,
        "text": "waxay odhan lahaayeen waxaan uun la awday (La tiray) aragyadannadii, waxaabanse nahay qoom la sixray.",
        "numberInSurah": 15,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1818,
        "text": "waxaan yeellay samada buruuj (Meelo) waxaan u qurxinnay kuwa dayi (eegi).",
        "numberInSurah": 16,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1819,
        "text": "waxaana ka dhawray shaydaan la tuuray «fogeeyey» dhammaantiis.",
        "numberInSurah": 17,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1820,
        "text": "mid dafa maqalka oo danab cad raaco mooyee.",
        "numberInSurah": 18,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1821,
        "text": "dhulkana waan fidinay waxaana ku ridnay «sugnay» dhexdeeda Buuro, waxaana ka soo bixinay wax kasta oo Miisaaman.",
        "numberInSurah": 19,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1822,
        "text": "waxaana idiinka yeellay dhexdiisa nolol iyo waxaydaan arzaaqi karin.",
        "numberInSurah": 20,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1823,
        "text": "waxaan agtanada khasnadihiisu «kaydkiisu» ahayn ma jiro, umana soo dejinno qaddar la ogyahay mooyee.",
        "numberInSurah": 21,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1824,
        "text": "waxaana dirnaa dabaysha oo Tallaasha (Daruurta) waxaana ka soo dajina samada Biyo aan idinku waraabinno idinkoo ahayn kuwo ka keydsan kara (gudhidda).",
        "numberInSurah": 22,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1825,
        "text": "annagaa wax looleeyna waxna dilla, anagaana wax dhaxalla uun (u hadha).",
        "numberInSurah": 23,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1826,
        "text": "waxaana ognahay kuwiinna hor maray waxaana ognahay kuwiina dib maray.",
        "numberInSurah": 24,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1827,
        "text": "Eebahaana isagaa soo kulmin dadka waana falsame oge ah.",
        "numberInSurah": 25,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1828,
        "text": "annagaana ka abuuray dadka dhoobo doorsoon.",
        "numberInSurah": 26,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1829,
        "text": "Jaankana (Jinniga) waxaan ka abuuray mar hore Naar samuum leh (Kulayl).",
        "numberInSurah": 27,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1830,
        "text": "Xusuuso markuu ku yidhi Eebahaa malaa'igta Anugu waxaan ka abuuri Dad dhoobo doorsoon.",
        "numberInSurah": 28,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1831,
        "text": "Ee markaan ekeeyo kuna afuufo dhexdiisa Ruuxdayda ugu dhaca Sujuud.",
        "numberInSurah": 29,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1832,
        "text": "wayna Sujuudeen malaa'igtii dhammaan oo Kulansan.",
        "numberInSurah": 30,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1833,
        "text": "Ibliis oo diiday inuu ka mid noqdo kuwa Sujuuday mooyee.",
        "numberInSurah": 31,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1834,
        "text": "Wuxuuna yidhi (Eebe) Ibliisow maxaad leedahay oodan u noqonin kuwa Sujuuda.",
        "numberInSurah": 32,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1835,
        "text": "Wuxuuna yidhi ma ihi Mid u sujuudi dad aad ka Abuurtay Dhoobo doorsoon.",
        "numberInSurah": 33,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1836,
        "text": "Wuxuuna yidhi Eebe ka bax xageeda adigu waa lagu fogeeyaye.",
        "numberInSurah": 34,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1837,
        "text": "Korkaagana waxaa ah Lacnad tan iyo Maalinta Abaal marinta (Qiyaamada).",
        "numberInSurah": 35,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1838,
        "text": "Wuxuuna yidhi Eebow i sug tan iyo maalinta La soo bixin dadka.",
        "numberInSurah": 36,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1839,
        "text": "Wuxuuna yidhi Eebe Adugu kuwa la sugi Yaad ka mid tahay.",
        "numberInSurah": 37,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1840,
        "text": "Tan iyo maalinta Wakhtiga la yaqaan leh.",
        "numberInSurah": 38,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1841,
        "text": "Wuxuuna yidhi Ibliis Eebow i baadiyayntaada Darteed waxaan u Qurxin dhulka Dhexdiisa waana Dhumin dhammaan.",
        "numberInSurah": 39,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1842,
        "text": "Addoomadaada cibaadada kuu kali Yeelay mooyee (lana doortay).",
        "numberInSurah": 40,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1843,
        "text": "Eebe wuxuu yidhi kaasi waa Jid igu toosan (oo layska abaal marin).",
        "numberInSurah": 41,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1844,
        "text": "Addoomadayda (toosan) Kuma lihid xujo Cid Ku raacday oo kuwa Baadida ah ka mida Mooyee.",
        "numberInSurah": 42,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1845,
        "text": "Jahanamaana yaboohoodu yahay (inta dhunta) Dhammaan.",
        "numberInSurah": 43,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1846,
        "text": "Waxayna leedahay todoba irridood oo irrid Kastana waxay leedahay Qaddar loo qaybiyay (oo Ibliis iyo Colkiisa ah).",
        "numberInSurah": 44,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1847,
        "text": "kuwa Eebe ka dhawrsadana waxay gali Janooyin iyo Ilo (durduri).",
        "numberInSurah": 45,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1848,
        "text": "waxaana la dhahaa ku gala Nabad galyo iyo Aaminimo.",
        "numberInSurah": 46,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1849,
        "text": "waxaana ka siibnaa waxa laabtooda ku jira oo xiqdi ah, waana walaalo sariira isqaabilsan ku sugan.",
        "numberInSurah": 47,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1850,
        "text": "kumana taabto jannada dhesdeeda dhib xageedana lagama Bixiyo.",
        "numberInSurah": 48,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1851,
        "text": "U warran addomadayda inaan anugu Dambi dhaafe Naxariista ahay,",
        "numberInSurah": 49,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1852,
        "text": "iyo in Cadaabkaygu uu yahay Cadaabka daran uun.",
        "numberInSurah": 50,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1853,
        "text": "Ugana waran dadka Martidii (Malaa'ig) Nabi Ibraahim.",
        "numberInSurah": 51,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1854,
        "text": "Markay u soo galeen kuna Dhaheen salaan Kuna yidhi waanu idinka cabsan.",
        "numberInSurah": 52,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1855,
        "text": "ooy Dhaheena ha cabsan Waxaanu kuugu Bishaarayn Wiil cilmi yeelane.",
        "numberInSurah": 53,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1856,
        "text": "Wuxuuna ku yidhi Ma waxaad ii bishaaraynaysaan Anoo odaynimo Itaabatay, Maxaad iigu Bishaaraynaysaan.",
        "numberInSurah": 54,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1857,
        "text": "Waxayna dhaheen waxaanu Kuugu Bishaarayn Xaq ee ha noqonin Mid quusta.",
        "numberInSurah": 55,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1858,
        "text": "Wuxuuna yidhi yaa ka Quusta Naxariista Eebihiis kuwa Baadida ah mooyee.",
        "numberInSurah": 56,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1859,
        "text": "Wuxuu yidhi Xaalkiinnu muxuu yahay Kuwan la Soo dirayaw.",
        "numberInSurah": 57,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1860,
        "text": "Waxayna dhaheen waxaa naloo Diray Qoom Danbiilayaal ah xagood.",
        "numberInSurah": 58,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1861,
        "text": "Ehelka (nabi) Luudh mooyee oo Waanu korin Dhammaantood.",
        "numberInSurah": 59,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1862,
        "text": "Haweenaydiisa mooyee oo noqon Kuwa ku Hadhi (Halaagga).",
        "numberInSurah": 60,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1863,
        "text": "Markay u yimaadeen Ehelki nabi Luudh kuwii La soo diray.",
        "numberInSurah": 61,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1864,
        "text": "Wuxuu yidhi Idinku waxaad Tihiin Qoom aan la Garanaynin.",
        "numberInSurah": 62,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1865,
        "text": "Waxayna dhaheen saas ma aha ee Waxaannu kuula Nimid waxay Shakisanaayeen (Halaagoodii).",
        "numberInSurah": 63,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1866,
        "text": "Waxaana kuula nimid Xaq, runna waan sheegaynaa.",
        "numberInSurah": 64,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1867,
        "text": "Ee guuri Ehelkaaga qayb Habeenka ka mida Socona Gadaashooda yayna soo Milicsan Ruux ka Mida (Dib) kuna socda meeja Laydin Faray.",
        "numberInSurah": 65,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1868,
        "text": "Waxaana u waxayoonay Amarkaas ah in Cidhibta Kuwaas la goyn Subaxdii.",
        "numberInSurah": 66,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1869,
        "text": "Waxaana yimid dadkii Baladka oo bishaaraysan (aragga martida).",
        "numberInSurah": 67,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1870,
        "text": "Wuxuuna yidhi kuwan waa Martidaydii ee Ha igu ceebaynina.",
        "numberInSurah": 68,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1871,
        "text": "kana dhawrsada Eebe hana i dullaynina.",
        "numberInSurah": 69,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1872,
        "text": "Waxayna dhaheen Miyaanaan kaa Reebin Caalamka (imaad marti galido cidna).",
        "numberInSurah": 70,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1873,
        "text": "wuxuuna yidhi Kuwaasi waa gabdhahaygii Iladaad wax Falaysaan (ood guursanaysaan).",
        "numberInSurah": 71,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1874,
        "text": "Cimrigaagee iyagu Baadibay ku Wareersan Yihiin.",
        "numberInSurah": 72,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1875,
        "text": "Waxaana qabatay Qayladii qorrax soo baxa.",
        "numberInSurah": 73,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1876,
        "text": "Waxaana ka yeellay korkoodii hoostooda Waxaana ku soo daadinnay Korkooda Dhagaxyo Naar ah.",
        "numberInSurah": 74,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1877,
        "text": "Arrintaasna calaamooyin baa ugu sugan Kuwa Fiirfiiriya (Fikira).",
        "numberInSurah": 75,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1878,
        "text": "Waxayna kutaal Magaaladaas waddo toosan (oy maraan oo cad).",
        "numberInSurah": 76,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1879,
        "text": "Arrintaasna calaamad yaa ugu sugan kuwa Xaqa rumeeyay.",
        "numberInSurah": 77,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1880,
        "text": "Waxayna ahaayeen Qoomkii kaynta Daalimiin (Qoomkii Nabi Shucayb).",
        "numberInSurah": 78,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1881,
        "text": "Waana ka aarsanay, waxayna labaduba degenaayeen Waddo cad (oo la yaqaanno).",
        "numberInSurah": 79,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1882,
        "text": "Waxayna beeniyeen Qoomkii Xijrina kuwii Loo soo diray (Saalax).",
        "numberInSurah": 80,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1883,
        "text": "Waana siinay Aayaadkanagii waxayna ahaayeen Kuwa xageeda ka jeedsada.",
        "numberInSurah": 81,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1884,
        "text": "Waxaana ahaayeen kuwo ka qota Buuraha Guryo aamina.",
        "numberInSurah": 82,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1885,
        "text": "Waxaana qabatay Qaylo Aroortii.",
        "numberInSurah": 83,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1886,
        "text": "Waxna uma tarin waxay Kasbanayeen.",
        "numberInSurah": 84,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1887,
        "text": "Samooyinka iyo Dhulka iyo waxa u dhaxeeyana waxaan Xaq ahayn umaanan abuurin Saacaduna way iman ee iska saamax saamixid quruxsan.",
        "numberInSurah": 85,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1888,
        "text": "Eebahaabaa ah abuuraha cilmiga lehe.",
        "numberInSurah": 86,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1889,
        "text": "waxaan ku siinay Todobo soo noqnoqota «faataxada» iyo Quraanka wayn.",
        "numberInSurah": 87,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1890,
        "text": "ee ha u taagin indhahaaga waxaan ugu raaxaynay qaybo ka mida. hana u murugoon una raarici (Jilei) garabkaaga (macaamiladooda) Mu'miniinta.",
        "numberInSurah": 88,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1891,
        "text": "waxaadna dhahdaa anugu waxaan uun ahay u dige cad.",
        "numberInSurah": 89,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1892,
        "text": "sidaan ugu soo dejinay Cadaab kuwii isu dhaarsaday (Dhibka xaqa).",
        "numberInSurah": 90,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1893,
        "text": "ee kayeelay Quraanka Qaybo.",
        "numberInSurah": 91,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1894,
        "text": "Eebahaa baan ku dhaartaye waan warsanaynaa dhammaantood.",
        "numberInSurah": 92,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1895,
        "text": "waxay falayeen.",
        "numberInSurah": 93,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1896,
        "text": "ee Muuji waxa lagu fari iskagana jeedso Mushrikiinta.",
        "numberInSurah": 94,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1897,
        "text": "anagaa kaa kaafiyeyna kuwa jeesjeesa.",
        "numberInSurah": 95,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1898,
        "text": "ee yeela Eebe Miciisa Ilaah kale wayna ogaan doonaan (waxa ku dhaca).",
        "numberInSurah": 96,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1899,
        "text": "waxaana ognahay Laabtaadu inay ka cidhiidhyami waxay sheegi (oo xun).",
        "numberInSurah": 97,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1900,
        "text": "ee ku Tasbiixso mahadda Eebahaa ahawna kuwa Sujuuda.",
        "numberInSurah": 98,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1901,
        "text": "Caabudna Eebahaa intay Yaqiintu kaaga timaaddo (Geeridu).",
        "numberInSurah": 99,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      }
