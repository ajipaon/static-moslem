{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 54,
    "name": "سُورَةُ القَمَرِ",
    "englishName": "Al-Qamar",
    "englishNameTranslation": "The Moon",
    "revelationType": "Meccan",
    "numberOfAyahs": 55,
    "ayahs": [
      {
        "number": 4847,
        "text": "Iqtarabatis Saa'atu wsan shaqqal qamar",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4848,
        "text": "Wa iny yaraw aayatany yu'ridoo wa yaqooloo sihrum mustamirr",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4849,
        "text": "Wa kazzaboo wattaba'ooo ahwaaa'ahum; wa kullu amrim mustaqirr",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4850,
        "text": "Wa laqad jaaa'ahum minal ambaaa'i maa feehi muzdajar",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4851,
        "text": "Hikmatum baalighatun famaa tughnin nuzur",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4852,
        "text": "Fatawalla 'anhum; yawma yad'ud daa'i ilaa shai 'in nukur",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4853,
        "text": "khushsha'an absaaruhum yakrujoona minal ajdaasi ka annahum jaraadum muntashir",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4854,
        "text": "Muhti'eena ilad daa'i yaqoolul kafiroona haazaa yawmun 'asir",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4855,
        "text": "Kazzabat qablahum qawmu Noohin fakazzaboo 'abdanaa wa qaaloo majnoo nunw wazdujir",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4856,
        "text": "Fada'aa Rabbahooo annee maghloobun fantasir",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4857,
        "text": "Fafatahnaaa abwaabas sa maaa'i bimaa'im munhamir",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4858,
        "text": "Wa fajjamal arda 'uyoonan faltaqal maaa'u 'alaaa amrin qad qudir",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4859,
        "text": "Wa hamalnaahu 'alaa zaati alwaahinw wa dusur",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4860,
        "text": "Tajree bi a'yuninaa jazaaa 'al liman kaana kufir",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4861,
        "text": "Wa laqat taraknaahaad aayatan fahal mim muddakir",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4862,
        "text": "Fakaifa kaana 'azaabee wa nuzur",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4863,
        "text": "Wa laqad yassamal Quraana liz zikri fahal mimmuddakir",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4864,
        "text": "Kazzabat 'Aadun fakaifa kaana 'azaabee wa nuzur",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4865,
        "text": "Innaa arsalnaa 'alaihim reehan sarsaran fee Yawmi nahsim mustamirr",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4866,
        "text": "Tanzi;un naasa ka anna huma'jaazu nakhlim munqa'ir",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4867,
        "text": "'Fakaifa kaana 'azaabee wa nuzur",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4868,
        "text": "Wa laqad yassamal Quraana liz zikri fahal mim muddakir",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 463,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4869,
        "text": "Kazzabat Samoodu binnuzur",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4870,
        "text": "Faqaalooo a-basharam minnaa waahidan nattabi'uhooo innaa izal lafee dalaalinw wa su'ur",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4871,
        "text": "'A-ulqiyaz zikru 'alaihi mim baininaa bal huwa kazzaabun ashir",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4872,
        "text": "Sa-ya'lamoona ghadam manil kazzaabul ashir",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4873,
        "text": "Innaa mursilun naaqati fitnatal lahum fartaqibhum wastabir",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 529,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4874,
        "text": "Wa nabbi'hum annal maaa'a qismatum bainahum kullu shirbim muhtadar",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4875,
        "text": "Fanaadaw saahibahum fata'aataa fa'aqar",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4876,
        "text": "Fakaifa kaana 'azaabee wa nuzur",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4877,
        "text": "Innaaa arsalnaa 'alaihim saihatanw waahidatan fakaano kahasheemil muhtazir",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4878,
        "text": "Wa laqad yassarnal quraana liz zikri fahal mim muddakir",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4879,
        "text": "Kazzabat qawmu lootim binnuzur",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4880,
        "text": "Innaa arsalnaa 'alaihim haasiban illaaa aala Loot najjainaahum bisahar",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4881,
        "text": "Ni'matam min 'indinaa; kazaalika najzee man shakar",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4882,
        "text": "Wa laqad anzarahum batshatanaa fatamaaraw binnuzur",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4883,
        "text": "Wa laqad raawadoohu 'andaifee fatamasnaaa a'yunahum fazooqoo 'azaabee wa nuzur",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4884,
        "text": "Wa laqad sabbahahum bukratan 'azaabum mustaqirr",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4885,
        "text": "Fazooqoo 'azaabee wa nuzur",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4886,
        "text": "Wa laqad yassarnal Quraana liz zikri fahal mim muddakir",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 464,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4887,
        "text": "Wa laqad jaaa'a Aala Fir'awnan nuzur",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4888,
        "text": "Kazzaboo bi Aayaatinaa kullihaa fa akhaznaahum akhza 'azeezim muqtadir",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4889,
        "text": "Akuffaarukum khairum min ulaaa'ikum am lakum baraaa'atun fiz Zubur",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4890,
        "text": "Am yaqooloona nahnu jamee'um muntasir",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4891,
        "text": "Sa yuhzamul jam'u wa yuwalloonad dubur",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4892,
        "text": "Balis Saa'atu maw'iduhum was Saa'atu adhaa wa amarr",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4893,
        "text": "Innal mujrimeena fee dalaalinw wa su'ur",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4894,
        "text": "Yawma yushaboona fin Naari'alaa wujoohimim zooqoo massa saqar",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4895,
        "text": "Innaa kulla shai'in khalaqnaahu biqadar",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 530,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4896,
        "text": "Wa maaa amrunaaa illaa waahidatun kalamhim bilbasar",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4897,
        "text": "Wa laqad ahlaknaaa ashyaa'akum fahal mim muddakir",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4898,
        "text": "Wa kullu shai'in fa'aloohu fiz Zubur",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4899,
        "text": "Wa kullu sagheerinw wa kabeerim mustatar",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4900,
        "text": "Innal muttaqeena fee jannaatinw wa nahar",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4901,
        "text": "Fee maq'adi sidqin 'inda Maleekim Muqtadir",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      }
    ],
