{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 52,
    "name": "سُورَةُ الطُّورِ",
    "englishName": "At-Tur",
    "englishNameTranslation": "The Mount",
    "revelationType": "Meccan",
    "numberOfAyahs": 49,
    "ayahs": [
      {
        "number": 4736,
        "text": "Wat-Toor",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4737,
        "text": "Wa kitaabim mastoor",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4738,
        "text": "Fee raqqim manshoor",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4739,
        "text": "Wal baitil ma'moor",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4740,
        "text": "Wassaqfil marfoo'",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4741,
        "text": "Wal bahril masjoor",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4742,
        "text": "Inna 'azaaba Rabbika lawaaqi'",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4743,
        "text": "Maa lahoo min daafi'",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4744,
        "text": "Yawma tamoorus samaaa'u mawraa",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4745,
        "text": "Wa taseerul jibaalu sairaa",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4746,
        "text": "Fawailuny yawma 'izil lil mukaazzibeen",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4747,
        "text": "Allazeena hum fee khawdiny yal'aboon",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4748,
        "text": "Yawma yuda'-'oona ilaa naari jahannama da'-'aa",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4749,
        "text": "Haazihin naarul latee kuntum bihaa tukazziboon",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4750,
        "text": "Afasihrun haazaaaa am antum laa tubsiroon",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4751,
        "text": "Islawhaa fasbirooo aw laa tasbiroo sawaaa'un 'alaikum innamaa tujzawna maa kuntum ta'maloon",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4752,
        "text": "Innal muttaqeena fee jannaatinw wa na'eem",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4753,
        "text": "Faakiheena bimaaa aataahum rabbuhum wa waqaahum rabbuhum 'azaabal jaheem",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4754,
        "text": "Kuloo washraboo haneee 'am bimaa kuntum ta'maloon",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4755,
        "text": "Muttaki'eena 'alaa sururim masfoofatinw wa zawwaj naahum bihoorin 'een",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4756,
        "text": "Wallazeena aamanoo wattaba'at hum zurriyyatuhum bieemaanin alhaqnaa bihim zurriyyatahum wa maaa alatnaahum min 'amalihim min shai'; kullum ri'im bimaa kasaba raheen",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4757,
        "text": "Wa amdadnaahum bifaa kihatinw wa lahmim mimmaa yashtahoon",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4758,
        "text": "Yatanaaza'oona feehaa kaasal laa laghwun feehaa wa laa taaseem",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4759,
        "text": "Wa yatoofu 'alaihim ghilmaanul lahum ka annahum lu'lu'um maknoon",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4760,
        "text": "Wa aqbala ba'duhum 'alaa ba'diny yatasaaa'aloon",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4761,
        "text": "Qaalooo innaa kunnaa qablu feee ahlinaa mushfiqeen",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4762,
        "text": "Famannnal laahu 'alainaa wa waqaanaa 'azaabas samoom",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4763,
        "text": "Innaa kunnaa min qablu nad'oohu innahoo huwal barrur raheem",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4764,
        "text": "Fazakkir famaaa anta bini'mati rabbika bikaahininw wa laa majnoon",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4765,
        "text": "Am yaqooloona shaa'irun natarabbasu bihee raibal manoon",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4766,
        "text": "Qul tarabbasoo fa innee ma'akum minal mutarabbiseen",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4767,
        "text": "Am taamuruhum ahlaamuhum bihaazaaa am hum qawmun taaghoon",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4768,
        "text": "Am yaqooloona taqawwalah; bal laa yu'minoon",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4769,
        "text": "Falyaatoo bihadeesim misliheee in kaanoo saadiqeen",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4770,
        "text": "Am khuliqoo min ghairi shai'in am humul khaaliqoon",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4771,
        "text": "Am khalaqus samaawaati wal ard; bal laa yooqinoon",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4772,
        "text": "Am'indahum khazaaa'inu rabbika am humul musaitiroon",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4773,
        "text": "Am lahum sullamuny yastami'oona feehi falyaati mustami'uhum bisultaanim mubeen",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4774,
        "text": "Am lahul banaatu wa lakumul banoon",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4775,
        "text": "Am tas'aluhum ajran fahum mim maghramim musqaloon",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4776,
        "text": "Am 'indahumul ghaibu fahum yaktuboon",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4777,
        "text": "Am yureedoona kaidan fallazeena kafaroo humul makeedoon",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4778,
        "text": "Am lahum ilaahun ghairul laa; subhaanal laahi 'ammaa yushrikoon",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4779,
        "text": "Wa iny yaraw kisfam minas samaaa'i saaqitany yaqooloo sahaabum markoom",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4780,
        "text": "Fazarhum hatta yulaaqoo yawmahumul lazee feehi yus'aqoon",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4781,
        "text": "Yawma laa yughnee 'anhum kaidumhum shai'anw wa laa hum yunsaroon",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4782,
        "text": "Wa inna lillazeena zalamoo 'azaaban doona zalika wa laakinna aksarahum laa ya'lamoon",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4783,
        "text": "Wasbir lihukmi rabbika fa innaka bi-a'yuninaa wa sabbih bihamdi rabbika heena taqoom",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4784,
        "text": "Wa minal laili fasabbihhu wa idbaaran nujoom",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      }
    ],
