        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5973,
        "text": "Laisa lahum ta'aamun illaa min daree'",
