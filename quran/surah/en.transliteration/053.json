{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 53,
    "name": "سُورَةُ النَّجۡمِ",
    "englishName": "An-Najm",
    "englishNameTranslation": "The Star",
    "revelationType": "Meccan",
    "numberOfAyahs": 62,
    "ayahs": [
      {
        "number": 4785,
        "text": "Wannajmi izaa hawaa",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4786,
        "text": "Maa dalla saahibukum wa maa ghawaa",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4787,
        "text": "Wa maa yyantiqu 'anilhawaaa",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4788,
        "text": "In huwa illaa Wahyuny yoohaa",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4789,
        "text": "'Allamahoo shadeedul quwaa",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4790,
        "text": "Zoo mirratin fastawaa",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4791,
        "text": "Wa huwa bil ufuqil a'laa",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4792,
        "text": "Summa danaa fatadalla",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4793,
        "text": "Fakaana qaaba qawsaini aw adnaa",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4794,
        "text": "Fa awhaaa ilaa 'abdihee maaa awhaa",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4795,
        "text": "Maa kazabal fu'aadu maa ra aa",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4796,
        "text": "Afatumaaroonahoo 'alaa maayaraa",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4797,
        "text": "Wa laqad ra aahu nazlatan ukhraa",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4798,
        "text": "'Inda sidratil muntaha",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4799,
        "text": "'Indahaa jannatul maawaa",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4800,
        "text": "Iz yaghshas sidrata maa yaghshaa",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4801,
        "text": "Maa zaaghal basaru wa maa taghaa",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4802,
        "text": "Laqad ra aa min aayaati Rabbihil kubraaa",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4803,
        "text": "Afara'aytumul laata wal 'uzzaa",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4804,
        "text": "Wa manaatas saalisatal ukhraa",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4805,
        "text": "A-lakumuz zakaru wa lahul unsaa",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4806,
        "text": "Tilka izan qismatun deezaa",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4807,
        "text": "In hiya illaaa asmaaa'un sammaitumoohaaa antum wa aabaaa'ukum maaa anzalal laahu bihaa min sultaan; inyyattabi'oona illaz zanna wa maa tahwal anfusu wa laqad jaaa'ahum mir Rabbihimul hudaa",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4808,
        "text": "Am lil insaani maa taman naa",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4809,
        "text": "Falillaahil aakhiratu wal oolaa",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4810,
        "text": "Wa kam mim malakin fissamaawaati laa tughnee shafaa'atuhum shai'an illaa mim ba'di anyyaazanal laahu limany yashaaa'u wa yardaa",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4811,
        "text": "innal lazeena laa yu'minoona bil aakhirati la yusammoonal malaaa'ikata tasmiyatal unsaa",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4812,
        "text": "Wa maa lahum bihee min 'ilmin iny yattabi'oona illaz zanna wa innaz zanna laa yughnee minal haqqi shai'aa",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4813,
        "text": "Fa a'rid 'am man tawallaa 'an zikrinaa wa lam yurid illal hayaatad dunyaa",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4814,
        "text": "Zalika mablaghuhum minal 'ilm; inna rabbaka huwa a'lamu biman dalla 'an sabee lihee wa huwa a'lamu bimanih tadaa",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4815,
        "text": "Wa lillaahi maa fis samaawaati wa maa fil ardi liyajziyal lazeena asaaa'oo bimaa 'amiloo wa yajziyal lazeena ahsanoo bilhusnaa",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4816,
        "text": "Allazeena yajtaniboona kabaaa'iral ismi walfawaa hisha illal lamam; inna rabbaka waasi'ul maghfirah; huwa a'lamu bikum iz ansha akum minal ardi wa iz antum ajinnatun fee butooni umma haatikum falaa tuzakkooo anfusakum huwa a'lamu bimanit taqaa",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4817,
        "text": "Afara'ayatal lazee tawallaa",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4818,
        "text": "Wa a'taa qaleelanw wa akdaa",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4819,
        "text": "A'indahoo 'ilmul ghaibi fahuwa yaraa",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4820,
        "text": "Am lam yunabbaa bimaa fee suhuhfi Moosa",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4821,
        "text": "Wa Ibraaheemal lazee waffaaa",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4822,
        "text": "Allaa taziru waaziratunw wizra ukhraa",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4823,
        "text": "Wa al laisa lil insaani illaa maa sa'aa",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4824,
        "text": "Wa anna sa'yahoo sawfa yuraa",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4825,
        "text": "Summa yujzaahul jazaaa 'al awfaa",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4826,
        "text": "Wa anna ilaa rabbikal muntahaa",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4827,
        "text": "Wa annahoo huwa adhaka wa abkaa",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4828,
        "text": "Wa annahoo huwa amaata wa ahyaa",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4829,
        "text": "Wa annahoo khalaqaz zawjainiz zakara wal unsaa",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4830,
        "text": "Min nutfatin izaa tumnaa",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4831,
        "text": "Wa anna 'alaihin nash atal ukhraa",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4832,
        "text": "Wa annahoo huwa aghnaa wa aqnaa",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4833,
        "text": "Wa annahoo huwa rabbush shi'raa",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4834,
        "text": "Wa annahooo ahlak a 'Aadanil oolaa",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4835,
        "text": "Wa samooda famaaa abqaa",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4836,
        "text": "Wa qawma Noohim min qablu innahum kaanoo hum azlama wa atghaa",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4837,
        "text": "Wal mu'tafikata ahwaa",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4838,
        "text": "Faghashshaahaa maa ghashshaa",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4839,
        "text": "Fabi ayyi aalaaa'i Rabbika tatamaaraa",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4840,
        "text": "Haazaa nazeerum minan nuzuril oolaa",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4841,
        "text": "Azifatil laazifah",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4842,
        "text": "Laisa lahaa min doonil laahi kaashifah",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4843,
        "text": "Afamin hazal hadeesi ta'jaboon",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4844,
        "text": "Wa tadhakoona wa laa tabkoon",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4845,
        "text": "Wa antum saamidoon",
        "numberInSurah": 61,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4846,
        "text": "Fasjudoo lillaahi wa'budoo",
        "numberInSurah": 62,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": {
          "id": 13,
          "recommended": false,
          "obligatory": true
        }
      }
    ],
