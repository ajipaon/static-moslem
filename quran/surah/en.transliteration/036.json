{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 36,
    "name": "سُورَةُ يسٓ",
    "englishName": "Yaseen",
    "englishNameTranslation": "Yaseen",
    "revelationType": "Meccan",
    "numberOfAyahs": 83,
    "ayahs": [
      {
        "number": 3706,
        "text": "Yaa-Seeen",
        "numberInSurah": 1,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3707,
        "text": "Wal-Qur-aanil-Hakeem",
        "numberInSurah": 2,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3708,
        "text": "Innaka laminal mursaleen",
        "numberInSurah": 3,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3709,
        "text": "'Alaa Siraatim Mustaqeem",
        "numberInSurah": 4,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3710,
        "text": "Tanzeelal 'Azeezir Raheem",
        "numberInSurah": 5,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3711,
        "text": "Litunzira qawmam maaa unzira aabaaa'uhum fahum ghaafiloon",
        "numberInSurah": 6,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3712,
        "text": "Laqad haqqal qawlu 'alaaa aksarihim fahum laa yu'minoon",
        "numberInSurah": 7,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3713,
        "text": "Innaa ja'alnaa feee a'naaqihim aghlaalan fahiya ilal azqaani fahum muqmahoon",
        "numberInSurah": 8,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3714,
        "text": "Wa ja'alnaa mim baini aydeehim saddanw-wa min khalfihim saddan fa aghshai naahum fahum laa yubsiroon",
        "numberInSurah": 9,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3715,
        "text": "Wa sawaaa'un 'alaihim 'a-anzartahum am lam tunzirhum laa yu'minoon",
        "numberInSurah": 10,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3716,
        "text": "Innamaa tunziru manit taba 'az-Zikra wa khashiyar Rahmaana bilghaib, fabashshirhu bimaghfiratinw-wa ajrin kareem",
        "numberInSurah": 11,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3717,
        "text": "Innaa Nahnu nuhyil mawtaa wa naktubu maa qaddamoo wa aasaarahum; wa kulla shai'in ahsainaahu feee Imaamim Mubeen",
        "numberInSurah": 12,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3718,
        "text": "Wadrib lahum masalan Ashaabal Qaryatih; iz jaaa'ahal mursaloon",
        "numberInSurah": 13,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3719,
        "text": "Iz arsalnaaa ilaihimusnaini fakazzaboohumaa fa'azzaznaa bisaalisin faqaalooo innaaa ilaikum mursaloon",
        "numberInSurah": 14,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3720,
        "text": "Qaaloo maaa antum illaa basharum mislunaa wa maaa anzalar Rahmaanu min shai'in in antum illaa takziboon",
        "numberInSurah": 15,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3721,
        "text": "Qaaloo Rabbunaa ya'lamu innaaa ilaikum lamursaloon",
        "numberInSurah": 16,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3722,
        "text": "Wa maa 'alainaaa illal balaaghul mubeen",
        "numberInSurah": 17,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3723,
        "text": "Qaaloo innaa tataiyarnaa bikum la'il-lam tantahoo lanar jumannakum wa la-yamassan nakum minnaa 'azaabun aleem",
        "numberInSurah": 18,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3724,
        "text": "Qaaloo taaa'irukum ma'akum; a'in zukkirtum; bal antum qawmum musrifoon",
        "numberInSurah": 19,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3725,
        "text": "Wa jaaa'a min aqsal madeenati rajuluny yas'aa qaala yaa qawmit tabi'ul mursaleen",
        "numberInSurah": 20,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3726,
        "text": "Ittabi'oo mal-laa yas'alukum ajranw-wa hum muhtadoon",
        "numberInSurah": 21,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3727,
        "text": "Wa maa liya laaa a'budul lazee fataranee wa ilaihi turja'oon",
        "numberInSurah": 22,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3728,
        "text": "'A-attakhizu min dooniheee aalihatan iny-yuridnir Rahmaanu bidurril-laa tughni 'annee shafaa 'atuhum shai 'anw-wa laa yunqizoon",
        "numberInSurah": 23,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3729,
        "text": "Inneee izal-lafee dalaa-lim-mubeen",
        "numberInSurah": 24,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3730,
        "text": "Inneee aamantu bi Rabbikum fasma'oon",
        "numberInSurah": 25,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3731,
        "text": "Qeelad khulil Jannnah; qaala yaa laita qawmee ya'lamoon",
        "numberInSurah": 26,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3732,
        "text": "Bimaa ghafara lee Rabbee wa ja'alanee minal mukrameen",
        "numberInSurah": 27,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3733,
        "text": "Wa maaa anzalnaa 'alaa qawmihee mim ba'dihee min jundim minas-samaaa'i wa maa kunnaa munzileen",
        "numberInSurah": 28,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 382,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3734,
        "text": "In kaanat illaa saihatanw waahidatan fa-izaa hum khaamidoon",
        "numberInSurah": 29,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 382,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3735,
        "text": "Yaa hasratan 'alal 'ibaad; maa yaateehim mir Rasoolin illaa kaanoo bihee yastahzi 'oon",
        "numberInSurah": 30,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 382,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3736,
        "text": "Alam yaraw kam ahlak naa qablahum minal qurooni annahum ilaihim laa yarji'oon",
        "numberInSurah": 31,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 382,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3737,
        "text": "Wa in kullul lammaa jamee'ul-ladainaa muhdaroon",
        "numberInSurah": 32,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 382,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3738,
        "text": "Wa Aayatul lahumul ardul maitatu ahyainaahaa wa akhrajnaa minhaa habban faminhu yaakuloon",
        "numberInSurah": 33,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3739,
        "text": "Wa ja'alnaa feehaa jannaatim min nakheelinw wa a'naabinw wa fajjarnaa feeha minal 'uyoon",
        "numberInSurah": 34,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3740,
        "text": "Liyaakuloo min samarihee wa maa 'amilat-hu aideehim; afalaa yashkuroon",
        "numberInSurah": 35,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3741,
        "text": "Subhaanal lazee khalaqal azwaaja kullahaa mimmaa tumbitul ardu wa min anfusihim wa mimmaa laa ya'lamoon",
        "numberInSurah": 36,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3742,
        "text": "Wa Aayatul lahumul lailu naslakhu minhun nahaara fa-izaa hum muzlimoon",
        "numberInSurah": 37,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3743,
        "text": "Wash-shamsu tajree limustaqarril lahaa; zaalika taqdeerul 'Azeezil Aleem",
        "numberInSurah": 38,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3744,
        "text": "Walqamara qaddarnaahu manaazila hattaa 'aada kal'ur joonil qadeem",
        "numberInSurah": 39,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3745,
        "text": "Lash shamsu yambaghee lahaaa an tudrikal qamara wa lal lailu saabiqun nahaar; wa kullun fee falaki yasbahoon",
        "numberInSurah": 40,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3746,
        "text": "Wa Aayatul lahum annaa hamalnaa zurriyatahum fil fulkil mashhoon",
        "numberInSurah": 41,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3747,
        "text": "Wa khalaqnaa lahum mim-mislihee maa yarkaboon",
        "numberInSurah": 42,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3748,
        "text": "Wa in nashaa nughriqhum falaa sareekha lahum wa laa hum yunqazoon",
        "numberInSurah": 43,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3749,
        "text": "Illaa rahmatam minnaa wa mataa'an ilaa heen",
        "numberInSurah": 44,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3750,
        "text": "Wa izaa qeela lahumuttaqoo maa baina aideekum wa maa khalfakum la'allakum turhamoon",
        "numberInSurah": 45,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3751,
        "text": "Wa maa taateehim min aayatim min ayataati Rabbihim illaa kaanoo 'anhaa mu'rideen",
        "numberInSurah": 46,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3752,
        "text": "Wa izaa qeela lahum anfiqoo mimmaa razaqakumul laahu qaalal lazeena kafaroo lillazeena aamanooo anut'imu mal-law yashaaa'ul laahu at'amahooo in antum illaa fee dalaalim mubeen",
        "numberInSurah": 47,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3753,
        "text": "Wa yaqooloona mataa haazal wa'du in kuntum saadiqeen",
        "numberInSurah": 48,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3754,
        "text": "Maa yanzuroona illaa saihatanw waahidatan taa khuzuhum wa hum yakhissimoon",
        "numberInSurah": 49,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3755,
        "text": "Falaa yastatee'oona taw siyatanw-wa laaa ilaaa ahlihim yarji'oon",
        "numberInSurah": 50,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3756,
        "text": "Wa nufikha fis-soori faizaa hum minal ajdaasi ilaa Rabbihim yansiloon",
        "numberInSurah": 51,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3757,
        "text": "Qaaloo yaa wailanaa mam ba'asanaa mim marqadinaa; haaza maa wa'adar Rahmanu wa sadaqal mursaloon",
        "numberInSurah": 52,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3758,
        "text": "In kaanat illaa saihatanw waahidatan fa-izaa hum jamee'ul ladainaa muhdaroon",
        "numberInSurah": 53,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3759,
        "text": "Fal-Yawma laa tuzlamu nafsun shai'anw-wa laa tujzawna illaa maa kuntum ta'maloon",
        "numberInSurah": 54,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3760,
        "text": "Inna Ashaabal jannatil Yawma fee shughulin faakihoon",
        "numberInSurah": 55,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3761,
        "text": "Hum wa azwaajuhum fee zilaalin 'alal araaa'iki muttaki'oon",
        "numberInSurah": 56,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3762,
        "text": "Lahum feehaa faakiha tunw-wa lahum maa yadda'oon",
        "numberInSurah": 57,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3763,
        "text": "Salaamun qawlam mir Rabbir Raheem",
        "numberInSurah": 58,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3764,
        "text": "Wamtaazul Yawma ayyuhal mujrimoon",
        "numberInSurah": 59,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3765,
        "text": "Alam a'had ilaikum yaa Baneee Aadama al-laa ta'budush Shaitaana innahoo lakum 'aduwwum mubeen",
        "numberInSurah": 60,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3766,
        "text": "Wa ani'budoonee; haazaa Siraatum Mustaqeem",
        "numberInSurah": 61,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3767,
        "text": "Wa laqad adalla minkum jibillan kaseeraa; afalam takoonoo ta'qiloon",
        "numberInSurah": 62,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3768,
        "text": "Haazihee Jahannamul latee kuntum too'adoon",
        "numberInSurah": 63,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3769,
        "text": "Islawhal Yawma bimaa kuntum takfuroon",
        "numberInSurah": 64,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3770,
        "text": "Al-Yawma nakhtimu 'alaaa afwaahihim wa tukallimunaaa aideehim wa tashhadu arjuluhum bimaa kaanoo yaksiboon",
        "numberInSurah": 65,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3771,
        "text": "Wa law nashaaa'u lata masna 'alaaa aiyunihim fasta baqus-siraata fa-annaa yubsiroon",
        "numberInSurah": 66,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3772,
        "text": "Wa law nashaaa'u lamasakhnaahum 'alaa makaanatihim famas-tataa'oo mudiyyanw-wa laa yarji'oon",
        "numberInSurah": 67,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3773,
        "text": "Wa man nu 'ammirhu nunakkishu fil-khalq; afalaa ya'qiloon",
        "numberInSurah": 68,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3774,
        "text": "Wa maa 'allamnaahush shi'ra wa maa yambaghee lah; in huwa illaa zikrunw-wa Qur-aanum mubeen",
        "numberInSurah": 69,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3775,
        "text": "Liyunzira man kaana haiyanw-wa yahiqqal qawlu 'alal-kaafireen",
        "numberInSurah": 70,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3776,
        "text": "Awalam yaraw annaa khalaqnaa lahum mimmaa 'amilat aideenaaa an'aaman fahum lahaa maalikoon",
        "numberInSurah": 71,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3777,
        "text": "Wa zallalnaahaa lahum faminhaa rakoobuhum wa minhaa yaakuloon",
        "numberInSurah": 72,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3778,
        "text": "Wa lahum feehaa manaa fi'u wa mashaarib; afalaa yashkuroon",
        "numberInSurah": 73,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3779,
        "text": "Wattakhazoo min doonil laahi aalihatal la'allahum yunsaroon",
        "numberInSurah": 74,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3780,
        "text": "Laa yastatee'oona nasrahum wa hum lahum jundum muhdaroon",
        "numberInSurah": 75,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3781,
        "text": "Falaa yahzunka qawluhum; innaa na'lamu maa yusirroona wa maa yu'linoon",
        "numberInSurah": 76,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3782,
        "text": "Awalam yaral insaanu annaa khalaqnaahu min nutfatin fa-izaa huwa khaseemum mubeen",
        "numberInSurah": 77,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3783,
        "text": "Wa daraba lanaa maslanw-wa nasiya khalqahoo qaala mai-yuhyil'izaama wa hiya rameem",
        "numberInSurah": 78,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3784,
        "text": "Qul yuh yeehal lazeee ansha ahaaa awwala marrah; wa Huwa bikulli khalqin 'Aleem",
        "numberInSurah": 79,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3785,
        "text": "Allazee ja'ala lakum minash shajaril akhdari naaran fa-izaaa antum minhu tooqidoon",
        "numberInSurah": 80,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3786,
        "text": "Awa laisal lazee khalaqas samaawaati wal arda biqaadirin 'alaaa ai-yakhluqa mislahum; balaa wa Huwal Khallaaqul 'Aleem",
        "numberInSurah": 81,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3787,
        "text": "Innamaa amruhooo izaaa araada shai'an ai-yaqoola lahoo kun fa-yakoon",
        "numberInSurah": 82,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3788,
        "text": "Fa Subhaanal lazee biyadihee malakootu kulli shai-inw-wa ilaihi turja'oon",
        "numberInSurah": 83,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      }
