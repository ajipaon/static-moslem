{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 56,
    "name": "سُورَةُ الوَاقِعَةِ",
    "englishName": "Al-Waaqia",
    "englishNameTranslation": "The Inevitable",
    "revelationType": "Meccan",
    "numberOfAyahs": 96,
    "ayahs": [
      {
        "number": 4980,
        "text": "Izaa waqa'atil waaqi'ah",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4981,
        "text": "Laisa liwaq'atihaa kaazibah",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4982,
        "text": "Khafidatur raafi'ah",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4983,
        "text": "Izaa rujjatil ardu rajjaa",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4984,
        "text": "Wa bussatil jibaalu bassaa",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4985,
        "text": "Fakaanat habaaa'am mumbassaa",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4986,
        "text": "Wa kuntum azwaajan salaasah",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4987,
        "text": "Fa as haabul maimanati maaa as haabul maimanah",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4988,
        "text": "Wa as haabul mash'amati maaa as haabul mash'amah",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4989,
        "text": "Wassaabiqoonas saabiqoon",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4990,
        "text": "Ulaaa'ikal muqarraboon",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4991,
        "text": "Fee Jannaatin Na'eem",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4992,
        "text": "Sullatum minal awwaleen",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4993,
        "text": "Wa qaleelum minal aa khireen",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4994,
        "text": "'Alaa sururim mawdoonah",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4995,
        "text": "Muttaki'eena 'alaihaa mutaqabileen",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4996,
        "text": "Yatoofu 'alaihim wildaa num mukkhalladoon",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4997,
        "text": "Bi akwaabinw wa abaareeq, wa kaasim mim ma'een",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4998,
        "text": "Laa yusadda'oona 'anhaa wa laa yunzifoon",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4999,
        "text": "Wa faakihatim mimmaa yatakhaiyaroon",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5000,
        "text": "Wa lahmi tairim mimmaa yashtahoon",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5001,
        "text": "Wa hoorun'een",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5002,
        "text": "Ka amsaalil lu'lu'il maknoon",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5003,
        "text": "Jazaaa'am bimaa kaanoo ya'maloon",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5004,
        "text": "Laa yasma'oona feehaa laghwanw wa laa taaseemaa",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5005,
        "text": "Illaa qeelan salaaman salaamaa",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5006,
        "text": "Wa as haabul yameeni maaa as haabul Yameen",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5007,
        "text": "Fee sidrim makhdood",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5008,
        "text": "Wa talhim mandood",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5009,
        "text": "Wa zillim mamdood",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5010,
        "text": "Wa maaa'im maskoob",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5011,
        "text": "Wa faakihatin kaseerah",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5012,
        "text": "Laa maqtoo'atinw wa laa mamnoo'ah",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5013,
        "text": "Wa furushim marfoo'ah",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5014,
        "text": "Innaaa anshaanaahunna inshaaa'aa",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5015,
        "text": "Faja'alnaahunna abkaaraa",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5016,
        "text": "'Uruban atraabaa",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5017,
        "text": "Li as haabil yameen",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5018,
        "text": "Sullatum minal awwa leen",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5019,
        "text": "Wa sullatum minal aakhireen",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5020,
        "text": "Wa as haabush shimaali maaa as haabush shimaal",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5021,
        "text": "Fee samoominw wa hameem",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5022,
        "text": "Wa zillim miny yahmoom",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5023,
        "text": "Laa baaridinw wa laa kareem",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5024,
        "text": "Innaahum kaanoo qabla zaalika mutrafeen",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5025,
        "text": "Wa kaanoo yusirroona 'alal hinsil 'azeem",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5026,
        "text": "Wa kaanoo yaqooloona a'izaa mitnaa wa kunnaa turaabanw wa izaaman'ainnaa lamab'oosoon",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5027,
        "text": "Awa aabaaa'unal awwaloon",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5028,
        "text": "Qul innal awwaleena wal aakhireen",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5029,
        "text": "Lamajmoo'oona ilaa meeqaati yawmim ma'loon",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5030,
        "text": "summa innakum ayyuhad daaalloonal mukazziboon",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5031,
        "text": "La aakiloona min shaja rim min zaqqoom",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5032,
        "text": "Famaali'oona minhal butoon",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5033,
        "text": "Fashaariboona 'alaihi minal hameem",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5034,
        "text": "Fashaariboona shurbal heem",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5035,
        "text": "Haazaa nuzuluhum yawmad deen",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5036,
        "text": "Nahnu khalaqnaakum falaw laa tusaddiqoon",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5037,
        "text": "Afara'aytum maa tumnoon",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5038,
        "text": "'A-antum takhluqoo nahooo am nahnul khaaliqoon",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5039,
        "text": "Nahnu qaddarnaa baina kumul mawta wa maa nahnu bimasbooqeen",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5040,
        "text": "'Alaaa an nubaddila amsaalakum wa nunshi'akum fee maa laa ta'lamoon",
        "numberInSurah": 61,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5041,
        "text": "Wa laqad 'alimtumun nash atal oolaa falaw laa tazakkaroon",
        "numberInSurah": 62,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5042,
        "text": "Afara'aytum maa tahrusoon",
        "numberInSurah": 63,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5043,
        "text": "'A-antum tazra'oonahooo am nahnuz zaari'ooon",
        "numberInSurah": 64,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5044,
        "text": "Law nashaaa'u laja'al naahu hutaaman fazaltum tafakkahoon",
        "numberInSurah": 65,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5045,
        "text": "Innaa lamughramoon",
        "numberInSurah": 66,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5046,
        "text": "Bal nahnu mahroomoon",
        "numberInSurah": 67,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5047,
        "text": "Afara'aytumul maaa'allazee tashraboon",
        "numberInSurah": 68,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5048,
        "text": "'A-antum anzaltumoohu minal muzni am nahnul munziloon",
        "numberInSurah": 69,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5049,
        "text": "Law nashaaa'u ja'alnaahu ujaajan falaw laa tashkuroon",
        "numberInSurah": 70,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5050,
        "text": "Afara'aytumun naaral latee tooroon",
        "numberInSurah": 71,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5051,
        "text": "'A-antum anshaatum shajaratahaaa am nahnul munshi'oon",
        "numberInSurah": 72,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5052,
        "text": "Nahnu ja'alnaahaa tazkira tanw wa mataa'al lilmuqween",
        "numberInSurah": 73,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5053,
        "text": "Fasabbih bismi Rabbikal 'azeem",
        "numberInSurah": 74,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5054,
        "text": "Falaa uqsimu bimaawaa qi'innujoom",
        "numberInSurah": 75,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5055,
        "text": "Wa innahoo laqasamul lawta'lamoona'azeem",
        "numberInSurah": 76,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5056,
        "text": "Innahoo la quraanun kareem",
        "numberInSurah": 77,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5057,
        "text": "Fee kitaabim maknoon",
        "numberInSurah": 78,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5058,
        "text": "Laa yamassuhooo illal mutahharoon",
        "numberInSurah": 79,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5059,
        "text": "Tanzeelum mir Rabbil'aalameen",
        "numberInSurah": 80,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5060,
        "text": "Afabihaazal hadeesi antum mudhinoon",
        "numberInSurah": 81,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5061,
        "text": "Wa taj'aloona rizqakum annakum tukazziboon",
        "numberInSurah": 82,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5062,
        "text": "Falaw laaa izaa balaghatil hulqoom",
