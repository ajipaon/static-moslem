{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 37,
    "name": "سُورَةُ الصَّافَّاتِ",
    "englishName": "As-Saaffaat",
    "englishNameTranslation": "Those drawn up in Ranks",
    "revelationType": "Meccan",
    "numberOfAyahs": 182,
    "ayahs": [
      {
        "number": 3789,
        "text": "Wassaaaffaati saffaa",
        "numberInSurah": 1,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3790,
        "text": "Fazzaajiraati zajraa",
        "numberInSurah": 2,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3791,
        "text": "Fattaaliyaati Zikra",
        "numberInSurah": 3,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3792,
        "text": "Inna Illaahakum la Waahid",
        "numberInSurah": 4,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3793,
        "text": "Rabbus samaawaati wal ardi wa maa bainahumaa wa Rabbul mashaariq",
        "numberInSurah": 5,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3794,
        "text": "Innaa zaiyannas samaaa 'ad dunyaa bizeenatinil kawaakib",
        "numberInSurah": 6,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3795,
        "text": "Wa hifzam min kulli Shaitaanim maarid",
        "numberInSurah": 7,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3796,
        "text": "Laa yassamma 'oona ilal mala il a'alaa wa yuqzafoona min kulli jaanib",
        "numberInSurah": 8,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3797,
        "text": "Duhooranw wa lahum 'azaabunw waasib",
        "numberInSurah": 9,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3798,
        "text": "Illaa man khatifal khatfata fa atba'ahoo shihaabun saaqib",
        "numberInSurah": 10,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3799,
        "text": "Fastaftihim ahum ashaddu khalqan am man khalaqnaa; innaa khalaqnaahum min teenil laazib",
        "numberInSurah": 11,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3800,
        "text": "Bal'ajibta wa yaskharoon",
        "numberInSurah": 12,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3801,
        "text": "Wa izaa zukkiroo laa yazkuroon",
        "numberInSurah": 13,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3802,
        "text": "Wa izaa ra aw Aayatinw yastaskhiroon",
        "numberInSurah": 14,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3803,
        "text": "Wa qaalooo in haazaa illaa sihrum mubeen",
        "numberInSurah": 15,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3804,
        "text": "'A-izaa mitnaa wa kunnaa turaabanw wa 'izaaman 'ainnaa lamab'oosoon",
        "numberInSurah": 16,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3805,
        "text": "Awa aabaa'unal awwaloon",
        "numberInSurah": 17,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3806,
        "text": "Qul na'am wa antum daakhiroon",
        "numberInSurah": 18,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3807,
        "text": "Fa innamaa hiya zajra tunw waahidatun fa izaa hum yanzuroon",
        "numberInSurah": 19,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3808,
        "text": "Qa qaaloo yaa wailanaa haazaa Yawmud-Deen",
        "numberInSurah": 20,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3809,
        "text": "Haazaa Yawmul Faslil lazee kuntum bihee tukaziboon",
        "numberInSurah": 21,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3810,
        "text": "Uhshurul lazeena zalamoo wa azwaajahum wa maa kaanoo ya'budoon",
        "numberInSurah": 22,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3811,
        "text": "Min doonil laahi fahdoohum ilaa siraatil Jaheem",
        "numberInSurah": 23,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3812,
        "text": "Wa qifoohum innahum mas'ooloon",
        "numberInSurah": 24,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3813,
        "text": "Maa lakum laa tanaasaroon",
        "numberInSurah": 25,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3814,
        "text": "Bal humul Yawma mustaslimoon",
        "numberInSurah": 26,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3815,
        "text": "Wa aqbala ba'duhum 'alaa ba'diny yatasaaa'aloon",
        "numberInSurah": 27,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3816,
        "text": "Qaalooo innakum kuntum taatoonanaa 'anil yameen",
        "numberInSurah": 28,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3817,
        "text": "Qaaloo bal lam takoonoo mu'mineen",
        "numberInSurah": 29,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3818,
        "text": "Wa maa kaana lanaa 'alaikum min sultaanim bal kuntum qawman taagheen",
        "numberInSurah": 30,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3819,
        "text": "Fahaqqa 'alainaa qawlu Rabbinaaa innaa lazaaa'iqoon",
        "numberInSurah": 31,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3820,
        "text": "Fa aghwainaakum innaa kunnaa ghaaween",
        "numberInSurah": 32,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3821,
        "text": "Fa innahum Yawma'izin fil'azaabi mushtarikoon",
        "numberInSurah": 33,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3822,
        "text": "Innaa kazaalika naf'alu bil mujrimeen",
        "numberInSurah": 34,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3823,
        "text": "Innahum kaanooo izaa qeela lahum laaa ilaaha illal laahu yastakbiroon",
        "numberInSurah": 35,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3824,
        "text": "Wa yaqooloona a'innaa lataarikooo aalihatinaa lishaa'irim majnoon",
        "numberInSurah": 36,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3825,
        "text": "bal jaaa'a bilhaqqi wa saddaqal mursaleen",
        "numberInSurah": 37,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3826,
        "text": "Innakum lazaaa'iqul 'azaabil aleem",
        "numberInSurah": 38,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3827,
        "text": "Wa maa tujzawna illaa maa kuntum ta'maloon",
        "numberInSurah": 39,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3828,
        "text": "Illaa 'ibaadal laahil mukhlaseen",
        "numberInSurah": 40,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3829,
        "text": "Ulaaa'ika lahum rizqum ma'loom",
        "numberInSurah": 41,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3830,
        "text": "Fa waakihu wa hum mukramoon",
        "numberInSurah": 42,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3831,
        "text": "Fee jannaatin Na'eem",
        "numberInSurah": 43,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3832,
        "text": "'Alaa sururim mutaqaa bileen",
        "numberInSurah": 44,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3833,
        "text": "Yutaafu 'alaihim bikaasim mim ma'een",
        "numberInSurah": 45,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3834,
        "text": "Baidaaa'a laz zatil lish shaaribeen",
        "numberInSurah": 46,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3835,
        "text": "Laa feehaa ghawlunw wa laa hum 'anhaa yunzafoon",
        "numberInSurah": 47,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3836,
        "text": "Wa 'indahum qaasiraatut tarfi 'een",
        "numberInSurah": 48,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3837,
        "text": "Ka annahunna baidum maknoon",
        "numberInSurah": 49,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3838,
        "text": "Fa aqbala ba'duhum 'alaa badiny yatasaaa 'aloon",
        "numberInSurah": 50,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3839,
        "text": "Qaala qaaa'ilum minhum innee kaana lee qareen",
        "numberInSurah": 51,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3840,
        "text": "Yaqoolu a'innnaka laminal musaddiqeen",
        "numberInSurah": 52,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3841,
        "text": "'A-izaa mitnaa wa kunnaa turaabanw wa 'izaaman 'ainnaa lamadeenoon",
        "numberInSurah": 53,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3842,
        "text": "Qaala hal antum muttali'oon",
        "numberInSurah": 54,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3843,
        "text": "Fattala'a fara aahu fee sawaaa'il Jaheem",
        "numberInSurah": 55,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3844,
        "text": "Qaala tallaahi in kitta laturdeen",
        "numberInSurah": 56,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3845,
        "text": "Wa law laa ni'matu Rabbee lakuntu minal muhdareen",
        "numberInSurah": 57,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3846,
        "text": "Afamaa nahnu bimaiyiteen",
        "numberInSurah": 58,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3847,
        "text": "Illa mawtatanal oola wa maa nahnu bimu'azzabeen",
        "numberInSurah": 59,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3848,
        "text": "Inna haazaa falya'ma lil'aamiloon",
        "numberInSurah": 60,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3849,
        "text": "Limisli haaza falya'ma lil 'aamiloon",
        "numberInSurah": 61,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3850,
        "text": "Azaalika khairun nuzulan am shajaratuz Zaqqom",
        "numberInSurah": 62,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3851,
        "text": "Innaa ja'alnaahaa fitnatal lizzaalimeen",
        "numberInSurah": 63,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3852,
        "text": "Innahaa shajaratun takhruju feee aslil Jaheem",
        "numberInSurah": 64,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3853,
        "text": "Tal'uhaa ka annahoo ru'oosush Shayaateen",
        "numberInSurah": 65,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3854,
        "text": "Fa innahum la aakiloona minhaa famaali'oona minhal butoon",
        "numberInSurah": 66,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3855,
        "text": "Summa inna lahum 'alaihaa lashawbam min hameem",
        "numberInSurah": 67,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3856,
        "text": "Summa inna marji'ahum la ilal Jaheem",
        "numberInSurah": 68,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3857,
        "text": "Innahum alfaw aabaaa'ahum daaalleen",
        "numberInSurah": 69,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3858,
        "text": "Fahum 'alaa aasaarihim yuhra'oon",
        "numberInSurah": 70,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3859,
        "text": "Wa laqad dalla qablahum aksarul awwaleen",
        "numberInSurah": 71,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3860,
        "text": "Wa laqad arsalnaa feehim munzireen",
        "numberInSurah": 72,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3861,
        "text": "Fanzur kaifa kaana 'aaqibatul munzareen",
        "numberInSurah": 73,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3862,
        "text": "Illaa 'ibaadal laahil mukhlaseen",
        "numberInSurah": 74,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3863,
        "text": "Wa laqad naadaanaa Noohun falani'mal mujeeboon",
        "numberInSurah": 75,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3864,
        "text": "Wa jajainaahu wa ahlahoo minal karbil 'azeem",
        "numberInSurah": 76,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3865,
        "text": "Wa ja'alnaa zurriyyatahoo hummul baaqeen",
        "numberInSurah": 77,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3866,
        "text": "Wa taraknaa 'alaihi fil aakhireen",
        "numberInSurah": 78,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3867,
        "text": "Salaamun 'alaa Noohin fil 'aalameen",
        "numberInSurah": 79,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3868,
        "text": "Innaa kazaalika najzil muhsineen",
        "numberInSurah": 80,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3869,
        "text": "Innahoo min 'ibaadinal mu'mineen",
        "numberInSurah": 81,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3870,
        "text": "Summa aghraqnal aakhareen",
        "numberInSurah": 82,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3871,
        "text": "Wa ina min shee'atihee la Ibraaheem",
        "numberInSurah": 83,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3872,
        "text": "Iz jaaa'a Rabbahoo bi qalbin saleem",
        "numberInSurah": 84,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3873,
        "text": "Iz qaala li abeehi wa qawmihee maazaa ta'budoon",
        "numberInSurah": 85,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3874,
        "text": "A'ifkan aalihatan doonal laahi tureedoon",
        "numberInSurah": 86,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3875,
        "text": "Famaa zannukum bi Rabbil'aalameen",
        "numberInSurah": 87,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3876,
        "text": "Fanazara nazratan finnujoom",
        "numberInSurah": 88,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3877,
        "text": "Faqaala inee saqeem",
        "numberInSurah": 89,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3878,
        "text": "Fatawallaw 'anhu mudbireen",
        "numberInSurah": 90,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3879,
        "text": "Faraagha ilaaa aalihatihim faqaala alaa taakuloon",
        "numberInSurah": 91,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3880,
        "text": "Maa lakum laa tantiqoon",
        "numberInSurah": 92,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3881,
        "text": "Faraagha 'alaihim darbam bilyameen",
        "numberInSurah": 93,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3882,
        "text": "Fa aqbalooo ilaihi yaziffoon",
        "numberInSurah": 94,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3883,
        "text": "Qaala ata'budoona maa tanhitoon",
        "numberInSurah": 95,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3884,
        "text": "Wallaahu khalaqakum wa maa ta'maloon",
        "numberInSurah": 96,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3885,
        "text": "Qaalub noo lahoo bun yaanan fa alqoohu fil jaheem",
        "numberInSurah": 97,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3886,
        "text": "Fa araadoo bihee kaidan faja 'alnaahumul asfaleen",
        "numberInSurah": 98,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3887,
        "text": "Wa qaala innee zaahibun ilaa Rabbee sa yahdeen",
        "numberInSurah": 99,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3888,
        "text": "Rabbi hab lee minas saaliheen",
        "numberInSurah": 100,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3889,
        "text": "Fabashsharnaahu bighulaamin haleem",
        "numberInSurah": 101,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3890,
        "text": "Falamma balagha ma'a hus sa'ya qaala yaa buniya inneee araa fil manaami anneee azbahuka fanzur maazaa taraa; qaala yaaa abatif 'al maa tu'maru satajidunee in shaaa'allaahu minas saabireen",
        "numberInSurah": 102,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3891,
        "text": "Falammaaa aslamaa wa tallahoo liljabeen",
        "numberInSurah": 103,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3892,
        "text": "Wa naadainaahu ai yaaaa Ibraheem",
        "numberInSurah": 104,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3893,
        "text": "Qad saddaqtar ru'yaa; innaa kazaalika najzil muhsineen",
        "numberInSurah": 105,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3894,
        "text": "Inna haazaa lahuwal balaaa'ul mubeen",
        "numberInSurah": 106,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3895,
        "text": "Wa fadainaahu bizibhin 'azeem",
        "numberInSurah": 107,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3896,
        "text": "Wa taraknaa 'alaihi fil aakhireen",
        "numberInSurah": 108,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3897,
        "text": "Salaamun 'alaaa Ibraaheem",
        "numberInSurah": 109,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3898,
        "text": "Kazaalika najzil muhsineen",
        "numberInSurah": 110,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3899,
        "text": "Innahoo min 'ibaadinal mu'mineen",
        "numberInSurah": 111,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3900,
        "text": "Wa bashsharnaahu bi Ishaaqa Nabiyayam minas saaliheen",
        "numberInSurah": 112,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3901,
        "text": "Wa baaraknaa 'alaihi wa 'alaaa Ishaaq; wa min zurriyya tihimaa muhsinunw wa zaalimul linafshihee mubeen",
        "numberInSurah": 113,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3902,
        "text": "Wa laqad mananna alaa Moosaa wa Haaroon",
        "numberInSurah": 114,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3903,
        "text": "Wa najjainaahumaa wa qawmahumaa minal karbil 'azeem",
        "numberInSurah": 115,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3904,
        "text": "Wa nasarnaahum fakaanoo humul ghaalibeen",
        "numberInSurah": 116,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3905,
        "text": "Wa aatainaahumal Kitaabal mustabeen",
        "numberInSurah": 117,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3906,
        "text": "Wa hadainaahumus Siraatal Mustaqeem",
        "numberInSurah": 118,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3907,
        "text": "Wa taraknaa 'alaihimaa fil aakhireen",
        "numberInSurah": 119,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3908,
        "text": "Salaamun 'alaa Moosaa wa Haaroon",
        "numberInSurah": 120,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3909,
        "text": "Innaa kazaalika najzil muhsineen",
        "numberInSurah": 121,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3910,
        "text": "Innahumaa min 'ibaadinal mu'mineen",
        "numberInSurah": 122,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3911,
        "text": "Wa inna Ilyaasa laminal mursaleen",
        "numberInSurah": 123,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3912,
        "text": "Iz qaala liqawmiheee alaa tattaqoon",
        "numberInSurah": 124,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3913,
        "text": "Atad'oona Ba'lanw wa tazaroona ahsanal khaaliqeen",
        "numberInSurah": 125,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3914,
        "text": "Allaaha Rabbakum wa Rabba aabaaa'ikumul awwaleen",
        "numberInSurah": 126,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3915,
        "text": "Fakazzaboohu fa inna hum lamuhdaroon",
        "numberInSurah": 127,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3916,
        "text": "Illaa 'ibaadal laahil mukhlaseen",
        "numberInSurah": 128,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3917,
        "text": "Wa taraknaa 'alaihi fil aakhireen",
        "numberInSurah": 129,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3918,
        "text": "Salaamun 'alaaa Ilyaaseen",
        "numberInSurah": 130,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3919,
        "text": "Innaa kazaalika najzil muhsineen",
        "numberInSurah": 131,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3920,
        "text": "Innahoo min 'ibaadinal mu'mineen",
        "numberInSurah": 132,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3921,
        "text": "Wa inna Lootal laminal mursaleen",
        "numberInSurah": 133,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3922,
        "text": "Iz najjainaahu wa ahlahooo ajma'een",
        "numberInSurah": 134,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3923,
        "text": "Illaa 'ajoozan fil ghaabireen",
        "numberInSurah": 135,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3924,
        "text": "Summa dammarnal aakhareen",
        "numberInSurah": 136,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3925,
        "text": "Wa innakum latamurroona 'alaihim musbiheen",
        "numberInSurah": 137,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3926,
        "text": "Wa billail; afalaa ta'qiloon",
        "numberInSurah": 138,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3927,
        "text": "Wa inna Yoonusa laminal mursaleen",
        "numberInSurah": 139,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3928,
        "text": "Iz abaqa ilal fulkil mash hoon",
        "numberInSurah": 140,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3929,
        "text": "Fasaahama fakaana minal mudhadeen",
        "numberInSurah": 141,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3930,
        "text": "Faltaqamahul hootu wa huwa muleem",
        "numberInSurah": 142,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3931,
        "text": "Falaw laaa annahoo kaana minal musabbiheen",
        "numberInSurah": 143,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3932,
        "text": "Lalabisa fee batniheee ilaa Yawmi yub'asoon",
        "numberInSurah": 144,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3933,
        "text": "Fanabaznaahu bil'araaa'i wa huwa saqeem",
        "numberInSurah": 145,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3934,
        "text": "Wa ambatnaa 'alaihi shajaratam mai yaqteen",
        "numberInSurah": 146,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3935,
        "text": "Wa arsalnaahu ilaa mi'ati alfin aw yazeedoon",
        "numberInSurah": 147,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3936,
        "text": "Fa aamanoo famatta' naahum ilaa heen",
        "numberInSurah": 148,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3937,
        "text": "Fastaftihim ali Rabbikal banaatu wa lahumul banoon",
        "numberInSurah": 149,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3938,
        "text": "Am khalaqnal malaaa'i kata inaasanw wa hm shaahidoon",
        "numberInSurah": 150,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3939,
        "text": "Alaaa innahum min ifkihim la yaqooloon",
        "numberInSurah": 151,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3940,
        "text": "Waladal laahu wa innhum lakaaziboon",
        "numberInSurah": 152,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3941,
        "text": "Astafal banaati 'alal baneen",
        "numberInSurah": 153,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3942,
        "text": "Maa lakum kaifa tahkumoon",
        "numberInSurah": 154,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3943,
        "text": "Afalaa tazakkaroon",
        "numberInSurah": 155,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3944,
        "text": "Am lakum sultaanum mubeen",
        "numberInSurah": 156,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3945,
        "text": "Faatoo bi Kitaabikum in kuntum saadiqeen",
        "numberInSurah": 157,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3946,
        "text": "Wa ja'aloo bainahoo wa bainal jinnati nasabaa; wa laqad 'alimatil jinnatu innahum lamuhdaroon",
        "numberInSurah": 158,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3947,
        "text": "Subhaanal laahi 'ammaa yasifoon",
        "numberInSurah": 159,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3948,
        "text": "Illaa 'ibaadal laahil mukhlaseen",
        "numberInSurah": 160,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3949,
        "text": "Fa innakum wa maa ta'ubdoon",
        "numberInSurah": 161,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3950,
        "text": "Maaa antum 'alaihi befaaatineen",
        "numberInSurah": 162,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3951,
        "text": "Illaa man huwa saalil jaheem",
        "numberInSurah": 163,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3952,
        "text": "Wa maa minnasa illaa lahoo maqaamum ma'loom",
        "numberInSurah": 164,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3953,
        "text": "Wa innaa llanah nus saaffoon",
        "numberInSurah": 165,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3954,
        "text": "Wa innaa lanah nul musabbihoon",
        "numberInSurah": 166,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3955,
        "text": "Wa in kaanoo la yaqooloon",
        "numberInSurah": 167,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3956,
        "text": "Law anna 'indana zikram minal awwaleen",
        "numberInSurah": 168,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3957,
        "text": "Lakunna 'ibaadal laahil mukhlaseen",
        "numberInSurah": 169,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3958,
        "text": "Fakafaroo bihee fasawfa ya'lamoon",
        "numberInSurah": 170,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3959,
        "text": "Wa laqad sabaqat Kalimatunaa li'ibaadinal mursa leen",
        "numberInSurah": 171,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3960,
        "text": "Innaa hum lahumul mansooroon",
        "numberInSurah": 172,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3961,
        "text": "Wa inna jundana lahumul ghaaliboon",
        "numberInSurah": 173,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3962,
        "text": "Fatawalla 'anhum hatta heen",
        "numberInSurah": 174,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3963,
        "text": "Wa absirhum fasawfa yubsiroon",
        "numberInSurah": 175,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3964,
        "text": "Afabi'azaabinaa yasta'jiloon",
        "numberInSurah": 176,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3965,
        "text": "Fa izaa nazala bisaahatihim fasaaa'a sabaahul munzareen",
        "numberInSurah": 177,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3966,
        "text": "Wa tawalla 'anhum hattaa heen",
        "numberInSurah": 178,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3967,
        "text": "Wa absir fasawfa yubsiroon",
        "numberInSurah": 179,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3968,
        "text": "Subhaana Rabbika Rabbil 'izzati 'amma yasifoon",
        "numberInSurah": 180,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3969,
        "text": "Wa salaamun 'alalmursaleen",
        "numberInSurah": 181,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3970,
        "text": "Walhamdu lillaahi Rabbil 'aalameen",
        "numberInSurah": 182,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      }
