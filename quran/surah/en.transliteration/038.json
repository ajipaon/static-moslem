{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 38,
    "name": "سُورَةُ صٓ",
    "englishName": "Saad",
    "englishNameTranslation": "The letter Saad",
    "revelationType": "Meccan",
    "numberOfAyahs": 88,
    "ayahs": [
      {
        "number": 3971,
        "text": "Saaad; wal-Qur-aani ziz zikr",
        "numberInSurah": 1,
        "juz": 23,
        "manzil": 6,
        "page": 453,
        "ruku": 391,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3972,
        "text": "Balil lazeena kafaroo fee 'izzatilnw wa shiqaaq",
        "numberInSurah": 2,
        "juz": 23,
        "manzil": 6,
        "page": 453,
        "ruku": 391,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3973,
        "text": "Kam ahlaknaa min qablihim min qarnin fanaadaw wa laata heena manaas",
        "numberInSurah": 3,
        "juz": 23,
        "manzil": 6,
        "page": 453,
        "ruku": 391,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3974,
        "text": "Wa 'ajibooo an jaaa'a hum munzirum minhum wa qaalal kaafiroona haazaa saahirun kazzaab",
        "numberInSurah": 4,
        "juz": 23,
        "manzil": 6,
        "page": 453,
        "ruku": 391,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3975,
        "text": "Aja'alal aalihata Ilaahanw Waahidan inna haazaa lashai'un 'ujaab",
        "numberInSurah": 5,
        "juz": 23,
        "manzil": 6,
        "page": 453,
        "ruku": 391,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3976,
        "text": "Wantalaqal mala-u minhum anim shoo wasbiroo 'alaaa aalihatikum innna haazaa lashai 'uny yuraad",
        "numberInSurah": 6,
        "juz": 23,
        "manzil": 6,
        "page": 453,
        "ruku": 391,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3977,
        "text": "Maa sami'naa bihaazaa fil millatil aakhirati in haazaaa illakh tilaaq",
        "numberInSurah": 7,
        "juz": 23,
        "manzil": 6,
        "page": 453,
        "ruku": 391,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3978,
        "text": "'A-unzila 'alaihiz zikru mim baininaa; bal hum fee shakkim min Zikree bal lammaa yazooqoo 'azaab",
        "numberInSurah": 8,
        "juz": 23,
        "manzil": 6,
        "page": 453,
        "ruku": 391,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3979,
        "text": "Am'indahum khazaaa 'inu rahmati Rabbikal 'Azeezil Wahhab",
        "numberInSurah": 9,
        "juz": 23,
        "manzil": 6,
        "page": 453,
        "ruku": 391,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3980,
        "text": "Am lahum mulkus samaawaati wal ardi wa maa bainahumaa falyartaqoo fil asbaab",
        "numberInSurah": 10,
        "juz": 23,
        "manzil": 6,
        "page": 453,
        "ruku": 391,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3981,
        "text": "Jundum maa hunaalika mahzoomum minal Ahzaab",
        "numberInSurah": 11,
        "juz": 23,
        "manzil": 6,
        "page": 453,
        "ruku": 391,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3982,
        "text": "Kazzabat qablahum qawmu Lootinw-wa 'Aadunw wa Fir'awnu zul awtaad",
        "numberInSurah": 12,
        "juz": 23,
        "manzil": 6,
        "page": 453,
        "ruku": 391,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3983,
        "text": "Wa Samoodu wa qawmu Lootinw wa Ashaabul 'Aykah; ulaaa'ikal Ahzaab",
        "numberInSurah": 13,
        "juz": 23,
        "manzil": 6,
        "page": 453,
        "ruku": 391,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3984,
        "text": "In kullun illaa kazzabar Rusula fahaqqa 'iqaab",
        "numberInSurah": 14,
        "juz": 23,
        "manzil": 6,
        "page": 453,
        "ruku": 391,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3985,
        "text": "Wa maa yanzuru haaa ulaaa'i illaa saihatanw waahidatam maa lahaa min fawaaq",
        "numberInSurah": 15,
        "juz": 23,
        "manzil": 6,
        "page": 453,
        "ruku": 392,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3986,
        "text": "Wa qaaloo Rabbanaa 'ajjil lanaa qittanaa qabla Yawmil Hisaab",
        "numberInSurah": 16,
        "juz": 23,
        "manzil": 6,
        "page": 453,
        "ruku": 392,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3987,
        "text": "Isbir 'alaa maa yaqooloona wazkur 'abdanaa Daawooda zal aidi aidi innahooo awwaab",
        "numberInSurah": 17,
        "juz": 23,
        "manzil": 6,
        "page": 454,
        "ruku": 392,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3988,
        "text": "Innaa sakhkharnal jibaala ma'ahoo yusabbihna bil'ashaiyi wal ishraaq",
        "numberInSurah": 18,
        "juz": 23,
        "manzil": 6,
        "page": 454,
        "ruku": 392,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3989,
        "text": "Wattayra mahshoorah; kullul lahooo awwaab",
        "numberInSurah": 19,
        "juz": 23,
        "manzil": 6,
        "page": 454,
        "ruku": 392,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3990,
        "text": "Wa shadadnaa mulkahoo wa aatainaahul Hikmata wa faslal khitaab",
        "numberInSurah": 20,
        "juz": 23,
        "manzil": 6,
        "page": 454,
        "ruku": 392,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3991,
        "text": "Wa hal ataaka naba'ul khasm; iz tasawwarul mihraab",
        "numberInSurah": 21,
        "juz": 23,
        "manzil": 6,
        "page": 454,
        "ruku": 392,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 3992,
        "text": "Iz dakhaloo 'alaa Daawooda fafazi'a minhum qaaloo la takhaf khasmaani baghaa ba'dunaa 'alaa ba'din fahkum bainanaaa bilhaqqi wa laa tushtit wahdinaaa ilaa Sawaaa'is Siraat",
        "numberInSurah": 22,
        "juz": 23,
        "manzil": 6,
        "page": 454,
        "ruku": 392,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 3993,
        "text": "Inna haazaaa akhee lahoo tis'unw wa tis'oona na'jatanw wa liya na'jatunw waahidah; faqaala akfilneeha wa 'azzanee filkhitaab",
        "numberInSurah": 23,
        "juz": 23,
        "manzil": 6,
        "page": 454,
        "ruku": 392,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 3994,
        "text": "Qaala laqad zalamaka bisu 'aali na'jatika ilaa ni'aajihee wa inna kaseeram minal khulataaa'i la-yabghee ba'duhum 'alaa ba'din illal lazeena aamanoo wa 'amilus saalihaati wa qaleehum maa hum; wa zanna Daawoodu annamaa fatannaahu fastaghrara Rabbahoo wa kharra raaki'anw wa anaab",
        "numberInSurah": 24,
        "juz": 23,
        "manzil": 6,
        "page": 454,
        "ruku": 392,
        "hizbQuarter": 182,
        "sajda": {
          "id": 11,
          "recommended": true,
          "obligatory": false
        }
      },
      {
        "number": 3995,
        "text": "Faghafarnaa lahoo zaalik; wa inna lahoo 'indanaa lazulfaa wa husna ma aab",
        "numberInSurah": 25,
        "juz": 23,
        "manzil": 6,
        "page": 454,
        "ruku": 392,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 3996,
        "text": "Yaa Daawoodu innaa ja'alnaaka khaleefatan fil ardi fahkum bainan naasi bilhaqqi wa laa tattabi'il hawaa fayudillaka 'an sabeelil laah; innal lazeena yadilloona 'an sabeelil laah; lahum 'azaabun shadeedum bimaa nasoo Yawmal Hisaab",
        "numberInSurah": 26,
        "juz": 23,
        "manzil": 6,
        "page": 454,
        "ruku": 392,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 3997,
        "text": "Wa ma khalaqnas samaaa'a wal arda wa maa bainahumaa baatilaa; zaalika zannul lazeena kafaroo; fawi lul lillazeena kafaroo minan Naar",
        "numberInSurah": 27,
        "juz": 23,
        "manzil": 6,
        "page": 455,
        "ruku": 393,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 3998,
        "text": "Am naj'alul lazeena aamanoo wa 'amilus saalihaati kalmufisdeena fil ardi am naj'alul muttaqeena kalfujjaar",
        "numberInSurah": 28,
        "juz": 23,
        "manzil": 6,
        "page": 455,
        "ruku": 393,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 3999,
        "text": "Kitaabun anzalnaahu ilaika mubaarakul liyaddabbarooo Aayaatihee wa liyatazakkara ulul albaab",
        "numberInSurah": 29,
        "juz": 23,
        "manzil": 6,
        "page": 455,
        "ruku": 393,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4000,
        "text": "Wa wahabnaa li Daawooda Sulaimaan; ni'mal 'abd; innahoo awwaab",
        "numberInSurah": 30,
        "juz": 23,
        "manzil": 6,
        "page": 455,
        "ruku": 393,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4001,
        "text": "Iz 'urida 'alaihi bil'ashiy yis saafinaatul jiyaad",
        "numberInSurah": 31,
        "juz": 23,
        "manzil": 6,
        "page": 455,
        "ruku": 393,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4002,
        "text": "Faqaala inneee ahbabtu hubbal khairi 'an zikri Rabbee hattaa tawaarat bilhijaab",
        "numberInSurah": 32,
        "juz": 23,
        "manzil": 6,
        "page": 455,
        "ruku": 393,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4003,
        "text": "Ruddoohaa 'alaiya fatafiqa masham bissooqi wal a'naaq",
        "numberInSurah": 33,
        "juz": 23,
        "manzil": 6,
        "page": 455,
        "ruku": 393,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4004,
        "text": "Wa laqad fatannaa Sulaimaana wa alqainaa 'alaa kursiyyihee jasadan summa anaab",
        "numberInSurah": 34,
        "juz": 23,
        "manzil": 6,
        "page": 455,
        "ruku": 393,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4005,
        "text": "Qaala Rabbigh fir lee wa hab lee mulkal laa yambaghee li ahadim mim ba'de inaka Antal Wahhab",
        "numberInSurah": 35,
        "juz": 23,
        "manzil": 6,
        "page": 455,
        "ruku": 393,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4006,
        "text": "Fasakhkharnaa lahur reeha tajree bi amrihee rukhaaa'an haisu asaab",
        "numberInSurah": 36,
        "juz": 23,
        "manzil": 6,
        "page": 455,
        "ruku": 393,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4007,
        "text": "Wash Shayaateena kulla bannaaa'inw wa ghawwaas",
        "numberInSurah": 37,
        "juz": 23,
        "manzil": 6,
        "page": 455,
        "ruku": 393,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4008,
        "text": "Wa aakhareena muqarraneena fil asfaad",
        "numberInSurah": 38,
        "juz": 23,
        "manzil": 6,
        "page": 455,
        "ruku": 393,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4009,
        "text": "Haazaa 'ataaa'unaa famnun aw amsik bighairi hisaab",
        "numberInSurah": 39,
        "juz": 23,
        "manzil": 6,
        "page": 455,
        "ruku": 393,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4010,
        "text": "Wa inna lahoo 'indanaa lazulfaa wa husna ma-aab",
        "numberInSurah": 40,
        "juz": 23,
        "manzil": 6,
        "page": 455,
        "ruku": 393,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4011,
        "text": "Wazkur 'abdanaaa Ayyoob; iz naada Rabbahooo annee massaniyash Shaitaanu binus binw wa 'azaab",
        "numberInSurah": 41,
        "juz": 23,
        "manzil": 6,
        "page": 455,
        "ruku": 394,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4012,
        "text": "Urkud birijlika haaza mughtasalum baaridunw wa sharaab",
        "numberInSurah": 42,
        "juz": 23,
        "manzil": 6,
        "page": 455,
        "ruku": 394,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4013,
        "text": "Wa wahabnaa lahoo ahlahoo wa mislahum ma'ahum rahmatam minna wa zikraa li ulil albaab",
        "numberInSurah": 43,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4014,
        "text": "Wa khuz biyadika dighsan fadrib bihee wa laa tahnas, innaa wajadnaahu saabiraa; ni'mal 'abd; innahooo awwaab",
        "numberInSurah": 44,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4015,
        "text": "Wazkur 'ibaadanaaa Ibraaheema wa Is-haaqa wa Ya'qooba ulil-aydee walabsaar",
        "numberInSurah": 45,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4016,
        "text": "Innaaa akhlasnaahum bi khaalisatin zikrad daar",
        "numberInSurah": 46,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4017,
        "text": "Wa innahum 'indanaa laminal mustafainal akhyaar",
        "numberInSurah": 47,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4018,
        "text": "Wazkur Ismaa'eela wal Yasa'a wa Zal-Kifli wa kullum minal akhyaar",
        "numberInSurah": 48,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4019,
        "text": "Haazaa zikr; wa inna lilmuttaqeena lahusna ma aab",
        "numberInSurah": 49,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4020,
        "text": "Jannaati 'adnim mufat tahatal lahumul abwaab",
        "numberInSurah": 50,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4021,
        "text": "Muttaki'eena feehaa yad'oona feehaa bifaakihatin kaseeratinw wa sharaab",
        "numberInSurah": 51,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 182,
        "sajda": false
      },
      {
        "number": 4022,
        "text": "Wa 'indahum qaasiraatut tarfi atraab",
        "numberInSurah": 52,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4023,
        "text": "Haaza maa too'odoona li Yawmil Hisaab",
        "numberInSurah": 53,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4024,
        "text": "Inna haazaa larizqunaa maa lahoo min nafaad",
        "numberInSurah": 54,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4025,
        "text": "Haazaa; wa inna littaagheena lasharra ma-aab",
        "numberInSurah": 55,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4026,
        "text": "Jahannama yaslawnahaa fai'sal mihaad",
        "numberInSurah": 56,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4027,
        "text": "Haazaa falyazooqoohu hameemunw wa ghassaaq",
        "numberInSurah": 57,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4028,
        "text": "Wa aakharu min shak liheee azwaaj",
        "numberInSurah": 58,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4029,
        "text": "Haazaa fawjum muqtahimum ma'akum laa marhabam bihim; innahum saalun Naar",
        "numberInSurah": 59,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4030,
        "text": "Qaaloo bal antum laa marhabam bikum; antum qaddamtumoohu lanaa fabi'sal qaraar",
        "numberInSurah": 60,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4031,
        "text": "Qaaloo Rabbanaa man qaddama lanaa haaza fazidhu 'azaaban di'fan fin Naar",
        "numberInSurah": 61,
        "juz": 23,
        "manzil": 6,
        "page": 456,
        "ruku": 394,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4032,
        "text": "Wa qaaloo ma lanaa laa naraa rijaalan kunnaa na'udduhum minal ashraar",
        "numberInSurah": 62,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 394,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4033,
        "text": "Attakhaznaahum sikh riyyan am zaaghat 'anhumul absaar",
        "numberInSurah": 63,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 394,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4034,
        "text": "Inna zaalika lahaqqun takhaasumu Ahlin Naar",
        "numberInSurah": 64,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 394,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4035,
        "text": "Qul innamaaa ana munzirunw wa maa min ilaahim illal laahul Waahidul Qahhaar",
        "numberInSurah": 65,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4036,
        "text": "Rabbus samaawaati wal ardi wa maa bainahumal 'Azeezul Ghaffaar",
        "numberInSurah": 66,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4037,
        "text": "Qul huwa naba'un 'azeem",
        "numberInSurah": 67,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4038,
        "text": "Antum 'anhu mu'ridoon",
        "numberInSurah": 68,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4039,
        "text": "Maa kaana liya min 'ilmim bilmala il a'laaa iz yakhtasimoon",
        "numberInSurah": 69,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4040,
        "text": "Iny-yoohaaa ilaiya illaaa annnamaaa ana nazeerum mubeen",
        "numberInSurah": 70,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4041,
        "text": "Iz qaala Rabbuka lilmalaaa'ikati innee khaaliqum basharam min teen",
        "numberInSurah": 71,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4042,
        "text": "Fa-iza sawwaituhoo wa nafakhtu feehi mir roohee faqa'oo lahoo saajideen",
        "numberInSurah": 72,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4043,
        "text": "Fasajadal malaaa'ikatu kulluhum ajma'oon",
        "numberInSurah": 73,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4044,
        "text": "Illaaa Iblees; istakbara wa kaana minal kaafireen",
        "numberInSurah": 74,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4045,
        "text": "Qaala yaaa Ibleesu maa man'aka an tasjuda limaa khalaqtu biyadai; astakbarta am kunta min 'aaleen",
        "numberInSurah": 75,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4046,
        "text": "Qaala ana khairum minah; khalaqtanee min naarinw wa khalaqtahoo min teen",
        "numberInSurah": 76,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4047,
        "text": "Qaala fakhruj minhaa fainnaka rajeem",
        "numberInSurah": 77,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4048,
        "text": "Wa inna 'alaika la'nateee ilaa Yawmid Deen",
        "numberInSurah": 78,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4049,
        "text": "Qaala Rabbi fa anzirneee ilaa Yawmi yub'asoon",
        "numberInSurah": 79,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4050,
        "text": "Qaala fa innaka minal munzareen",
        "numberInSurah": 80,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4051,
        "text": "Ilaa Yawmil waqtil ma'loom",
        "numberInSurah": 81,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4052,
        "text": "Qaala fabi'izzatika la ughwiyannahum ajma'een",
        "numberInSurah": 82,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4053,
        "text": "Illaa 'ibaadaka minhumul mukhlaseen",
        "numberInSurah": 83,
        "juz": 23,
        "manzil": 6,
        "page": 457,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4054,
        "text": "Qaala falhaqq, walhaqqa aqool",
        "numberInSurah": 84,
        "juz": 23,
        "manzil": 6,
        "page": 458,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4055,
        "text": "La amla'annna Jahannama minka wa mimman tabi'aka minhum ajma'een",
        "numberInSurah": 85,
        "juz": 23,
        "manzil": 6,
        "page": 458,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4056,
        "text": "Qul maaa as'alukum 'alaihi min ajrinw wa maaa ana minal mutakallifeen",
        "numberInSurah": 86,
        "juz": 23,
        "manzil": 6,
        "page": 458,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4057,
        "text": "In huwa illaa zikrul lil'aalameen",
        "numberInSurah": 87,
        "juz": 23,
        "manzil": 6,
        "page": 458,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      },
      {
        "number": 4058,
        "text": "Wa lata'lamunna naba ahoo ba'da heen",
        "numberInSurah": 88,
        "juz": 23,
        "manzil": 6,
        "page": 458,
        "ruku": 395,
        "hizbQuarter": 183,
        "sajda": false
      }
