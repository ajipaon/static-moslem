{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 51,
    "name": "سُورَةُ الذَّارِيَاتِ",
    "englishName": "Adh-Dhaariyat",
    "englishNameTranslation": "The Winnowing Winds",
    "revelationType": "Meccan",
    "numberOfAyahs": 60,
    "ayahs": [
      {
        "number": 4676,
        "text": "Tako mi onih koji uzdižu prašinu.\n",
        "numberInSurah": 1,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4677,
        "text": "i onih koji teret nose,\n",
        "numberInSurah": 2,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4678,
        "text": "i onih kojih plove lahko.\n",
        "numberInSurah": 3,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4679,
        "text": "i onih koji naredbe sprovode,\n",
        "numberInSurah": 4,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4680,
        "text": "Ono što vam se obećava, zaista, je istina.\n",
        "numberInSurah": 5,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4681,
        "text": "Polaganje računa sigurno će se desiti.\n",
        "numberInSurah": 6,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4682,
        "text": "Tako Mi neba skladne ljepote,\n",
        "numberInSurah": 7,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4683,
        "text": "vi govorite oprečno.\n",
        "numberInSurah": 8,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4684,
        "text": "Od njega - Kur'ana biva okrenut onaj ko je zalutao.\n",
        "numberInSurah": 9,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4685,
        "text": "Neka prokleti budu poricatelji\n",
        "numberInSurah": 10,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4686,
        "text": "koji su, utonuli u nevjerovanje, ravnodušni.\n",
        "numberInSurah": 11,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4687,
        "text": "Oni pitaju: \"Kada će Dan sudnji?\"\n",
        "numberInSurah": 12,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4688,
        "text": "Bit će onaj dan kada u Vatri budu paćeni.\n",
        "numberInSurah": 13,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4689,
        "text": "\"Iskusite patnju svoju - to je ono što ste požurivali!\"\n",
        "numberInSurah": 14,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4690,
        "text": "A bogobojazni će, zaista, u džennetskim baščama, među izvorima, boraviti.\n",
        "numberInSurah": 15,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4691,
        "text": "Primat će ono što im Gospodar njihov bude darovao; oni su prije toga dobra djela činili,\n",
        "numberInSurah": 16,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4692,
        "text": "noću samo malo spavali,\n",
        "numberInSurah": 17,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4693,
        "text": "i u praskozorje oprost od grijeha molili,\n",
        "numberInSurah": 18,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4694,
        "text": "a u imecima njihovim bio je udio i za onoga koji prosi i za onoga koji je uskraćen a ne prosi.\n",
        "numberInSurah": 19,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4695,
        "text": "Na Zemlji su znakovi za one koji čvrsto vjeruju,\n",
        "numberInSurah": 20,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4696,
        "text": "a i u vama samima - zar ne vidite?\n",
        "numberInSurah": 21,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4697,
        "text": "A na nebu je opskrba vaša i ono što vam se obećava\n",
        "numberInSurah": 22,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4698,
        "text": "i, tako Mi Gospodara neba i Zemlje, to je istina, kao što je istina da govorite!\n",
        "numberInSurah": 23,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4699,
        "text": "Da li je doprla do tebe vijest o počašćenim gostima lbrahimovim.\n",
        "numberInSurah": 24,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4700,
        "text": "Kad mu oni uđoše i rekoše: \"Selam vama!\", i on reče: \"Selam vama, ljudi neznani.\"\n",
        "numberInSurah": 25,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4701,
        "text": "I on neprimjetno ode ukućanima svojim i donese debelo tele.\n",
        "numberInSurah": 26,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4702,
        "text": "primače im ga: \"Zar nećete jesti?\", upita\n",
        "numberInSurah": 27,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4703,
        "text": "i osjeti od njih u sebi strah. \"Ne boj se.\", rekoše i obradovaše ga dječakom koji će učen biti.\n",
        "numberInSurah": 28,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4704,
        "text": "I pojavi se žena njegova uzvikujući i po licu se udarajući i reče: \"Ja sam stara, nerotkinja!\n",
        "numberInSurah": 29,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4705,
        "text": "\"Tako je odredio Gospodar tvoj\", rekoše oni, \"On je Mudri i Sveznajući.\"\n",
        "numberInSurah": 30,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4706,
        "text": "\"A što vi hoćete, o izaslanici?\", upita Ibrahim.\n",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4707,
        "text": "\"Poslani smo narodu prestupničkom'', rekoše,\n",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4708,
        "text": "\"da sručimo na njih grumenje od gline,\n",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4709,
        "text": "obilježeno u Gospodara tvoga za one koji su u razvratu svaku mjeru prešli.\"\n",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4710,
        "text": "Mi iz njega vjernike izvedosmo,\n",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4711,
        "text": "a u njemu samo jednu kuću muslimansku nađosmo,\n",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4712,
        "text": "i u njemu, za sve one koji se boje bolne patnje, znak ostavismo.\n",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4713,
        "text": "I u Musau, također - kada ga s očiglednim dokazom faraonu poslasmo,\n",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4714,
        "text": "a on, oholeći se, okrenu se nastranu i reče: \"Čarobnjak je ili lud.\"\n",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4715,
        "text": "I Mi i njega i vojske njegove dohvatismo, pa ih u more bacismo, jer je bio osudu zaslužio.\n",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4716,
        "text": "I u Adu - kad na njih vjetar poslasmo u kome nije bilo nikakva dobra;\n",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4717,
        "text": "Pored čega god je prošao, ništa nije poštedio, sve je u gnjilež pretvorio.\n",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4718,
        "text": "I u Semudu'\"\" - kad im bi rečeno: \"Uživajte još neko vrijeme!\"\n",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4719,
        "text": "Oni se oglušiše o naređenje Gospodara svoga, pa ih uništi strašan glas na oči njihove,\n",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4720,
        "text": "i ne mogaše se ni dići ni od kazne odbraniti.\n",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4721,
        "text": "I uništili smo prije narod Nuhov: to, zaista, bijaše narod buntovni.\n",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4722,
        "text": "Mi smo nebo snagom sazdali, i Mi smo, uistinu, proširitelji.\n",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4723,
        "text": "I Zemlju smo prostrli - tako smo divni Mi Koji smo je prostrli!\n",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4724,
        "text": "I od svega po par stvaramo da biste se vi prisjećali.\n",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4725,
        "text": "\"Zato požurite Allahu, ja sam vam Njegov upozoritelj jasni!\n",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4726,
        "text": "I ne pripisujte uz Allaha boga drugoga! Ja sam vam Njegov upozoritelj jasni!\n",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4727,
        "text": "I tako je bilo: ni onima prije ovih nije došao nijedan poslanik, a da nisu rekli:  \"Čarobnjak je!\" ili: \"Lud je!\"\n",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4728,
        "text": "Jesu li to oni jedni drugima oporučili? Nisu, nego su oni ljudi koji su u zlu svaku mjeru bili prevršili.\n",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4729,
        "text": "Pa, od njih ti se okreni, prekoren nećeš biti!\n",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4730,
        "text": "I opominji! Doista, opomena vjernicima koristi!\n",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4731,
        "text": "Džine i ljude sam stvorio samo zato da Meni u ibadetu budu.\n",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4732,
        "text": "Ja ne tražim od njih opskrbu niti želim da me hrane,\n",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4733,
        "text": "Opskrbitelj je jedino Allah, Snažni i Silni!\n",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4734,
        "text": "A one koji su zulum činili stići će kazna kao što je stigla i one koji su bili kao oni, i neka Me ne požuruju;\n",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4735,
        "text": "a, teško nevjernicima na Dan kojim im se prijeti!\n",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      }
    ],
