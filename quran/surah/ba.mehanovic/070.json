{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 70,
    "name": "سُورَةُ المَعَارِجِ",
    "englishName": "Al-Ma'aarij",
    "englishNameTranslation": "The Ascending Stairways",
    "revelationType": "Meccan",
    "numberOfAyahs": 44,
    "ayahs": [
      {
        "number": 5376,
        "text": "Neko je hitno zatražio patnju koja će se dogoditi\n",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5377,
        "text": "nevjernicima - niko nju ne može spriječiti\n",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5378,
        "text": "da to Allah, Gospodar usponišta i deredža, ne učini,\n",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5379,
        "text": "Njemu se penju meleki i Duh u danu koji pedeset hiljada godina traje.\n",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5380,
        "text": "Ti se lijepo strpi.\n",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5381,
        "text": "Oni, zaista, misle da je daleko - da se dogoditi neće,\n",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5382,
        "text": "a mi smatramo da je blizu.\n",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5383,
        "text": "Na dan kada nebo bude kao talog od zejtina,\n",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5384,
        "text": "a brda kao vuna raščupana,\n",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5385,
        "text": "kada bližnji neće bližnjeg ništa pitati,\n",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5386,
        "text": "iako će jedni druge vidjeti. Prestupnik će htjeti da se od patnje taj dan iskupi sinovima svojim,\n",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5387,
        "text": "i suprugom svojom, i bratom svojim,\n",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5388,
        "text": "i porodicom svojom koja ga štiti,\n",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5389,
        "text": "i svima ostalima na Zemlji, samo da se izbavi.\n",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5390,
        "text": "Nikako! Ona je, zaista, buktinja žestoka\n",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5391,
        "text": "koja će kože guliti,\n",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5392,
        "text": "zvat će onoga ko se okretao i izbjegavao\n",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5393,
        "text": "i zgrtao i skrivao.\n",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5394,
        "text": "Čovjek je, uistinu, stvorenje lakomo:\n",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5395,
        "text": "kada ga nevolja snađe - očajan je,\n",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5396,
        "text": "a kada ga zadesi dobro, on ga uskraćuje,\n",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5397,
        "text": "osim klanjača\n",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5398,
        "text": "koji su u svojim namazima ustrajni,\n",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5399,
        "text": "i oni u čijim imecima ima poznato pravo\n",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5400,
        "text": "onaj koji prosi i za onoga koji je uskraćen a ne prosi,\n",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5401,
        "text": "i oni koji u Sudnji dan vjeruju,\n",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5402,
        "text": "i oni koji od kazne Allahove strahuju,\n",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5403,
        "text": "a od kazne Gospodara svoga niko nije siguran,\n",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5404,
        "text": "i oni koji stidna mjesta svoja čuvaju,\n",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5405,
        "text": "osim od supruga svojih ili onih koje su u vlasništvu njihovu - oni, doista, prijekor ne zaslužuju.\n",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5406,
        "text": "Oni koji i pored toga nešto traže, to su baš oni koji prelaze granicu,\n",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5407,
        "text": "i oni koji na povjerene im amanete i obaveze svoje paze,\n",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5408,
        "text": "i oni koji iza svojih svjedočenja čvrsto stoje,\n",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5409,
        "text": "i oni koji na namaze svoje paze,\n",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5410,
        "text": "oni će u džennetskim baščama biti počašćeni.\n",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5411,
        "text": "Šta je onima oko tebe koji ne vjeruju pa žure,\n",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5412,
        "text": "zdesna i slijeva, u gomilama?!\n",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5413,
        "text": "Zar svaki od njih žudi da u Džennet uživanja uđe?!\n",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5414,
        "text": "Naprotiv! Mi smo ih stvorili od onoga što oni znaju!\n",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5415,
        "text": "Ja se kunem Gospodarom istoka i zapada da ih možemo\n",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5416,
        "text": "boljim od njih zamijeniti i niko Nas u tome ne može spriječiti.\n",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5417,
        "text": "Zato ih ostavi neka se u besposlice udubljuju i zabavljaju dok Dan kojim im se prijeti ne dočekaju,\n",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5418,
        "text": "Dan u kome će žurno, kao da kumirima hrle, iz grobova izići,\n",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5419,
        "text": "oborenih pogleda i poniženjem ophrvani. To će biti Dan kojim im je prijećeno!\n",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "ba.mehanovic",
      "language": "ba",
      "name": "Kur'an - sa prevodom (značenja) na bosanski jezik, utemeljen na Ibn Kesirovom tumačenju, i kratki komentar",
      "englishName": "Quran translation by Muhamed Mehanovic",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}