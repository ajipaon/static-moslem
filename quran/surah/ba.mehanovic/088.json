  "data": {
    "number": 88,
    "name": "سُورَةُ الغَاشِيَةِ",
    "englishName": "Al-Ghaashiya",
    "englishNameTranslation": "The Overwhelming",
    "revelationType": "Meccan",
    "numberOfAyahs": 26,
    "ayahs": [
      {
        "number": 5968,
        "text": "Da li je doprla do tebe vijest o Općoj nevolji?\n",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5969,
        "text": "Neka lica Dan taj bit će potištena,\n",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5970,
        "text": "premorena, napaćena,\n",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5971,
        "text": "u vatri užarenoj pržena,\n",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5972,
        "text": "sa vrela uzavrelog napajana,\n",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5973,
        "text": "kada drugog jela osim trnja neće imati,\n",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5974,
        "text": "koje neće ni ugojiti ni glad utoliti.\n",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5975,
        "text": "Neka lica Dan taj bit će radosna,\n",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5976,
        "text": "trudom svojim zadovoljna,\n",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5977,
        "text": "u Džennetu visokome,\n",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5978,
        "text": "u kome prazne besjede neće slušati!\n",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5979,
        "text": "U njemu su izvor-vode koje teku,\n",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5980,
        "text": "u njemu su divani uzdignuti,\n",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5981,
        "text": "i pehari postavljeni,\n",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5982,
        "text": "i jastuci poređani\n",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5983,
        "text": "i ćilimi rašireni.\n",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5984,
        "text": "Pa, zašto oni ne gledaju kamilu kako je stvorena,\n",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5985,
        "text": "i nebo kako je uzdignuto,\n",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5986,
        "text": "i planine kako su postavljene,\n",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5987,
        "text": "i Zemlju kako je prostrta?!\n",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5988,
        "text": "Ti upozoravaj, ti samo i jesi upozoritelj,\n",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5989,
        "text": "ti nisi onaj koji ih prisiljava!\n",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5990,
        "text": "A onoga koji se okreće i ne vjeruje,\n",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5991,
        "text": "njega će Allah najvećom patnjom kazniti.\n",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5992,
        "text": "Nama će se oni, zaista, vratiti\n",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5993,
        "text": "A potom, na Nama je, doista, da račun njihov svedemo!\n",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      }
