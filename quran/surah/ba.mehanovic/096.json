  "data": {
    "number": 96,
    "name": "سُورَةُ العَلَقِ",
    "englishName": "Al-Alaq",
    "englishNameTranslation": "The Clot",
    "revelationType": "Meccan",
    "numberOfAyahs": 19,
    "ayahs": [
      {
        "number": 6107,
        "text": "Čitaj s imenom Gospodara tvoga Koji stvara,\n",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6108,
        "text": "stvara čovjeka od zakvačka.\n",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6109,
        "text": "Čitaj, plemenit je Gospodar tvoj,\n",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6110,
        "text": "Koji poučava peru,\n",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6111,
        "text": "Koji čovjeka poučava onome što ne zna.\n",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6112,
        "text": "Uistinu čovjek se uzobijesti\n",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6113,
        "text": "čim se neovisnim osjeti\n",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6114,
        "text": "a Gospodaru tvome će se, doista, svi vratiti!\n",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6115,
        "text": "Vidjeli ti onoga koji sprečava\n",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6116,
        "text": "roba da klanja?\n",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6117,
        "text": "Šta misliš, ako on je on na Pravom putu,\n",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6118,
        "text": "ili ako bogobojaznost naređuje?!\n",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6119,
        "text": "Šta misliš, ako on poriče i okreće se,\n",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6120,
        "text": "zar on ne zna da Allah vidi?!\n",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6121,
        "text": "Naprotiv! Ako se ne okani, ocrnit ćemo mu čelo,\n",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6122,
        "text": "čelo lažno i grešno,\n",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6123,
        "text": "pa neka on pozove društvo svoje.\n",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6124,
        "text": "Mi ćemo pozvati zebanije.\n",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6125,
        "text": "Naprotiv! Ti se njemu ne pokoravaj, već čini sedždu i približi se Gospodaru svome!\n",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
