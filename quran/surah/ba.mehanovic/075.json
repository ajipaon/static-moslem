{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 75,
    "name": "سُورَةُ القِيَامَةِ",
    "englishName": "Al-Qiyaama",
    "englishNameTranslation": "The Resurrection",
    "revelationType": "Meccan",
    "numberOfAyahs": 40,
    "ayahs": [
      {
        "number": 5552,
        "text": "Kunem se Kijametskim danom\n",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5553,
        "text": "i kunem se dušom koja sebe kori.\n",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5554,
        "text": "Zar čovjek misli da kosti njegove nećemo sakupiti?!\n",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5555,
        "text": "Svakako, a Mi možemo izravnati jagodice prstiju njegovih.\n",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5556,
        "text": "Ali, čovjek hoće da negira ono što pred njim je\n",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5557,
        "text": "pa pita: \"Kada će Kijametski dan biti?\"\n",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5558,
        "text": "Kada se pogled zbuni\n",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5559,
        "text": "i Mjesec pomrači\n",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5560,
        "text": "i Sunce i Mjesec smotaju, pa sjaj izgube,\n",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5561,
        "text": "taj dan čovjek će povikati: \"Gdje da se bježi?\"\n",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5562,
        "text": "Ne, nema pribježišta!\n",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5563,
        "text": "Taj dan tvome je Gospodaru povratak,\n",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5564,
        "text": "taj dan čovjek će o onome što je pripremio, a što je propustio, obaviješten biti.\n",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5565,
        "text": "Štaviše, čovjek sebe dobro zna - sam će protiv sebe svjedok biti,\n",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5566,
        "text": "makar iznosio svoja opravdanja.\n",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5567,
        "text": "Ne izgovaraj Kur'an jezikom svojim da bi ga što prije zapamtio,\n",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5568,
        "text": "Mi smo ga dužni sakupiti da bi ga ti čitao.\n",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5569,
        "text": "A kada ga čitamo, ti prati čitanje njegovo,\n",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5570,
        "text": "a poslije, Mi smo ga dužni objasniti.\n",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5571,
        "text": "Ali, naprotiv, vi ovaj prolazni svijet volite,\n",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5572,
        "text": "a onaj drugi - ahiret zapostavljate.\n",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5573,
        "text": "Taj dan neka lica blistava će biti,\n",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5574,
        "text": "u Gospodara svoga će gledati.\n",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5575,
        "text": "Taj dan neka lica smrknuta će biti,\n",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5576,
        "text": "znat će da će ih snaći velika nesreća!\n",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5577,
        "text": "Pazi, kada duša dopre do ključnih kostiju\n",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5578,
        "text": "i vikne se: \"Ko je vidar da vida?!\",\n",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5579,
        "text": "i on se uvjeri da je to čas rastanka\n",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5580,
        "text": "i noga se uz nogu savije.\n",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5581,
        "text": "Taj dan će Gospodaru tvome privođenje biti.\n",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5582,
        "text": "Nije vjerovao i nije klanjao,\n",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5583,
        "text": "nego je poricao i okretao se,\n",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5584,
        "text": "a onda je svojim oholo odlazio.\n",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5585,
        "text": "Teško tebi! Teško tebi!,\n",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5586,
        "text": "i još jednom: Teško tebi! Teško tebi!\n",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5587,
        "text": "Zar čovjek misli da će zaludu ostavljen biti?!\n",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5588,
        "text": "Zar nije bio kap sjemena koja se ubaci,\n",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5589,
        "text": "zatim zakvačak kome On onda razmjer odredi i skladnim mu lik učini,\n",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5590,
        "text": "i od njega dvije vrste, muškarca i ženu, stvori,\n",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5591,
        "text": "i zar Taj nije kadar mrtve oživiti?!\n",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      }
    ],
