{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 56,
    "name": "سُورَةُ الوَاقِعَةِ",
    "englishName": "Al-Waaqia",
    "englishNameTranslation": "The Inevitable",
    "revelationType": "Meccan",
    "numberOfAyahs": 96,
    "ayahs": [
      {
        "number": 4980,
        "text": "Kada se Neizbježni događaj zbije,\n",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4981,
        "text": "događanju njegovom nema poništenja,\n",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4982,
        "text": "neke će poniziti, a neke uzvisiti.\n",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4983,
        "text": "Kad se Zemlja jako potrese\n",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4984,
        "text": "i brda se u komadiće zdrobe,\n",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4985,
        "text": "i postanu prašina razasuta,\n",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4986,
        "text": "vas će tri vrste biti:\n",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4987,
        "text": "oni zdesna - šta će biti s onim zdesna?!\n",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4988,
        "text": "i oni slijeva - šta će biti s onim slijeva?\n",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4989,
        "text": "i oni koji prednjače - oni će prednjačiti!\n",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4990,
        "text": "To su oni bliski,\n",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4991,
        "text": "u džennetskim baščama naslada bit će.\n",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4992,
        "text": "Bit će ih mnogo iz prvih,\n",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4993,
        "text": "a malo iz zadnjih!\n",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4994,
        "text": "na divanima izvezenim,\n",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4995,
        "text": "jedni prema drugima na njima će naslonjeni biti;\n",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4996,
        "text": "služit će ih vječno mladi dječaci,\n",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4997,
        "text": "sa čašama i ibricima, i peharom punim pića iz izvora tekućeg\n",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4998,
        "text": "od koga ih glava neće boljeti i zbog koga neće pamet izgubiti,\n",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4999,
        "text": "i voćem koje će sami birati,\n",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5000,
        "text": "i mesom ptičijim kakvo budu željeli.\n",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5001,
        "text": "U njima će biti i hurije očiju krupnih,\n",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5002,
        "text": "slične biseru u školjkama skrivenom,\n",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5003,
        "text": "kao nagrada za ono što su činili.\n",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5004,
        "text": "U njima neće slušati prazne besjede, ni govor grešni,\n",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5005,
        "text": "nego samo riječi: \"Selam, selam!\"\n",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5006,
        "text": "A oni s desne strane - šta će biti s onim s desne strane?\n",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5007,
        "text": "Bit će među lotosovim drvećem bez bodlji,\n",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5008,
        "text": "i među drvećem akacije plodovima nanizanim,\n",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5009,
        "text": "i u hladovini prostranoj,\n",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5010,
        "text": "pored vode tekuće\n",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5011,
        "text": "i usred voća svakovrsnog\n",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5012,
        "text": "koje će uvijek imati i koje neće zabranjeno biti\n",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5013,
        "text": "i na posteljama uzdignutim.\n",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5014,
        "text": "Stvaranje njihovo, zaista, ćemo obnoviti.\n",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5015,
        "text": "i djevicama ih učiniti,\n",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5016,
        "text": "te onim koje se umiljavaju muževima svojim i godina istih\n",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5017,
        "text": "za one s desne strane.\n",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5018,
        "text": "Bit će ih mnogo iz prvih,\n",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5019,
        "text": "a mnogo iz kasnijih.\n",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5020,
        "text": "A oni s lijeve strane - šta je s onima s lijeve strane?!\n",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5021,
        "text": "Oni će biti u vrelom dašku vatre i vodi ključaloj,\n",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5022,
        "text": "i u sjeni dima mrkog,\n",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5023,
        "text": "u kojoj neće biti svježine ni bilo kakve ugodnosti.\n",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5024,
        "text": "Oni su prije toga raskošnim životom živjeli\n",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5025,
        "text": "i uporno teške grijehe činili\n",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5026,
        "text": "i govorili: \"Zar kad umremo i zemlja i kosti postanemo - zar ćemo zbilja biti oživljeni;\n",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5027,
        "text": "zar i drevni naši preci?!\"\n",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5028,
        "text": "Reci: \"I drevni i kasniji,\n",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5029,
        "text": "u određeno vrijeme, i u Dan određeni bit će sakupljeni,\n",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5030,
        "text": "i tada ćete vi, o zabludjeli, koji poričete oživljavanje,\n",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5031,
        "text": "sigurno s drveta zekkum jesti,\n",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5032,
        "text": "i njime ćete trbuhe puniti,\n",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5033,
        "text": "pa zatim na to vodu ključalu piti,\n",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5034,
        "text": "poput kamila koje ne mogu žeđ ugasiti\",\n",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5035,
        "text": "to će im na Dan polaganja računa ugošćenje biti!\n",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5036,
        "text": "Mi smo vas stvorili - pa zašto ne povjerujete?\n",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5037,
        "text": "Kažite vi Meni: da li sjemenu koje ubacujete\n",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5038,
        "text": "vi oblik dajete ili Mi to činimo?\n",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5039,
        "text": "Mi određujemo kad će ko od vas umrijeti, i niko Nas ne može spriječiti\n",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5040,
        "text": "da likove vaše izmijenimo i da vas iznova u likovima koje vi ne poznajete stvorimo.\n",
        "numberInSurah": 61,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5041,
        "text": "Poznato vam je kako ste prvi put stvoreni, pa zašto se ne urazumite?\n",
        "numberInSurah": 62,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5042,
        "text": "Kažite vi Meni: šta biva s onim što posijete?\n",
        "numberInSurah": 63,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5043,
        "text": "Da li mu vi dajete snagu da niče, ili to Mi činimo?\n",
        "numberInSurah": 64,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5044,
        "text": "Ako hoćemo, možemo ga u suho rastinje pretvoriti, pa biste se snebivali:\n",
        "numberInSurah": 65,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5045,
        "text": "\"Mi smo, doista, oštećeni,\n",
        "numberInSurah": 66,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5046,
        "text": "čak smo svega lišeni!\"\n",
        "numberInSurah": 67,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5047,
        "text": "Kažite vi Meni: vodu koju pijete -\n",
        "numberInSurah": 68,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5048,
        "text": "da li je vi ili Mi iz oblaka spuštamo?\n",
        "numberInSurah": 69,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5049,
        "text": "Ako želimo, možemo je slanom učiniti - pa zašto niste zahvalni?\n",
        "numberInSurah": 70,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5050,
        "text": "Kažite vi Meni: vatru koju palite -\n",
        "numberInSurah": 71,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5051,
        "text": "da li drvo za nju Mi stvaramo ili vi?\n",
        "numberInSurah": 72,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5052,
        "text": "Mi činimo da ona podsjeća i da bude korisna putnicima kada konače;\n",
        "numberInSurah": 73,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5053,
        "text": "zato slavi i veličaj ime Gospodara svoga Veličanstvenog!\n",
        "numberInSurah": 74,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5054,
        "text": "I kunem se mjestima postepenog spuštanja Kur'ana,\n",
        "numberInSurah": 75,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5055,
        "text": "a to je, da znate, zakletva velika!\n",
        "numberInSurah": 76,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5056,
        "text": "On je, zaista, Kur'an plemeniti\n",
        "numberInSurah": 77,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5057,
        "text": "u Knjizi brižljivo čuvanoj.\n",
        "numberInSurah": 78,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5058,
        "text": "Dodirnuti ga smiju samo oni koji su čisti!\n",
        "numberInSurah": 79,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5059,
        "text": "On je objava od Gospodara svjetova,\n",
        "numberInSurah": 80,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5060,
        "text": "pa kako ovaj govor omalovažavate,\n",
        "numberInSurah": 81,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5061,
        "text": "i umjesto zahvalnosti što vam je opskrba darovana, vi u njega ne vjerujete?\n",
        "numberInSurah": 82,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5062,
        "text": "A zašto vi, kad duša do guše dopre,\n",
        "numberInSurah": 83,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5063,
        "text": "i kad vi budete tada gledali -\n",
        "numberInSurah": 84,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5064,
        "text": "a Mi smo mu bliži od vas, ali vi ne vidite -\n",
        "numberInSurah": 85,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5065,
        "text": "zašto je onda, ako račun nećete polagati,\n",
        "numberInSurah": 86,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5066,
        "text": "ne povratite, ako istinu govorite?\n",
        "numberInSurah": 87,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5067,
        "text": "I ako bude jedan od onih koji su Allahu bliski -\n",
        "numberInSurah": 88,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5068,
        "text": "udobnost i opskrba lijepa i džennetske blagodati pripast će njemu!\n",
        "numberInSurah": 89,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5069,
        "text": "A ako bude jedan od onih koji su s desne strane,\n",
        "numberInSurah": 90,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5070,
        "text": "pa, selam tebi od onih s desne sretne!\n",
        "numberInSurah": 91,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5071,
        "text": "A ako bude jedan od onih koji su poricali i u zabludi ostali,\n",
        "numberInSurah": 92,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5072,
        "text": "pa, ključalom vodom bit će ugošćen\n",
        "numberInSurah": 93,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5073,
        "text": "i u Ognju pržen.\n",
        "numberInSurah": 94,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5074,
        "text": "Sama je istina, zbilja, sve ovo!\n",
        "numberInSurah": 95,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5075,
        "text": "Pa, slavi i veličaj ime Gospodara svoga Veličanstvenog!\n",
        "numberInSurah": 96,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      }
