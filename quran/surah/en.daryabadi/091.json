    "name": "سُورَةُ الشَّمۡسِ",
    "englishName": "Ash-Shams",
    "englishNameTranslation": "The Sun",
    "revelationType": "Meccan",
    "numberOfAyahs": 15,
    "ayahs": [
      {
        "number": 6044,
        "text": "By the sun and his morning bright-ness,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6045,
        "text": "By the moon, when she followeth him,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6046,
        "text": "By the day when it glorifieth him.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6047,
        "text": "By the night; when it envelopeth him,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6048,
        "text": "By the heaven and Him Who builded it,",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6049,
        "text": "By the earth and Him Who spread it forth,",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6050,
        "text": "By the soul and Him Who propertioned it,",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6051,
        "text": "And inspired it with the wickedness thereof and the piety thereof,",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6052,
        "text": "Surely blissful is he who hath cleansed, it,",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6053,
        "text": "And miserable is he who hath buried it.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6054,
        "text": "Thamud belied in their exorbitance.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6055,
        "text": "What time the greatest wretch of them rose up.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6056,
        "text": "Then the apostle of Allah said Unto them: beware of the she-camel of Allah and her drink.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6057,
        "text": "Then they belied him, and they hamstrung her; wherefore their Lord overwhelmed them for their crime, and made it equal.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6058,
        "text": "And He feared not the consequence thereof",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      }
