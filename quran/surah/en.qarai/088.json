    "numberOfAyahs": 26,
    "ayahs": [
      {
        "number": 5968,
        "text": "Did you receive the account of the Enveloper?",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5969,
        "text": "Some faces on that day will be humbled,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5970,
        "text": "wrought-up and weary:",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5971,
        "text": "they will enter a scorching fire",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5972,
        "text": "and made to drink from a boiling spring.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5973,
        "text": "They will have no food except cactus,",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5974,
        "text": "neither nourishing, nor of avail against hunger.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5975,
        "text": "Some faces on that day will be joyous,",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5976,
        "text": "pleased with their endeavour,",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5977,
        "text": "in a lofty garden,",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5978,
        "text": "where they will not hear any vain talk.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5979,
        "text": "In it is a flowing spring",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5980,
        "text": "and raised couches,",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5981,
        "text": "with goblets set,",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5982,
        "text": "and cushions laid out in an array,",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5983,
        "text": "and carpets spread out.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5984,
        "text": "Do they not observe the camel, [to see] how it has been created?",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
