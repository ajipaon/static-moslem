{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 91,
    "name": "سُورَةُ الشَّمۡسِ",
    "englishName": "Ash-Shams",
    "englishNameTranslation": "The Sun",
    "revelationType": "Meccan",
    "numberOfAyahs": 15,
    "ayahs": [
      {
        "number": 6044,
        "text": "Yemin olsun Güneş'e ve ışığının parladığı kuşluk vaktine,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6045,
        "text": "Onu izlediğinde Ay'a,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6046,
        "text": "Onu iyice açtığı vakit gündüze,",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6047,
        "text": "Ve onu sarıp sarmaladığı zaman geceye.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6048,
        "text": "Göğe ve onu kurana,",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6049,
        "text": "Yere ve onu döşeyene.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6050,
        "text": "Nefse ve onu düzgün bir biçimde şekillendirene.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6051,
        "text": "Ardından da ona bozukluğunu ve takvasını ilham edene ki,",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6052,
        "text": "Benliği temizleyip arındıran, gerçekten kurtulmuştur.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6053,
        "text": "Onu kirletip örtense kayba uğramıştır.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6054,
        "text": "Semûd kavmi, azgınlığı yüzünden yalanladı.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6055,
        "text": "En haydutları ortaya fırladığı zaman,",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6056,
        "text": "Allah'ın elçisi onlara şöyle demişti: \"Allah'ın devesini ve onun su içme hakkını koruyun.\"",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6057,
        "text": "Fakat elçiye inanmadılar da deveyi devirip boğazladılar. Bunun üzerine, Rableri onların günahlarını kendi başlarına geçirdi de o yurdu dümdüz etti.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6058,
        "text": "Allah, işin sonundan korkacak değil ya!",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "tr.ozturk",
