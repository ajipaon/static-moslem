{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 89,
    "name": "سُورَةُ الفَجۡرِ",
    "englishName": "Al-Fajr",
    "englishNameTranslation": "The Dawn",
    "revelationType": "Meccan",
    "numberOfAyahs": 30,
    "ayahs": [
      {
        "number": 5994,
        "text": "Yemin olsun tan yerinin ağarma vaktine,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5995,
        "text": "On geceye,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5996,
        "text": "Çifte ve teke,",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5997,
        "text": "Yola koyulduğu zaman geceye.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5998,
        "text": "Nasıl, bunlarda akıl sahibi için bir yemin var mı?",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5999,
        "text": "Görmedin mi ne yaptı Rabbin Âd kavmine?",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6000,
        "text": "Sütunlarla dolu İrem'e,",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6001,
        "text": "Ki beldeler içinde onun benzeri yaratılmamıştı.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6002,
        "text": "Ve ne yaptı vadide kayaları oyan Semûd kavmine?",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6003,
        "text": "Ve kazıklar sahibi Firavun'a.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6004,
        "text": "Bunlar, ülkelerde azıp zulmetmişlerdi.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6005,
        "text": "Ve oralarda bozgunu çoğaltmışlardı.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6006,
        "text": "Bu yüzden Rabbin, üzerlerine azap kamçısını yağdırıverdi.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6007,
        "text": "Çünkü Rabbin tam gözetleme yerindedir/tam bir biçimde gözetlemektedir.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6008,
        "text": "İnsan böyledir; Rabbi kendisini deneyip de ona cömert davranır, nimet yağdırırsa: \"Rabbim bana ikramda bulundu!\" der.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6009,
        "text": "Ama Rabbi onu sıkıntıya uğratıp rızkını ölçüye bağlarsa: \"Rabbim bana ihanet etti!\" der.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6010,
        "text": "Doğrusu şu ki, siz yetime ikramda bulunmuyorsunuz.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6011,
        "text": "Yoksulun doyurulmasını teşvik etmiyorsunuz.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6012,
        "text": "Mirası derleyip toplayıp yiyorsunuz.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6013,
        "text": "Malı, devşirip depolatacak bir sevgiyle seviyorsunuz.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6014,
        "text": "İş böyle gitmeyecektir! Yer birbirine çarpılıp dümdüz hale getirildiğinde,",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6015,
        "text": "Rabbin gelip melekler saf saf dizildiğinde,",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6016,
        "text": "O gün cehennem de getirilir. İşte o gün düşünüp anlar insan. Ama düşünüp hatırlamanın ona ne yararı var!",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6017,
        "text": "Der ki: \"Keşke şu hayatım için önden bir şeyler gönderseydim.\"",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6018,
        "text": "O gün hiç kimse O'nun azabı gibi azap edemez.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6019,
        "text": "Ve hiç kimse O'nun vurduğu bağ gibi bağ vuramaz.",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6020,
        "text": "Ey sükûna kavuşmuş benlik!",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6021,
        "text": "Dön Rabbine, razı etmiş ve razı edilmiş olarak!",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6022,
        "text": "Gir kullarımın arasına!",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6023,
        "text": "Gir cennetime!",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      }
    ],
