{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 77,
    "name": "سُورَةُ المُرۡسَلَاتِ",
    "englishName": "Al-Mursalaat",
    "englishNameTranslation": "The Emissaries",
    "revelationType": "Meccan",
    "numberOfAyahs": 50,
    "ayahs": [
      {
        "number": 5623,
        "text": "Yemin olsun, o art arda gönderilenlere/meleklere/rüzgârlara/vahyin bölümlerine/kalplere inen doğuşlara,",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5624,
        "text": "Esip de büküp devirenlere,",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5625,
        "text": "Dağıtıp yayanlara/diriltip harekete getirenlere,",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5626,
        "text": "Gerektiği şekilde ayıranlara,",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5627,
        "text": "Öğüt ulaştıranlara/Kur'an'ı ulaştıranlara,",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5628,
        "text": "Özür yahut uyarı için,",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5629,
        "text": "Ki size duyurulmuş olan mutlaka gerçekleşecektir.",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5630,
        "text": "Yıldızlar silinip süpürüldüğünde,",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5631,
        "text": "Gök yarıldığında,",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5632,
        "text": "Dağlar un-ufak edilip savrulduğunda,",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5633,
        "text": "Resuller vakte bağlandığında,",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5634,
        "text": "Hangi gün için vakte bağlandılar?",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5635,
        "text": "Ayrım ve hüküm günü için.",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5636,
        "text": "Ayrım ve hüküm gününü sana bildiren nedir?",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5637,
        "text": "Yalanlayanların vay haline o gün!",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5638,
        "text": "Öncekileri helâk etmedik mi?",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5639,
        "text": "Sonra, geriden gelenleri de onların peşlerine takarız.",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5640,
        "text": "Biz, suçlulara işte böyle yaparız.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5641,
        "text": "Yalanlayanların o gün vay haline!",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5642,
        "text": "Sizi basit bir sudan yaratmadık mı?",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5643,
        "text": "Onu dayanıklı karargâhta tuttuk.",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5644,
        "text": "Bilinen bir ölçüye/süreye kadar.",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5645,
        "text": "Bir ölçüyle yaptık. Ne güzel ölçü koyanlarız biz!",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5646,
        "text": "Vay başına o gün, yalanlayanların!",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5647,
        "text": "Yeri, bir toplanma zemini yapmadık mı?",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5648,
        "text": "Diriler bakımından da ölüler bakımından da.",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5649,
        "text": "Orada oturaklı, başını yücelere kaldırmış dağlar oluşturduk. Ve size tatlı bir su içirdik.",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5650,
        "text": "Vay haline o gün, yalanlayanların!",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5651,
        "text": "Haydi, yalanlamakta olduğunuz şeye gidin!",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5652,
        "text": "Haydi, üç çatallı gölgeye gidin!",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5653,
        "text": "Ne gölgelendirir ne alevden korur.",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5654,
        "text": "Gerçekten o, köşke benzer kıvılcımlar saçar.",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5655,
        "text": "O kıvılcım sanki sarımtırak bir halat/bir deve kervanı/bakırdan bir ip gibidir.",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5656,
        "text": "Vay haline o gün, yalanlayanların!",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5657,
        "text": "Konuşamayacakları gündür bu!",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5658,
        "text": "İzin verilmez ki onlara özür dilesinler.",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5659,
        "text": "Vay haline o gün, yalanlayanların!",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5660,
        "text": "Ayırma günüdür bu! Sizinle öncekileri bir yere topladık.",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5661,
        "text": "Eğer bir hileniz/bir tuzağınız varsa, hadi hile yapıp tuzak kurun bana!",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5662,
        "text": "Vay haline o gün, yalanlayanların!",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5663,
        "text": "Takvaya sarılanlar gölgeler altında, su kaynaklarındadır.",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5664,
        "text": "Canlarının çektiği meyvelerle yanyanadırlar.",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5665,
        "text": "\"Yapıp ürettiklerinize karşılık olarak afiyetle yiyip için.\"",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5666,
        "text": "İşte böyle ödüllendiririz biz, güzellikler sergileyenleri!",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5667,
        "text": "Vay haline o gün, yalanlayanların!",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5668,
        "text": "Yiyin ve birazcık nimetlenin. Suçlularsınız siz.",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5669,
        "text": "Vay haline o gün, yalanlayanların!",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5670,
        "text": "Onlara, \"rukû' edin!\" dendiğinde rukû etmezler.",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5671,
        "text": "Vay haline o gün, yalanlayanların.",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5672,
        "text": "Artık bundan sonra hangi hadise/söze iman edecekler?",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      }
    ],
