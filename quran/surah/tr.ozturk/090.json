{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 90,
    "name": "سُورَةُ البَلَدِ",
    "englishName": "Al-Balad",
    "englishNameTranslation": "The City",
    "revelationType": "Meccan",
    "numberOfAyahs": 20,
    "ayahs": [
      {
        "number": 6024,
        "text": "Yemin ederim bu kente ki, iş onların sandığı gibi değildir!",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6025,
        "text": "Sen bu kente mahremsin/bu kente gireceksin.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6026,
        "text": "Ve doğurana ve doğurduğuna da yemin olsun ki,",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6027,
        "text": "Biz insanı gerçekten bir sıkıntı ve zorluk içinde yarattık.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6028,
        "text": "O sanıyor mu ki, hiç kimse ona asla güç yetiremeyecektir!",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6029,
        "text": "\"Yığınlarla mal telef ettim!\" diyor.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6030,
        "text": "Hiç kimsenin kendisini görmediğini mi sanıyor?",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6031,
        "text": "Biz ona vermedik mi iki göz,",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6032,
        "text": "Bir dil, iki dudak?",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6033,
        "text": "Kılavuzladık onu iki tepeye.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6034,
        "text": "Akabeye, sarp yokuşa atılamadı o.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6035,
        "text": "Sarp yokuşun ne olduğunu sana bildiren nedir?",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6036,
        "text": "Özgürlüğü zincirlenenin bağını çözmektir o.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6037,
        "text": "Yahut da açlık ve perişanlık gününde doyurmaktır o,",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6038,
        "text": "Yakındaki bir yetimi,",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6039,
        "text": "Yahut ezilmiş-boynu bükük bir yoksulu.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6040,
        "text": "Sonra da iman eden ve birbirlerine sabrı öneren, merhameti öneren kişilerden olmaktır o.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6041,
        "text": "İşte böyleleridir uğur ve bereket dostları.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6042,
        "text": "Bizim ayetlerimizi tanımayanlara gelince bunlar; şomluk, uğursuzluk yâranıdır.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6043,
        "text": "Bunların üzerine, kilitlenecek bir ateş gelecektir.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      }
    ],
