{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 79,
    "name": "سُورَةُ النَّازِعَاتِ",
    "englishName": "An-Naazi'aat",
    "englishNameTranslation": "Those who drag forth",
    "revelationType": "Meccan",
    "numberOfAyahs": 46,
    "ayahs": [
      {
        "number": 5713,
        "text": "Yemin olsun, çekip koparanlara/yay çekenlere/kuyudan su çekenlere/bağsız-bekçisiz koşan atlara/ayrılık yüzünden hasret çekenlere/daldırıp daldırıp çıkaranlara,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5714,
        "text": "Yemin olsun, rahatça, incitmeden çekenlere/düğümü hünerle çözenlere/bir yerden bir yere gidenlere/coşkuyla iç çekenlere,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5715,
        "text": "Yemin olsun, boşlukta yahut suda yüzüp gidenlere,",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5716,
        "text": "Derken öne geçip yarışı kazananlara,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5717,
        "text": "Bir iş ve oluşu çekip çevirenlere,",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5718,
        "text": "Ki o gün şiddetle sarsacak olan saracaktır.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5719,
        "text": "Onu, ardısıra gelen izleyecektir.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5720,
        "text": "Bazı kalpler o gün kaygıdan titreyecektir.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5721,
        "text": "Onların gözleri yerlere eğilecektir.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5722,
        "text": "\"Biz gerçekten bu çukurda eski halimize döndürülecek miyiz?\" diyorlar.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5723,
        "text": "\"Un-ufak kemikler haline geldikten sonra, öyle mi!\"",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5724,
        "text": "\"Hüsran dolu bir dönüştür bu öyleyse!\" diye konuştular.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5725,
        "text": "Oysaki o, sert bir komut sesinden ibarettir.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5726,
        "text": "Bir anda hepsi uyanıp ortaya geliverir.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5727,
        "text": "Ulaştı mı sana Mûsa'nın haberi?",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5728,
        "text": "Hani, Rabbi ona, kutsal vadide, Tuva'da seslenmişti:",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5729,
        "text": "\"Firavun'a git! İyice azdı o.\"",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5730,
        "text": "\"De ki ona: 'Arınıp temizlenmeye ne dersin?\"",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5731,
        "text": "\"Seni Rabbine kılavuzlayayım da gönülden ürperesin!\"",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5732,
        "text": "Derken, ona o en büyük mucizeyi gösterdi.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5733,
        "text": "Ama o yalanladı, isyan etti.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5734,
        "text": "Sonra, sırtını döndü; koşuyordu.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5735,
        "text": "Derken, bir araya toplayıp bağırdı.",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5736,
        "text": "Dedi ki: \"Ben sizin en yüce rabbinizim.\"",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5737,
        "text": "Bunun üzerine Allah, onu sonraya ve önceye ibret olmak üzere bir ceza ile çarptı.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5738,
        "text": "Kuşkusuz, bunda, içine ürperti düşen için tam bir ibret vardır.",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5739,
        "text": "Siz mi daha zorsunuz yaratılışça, gök mü?",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5740,
        "text": "Onu O yapıp kurdu. Onun boyunu yükseltti; ardından ona ahenk ve düzen verdi.",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5741,
        "text": "Gecesini kararttı, kuşluğunu ortaya çıkardı.",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5742,
        "text": "Bundan sonra da yeri yayıp deve kuşu yumurtası biçiminde yuvarlattı.",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5743,
        "text": "Ondan suyunu, otlağını çıkardı.",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5744,
        "text": "Dağları, demir atmış gibi oturttu;",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5745,
        "text": "Sizin için ve hayvanlarınız için bir geçim aracı olarak.",
        "numberInSurah": 33,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5746,
        "text": "O güç yetmez büyük felaket geldiğinde,",
        "numberInSurah": 34,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5747,
        "text": "O gün insan, uğrunda gayret sarfettiği şeyi hatırlar.",
        "numberInSurah": 35,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5748,
        "text": "Gören kişi için cehennem apaçık ortaya çıkarılmıştır.",
        "numberInSurah": 36,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5749,
        "text": "Artık azmış olan,",
        "numberInSurah": 37,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5750,
        "text": "Ve iğreti hayatı yeğlemiş olan için,",
        "numberInSurah": 38,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5751,
        "text": "Cehennem, barınağın ta kendisidir.",
        "numberInSurah": 39,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5752,
        "text": "Rabbinin yüceliğinden korkup nefsini boş heveslerden yasaklamış olan içinse,",
        "numberInSurah": 40,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5753,
        "text": "Cennet, barınağın ta kendisidir.",
        "numberInSurah": 41,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5754,
        "text": "O saatten soruyorlar sana, \"gelip demir atması ne zaman?\" diye.",
        "numberInSurah": 42,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5755,
        "text": "Nerede sende, onu hatırlatacak şey!",
        "numberInSurah": 43,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5756,
        "text": "Ona ilişkin bilginin sonu Rabbine varır.",
        "numberInSurah": 44,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5757,
        "text": "Sen sadece, ondan korkanları uyaransın.",
        "numberInSurah": 45,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5758,
        "text": "Onu gördükleri gün onlar, dünyada sanki bir akşam veya onun kuşluk vaktinden başka kalmamışa dönerler.",
        "numberInSurah": 46,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      }
    ],
