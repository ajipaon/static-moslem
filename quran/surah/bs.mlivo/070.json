{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 70,
    "name": "سُورَةُ المَعَارِجِ",
    "englishName": "Al-Ma'aarij",
    "englishNameTranslation": "The Ascending Stairways",
    "revelationType": "Meccan",
    "numberOfAyahs": 44,
    "ayahs": [
      {
        "number": 5376,
        "text": "Upitao je pitalac o kazni koja će se dogoditi,",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5377,
        "text": "Nevjernicima - nema tog ko će je odbiti -",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5378,
        "text": "Od Allaha, Vlasnika usponišta,",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5379,
        "text": "Penju se meleci i Duh Njemu, u danu čija je mjera pedeset hiljada godina.",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5380,
        "text": "Zato se strpi strpljenjem lijepim.",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5381,
        "text": "Uistinu! Oni ga vide dalekim,",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5382,
        "text": "A vidimo ga bliskim.",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5383,
        "text": "Na Dan kad nebo bude kao rastalina,",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5384,
        "text": "I brda budu kao vuna obojena,",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5385,
        "text": "I ne bude pitao bližnji bližnjega,",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5386,
        "text": "(A) vidjeće ih - voliće krivac da se iskupi od kazne Tog dana sinovima svojim,",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5387,
        "text": "I ženom svojom i bratom svojim,",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5388,
        "text": "I porodicom svojom koja ga je štitila,",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5389,
        "text": "I onim ko je na Zemlji, svima, zatim da ga spasi.",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5390,
        "text": "Nikako! Uistinu, ona će biti buktinja,",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5391,
        "text": "Trgajući, da bi pržila.",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5392,
        "text": "Pozivaće onog ko je bježao i okretao se,",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5393,
        "text": "I skupljao pa zadržavao.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5394,
        "text": "Uistinu, čovjek je stvoren nestrpljivim,",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5395,
        "text": "Kad ga takne zlo, razdražljiv je,",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5396,
        "text": "A kad ga takne dobro, odbojan je,",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5397,
        "text": "Osim musallija,",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5398,
        "text": "Onih koji su u salatu svom stalni,",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5399,
        "text": "I onih u imecima čijim je pravo poznato,",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5400,
        "text": "Za prosjaka i lišenog,",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5401,
        "text": "I onih koji vjeruju u Dan sudnji,",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5402,
        "text": "I onih koji od kazne Gospodara svog strahuju -",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5403,
        "text": "Uistinu! Kazna Gospodara njihovog je (stvar) od koje nema sigurnog -",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5404,
        "text": "I onih koji su ferdžova svojih čuvari,",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5405,
        "text": "Izuzev od žena svojih ili onog šta posjeduju desnice njihove - pa uistinu, oni neće biti prekoreni;",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5406,
        "text": "Pa ko traži iza toga, pa ti takvi su prestupnici;",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5407,
        "text": "I onih koji su prema emanetima svojim i zavjetu svom čuvari,",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5408,
        "text": "I onih koji uz svjedočenja svoja stoje,",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5409,
        "text": "I onih koji salat svoj čuvaju -",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5410,
        "text": "Takvi će u baščama biti počašćeni.",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5411,
        "text": "Pa šta je onima koji ne vjeruju? - prema tebi žure,",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5412,
        "text": "Zdesna i slijeva u grupama.",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5413,
        "text": "Želi li svaki čovjek od njih da mu bude dato da uđe u Džennet zadovoljstva?",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5414,
        "text": "Nikako! Uistinu, Mi smo ih stvorili, od čega - znaju.",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5415,
        "text": "Pa ne! Kunem se Gospodarom istoka i zapada, uistinu, Mi smo kadri,",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5416,
        "text": "Da (ih) zamijenimo boljim od njih, i nećemo Mi biti pretečeni.",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5417,
        "text": "Zato ih pusti (da) se udube i zabavljaju, dok ne sretnu dan svoj kojim im se prijeti.",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5418,
        "text": "Na Dan kad iz kabura izađu žurno, kao da oni statui hrle,",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5419,
        "text": "Oborenih pogleda svojih, prekrivaće ih poniženje. To će biti Dan kojim im se prijeti.",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "bs.mlivo",
      "language": "bs",
      "name": "Mlivo",
      "englishName": "Mustafa Mlivo",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}