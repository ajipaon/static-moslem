  "data": {
    "number": 98,
    "name": "سُورَةُ البَيِّنَةِ",
    "englishName": "Al-Bayyina",
    "englishNameTranslation": "The Evidence",
    "revelationType": "Medinan",
    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6131,
        "text": "Nisu se oni koji ne vjeruju od pripadnika Knjige i mušrici razdvojili, dok im nije došao jasan dokaz:",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 598,
        "ruku": 540,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6132,
        "text": "Poslanik od Allaha, recitira listove čiste,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 598,
        "ruku": 540,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6133,
        "text": "U njima knjige ispravne.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 598,
        "ruku": 540,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6134,
        "text": "Razdijelili su se oni kojima je data Knjiga tek pošto im je došao jasan dokaz,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 598,
        "ruku": 540,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6135,
        "text": "A naređeno im je da obožavaju jedino Allaha, odani Njegovoj vjeri, hanife da budu i obavljaju salat i daju zekat, a to je vjera prava.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 598,
        "ruku": 540,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6136,
        "text": "Uistinu! Oni od sljedbenika Knjige i mušrici koji ne vjeruju biće u vatri Džehennema, vječno u njoj. Ti takvi su najgora stvorenja.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 598,
        "ruku": 540,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6137,
        "text": "Uistinu! Oni koji vjeruju i čine dobra djela - ti takvi su najbolja stvorenja.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 598,
        "ruku": 540,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6138,
        "text": "Plaća njihova kod Gospodara njihovog je: bašče Adna ispod kojih teku rijeke, vječno će biti u njima, zauvijek. Zadovoljan će Allah biti sa njima, i zadovoljni Njime. To će biti za onog ko se boji Gospodara svog.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 540,
        "hizbQuarter": 239,
        "sajda": false
      }
    ],
