{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 37,
    "name": "سُورَةُ الصَّافَّاتِ",
    "englishName": "As-Saaffaat",
    "englishNameTranslation": "Those drawn up in Ranks",
    "revelationType": "Meccan",
    "numberOfAyahs": 182,
    "ayahs": [
      {
        "number": 3789,
        "text": "Tako mi u saffove poredanih,",
        "numberInSurah": 1,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3790,
        "text": "Te onih koji gone ukorom,",
        "numberInSurah": 2,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3791,
        "text": "Te onih koji recituju Opomenu,",
        "numberInSurah": 3,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3792,
        "text": "Uistinu! Bog vaš je sigurno Jedini,",
        "numberInSurah": 4,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3793,
        "text": "Gospodar nebesa i Zemlje i onog između njih, i Gospodar istoka.",
        "numberInSurah": 5,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3794,
        "text": "Uistinu! Mi smo najniže nebo ukrasili ukrasom - planetama,",
        "numberInSurah": 6,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3795,
        "text": "I zaštitom od svakog šejtana zadrtog.",
        "numberInSurah": 7,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3796,
        "text": "Ne čuju Najviši skup, a bivaju gađani sa svake strane,",
        "numberInSurah": 8,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3797,
        "text": "Prognani, a njihova kazna je trajna.",
        "numberInSurah": 9,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3798,
        "text": "Izuzev ko otrgne grabljenjem - tad ga slijedi svjetlica prodorna.",
        "numberInSurah": 10,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3799,
        "text": "Pa upitaj njih: \"Da li su oni jače stvorenje ili onaj koga smo stvorili?\" Uistinu! Mi smo ih stvorili od ilovače ljepljive.",
        "numberInSurah": 11,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3800,
        "text": "Naprotiv, zadivio si se, a rugaju se,",
        "numberInSurah": 12,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3801,
        "text": "I kad se opomenu, ne paze,",
        "numberInSurah": 13,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3802,
        "text": "A kad znak vide, rugaju se,",
        "numberInSurah": 14,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3803,
        "text": "I govore: \"Ovo je samo sihr očiti!",
        "numberInSurah": 15,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3804,
        "text": "Hoćemo li kad umremo i budemo prah i kosti, uistinu mi biti podignuti,",
        "numberInSurah": 16,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3805,
        "text": "Ili očevi naši raniji?\"",
        "numberInSurah": 17,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3806,
        "text": "Reci: \"Da, i vi ćete biti poniženi.\"",
        "numberInSurah": 18,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3807,
        "text": "Pa to će biti samo povik jedan, tad gle! - oni će gledati,",
        "numberInSurah": 19,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3808,
        "text": "I reći: \"O teško nama! Ovo je Dan sudnji!\"",
        "numberInSurah": 20,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3809,
        "text": "\"Ovo je Dan odluke, onaj kojeg ste poricali.\"",
        "numberInSurah": 21,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3810,
        "text": "Sakupite one koji su činili zulm i drugare njihove, i ono šta su obožavali,",
        "numberInSurah": 22,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3811,
        "text": "Mimo Allaha, pa ih povedite na put džehimu,",
        "numberInSurah": 23,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3812,
        "text": "I zaustavite ih, uistinu, oni će biti pitani.",
        "numberInSurah": 24,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3813,
        "text": "\"Šta vam je, ne pomažete se?\"",
        "numberInSurah": 25,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3814,
        "text": "Naprotiv, oni su Danas pokorni.",
        "numberInSurah": 26,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3815,
        "text": "I pristupaće oni jedni drugima - raspitivaće se,",
        "numberInSurah": 27,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3816,
        "text": "Govoriće: \"Uistinu, vi ste nam dolazili zdesna.\"",
        "numberInSurah": 28,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3817,
        "text": "Reći će: \"Naprotiv, niste bili vjernici,",
        "numberInSurah": 29,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3818,
        "text": "I nismo mi imali nad vama nikakve moći. Naprotiv! Bili ste narod pretjerujućih.",
        "numberInSurah": 30,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3819,
        "text": "Zato se na nama obistinila Riječ Gospodara našeg: uistinu, mi smo ti koji kušamo.",
        "numberInSurah": 31,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3820,
        "text": "Tad smo vas zaveli. Uistinu, mi smo bili zalutali.\"",
        "numberInSurah": 32,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3821,
        "text": "Pa uistinu, oni će u kazni Tog dana biti sudionici.",
        "numberInSurah": 33,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3822,
        "text": "Uistinu, Mi ćemo tako postupiti sa prestupnicima.",
        "numberInSurah": 34,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3823,
        "text": "Uistinu, oni su se - kad im je rečeno: \"Nema boga osim Allaha\" - uzoholili,",
        "numberInSurah": 35,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3824,
        "text": "I govorili: \"Zar da mi uistinu napustimo bogove naše radi pjesnika ludog?\"",
        "numberInSurah": 36,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3825,
        "text": "Naprotiv, došao je s Istinom i potvrdio izaslanike.",
        "numberInSurah": 37,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3826,
        "text": "Uistinu, vi ćete kušati kaznu bolnu -",
        "numberInSurah": 38,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3827,
        "text": "I bićete plaćeni samo kako ste radili -",
        "numberInSurah": 39,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3828,
        "text": "Izuzev robova Allahovih čistih,",
        "numberInSurah": 40,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3829,
        "text": "Ti takvi će imati opskrbu poznatu:",
        "numberInSurah": 41,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3830,
        "text": "Voće; i oni će biti počašćeni,",
        "numberInSurah": 42,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3831,
        "text": "U baščama uživanja,",
        "numberInSurah": 43,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3832,
        "text": "Na sofama će naspramni biti,",
        "numberInSurah": 44,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3833,
        "text": "Obilaziće se oko njih sa čašom izvora,",
        "numberInSurah": 45,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3834,
        "text": "Bijelog, prijatnog za pijače.",
        "numberInSurah": 46,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3835,
        "text": "Neće biti u njemu glavobolje, niti će se oni od njega opiti.",
        "numberInSurah": 47,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3836,
        "text": "A kod njih će biti (hurije) kratkih pogleda, krupnooke,",
        "numberInSurah": 48,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3837,
        "text": "Kao da su one jaja skrivena.",
        "numberInSurah": 49,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3838,
        "text": "Tad će pristupati oni jedni drugima, raspitivaće se.",
        "numberInSurah": 50,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3839,
        "text": "Reći će govornik između njih: \"Uistinu, ja sam imao druga,",
        "numberInSurah": 51,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3840,
        "text": "Govorio je: 'Jesi li uistinu ti od onih koji potvrđuju?",
        "numberInSurah": 52,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3841,
        "text": "Zar ćemo kad umremo i budemo prah i kosti, uistinu mi polagati račun?\"",
        "numberInSurah": 53,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3842,
        "text": "Reći će: \"Da li biste vi pogledali?\"",
        "numberInSurah": 54,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3843,
        "text": "Tad će pogledati pa će ga vidjeti usred džehima.",
        "numberInSurah": 55,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3844,
        "text": "Reći će: \"Tako mi Allaha, zamalo da me upropastiš.",
        "numberInSurah": 56,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3845,
        "text": "I da nije bilo blagodati Gospodara mog, sigurno bih bio od privedenih.\"",
        "numberInSurah": 57,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3846,
        "text": "\"Pa zar nećemo mi biti mejjiti,",
        "numberInSurah": 58,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3847,
        "text": "Izuzev smrti naše ranije, i nećemo mi biti mučeni?\"",
        "numberInSurah": 59,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3848,
        "text": "Uistinu, ovo je to, uspjeh veličanstven.",
        "numberInSurah": 60,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3849,
        "text": "Za slično ovome, ta neka rade radnici!",
        "numberInSurah": 61,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3850,
        "text": "Da li je to bolje ugošćenje ili drvo zekkum?",
        "numberInSurah": 62,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3851,
        "text": "Uistinu, Mi ćemo ga učiniti iskušenjem za zalime.",
        "numberInSurah": 63,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3852,
        "text": "Uistinu, to drvo izlazi iz dna džehima,",
        "numberInSurah": 64,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3853,
        "text": "Plod njegov - kao da su to glave šejtana.",
        "numberInSurah": 65,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3854,
        "text": "Pa uistinu, oni će jesti od njega te puniti njime trbuhe.",
        "numberInSurah": 66,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3855,
        "text": "Zatim će oni uistinu imati na njega mješavinu ključale vode,",
        "numberInSurah": 67,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3856,
        "text": "Potom je uistinu povratište njihovo džehimu.",
        "numberInSurah": 68,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3857,
        "text": "Uistinu, oni su našli očeve svoje zalutale,",
        "numberInSurah": 69,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3858,
        "text": "Pa oni tragovima njihovim hite.",
        "numberInSurah": 70,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3859,
        "text": "A doista je prije njih zalutala većina prijašnjih,",
        "numberInSurah": 71,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3860,
        "text": "I doista smo slali među njih opominjače.",
        "numberInSurah": 72,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3861,
        "text": "Zato pogledaj kakav je bio kraj opominjanih,",
        "numberInSurah": 73,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3862,
        "text": "Izuzev robova Allahovih čistih.",
        "numberInSurah": 74,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3863,
        "text": "A doista Nas je zvao Nuh, pa divni su Oni koji su se odazvali.",
        "numberInSurah": 75,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3864,
        "text": "I spasili smo njega i porodicu njegovu od žala strašnog,",
        "numberInSurah": 76,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3865,
        "text": "I učinili potomstvo njegovo - njih preživjelima,",
        "numberInSurah": 77,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3866,
        "text": "I ostavili nad njim u kasnijim (pokoljenjima):",
        "numberInSurah": 78,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3867,
        "text": "\"Selam Nuhu u svjetovima!\"",
        "numberInSurah": 79,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3868,
        "text": "Uistinu, Mi tako nagrađujemo dobročinitelje!",
        "numberInSurah": 80,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3869,
        "text": "Uistinu, on je bio od robova Naših vjernika,",
        "numberInSurah": 81,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3870,
        "text": "Zatim smo potopili druge.",
        "numberInSurah": 82,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3871,
        "text": "A uistinu, od stranke njegove je Ibrahim.",
        "numberInSurah": 83,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3872,
        "text": "Kad dođe Gospodaru svom sa srcem zdravim,",
        "numberInSurah": 84,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3873,
        "text": "Kad reče ocu svom i narodu svom: \"Šta to obožavate?",
        "numberInSurah": 85,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3874,
        "text": "Zar lažne bogove mimo Allaha želite?",
        "numberInSurah": 86,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3875,
        "text": "Pa šta je vaše mišljenje o Gospodaru svjetova?\"",
        "numberInSurah": 87,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3876,
        "text": "Tad osmotri pogledom zvijezde,",
        "numberInSurah": 88,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3877,
        "text": "Pa reče: \"Uistinu, ja sam bolestan!\"",
        "numberInSurah": 89,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3878,
        "text": "Tad se okrenuše od njega odlazeći.",
        "numberInSurah": 90,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3879,
        "text": "Pa se prikrade bogovima njihovim, te reče: \"Zar ne jedete?,",
        "numberInSurah": 91,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3880,
        "text": "Šta vam je, ne govorite?\"",
        "numberInSurah": 92,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3881,
        "text": "Pa im se prikrade udarajući desnicom.",
        "numberInSurah": 93,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3882,
        "text": "Tad mu se približiše, žureći.",
        "numberInSurah": 94,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3883,
        "text": "Reče: \"Zar obožavate ono što klešete,",
        "numberInSurah": 95,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3884,
        "text": "A Allah je stvorio vas i ono što pravite?\"",
        "numberInSurah": 96,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3885,
        "text": "Rekoše: \"Sagradite mu građevinu, pa ga bacite u vatru.\"",
        "numberInSurah": 97,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3886,
        "text": "Tad mu zaželiše spletku, pa njih učinismo donjima.",
        "numberInSurah": 98,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3887,
        "text": "I reče: \"Uistinu, ja sam putnik Gospodaru svom, uputiće me.",
        "numberInSurah": 99,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3888,
        "text": "Gospodaru moj! Daruj mi (potomka) od dobrih.\"",
        "numberInSurah": 100,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3889,
        "text": "Pa smo ga obradovali dječakom blagim.",
        "numberInSurah": 101,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3890,
        "text": "Pa pošto je dostigao s njim (dobu) privređivanja, reče: \"O sinčiću moj! Uistinu sam ja u snu vidio da te ja koljem, pa gledaj, šta vidiš?\" Reče: \"O oče moj! Učini šta ti se naređuje. Naći ćeš me - ako htjedne Allah - (jednim) od strpljivih.\"",
        "numberInSurah": 102,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3891,
        "text": "Pa pošto se oba pokoriše, i položi ga na čelo,",
        "numberInSurah": 103,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3892,
        "text": "I pozvasmo ga: \"O Ibrahime!",
        "numberInSurah": 104,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3893,
        "text": "Već si ispunio snoviđenje. Uistinu, mi tako nagrađujemo dobročinitelje.",
        "numberInSurah": 105,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3894,
        "text": "Uistinu, ovo je to, kušnja očita.\"",
        "numberInSurah": 106,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3895,
        "text": "I iskupismo ga žrtvom golemom,",
        "numberInSurah": 107,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3896,
        "text": "I ostavismo mu među kasnijima:",
        "numberInSurah": 108,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3897,
        "text": "\"Selam Ibrahimu!\"",
        "numberInSurah": 109,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3898,
        "text": "Tako nagrađujemo dobročinitelje!",
        "numberInSurah": 110,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3899,
        "text": "Uistinu, on je bio od robova Naših vjernika,",
        "numberInSurah": 111,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3900,
        "text": "A obradovali smo ga Ishakom, vjerovjesnikom, od pravednih,",
        "numberInSurah": 112,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3901,
        "text": "I blagoslovili njega i Ishaka; a od potomstva njih dvojice je dobročinitelj i zalim duši svojoj, očiti;",
        "numberInSurah": 113,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3902,
        "text": "I doista smo obdarili Musaa i Haruna,",
        "numberInSurah": 114,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3903,
        "text": "I spasili njih dvojicu i narod njihov od žala strašnog,",
        "numberInSurah": 115,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3904,
        "text": "I pomogli ih, pa su oni bili pobjednici;",
        "numberInSurah": 116,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3905,
        "text": "I dali smo njima dvojici Knjigu jasnih (stvari),",
        "numberInSurah": 117,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3906,
        "text": "I uputili ih putem pravim,",
        "numberInSurah": 118,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3907,
        "text": "I ostavili njima dvojici među kasnijima:",
        "numberInSurah": 119,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3908,
        "text": "\"Selam Musau i Harunu!\"",
        "numberInSurah": 120,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3909,
        "text": "Uistinu, Mi tako nagrađujemo dobročinitelje!",
        "numberInSurah": 121,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3910,
        "text": "Uistinu, njih dvojica su bili od robova Naših vjernika.",
        "numberInSurah": 122,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3911,
        "text": "I uistinu, Il'jas je bio od izaslanika.",
        "numberInSurah": 123,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3912,
        "text": "Kad reče narodu svom: \"Zar se ne bojite?",
        "numberInSurah": 124,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3913,
        "text": "Zar prizivate Ba'la, a ostavljate Najboljeg od stvoritelja,",
        "numberInSurah": 125,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3914,
        "text": "Allaha, Gospodara vašeg i Gospodara očeva vaših ranijih?\"",
        "numberInSurah": 126,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3915,
        "text": "Pa su ga porekli, pa uistinu će oni biti privedeni,",
        "numberInSurah": 127,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3916,
        "text": "Izuzev robova Allahovih čistih.",
        "numberInSurah": 128,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3917,
        "text": "I ostavili smo njemu među kasnijima:",
        "numberInSurah": 129,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3918,
        "text": "\"Selam Il'jasu!\"",
        "numberInSurah": 130,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3919,
        "text": "Uistinu, Mi tako nagrađujemo dobročinitelje!",
        "numberInSurah": 131,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3920,
        "text": "Uistinu, on je bio od robova Naših vjernika.",
        "numberInSurah": 132,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3921,
        "text": "I uistinu, Lut je bio od izaslanika.",
        "numberInSurah": 133,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3922,
        "text": "Kad spasismo njega i porodicu njegovu - sve,",
        "numberInSurah": 134,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3923,
        "text": "Izuzev starice među izostalima,",
        "numberInSurah": 135,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3924,
        "text": "Zatim smo uništili druge.",
        "numberInSurah": 136,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3925,
        "text": "A uistinu, vi prolazite pored njih jutrom,",
        "numberInSurah": 137,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3926,
        "text": "I noću; pa zar ne shvatate?",
        "numberInSurah": 138,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3927,
        "text": "I uistinu, Junus je bio od izaslanika;",
        "numberInSurah": 139,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3928,
        "text": "Kad pobježe k lađi natovarenoj,",
        "numberInSurah": 140,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3929,
        "text": "Pa je bacio kocku, te bio od odbačenih,",
        "numberInSurah": 141,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3930,
        "text": "Tad ga je progutala (velika) riba, i on je prijekora vrijedan:",
        "numberInSurah": 142,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3931,
        "text": "Pa da on nije bio od onih koji slave (Allaha),",
        "numberInSurah": 143,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3932,
        "text": "Sigurno bi ostao u njenoj utrobi do Dana (kad) će biti podignuti.",
        "numberInSurah": 144,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3933,
        "text": "Pa smo ga izbacili na pusto mjesto, a on je bio bolestan,",
        "numberInSurah": 145,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3934,
        "text": "I dali smo da nad njim izraste žbun tikve;",
        "numberInSurah": 146,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3935,
        "text": "I poslali ga ka stotini hiljada ili više,",
        "numberInSurah": 147,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3936,
        "text": "Pa su vjerovali, te smo im dali uživanje do izvjesnog vremena.",
        "numberInSurah": 148,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3937,
        "text": "Zato ih upitaj: \"Jesu li Gospodara tvog kćeri, a njihovi sinovi?\"",
        "numberInSurah": 149,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3938,
        "text": "Ili smo stvarali meleke ženskima, a oni svjedoci bili?",
        "numberInSurah": 150,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3939,
        "text": "Besumnje! Uistinu oni zbog laži svoje govore:",
        "numberInSurah": 151,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3940,
        "text": "\"Rodio je Allah\", a uistinu, oni su lažljivci.",
        "numberInSurah": 152,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3941,
        "text": "Zar je odabrao kćeri nad sinovima?",
        "numberInSurah": 153,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3942,
        "text": "Šta vam je? Kako sudite?",
        "numberInSurah": 154,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3943,
        "text": "Pa zar se nećete poučiti?",
        "numberInSurah": 155,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3944,
        "text": "Ili vi imate dokaz očit?",
        "numberInSurah": 156,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3945,
        "text": "Pa donesite knjigu svoju, ako istinu govorite!",
        "numberInSurah": 157,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3946,
        "text": "I uspostavljaju između Njega i džinna srodstvo, a doista džinni znaju da će oni uistinu biti privedeni -",
        "numberInSurah": 158,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3947,
        "text": "Slavljen neka je Allah od onog šta pripisuju -",
        "numberInSurah": 159,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3948,
        "text": "Izuzev robova Allahovih čistih.",
        "numberInSurah": 160,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3949,
        "text": "Pa uistinu, vi i ono šta obožavate,",
        "numberInSurah": 161,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3950,
        "text": "Niste vi protiv Njega zavodnici,",
        "numberInSurah": 162,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3951,
        "text": "Izuzev onog koji će biti pržen džehimom.",
        "numberInSurah": 163,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3952,
        "text": "A nema od nas nijednog, a da njegovo mjesto nije poznato,",
        "numberInSurah": 164,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3953,
        "text": "I uistinu! Mi, Mi smo poredani,",
        "numberInSurah": 165,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3954,
        "text": "I uistinu, Mi, Mi smo slavitelji.",
        "numberInSurah": 166,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3955,
        "text": "A oni su sigurno govorili:",
        "numberInSurah": 167,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3956,
        "text": "\"Da je kod nas Opomena od ranijih,",
        "numberInSurah": 168,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3957,
        "text": "Sigurno bismo bili robovi Allahovi čisti.\"",
        "numberInSurah": 169,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3958,
        "text": "Pa nisu u njega vjerovali, pa saznaće!",
        "numberInSurah": 170,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3959,
        "text": "A već je prethodila Riječ Naša robovima Našim poslanim.",
        "numberInSurah": 171,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3960,
        "text": "Uistinu, oni - oni će biti pomognuti,",
        "numberInSurah": 172,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3961,
        "text": "I uistinu, vojska Naša - oni će biti pobjednici.",
        "numberInSurah": 173,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3962,
        "text": "Zato se okreni od njih izvjesno vrijeme,",
        "numberInSurah": 174,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3963,
        "text": "I posmatraj ih, pa vidjeće.",
        "numberInSurah": 175,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3964,
        "text": "Pa zar kaznu Našu požuruju?",
        "numberInSurah": 176,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3965,
        "text": "Pa kad se spusti u avliju njihovu, tad će biti zlo jutro upozoravanih.",
        "numberInSurah": 177,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3966,
        "text": "I okreni se od njih izvjesno vrijeme,",
        "numberInSurah": 178,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3967,
        "text": "I posmatraj, pa vidjet će!",
        "numberInSurah": 179,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3968,
        "text": "Slavljen neka je Gospodar tvoj, Gospodar moći, od onog šta pripisuju!",
        "numberInSurah": 180,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3969,
        "text": "I selam izaslanicima,",
        "numberInSurah": 181,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3970,
        "text": "I hvala Allahu, Gospodaru svjetova!",
        "numberInSurah": 182,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      }
    ],
