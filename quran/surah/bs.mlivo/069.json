{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 69,
    "name": "سُورَةُ الحَاقَّةِ",
    "englishName": "Al-Haaqqa",
    "englishNameTranslation": "The Reality",
    "revelationType": "Meccan",
    "numberOfAyahs": 52,
    "ayahs": [
      {
        "number": 5324,
        "text": "Istinitost!",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5325,
        "text": "Šta je istinitost?",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5326,
        "text": "I šta znaš ti šta je istinitost!",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5327,
        "text": "Poricao je Semud i Ad udar.",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5328,
        "text": "Pa što se tiče Semuda, pa uništeni su tagijetom,",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5329,
        "text": "A što se tiče Ada, pa uništeni su vjetrom žestokim, silovitim,",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5330,
        "text": "Nametnuo ga je na njih sedam noći i osam dana uzastopno, pa si vidio ljude u njemu oborene, kao da su oni debla datulina prazna.",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5331,
        "text": "Pa da li vidiš od njih ikakav ostatak?",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5332,
        "text": "I došao je faraon i onaj ko je bio prije njega, i Mu'tefikat sa greškom.",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5333,
        "text": "Pa nisu poslušali poslanika Gospodara svog, pa ih je dograbio grabljenjem žestokim.",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5334,
        "text": "Uistinu, Mi smo - pošto je porasla voda - nosili vas u lađi,",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5335,
        "text": "Da bismo vam to učinili podsjećanjem i (da) to zadrži uho koje pamti.",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5336,
        "text": "Pa kad se puhne u sur, puhanjem jednim:",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5337,
        "text": "I ponese se Zemlja i planine, pa se zdrobe drobljenjem jednim,",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5338,
        "text": "Tad, Tog dana, dogodiće se događaj,",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5339,
        "text": "I raspuknuće nebo, pa će ono Tog dana krhko biti:",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5340,
        "text": "I meleci će na krajevima njegovim biti, a nosiće Arš Gospodara tvoga nad njima Tog dana osmerica.",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5341,
        "text": "Tog dana bićete izloženi; neće se sakriti vaša tajna.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5342,
        "text": "Pa kome se da knjiga njegova u desnicu njegovu, tad će reći: \"Evo vam, čitajte knjigu moju!",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5343,
        "text": "Uistinu, ja sam znao da ću ja sresti obračun svoj.\"",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5344,
        "text": "Pa on će biti u životu zadovoljstva,",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5345,
        "text": "U Džennetu visokom,",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5346,
        "text": "Plodovi njegovi biće blizu.",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5347,
        "text": "\"Jedite i pijte prijatno, zbog onog što ste činili u danima prošlim.\"",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5348,
        "text": "A kome se da knjiga njegova u ljevicu njegovu, tad će reći: \"O da mi nije data knjiga moja,",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5349,
        "text": "I da nisam saznao šta je obračun moj!",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5350,
        "text": "O da je ona bila kraj,",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5351,
        "text": "Nije mi koristilo bogatstvo moje,",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5352,
        "text": "Propala je moć moja.\"",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5353,
        "text": "\"Ščepajte ga, pa ga okujte,",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5354,
        "text": "Zatim džehimom pržite ga,",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5355,
        "text": "Potom (stavite) u lanac čija je dužina sedamdeset lakata, pa ga uvežite.\"",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5356,
        "text": "Uistinu, on nije vjerovao u Allaha Veličanstvenog,",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5357,
        "text": "Niti podsticao hranjenju siromaha.",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5358,
        "text": "Zato on Danas ovdje nema prisnog prijatelja,",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5359,
        "text": "Niti hrane, izuzev od splačina,",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5360,
        "text": "Ješće je samo oni koji su činili greške!",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5361,
        "text": "Pa ne! Kunem se onim što vidite,",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5362,
        "text": "I onim što ne vidite,",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5363,
        "text": "Uistinu je on govor Poslanika plemenitog,",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5364,
        "text": "I nije on govor pjesnika; malo što vjerujete!",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5365,
        "text": "Niti je govor proroka, malo čega se sjećate!",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5366,
        "text": "Objava je od Gospodara svjetova.",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5367,
        "text": "A da je slagao na Nas neke riječi,",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5368,
        "text": "Doista bismo ga zgrabili za desnicu,",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5369,
        "text": "Potom doista presjekli njegovu aortu,",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5370,
        "text": "Pa ne biste bili njegovi branitelji, nijedan od vas.",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5371,
        "text": "I uistinu, on je bogobojaznima Opomena,",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5372,
        "text": "A uistinu, Mi znamo da su od vas poricatelji.",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5373,
        "text": "I uistinu, on je žalost za nevjernike,",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5374,
        "text": "I uistinu, on je Istina sigurna.",
        "numberInSurah": 51,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5375,
        "text": "Zato slavi ime Gospodara svog, Veličanstvenog!",
        "numberInSurah": 52,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      }
    ],
