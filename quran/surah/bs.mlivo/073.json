  "data": {
    "number": 73,
    "name": "سُورَةُ المُزَّمِّلِ",
    "englishName": "Al-Muzzammil",
    "englishNameTranslation": "The Enshrouded One",
    "revelationType": "Meccan",
    "numberOfAyahs": 20,
    "ayahs": [
      {
        "number": 5476,
        "text": "O umotani!",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5477,
        "text": "Ustaj noću, izuzev malo,",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5478,
        "text": "Polovinu njenu, ili umanji od nje malo,",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5479,
        "text": "Ili joj dodaj, i uči Kur'an tertilom.",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5480,
        "text": "Uistinu, Mi ćemo ti objavljivati govor težak.",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5481,
        "text": "Uistinu, podizanje noću - ono je jačeg gaza i ispravnijeg izgovora.",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5482,
        "text": "Uistinu, tebi je danju posao dug.",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5483,
        "text": "I spominji ime Gospodara svog i posveti Mu se (punom) posvećenošću.",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5484,
        "text": "Gospodar Istoka i Zapada! Nema boga osim Njega, pa uzmi Njega zaštitnikom.",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5485,
        "text": "I otrpi šta govore i napusti ih napuštanjem lijepim,",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5486,
        "text": "A ostavi Mene i poricatelje, posjednike raskoši, i odgodi im malo.",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5487,
        "text": "Uistinu, kod Nas su okovi i džehim,",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5488,
        "text": "I hrana koja guši i kazna bolna:",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5489,
        "text": "Na Dan kad se zatrese Zemlja i planine, i budu planine pješčana hrpa sasuta.",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5490,
        "text": "Uistinu, Mi smo vam poslali Poslanika, svjedokom protiv vas, kao što smo faraonu poslali poslanika.",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5491,
        "text": "Pa nije faraon poslušao poslanika, te smo ga zgrabili grabljenjem kobnim.",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5492,
        "text": "Pa kako ćete se zaštititi, ako ne vjerujete, Dana (koji) će učiniti djecu sijedom?",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5493,
        "text": "Nebo će tad biti rascijepljeno. Obećanje Njegovo biće ispunjeno.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5494,
        "text": "Uistinu! Ovo je Opomena, pa ko htjedne, uzeće Gospodaru svom put.",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5495,
        "text": "Uistinu! Gospodar tvoj zna da ti bdiješ blizu dvije trećine noći i polovinu njenu i trećinu njenu, i grupa onih koji su s tobom; a Allah odmjerava noć i dan. Znao je da to ne možete, pa vam je oprostio. Zato učite šta je lahko iz Kur'ana. Znao je da će među vama biti bolesnih i drugih: putuju po zemlji, traže od blagodati Allahove, i drugih: bore se na putu Allahovom. Pa učite šta je lahko iz njega! I obavljajte salat i dajite zekat i uzajmite Allahu zajam lijep; a šta unaprijed pošaljete za duše svoje od dobrog, naći ćete ga kod Allaha; to je najbolje i najveličanstvenije nagrade! I tražite oprost od Allaha! Uistinu, Allah je Oprosnik, Milosrdni.",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 509,
        "hizbQuarter": 230,
        "sajda": false
      }
    ],
