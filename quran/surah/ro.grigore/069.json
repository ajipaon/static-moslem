{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 69,
    "name": "سُورَةُ الحَاقَّةِ",
    "englishName": "Al-Haaqqa",
    "englishNameTranslation": "The Reality",
    "revelationType": "Meccan",
    "numberOfAyahs": 52,
    "ayahs": [
      {
        "number": 5324,
        "text": "Cea care înconjoară!",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5325,
        "text": "Ce este cea care înconjoară?",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5326,
        "text": "Cum vei şti tu oare ce este cea care înconjoară?",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5327,
        "text": "Tamudiţii şi adiţii au socotit-o pe cea care bate minciună!",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5328,
        "text": "Tamudiţii pieriră în tunete şi fulgere.",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5329,
        "text": "Adiţii pieriră în vântul şuierător, năpraznic,",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5330,
        "text": "pe care l-am dezlănţuit asupra lor şapte nopţi şi opt zile de prăpăd. Să fi văzut poporul acela trântit ca nişte găunoase buturugi de curmal!",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5331,
        "text": "Vezi, cumva, c-ar fi rămas ceva după ei?",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 566,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5332,
        "text": "Faraon şi cei care au trăit înaintea lui, precum şi cetăţile cele de-a-ndoaselea, au adus păcatul.",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5333,
        "text": "Ei nu au dat ascultare trimisului Domnului lor şi Dumnezeu i-a luat fără zăbavă.",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5334,
        "text": "Când apele s-au revărsat, v-am luat în arcă,",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5335,
        "text": "ca să vă facem vouă din aceasta o amintire pe care urechea păstrătoare să o păstreze.",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5336,
        "text": "Când se va suna din trâmbiţă o dată,",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5337,
        "text": "când pământul şi munţii vor fi căraţi şi fărâmiţaţi deodată,",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5338,
        "text": "cea care cade neapărat în Ziua aceea va cădea,",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5339,
        "text": "iar cerul se va crăpa şi se va despica în Ziua aceea.",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5340,
        "text": "Îngerii se vor ţine de marginile lui, iar opt dintre ei vor purta Tronul Domnului în Ziua aceea.",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5341,
        "text": "În Ziua aceea veţi fi întorşi pe toate părţile şi nici o taină de-a voastră nu va rămâne ascunsă.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5342,
        "text": "Celui ce i se va da cartea sa în mâna dreaptă va spune: “Iată, citiţi cartea mea!",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5343,
        "text": "Îmi închipui că îmi voi găsi socoteala!”",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5344,
        "text": "El va avea un trai plăcut",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5345,
        "text": "într-o Grădină prea-înaltă,",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5346,
        "text": "cu roade de cules la îndemână.",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5347,
        "text": "“Mâncaţi şi beţi în pace ca răsplată a ceea ce aţi săvârşit în zilele duse.”",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5348,
        "text": "Celui ce i se va da cartea sa în mâna stângă va spune: “Vai mie! Ce bine ar fi fost de nu mi s-ar fi dat cartea",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5349,
        "text": "şi nu mi-aş fi ştiut socoteala!",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5350,
        "text": "Vai! Ce bine era de-ar fi fost săvârşitoare (moartea)!",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5351,
        "text": "Averea la nimic nu mi-a slujit",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5352,
        "text": "şi puterea mi-a pierit!”",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5353,
        "text": "“Luaţi-l şi înjugaţi-l!",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5354,
        "text": "Aruncaţi-l în Iad!",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5355,
        "text": "Legaţi-l apoi cu un lanţ de şaptezeci de coţi.”",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5356,
        "text": "El nu credea în Dumnezeu cel Mare",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5357,
        "text": "şi nu îndemna pe nimeni să ospăteze sărmanul.",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 567,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5358,
        "text": "Astăzi, nu mai are nici un prieten apropiat,",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5359,
        "text": "nici mâncare afară de ghislin",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5360,
        "text": "pe care o mănâncă numai păcătoşii.",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 500,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5361,
        "text": "Ba nu! Jur pe ceea ce vedeţi",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5362,
        "text": "şi pe ceea ce nu vedeţi!",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5363,
        "text": "Acestea sunt spusele unui trimis cinstit,",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5364,
        "text": "şi nu sunt spusele unui poet — atât de puţin credeţi!",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5365,
        "text": "şi nici ale unui prezicător — atât de puţin cugetaţi!",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5366,
        "text": "Ci este o Pogorâre de la Domnul lumilor!",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5367,
        "text": "Dacă acesta ar fi făcut născociri asupra Noastră,",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5368,
        "text": "l-am fi apucat cu mâna dreaptă,",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5369,
        "text": "apoi i-am fi tăiat vena gâtului",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5370,
        "text": "şi nici unul dintre voi nu ne-ar fi putut sta în cale!",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5371,
        "text": "Aceasta este drept amintire celor temători,",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5372,
        "text": "însă Noi ştim că unii dintre voi o socotesc minciună!",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5373,
        "text": "Ea este tânguire celor tăgăduitori!",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5374,
        "text": "Acesta este Adevărul cel adevărat!",
        "numberInSurah": 51,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5375,
        "text": "Preamăreşte Numele Domnului tău cel Mare!",
        "numberInSurah": 52,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 501,
        "hizbQuarter": 227,
        "sajda": false
      }
    ],
