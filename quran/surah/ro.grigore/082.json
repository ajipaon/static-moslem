        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5847,
        "text": "Şi încă o dată, cum vei şti tu oare când va fi Ziua Judecăţii de Apoi?",
