{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 70,
    "name": "سُورَةُ المَعَارِجِ",
    "englishName": "Al-Ma'aarij",
    "englishNameTranslation": "The Ascending Stairways",
    "revelationType": "Meccan",
    "numberOfAyahs": 44,
    "ayahs": [
      {
        "number": 5376,
        "text": "Unul a cerut o osândă ce va să cadă",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5377,
        "text": "asupra tăgăduitorilor şi nimeni să nu-i poată sta în cale,",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5378,
        "text": "căci vine de la Dumnezeu, Stăpânul Treptelor",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5379,
        "text": "pe care îngerii şi Duhul le urcă către El într-o zi cât cincizeci de mii de ani.",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5380,
        "text": "Rabdă-i cu cuviincioasă răbdare.",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5381,
        "text": "Ei o văd departe,",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5382,
        "text": "însă Noi o vedem aproape.",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5383,
        "text": "Cerul, în Ziua aceea, va fi ca argintul topit,",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5384,
        "text": "iar munţii ca lâna scărmănată.",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5385,
        "text": "Nici un prieten nu va întreba de prietenul său apropiat,",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5386,
        "text": "chiar de le va fi dat să se vadă. Nelegiuitul ar dori în Ziua aceea să-şi ispăşească osânda prin fiii săi,",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5387,
        "text": "prin soţia sa, prin fratele său,",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5388,
        "text": "prin obştea sa care-l adăpostea",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5389,
        "text": "şi prin toţi de pe pământ, numai ca el să scape.",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5390,
        "text": "Iată însă văpaia,",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5391,
        "text": "jupuitoare de ţeste,",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5392,
        "text": "chemându-i pe cei care au dat înapoi şi au întors spatele,",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5393,
        "text": "pe cei care au strâns şi au grămădit.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5394,
        "text": "Omul a fost creat nestatornic:",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5395,
        "text": "când îl atinge răul, este neliniştit;",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5396,
        "text": "când îl atinge binele, este zgârcit",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5397,
        "text": "afară de cei rugători,",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5398,
        "text": "de cei care în rugăciune sunt stăruitori",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5399,
        "text": "şi de cei din ale căror averi au parte",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5400,
        "text": "creşetorul şi lipsitul,",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5401,
        "text": "afară de cei care socot adevărată Ziua Judecăţii",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5402,
        "text": "şi se tem de osânda Domnului lor",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5403,
        "text": "— osânda Domnului lor este fără scăpare,",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5404,
        "text": "de cei care îşi păzesc cele ruşinoase",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5405,
        "text": "şi intră numai la soţiile lor ori la roabele lor şi n-au de ce să fie dojeniţi,",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5406,
        "text": "pe când cei care poftesc dincolo de aceasta sunt călcători de lege,",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5407,
        "text": "afară de cei care păzesc cele încredinţate lor, precum şi legămintele lor;",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5408,
        "text": "de cei care sunt drepţi în mărturia lor,",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5409,
        "text": "de cei care se îngrijesc de rugăciunile lor.",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5410,
        "text": "Aceştia vor fi cei cinstiţi în Grădini!",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5411,
        "text": "De ce tăgăduitorii se grăbesc către tine,",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5412,
        "text": "venind în cete din dreapta şi din stânga?",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5413,
        "text": "Fiece ins dintre ei ar dori să intre într-o Grădină a Plăcerii!",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5414,
        "text": "Ba nu! Noi i-am creat din ceea ce ştiu.",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5415,
        "text": "Ba nu! Jur pe Domnul Răsăritului şi al Asfinţitului că Noi avem putinţa",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5416,
        "text": "să-i înlocuim oricând cu alţii mai buni decât ei şi nimeni nu Ne-o poate lua înainte.",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5417,
        "text": "Lasă-i să pălăvrăgească şi să se joace până când vor întâlni Ziua ce le-a fost făgăduită,",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5418,
        "text": "Ziua când vor ieşi din morminte în goană de parcă ar fugi spre pietrele înălţate,",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5419,
        "text": "cu ochii în pământ, prigoniţi de umilinţă. Aceasta este Ziua ce le-a fost făgăduită!",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      }
    ],
    "edition": {
