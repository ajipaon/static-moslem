  "data": {
    "number": 56,
    "name": "سُورَةُ الوَاقِعَةِ",
    "englishName": "Al-Waaqia",
    "englishNameTranslation": "The Inevitable",
    "revelationType": "Meccan",
    "numberOfAyahs": 96,
    "ayahs": [
      {
        "number": 4980,
        "text": "Kada se Događaj dogodi –",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4981,
        "text": "događanje njegovo niko neće poricati –",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4982,
        "text": "neke će poniziti, a neke uzvisiti;",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4983,
        "text": "kad se Zemlja jako potrese",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4984,
        "text": "i brda se u komadiće zdrobe,",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4985,
        "text": "i postanu prašina razasuta,",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4986,
        "text": "vas će tri vrste biti:",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4987,
        "text": "oni sretni – ko su sretni?!",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4988,
        "text": "i oni nesretni – ko su nesretni?!",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4989,
        "text": "i oni prvi – uvijek prvi!",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4990,
        "text": "Oni će Allahu bliski biti",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4991,
        "text": "u džennetskim baščama naslada –",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4992,
        "text": "biće ih mnogo od naroda drevnih,",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4993,
        "text": "a malo od kasnijih –",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4994,
        "text": "na divanima izvezenim,",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4995,
        "text": "jedni prema drugima na njima će naslonjeni biti;",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4996,
        "text": "služiće ih vječno mladi mladići,",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4997,
        "text": "sa čašama i ibricima i peharom punim pića iz izvora tekućeg –",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4998,
        "text": "od koga ih glava neće boljeti i zbog kojeg neće pamet izgubiti –",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4999,
        "text": "i voćem koje će sami birati,",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5000,
        "text": "i mesom ptičijim kakvo budu željeli.",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5001,
        "text": "U njima će biti i hurije očiju krupnih,",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5002,
        "text": "slične biseru u školjkama skrivenom –",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5003,
        "text": "kao nagrada za ono što su činili.",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5004,
        "text": "U njima neće slušati prazne besjede ni govor grješni,",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5005,
        "text": "nego samo riječi: \"Mir, mir!\"",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5006,
        "text": "A onî sretni – ko su sretni?!",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5007,
        "text": "Biće među lotosovim drvećem bez bodlji,",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5008,
        "text": "i među bananama plodovima nanizanim",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5009,
        "text": "i u hladovini prostranoj,",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5010,
        "text": "pored vode tekuće",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5011,
        "text": "i usred voća svakovrsnog",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5012,
        "text": "kojeg će uvijek imati i koje neće zabranjeno biti,",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5013,
        "text": "i na posteljama uzdignutim.",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5014,
        "text": "Stvaranjem novim Mi ćemo hurije stvoriti",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5015,
        "text": "i djevicama ih učiniti",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5016,
        "text": "milim muževima njihovim, i godina istih",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5017,
        "text": "za one sretne;",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5018,
        "text": "biće ih mnogo od naroda drevnih,",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5019,
        "text": "a mnogo i od kasnijih.",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5020,
        "text": "A onî nesretni – ko su nesretni?!",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5021,
        "text": "Oni će biti u vatri užarenoj i vodi ključaloj",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5022,
        "text": "i u sjeni dima čađavog,",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5023,
        "text": "u kojoj neće biti svježine ni ikakve dobrine.",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5024,
        "text": "Oni su prije toga raskošnim životom živjeli",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5025,
        "text": "i uporno teške grijehe činili",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5026,
        "text": "i govorili: \"Zar kada umremo i zemlja i kosti postanemo – zar ćemo zbilja biti oživljeni,",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5027,
        "text": "zar i drevni naši preci?\"",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5028,
        "text": "Reci: \"I drevni i kasniji,",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5029,
        "text": "u određeno vrijeme, jednog određenog dana biće sakupljeni,",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 535,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5030,
        "text": "i tada ćete vi, o zabludjeli, koji poričete oživljenje,",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5031,
        "text": "sigurno, s drveta Zekkum jesti,",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5032,
        "text": "i njime ćete trbuhe puniti,",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5033,
        "text": "pa zatim na to ključalu vodu piti,",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5034,
        "text": "poput kamila koje ne mogu žeđ ugasiti\";",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5035,
        "text": "to će na onome svijetu biti gošćenje njihovo!",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5036,
        "text": "Mi vas stvaramo – pa zašto ne povjerujete?",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5037,
        "text": "Kažite vi Meni: da li sjemenu koje ubacujete",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5038,
        "text": "vi oblik dajete ili Mi to činimo?",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5039,
        "text": "Mi određujemo kada će ko od vas umrijeti, i niko nas ne može spriječiti",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5040,
        "text": "da likove vaše izmijenimo i da vas iznova u likovima koje vi ne poznajete stvorimo.",
        "numberInSurah": 61,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5041,
        "text": "Poznato vam je kako ste prvi put stvoreni, pa zašto se ne urazumite?",
        "numberInSurah": 62,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5042,
        "text": "Kažite vi Meni: šta biva sa onim što posijete?",
        "numberInSurah": 63,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5043,
        "text": "Da li mu vi dajete snagu da niče, ili to Mi činimo?",
        "numberInSurah": 64,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5044,
        "text": "Ako hoćemo možemo ga u suho rastinje pretvoriti, pa biste se snebivali:",
        "numberInSurah": 65,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5045,
        "text": "\"Mi smo, doista, oštećeni,",
        "numberInSurah": 66,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5046,
        "text": "čak smo svega lišeni!\"",
        "numberInSurah": 67,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5047,
        "text": "Kažite vi Meni: vodu koju pijete –",
        "numberInSurah": 68,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5048,
        "text": "da li je vi ili Mi iz oblaka spuštamo?",
        "numberInSurah": 69,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5049,
        "text": "Ako želimo, možemo je slanom učiniti – pa zašto niste zahvalni?",
        "numberInSurah": 70,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5050,
        "text": "Kažite vi Meni: vatru koju palite –",
        "numberInSurah": 71,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5051,
        "text": "da li drvo za nju vi ili Mi stvaramo?",
        "numberInSurah": 72,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5052,
        "text": "Mi činimo da ona podsjeća i da bude korisna onima koji konače;",
        "numberInSurah": 73,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5053,
        "text": "zato hvali Gospodara svoga Veličanstvenog!",
        "numberInSurah": 74,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 470,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 5054,
        "text": "I kunem se časom kad se zvijezde gube –",
        "numberInSurah": 75,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5055,
        "text": "a to je, da znate, zakletva velika,",
        "numberInSurah": 76,
        "juz": 27,
        "manzil": 7,
        "page": 536,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5056,
        "text": "on je, zaista, Kur'an plemeniti",
        "numberInSurah": 77,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5057,
        "text": "u Knjizi brižljivo čuvanoj –",
        "numberInSurah": 78,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5058,
        "text": "dodirnuti ga smiju samo oni koji su čisti,",
        "numberInSurah": 79,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5059,
        "text": "on je Objava od Gospodara svjetova.",
        "numberInSurah": 80,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5060,
        "text": "Pa, kako ovaj govor omalovažavate",
        "numberInSurah": 81,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5061,
        "text": "i umjesto zahvalnosti što vam je hrana darovana – vi u njega ne vjerujete?",
        "numberInSurah": 82,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5062,
        "text": "A zašto vi kad duša do guše dopre,",
        "numberInSurah": 83,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5063,
        "text": "i kad vi budete tada gledali,",
        "numberInSurah": 84,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5064,
        "text": "a Mi smo mu bliži od vas, ali vi ne vidite,",
        "numberInSurah": 85,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5065,
        "text": "zašto je onda kad niste u tuđoj vlasti",
        "numberInSurah": 86,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5066,
        "text": "ne povratite, ako istinu govorite?",
        "numberInSurah": 87,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5067,
        "text": "I ako bude jedan od onih koji su Allahu bliski –",
        "numberInSurah": 88,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5068,
        "text": "udobnost i opskrba lijepa i džennetske blagodati njemu!",
        "numberInSurah": 89,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5069,
        "text": "A ako bude jedan od onih koji su sretni –",
        "numberInSurah": 90,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5070,
        "text": "pa, pozdrav tebi od onih koji su sretni!",
        "numberInSurah": 91,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5071,
        "text": "A ako bude jedan od onih koji su poricali i u zabludi ostali,",
        "numberInSurah": 92,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5072,
        "text": "pa, ključalom vodom biće ugošćen",
        "numberInSurah": 93,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5073,
        "text": "i u ognju prženjem.",
        "numberInSurah": 94,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5074,
        "text": "Sama je istina, zbilja, sve ovo –",
        "numberInSurah": 95,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      },
      {
        "number": 5075,
        "text": "zato hvali ime Gospodara svoga Veličanstvenog!",
        "numberInSurah": 96,
        "juz": 27,
        "manzil": 7,
        "page": 537,
        "ruku": 471,
        "hizbQuarter": 215,
        "sajda": false
      }
