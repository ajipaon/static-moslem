        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4994,
        "text": "عَلَىٰ سُرُرࣲ مَّوۡضُونَةࣲ ",
