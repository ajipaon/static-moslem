        "ruku": 469,
        "hizbQuarter": 214,
        "sajda": false
      },
      {
        "number": 4995,
        "text": "مُّتَّكِءِینَ عَلَیۡهَا مُتَقَـٰبِلِینَ ",
        "numberInSurah": 16,
        "juz": 27,
