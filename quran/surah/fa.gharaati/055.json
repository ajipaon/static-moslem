        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4954,
        "text": "پس کدام یک از نعمت‌های پروردگارتان را انکار می‌کنید؟",
        "numberInSurah": 53,
