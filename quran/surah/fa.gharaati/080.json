{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 80,
    "name": "سُورَةُ عَبَسَ",
    "englishName": "Abasa",
    "englishNameTranslation": "He frowned",
    "revelationType": "Meccan",
    "numberOfAyahs": 42,
    "ayahs": [
      {
        "number": 5759,
        "text": "چهره درهم کشید و روی برتافت.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5760,
        "text": "از این که مردی نابینا به سراغ او آمد.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5761,
        "text": "و تو چه می‌دانی؟ شاید او در پی پاکی و پارسایی باشد.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5762,
        "text": "یا پند گیرد و آن پند سودش بخشد.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5763,
        "text": "امّا کسی که خود را بی‌نیاز می‌بیند.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5764,
        "text": "پس تو به او روی می‌آوری.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5765,
        "text": "با آن که اگر نخواهد خود را پاک سازد، بر تو چیزی نیست.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5766,
        "text": "و امّا کسی که شتابان نزد تو آمده،",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5767,
        "text": "در حالی که [از خدا] می‌ترسد،",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5768,
        "text": "از او به دیگران می‌پردازی.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5769,
        "text": "این چنین [برخوردی شایسته‌] نیست! بی‌تردید این [قرآن] مایه‌ی پند است.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5770,
        "text": "پس هر کس بخواهد، آن را یاد کند و از آن پند گیرد.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5771,
        "text": "در میان صحیفه‌هایی ارجمند،",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5772,
        "text": "بلندمرتبه و پاکیزه است.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5773,
        "text": "به دست سفیرانی",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5774,
        "text": "بزرگوار و نیکوکار.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5775,
        "text": "مرگ بر این انسان [سرکش] که چه ناسپاس است!",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5776,
        "text": "[مگر خداوند] او را از چه چیز آفریده است؟",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5777,
        "text": "از نطفه‌ای [ناچیز] او را آفرید و سامانش بخشید.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5778,
        "text": "سپس راه [رشد و کمال] را برای او آسان و فراهم نمود.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5779,
        "text": "آن‌گاه او را میراند و در گورش نهاد.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5780,
        "text": "پس هر زمان که بخواهد، او را برانگیزد.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5781,
        "text": "هرگز! هنوز آنچه را به او فرمان داده، بجا نیاورده است.",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5782,
        "text": "پس باید انسان به غذای خود بنگرد.",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5783,
        "text": "ما آب را آن‌گونه که باید، [از آسمان] فروریختیم.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5784,
        "text": "سپس زمین را به خوبی شکافتیم.",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5785,
        "text": "و در آن، دانه رویاندیم.",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5786,
        "text": "و نیز انگور و سبزیجات،",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5787,
        "text": "و زیتون و خرما،",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5788,
        "text": "و باغ‌های پردرخت،",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5789,
        "text": "و میوه و چراگاه،",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5790,
        "text": "برای برخورداری شما و چهارپایانتان.",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5791,
        "text": "پس هنگامی که آن صدای هولناک برآید.",
        "numberInSurah": 33,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5792,
        "text": "روزی که انسان از برادرش بگریزد،",
        "numberInSurah": 34,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5793,
        "text": "و [از] مادر و پدرش،",
        "numberInSurah": 35,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5794,
        "text": "و [از] همسر و فرزندانش،",
        "numberInSurah": 36,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5795,
        "text": "در آن روز برای هر یک از آنان گرفتاری است که او را [از دیگران] بازمی‌دارد.",
        "numberInSurah": 37,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5796,
        "text": "چهره‌هایی در آن روز درخشانند،",
        "numberInSurah": 38,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5797,
        "text": "خندان و شادمانند،",
        "numberInSurah": 39,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5798,
        "text": "و بر چهره‌هایی در آن روز، غبار [غم] نشسته.",
        "numberInSurah": 40,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5799,
        "text": "و تیرگی و سیاهی، چهره‌ی آنان را پوشانده است.",
        "numberInSurah": 41,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5800,
        "text": "آنان، همان کافران بدکردارند.",
        "numberInSurah": 42,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      }
    ],
