  "data": {
    "number": 87,
    "name": "سُورَةُ الأَعۡلَىٰ",
    "englishName": "Al-A'laa",
    "englishNameTranslation": "The Most High",
    "revelationType": "Meccan",
    "numberOfAyahs": 19,
    "ayahs": [
      {
        "number": 5949,
        "text": "نام پروردگار بلندمرتبه‌ات را به پاکی یاد کن!",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5950,
        "text": "همان که آفرید و سامان بخشید.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5951,
        "text": "و آن که [هر چیز را] اندازه نهاد و هدایت کرد.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5952,
        "text": "و آن که چراگاه را رویانید.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5953,
        "text": "و پس [از چندی] آن را خشک و تیره کرد.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5954,
        "text": "ما [قرآن را] بر تو می‌خوانیم، پس فراموش نخواهی کرد.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5955,
        "text": "مگر آنچه را خدا بخواهد که او بی‌گمان آشکار و نهان را می‌داند.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5956,
        "text": "و ما تو را برای آسان‌ترین [راه دعوت] آماده می‌سازیم.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5957,
        "text": "پس پند بده، اگر پند دادن سودمند باشد.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5958,
        "text": "به زودی آن که [از خدا] می‌ترسد، پند گیرد.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5959,
        "text": "و بدبخت‌ترینِ افراد از آن دوری کند.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5960,
        "text": "همان کس که به آتش بزرگ‌تر درمی‌آید.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5961,
        "text": "سپس در آنجا نه بمیرد و نه زندگی کند.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5962,
        "text": "به راستی رستگار شد آن کس که خود را پاک گردانید.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5963,
        "text": "و نام پروردگارش را یاد کرد و نماز گزارد.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5964,
        "text": "[بلکه شما منکران لجوج] زندگی دنیا را ترجیح می‌دهید.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5965,
        "text": "و حال آن که آخرت بهتر و ماندگارتر است.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5966,
        "text": "این [سخن] در کتاب‌های آسمانی پیشین نیزآمده است،",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5967,
        "text": "کتاب‌های ابراهیم و موسی.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      }
    ],
