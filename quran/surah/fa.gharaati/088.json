  "code": 200,
  "status": "OK",
  "data": {
    "number": 88,
    "name": "سُورَةُ الغَاشِيَةِ",
    "englishName": "Al-Ghaashiya",
    "englishNameTranslation": "The Overwhelming",
    "revelationType": "Meccan",
    "numberOfAyahs": 26,
    "ayahs": [
      {
        "number": 5968,
        "text": "آیا خبر حادثه‌ی فراگیر به تو رسیده است؟",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5969,
        "text": "در آن روز، چهره‌هایی خوار و زبونند.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5970,
        "text": "[با آن که] تلاش کرده‌اند و رنج برده‌اند،",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5971,
        "text": "[امّا] به آتشی سوزان درآیند.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5972,
        "text": "از چشمه‌ای جوشان نوشانده شوند.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5973,
        "text": "و خوراکی جز خار خشک و تلخ ندارند.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5974,
        "text": "که نه فربه کند و نه گرسنگی را از میان برد.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5975,
        "text": "در چنین روزی چهره‌هایی خرّم و شادابند.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5976,
        "text": "از کوشش خود خشنودند.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5977,
        "text": "در بهشت برین [جای دارند].",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5978,
        "text": "آنجا سخن بیهوده‌ای نشنوند.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5979,
        "text": "در آنجا چشمه‌ای جاری است.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5980,
        "text": "در آنجا تخت‌هایی است پرارج و برافراشته.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5981,
        "text": "و سبوهایی چیده شده،",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5982,
        "text": "و بالش‌هایی ردیف شده،",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5983,
        "text": "و فرش‌هایی گرانبها که گسترده شده است.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5984,
        "text": "پس آیا به شتر نمی‌نگرند که چگونه آفریده شده؟",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5985,
        "text": "و به آسمان که چگونه برافراشته شده؟",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5986,
        "text": "و به کوه‌ها که چگونه برپا داشته شده؟",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5987,
        "text": "و به زمین که چگونه گسترده شده است؟",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5988,
        "text": "پس یادآوری کن که تو فقط یادآوری کننده‌ای.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5989,
        "text": "تو بر آنان تسلّطی نداری [که به ایمان آوردن مجبورشان کنی].",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5990,
        "text": "ولی آن کس که روی گرداند و کفر ورزید،",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5991,
        "text": "پس خداوند او را به بزرگ‌ترین عذاب مجازات می‌کند.",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5992,
        "text": "بازگشت آنان به سوی ماست.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5993,
        "text": "سپس حسابشان [نیز] بر عهده‌ی ماست.",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 530,
        "hizbQuarter": 237,
        "sajda": false
      }
    ],
