  "data": {
    "number": 51,
    "name": "سُورَةُ الذَّارِيَاتِ",
    "englishName": "Adh-Dhaariyat",
    "englishNameTranslation": "The Winnowing Winds",
    "revelationType": "Meccan",
    "numberOfAyahs": 60,
    "ayahs": [
      {
        "number": 4676,
        "text": "S wavu iwezzâen!",
        "numberInSurah": 1,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4677,
        "text": "I ibubben taâkkwemt!",
        "numberInSurah": 2,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4678,
        "text": "Wid ippazalen s shala!",
        "numberInSurah": 3,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4679,
        "text": "Tid ifeôqen lameô!",
        "numberInSurah": 4,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4680,
        "text": "Ar ayen i wen ippuâahden, d tidep.",
        "numberInSurah": 5,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4681,
        "text": "Aqeîîi, ih, ad iûaô.",
        "numberInSurah": 6,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4682,
        "text": "S igenni bu idurra!",
        "numberInSurah": 7,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4683,
        "text": "Kunwi temgarradem $ef wawal.",
        "numberInSurah": 8,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4684,
        "text": "Iooa t win iooan.",
        "numberInSurah": 9,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4685,
        "text": "Tameppant i ikeddaben!",
        "numberInSurah": 10,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4686,
        "text": "Nitni, di maylellif, stehzan,",
        "numberInSurah": 11,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4687,
        "text": "steqsayen: \"anda t wass uqeîîi\"?",
        "numberInSurah": 12,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4688,
        "text": "Ass ideg, ara ppuhewlen di tmes:",
        "numberInSurah": 13,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4689,
        "text": "\"Jeôôbet ahewwel nnwen agi i$ef têarem\"!",
        "numberInSurah": 14,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4690,
        "text": "Ih, wid ipêezziben, di leonanat akked tliwa;",
        "numberInSurah": 15,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4691,
        "text": "Ïîfen ayen i sen Ifka Mass nnsen. Ih, nitni llan, iu$ lêal, d lâuûam.",
        "numberInSurah": 16,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4692,
        "text": "Deg iv, cwiî ay gganen.",
        "numberInSurah": 17,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4693,
        "text": "Di tafrara, nitni ste$fiôen;",
        "numberInSurah": 18,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4694,
        "text": "deg wayla nnsen: amur i umappar akked ume$bun.",
        "numberInSurah": 19,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4695,
        "text": "U di tmurt, issekniyen i wid ipputebbten.",
        "numberInSurah": 20,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4696,
        "text": "Llant deg iman nnwen. Aâni ur tepwalim ara?",
        "numberInSurah": 21,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4697,
        "text": "U deg igenni, ôôeéq nnwen, akked wayen i wen ippuwaââden.",
        "numberInSurah": 22,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4698,
        "text": "S Mass n tmurt akked igenwan, ar neppa d tidep, am akken kunwi tepmeslayem!",
        "numberInSurah": 23,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4699,
        "text": "Ma tebbwev ik id tedyant inebgawen ukyisen n Ibôahim?",
        "numberInSurah": 24,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4700,
        "text": "Kecmen $uôes, nnan as: \"tifrat\"! Inna: \"tifrat a wid ur nessin!\"",
        "numberInSurah": 25,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4701,
        "text": "Inser ar twacult is, ibbwi d aâjmi iqebbwan.",
        "numberInSurah": 26,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4702,
        "text": "Iqeôb asen t id; inna: \"day ur teppeppem ara\"?",
        "numberInSurah": 27,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4703,
        "text": "Iêus iugad iten. Nnan as: \"ur ppagwad\"! D$a beccôen t s uqcic amusnaw",
        "numberInSurah": 28,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4704,
        "text": "Tqeôb ed tmeîîut is, tsu$, tewwet deg udem is, tenna d: \"tam$aôt tiâiqeôt?\"",
        "numberInSurah": 29,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4705,
        "text": "Nnan: \"akka id Inna Mass im. Ih, Neppa, d Bab n ûûwab, d Amusnaw\".",
        "numberInSurah": 30,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4706,
        "text": "Inna: \"anwa ay d awal nnwen, a yimceggâan\"?",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4707,
        "text": "Nnan: \"nekwni neppuceggaâ d ar ugdud ibe$wviyen.",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4708,
        "text": "Iwakken a ten neôoem s uéôu n talaxt",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4709,
        "text": "i ipwaôecmen s$uô Mass ik i ucmiten\".",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4710,
        "text": "Nessuffe$ kra illan, degs, n lmumnin.",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4711,
        "text": "Nufa kan degs, yiwen uxxam n imvuâen.",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4712,
        "text": "Neooa p id d assekni i wid iugaden aâaqeb aqeôêan.",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4713,
        "text": "I di Musa, mi t id Nuzen i Ferâun, s udabu n ûûeê.",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4714,
        "text": "Iaâûa, akked imalalen is, inna: \"d aseêêar, ne$ d aôehbani\".",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4715,
        "text": "Neddem it, neppa akked iâsekriwen is, Nveggeô iten ar yill. Tiyta degs.",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4716,
        "text": "I di Âad, mi sen Nuzen avu asexvi.",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4717,
        "text": "Ur ippaooa ayen immuger, alamma irra t am rrmim.",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4718,
        "text": "I Tamud, mi pwanecden: \"siwa tallit ara tezhum\".",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4719,
        "text": "Âuûan lameô n Mass nnsen. Tewwet iten id ssiâqa, nitni la pwalin.",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4720,
        "text": "Ur zmiren a d kren, ur pwasellken.",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4721,
        "text": "Agdud n Nuê, uqbel, llan, s tidep, d agdud iballa$en.",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4722,
        "text": "Igenni, Nebna t s loehd, d Nekwni it Issewsaân.",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4723,
        "text": "Tamurt, Nessa p. Ay gerzen Wid Itessun!",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4724,
        "text": "Di yal cci, Nexleq tayuga, ahat a d tesmektim.",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4725,
        "text": "Ihi, srifget ar Öebbi. Nek, ih, s$uôeS ar $uôwen, d aneddaô ipbegginen.",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4726,
        "text": "Ur sâut, ar tama n Öebbi, ôebbi nniven! Nek, ih, s$uôeS ar $uôwen, d aneddaô ipbegginen.",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4727,
        "text": "Akka, ur iusi umazan ar wid i ten id izwaren, mebla ma nnan as: \"d aseêêar ne$ d aôehbani\".",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4728,
        "text": "Day ppuweûûan $ef waya? Xaîi, nitni d agdud izegren tilas.",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4729,
        "text": "Ïîixxeô asen; ur deg k tiyta.",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4730,
        "text": "Smektay ed. Asmekti, s tidep, infaâ lmumnin.",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4731,
        "text": "Xelqe$ leonun akked imdanen, iwakken kan ad iyI âabden.",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4732,
        "text": "Ur b$i$ segsen ôôeéq, ur b$i$ ad iyi cceççen.",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4733,
        "text": "Öebbi, s tidep, d Aôeééaq, d Bu tezmert n ûûeê.",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4734,
        "text": "Ih, i wid idensen, amur am umur imeddukwal nnsen. Ulay$eô iyI $eûben.",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4735,
        "text": "A tawa$it n wid ijehlen, ilmend n wass i sen ippuwaââden!",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      }
