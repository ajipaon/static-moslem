        "juz": 19,
        "manzil": 4,
        "page": 362,
        "ruku": 312,
        "hizbQuarter": 145,
        "sajda": false
      },
      {
        "number": 2884,
        "text": "Ni$ itelf iyi seg usmekti i illan iusa yi d. Cciîan, i umdan, izga d axeddaâ\".",
