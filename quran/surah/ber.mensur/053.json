        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4825,
        "text": "Sinna, a t Ikafi s lufa.",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4826,
        "text": "Ih, ar Mass ik, taggara.",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4827,
        "text": "D Neppa i Issevûan, i Issruyen.",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4828,
        "text": "D Neppa i Ineqqen, i Issidiren.",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4829,
        "text": "D Neppa i Ixelqen tayuga, ddkeô akked nnta,",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4830,
        "text": "Si tiqqit n l$ella ma tuzzel.",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4831,
        "text": "FellaS, axlaq nniven.",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4832,
        "text": "D Neppa i I$ennun, i Iqenâan.",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4833,
        "text": "D Neppa i d Mass n Ciâra.",
