        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4840,
        "text": "Wagi d aneddaô seg ineddaôen n zik.",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4841,
        "text": "A d taéé tin d ippaééen.",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4842,
        "text": "War Öebbi, ur illi ara p id issdehôen.",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4843,
        "text": "Iswehm ikwen wawal a?",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4844,
        "text": "La tdessem ur la tepprum!",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
