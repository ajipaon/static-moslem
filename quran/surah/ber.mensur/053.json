    "revelationType": "Meccan",
    "numberOfAyahs": 62,
    "ayahs": [
      {
        "number": 4785,
        "text": "S yitri mara ixeppi!",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4786,
        "text": "Ur innetlaf, ur i$liv umeddakwel nnwen.",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4787,
        "text": "Ur d iqqaô akken i s ihwan.",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4788,
        "text": "D aweêêi kan, id ippuweêêan.",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4789,
        "text": "Islemd it bab n tezmert iqwan.",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4790,
        "text": "D bab n tezmert irs ed",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4791,
        "text": "u neppa di lejba aâlayen.",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4792,
        "text": "Sinna iqeôb ed, ippucengel,",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4793,
        "text": "atan tasurift n sin ixtucen, ne$ ur tebbwiv.",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4794,
        "text": "D$a iweêêa i umdan iS, ayen iweêêa.",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4795,
        "text": "Ur iskaddeb wul deg wayen iwala.",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4796,
        "text": "Day a t tnaqwmem, $ef wayen iwala?",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4797,
        "text": "Ni$ iéôa t di trusi s nniven,",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4798,
        "text": "ar tedxwent n talast,",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4799,
        "text": "u$uô illa leonan usemlili.",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4800,
        "text": "Mi tesbur tedxwent ta$wlaft,",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4801,
        "text": "ur iwweônenni yiéôi, ur izgir.",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4802,
        "text": "Ni$ iwala kra issekniyen imeqwôaêanen, n Mass is.",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4803,
        "text": "Twalam Allat akked Lâuzza?",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4804,
        "text": "Akked Manat, tis krav nniven?",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4805,
        "text": "Day i kunwi ddkeô, i Neppa nnta?",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4806,
        "text": "Wagi, ihi, d beîîu aneêyaf.",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4807,
        "text": "Wigi d ismawen kan i sent tefkam, kunwi akked imezwura nnwen. Ur sent d Issers Öebbi kra udabu. Ïîafaôen kan nnwaya, akked wayen cehwant teôwiêin. Ni$ tusa ten id nnhaya s$uô Mass nnsen.",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4808,
        "text": "Aâni, i umdan, ayen issaram?",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4809,
        "text": "Tamezwarut akked tneggarut, n Öebbi.",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4810,
        "text": "Acêal n lmalayek deg igenwan, ur d teppawi acemma ccafuâa nnsent, siwa deffir tsriê n Öebbi, $ef win Ib$a u Iôuva.",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4811,
        "text": "Ih, wid ur numin ara di laxeôt, ppaken i lmalayek ismawen n nnta.",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4812,
        "text": "Ur sâin fellas kra n tmusni. Ïîafaôen kan nnwaya. Wanag nnwaya ur tebbwi acemma si tidep.",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4813,
        "text": "Ihi, eoo win iwexxôen i usmekti nne£, i ib$an kan tudert n ddunit.",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4814,
        "text": "D aya ay sen tefka tmusni. Mass ik, war ccekk, Iéôa sennig akw, anwa innetlafen seg ubrid iS; Iéôa sennig akw, anwa i ipwanhan.",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4815,
        "text": "N Öebbi ayen akw illan deg igenwan, illan di tmurt. Ad Iqeîîi wid ifalîan $ef wayen xedmen, ad Ioazi wid issweqmen, s ûûam.",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4816,
        "text": "Wid ijunben ibekkaven ibararen akked tucmitin, siwa lfalîat fessusen, ihi, d uwsiâ ssmaê n Mass ik. Neppa Issen ikwen sennig akw, mi kwen id Ixleq seg wakal, mi tellam d icelleqluqen, deg iâabbav n tyimmatin nnwen. Ur staâmilet tezdeg deg iman nnwen! Neppa Iéôa sennig akw, anwa iêezzben.",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4817,
        "text": "Twalav win iwexxôen,",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4818,
        "text": "i ifkan cwiî, iccuêen?",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4819,
        "text": "Day isâa tamusni n tbavnit, d$a iwala?",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4820,
        "text": "Ne$ ur ippuxebbeô $ef tewôiqin n Musa?",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4821,
        "text": "Akked Ibôahim ineûêen?",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4822,
        "text": "Ur ipbibbi yiwen taâkkwemt n wayev.",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4823,
        "text": "I umdan, siwa ayen inki.",
