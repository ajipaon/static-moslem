        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4836,
        "text": "Agdud n Nuê, uqbel nnsen, war ccekk, iugar iten ddens, lbbwe$v.",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4837,
        "text": "Issenger tid inneqlaben;",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4838,
        "text": "Idel iten s wayen idellen.",
