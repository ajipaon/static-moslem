        "juz": 25,
        "manzil": 6,
        "page": 489,
        "ruku": 424,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4330,
        "text": "Day a kwen Nêewwej seg usmekti, imi kunwi d agdud ipêawalen?",
