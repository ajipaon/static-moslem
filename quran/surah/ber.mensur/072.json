    "ayahs": [
      {
        "number": 5448,
        "text": "Ini: \"ippuweêêa yi d, ar agraw n leonun ssemêessen, u nnan: ih, nesla i Leqwôan, d lwehma.",
        "numberInSurah": 1,
        "juz": 29,
