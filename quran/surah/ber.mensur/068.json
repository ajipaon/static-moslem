        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5292,
        "text": "Di tûebêit msawalen:",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5293,
        "text": "\"ekkret zik, ar lexla nnwen, ma a p$elltem\"!",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5294,
        "text": "Teddun, sbecbucen garasen:",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5295,
        "text": "\"igellil, assa, ur t ppaooat a p id ikçem\". ö",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5296,
        "text": "Öuêen ppeklen, zemren.",
