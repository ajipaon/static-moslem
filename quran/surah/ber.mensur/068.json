        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5274,
        "text": "Ih, i keçç arraz ur nepnegzay.",
