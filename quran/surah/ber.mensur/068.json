        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5307,
        "text": "Acu ikwen iu$en? Amek tepêuddum?",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5308,
        "text": "Ne$ tesâam tazmamt ideg tlemdem,",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 565,
        "ruku": 499,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5309,
        "text": "degs, i kunwi, s tidep, ayen tesmenyafem?",
