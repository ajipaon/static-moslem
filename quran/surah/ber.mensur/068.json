    "numberOfAyahs": 52,
    "ayahs": [
      {
        "number": 5272,
        "text": "N. S wamru akked wayen pjerriden!",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 564,
        "ruku": 498,
        "hizbQuarter": 226,
        "sajda": false
      },
      {
        "number": 5273,
        "text": "Keçç, s l$it n Mass ik, ur telliv d aôehbani!",
