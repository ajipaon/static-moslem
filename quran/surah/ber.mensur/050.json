        "juz": 26,
        "manzil": 7,
        "page": 518,
        "ruku": 452,
        "hizbQuarter": 207,
        "sajda": false
      },
      {
        "number": 4642,
        "text": "Inkeô, uqbel nnsen, ugdud n Nuê, At Rrass, Tamud,",
        "numberInSurah": 12,
        "juz": 26,
        "manzil": 7,
        "page": 518,
        "ruku": 452,
        "hizbQuarter": 207,
        "sajda": false
      },
      {
        "number": 4643,
        "text": "Âad, Ferâun, atmaten n Luî,",
        "numberInSurah": 13,
