        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 454,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4668,
        "text": "Ni$ Nxelq ed tamurt, igenwan, akked wayen illan garasen, di sdis wussan, ur Naâyi.",
