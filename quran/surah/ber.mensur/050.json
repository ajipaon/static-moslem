        "juz": 26,
        "manzil": 7,
        "page": 518,
        "ruku": 452,
        "hizbQuarter": 207,
        "sajda": false
      },
      {
        "number": 4638,
        "text": "i tmu$li akked usmekti n yal amdan isteôoiâen.",
