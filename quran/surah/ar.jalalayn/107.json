    "numberOfAyahs": 7,
    "ayahs": [
      {
        "number": 6198,
        "text": "«أرأيت الذي يُكذِّب بالدين» بالجزاء والحساب، أي هل عرفته وإن لم تعرفه.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6199,
        "text": "«فذلك» بتقدير هو بعد الفاء «الذي يَدُعُّ اليتيم» أي يدفعه بعنف عن حقه.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6200,
        "text": "«ولا يحض» نفسه ولا غيره «على طعام المسكين» أي إطعامه، نزلت في العاص بن وائل أو الوليد بن المغيرة.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6201,
        "text": "«فويل للمصلين».",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6202,
        "text": "«الذين هم عن صلاتهم ساهون» غافلون يؤخرونها عن وقتها.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6203,
        "text": "«الذين هم يراءون» في الصلاة وغيرها.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
