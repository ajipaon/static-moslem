  "data": {
    "number": 94,
    "name": "سُورَةُ الشَّرۡحِ",
    "englishName": "Ash-Sharh",
    "englishNameTranslation": "The Consolation",
    "revelationType": "Meccan",
    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6091,
        "text": "«ألم نشرح» استفهام تقرير أي شرحنا «لك» يا محمد «صدرك» بالنبوة وغيرها.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6092,
        "text": "«ووضعنا» حططنا «عنك وزرك».",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6093,
        "text": "(الذي أنقض) أثقل (ظهرك) وهذا كقوله تعالى: \"\" ليغفر لك الله ما تقدم من ذنبك \"\".",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6094,
        "text": "«ورفعنا لك ذكرك» بأن تُذكر مع ذكري في الآذان والإقامة والتشهد والخطبة وغيرها.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6095,
        "text": "«فإن مع العسر» الشدة «يسرا» سهولة.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6096,
        "text": "«إن مع العسر يسراً» والنبي صلى الله عليه وسلم قاسى من الكفار شدة ثم حصل له اليسر بنصره عليهم.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6097,
        "text": "«فإذا فرغت» من الصلاة «فانصب» اتعب في الدعاء.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6098,
        "text": "«وإلى ربك فارغب» تضرع.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 596,
