    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6091,
        "text": "நாம், உம் இதயத்தை உமக்காக விரிவாக்கவில்லையா?",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6092,
        "text": "மேலும், நாம் உம்மை விட்டும் உம் சுமையை இறக்கினோம்.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6093,
        "text": "அது உம் முதுகை முறித்துக் கொண்டுடிருந்தது.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6094,
        "text": "மேலும், நாம் உமக்காக உம்முடைய புகழை மேலோங்கச் செய்தோம்.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6095,
        "text": "ஆதலின் நிச்சயமாகத் துன்பத்துடன் இன்பம் இருக்கிறது.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6096,
        "text": "நிச்சயமாக துன்பத்துடன் இன்பம் இருக்கிறது.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6097,
        "text": "எனவே, (வேலைகளிலிருந்து) நீர் ஓய்ந்ததும் (இறைவழியிலும், வணக்கத்திலும்) முயல்வீராக.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 536,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6098,
        "text": "மேலும், முழு மனத்துடன் உம் இறைவன் பால் சார்ந்து விடுவீராக.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
