        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6184,
        "text": "ھۆتەمەنىڭ نېمىلىكىنى قانداق بىلەلەيسەن؟",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
