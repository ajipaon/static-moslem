  "data": {
    "number": 99,
    "name": "سُورَةُ الزَّلۡزَلَةِ",
    "englishName": "Az-Zalzala",
    "englishNameTranslation": "The Earthquake",
    "revelationType": "Medinan",
    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6139,
        "text": "زېمىن قاتتىق تەۋرىتىلگەندە،",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6140,
        "text": "زېمىن (قوينىدىكى) يۈكلىرىنى (يەنى مەدەنلەر، ئوتلار، ئۆلۈكلەرنى) چىقارغاندا،",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6141,
        "text": "ئىنسان (يەنى كاپىر) ئۇنىڭغا (يەنى زېمىنغا): «نېمە بولدى؟» دېگەندە،",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6142,
        "text": "بۇ كۈندە زېمىن ئۆزىنىڭ خەۋەرلىرىنى (يەنى ئۇنىڭ ئۈستىدە ئىشلەنگەن ياخشى - يامان ئىشلارنى) مەلۇم قىلىدۇ",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6143,
        "text": "(ئۇنىڭ مەلۇم قىلىشى) پەرۋەردىگارىنىڭ ئۇنى شۇنداق قىلىشقا ئەمر قىلغانلىقىدىندۇر",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6144,
        "text": "بۇ كۈندە كىشىلەر قىلغان ئەمەللىرىنىڭ نەتىجىسىنى كۆرۈش (يەنى نەتىجىسىگە ئېرىشىش) ئۈچۈن توپ - توپ بولۇپ تارىلىپ كېتىدۇ",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6145,
        "text": "كىمكى زەررىچىلىك ياخشى ئىش قىلىدىكەن، ئۇنىڭ مۇكاپاتىنى كۆرىدۇ",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6146,
        "text": "كىمكى زەررىچىلىك يامان ئىش قىلىدىكەن، ئۇنىڭ جازاسىنى تارتىدۇ",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      }
    ],
