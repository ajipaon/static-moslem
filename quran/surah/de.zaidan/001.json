        "sajda": false
      },
      {
        "number": 2,
        "text": "Alhamdulillah: Alles Lob gebührt ALLAH, Dem HERRN aller Geschöpfe,",
        "numberInSurah": 2,
        "juz": 1,
        "manzil": 1,
        "page": 1,
        "ruku": 1,
        "hizbQuarter": 1,
        "sajda": false
      },
      {
        "number": 3,
        "text": "Dem Allgnade Erweisenden, Dem Allgnädigen,",
