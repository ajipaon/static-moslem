{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 55,
    "name": "سُورَةُ الرَّحۡمَٰن",
    "englishName": "Ar-Rahmaan",
    "englishNameTranslation": "The Beneficent",
    "revelationType": "Medinan",
    "numberOfAyahs": 78,
    "ayahs": [
      {
        "number": 4902,
        "text": "Der Allgnade Erweisende",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4903,
        "text": "lehrte den Quran,",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4904,
        "text": "erschuf den Menschen,",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4905,
        "text": "lehrte ihn das Artikulieren.",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4906,
        "text": "Die Sonne und der Mond sind nach einer Berechnung.",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4907,
        "text": "Und das Bodengewächs und die Bäume vollziehen Sudschud.",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4908,
        "text": "Und den Himmel erhob ER und setzte Al-mizan ein:",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4909,
        "text": "\"Begeht keine Übertretungen hinsichtlich von Al-mizan",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4910,
        "text": "Und haltet das (mit Al-mizan) Festgestellte nach Gerechtigkeit ein und mindert Al-mizan nicht!\"",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4911,
        "text": "Und die Erde setzte ER für die Lebewesen ein.",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4912,
        "text": "Auf ihr sind Obst und Dattelbäume mit Blütenkelchen",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4913,
        "text": "sowie Körner mit Halmen und Blättern.",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4914,
        "text": "Also, welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4915,
        "text": "ER erschuf den Menschen aus Salsal wie Ton",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4916,
        "text": "und erschuf die Dschinn aus rauchlosem Feuer.",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4917,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4918,
        "text": "ER ist der HERR beider Osten und beider Westen.",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4919,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4920,
        "text": "ER ließ beide Meere aufeinandertreffen,",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4921,
        "text": "zwischen beiden gibt es eine Trennung, sie vermischen sich nicht.",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4922,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4923,
        "text": "Aus beiden kommen Perlen und Korallen hervor.",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4924,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4925,
        "text": "Und Ihm gehören die gebauten Fahrenden auf dem Meer wie die Berge.",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4926,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4927,
        "text": "Alles, was auf ihr (der Erde) ist, vergeht,",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4928,
        "text": "und es bleibt dein HERR, Der mit der Majestät und der Würde.",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4929,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4930,
        "text": "Ihn bitten alle, die in den Himmeln und auf Erden sind. Jeden Yaum ist ER befaßt mit den Angelegenheiten.",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4931,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4932,
        "text": "WIR werden euch Uns noch vornehmen, ihr Menschen und Dschinn.",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4933,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4934,
        "text": "Ihr Gruppe von Dschinn und Menschen! Wenn ihr die Gegenden der Himmel und der Erde durchdringen könnt, so durchdringt! Ihr durchdringt nur mit Macht.",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4935,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4936,
        "text": "ER schickt über euch beide Flammen von Feuer und Rauch, so könnt ihr euch nicht helfen.",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4937,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4938,
        "text": "Und wenn der Himmel sich spaltete, dann rot wie rotes Leder wird.",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4939,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4940,
        "text": "Also an diesem Tag wird nach seiner Verfehlung weder ein Mensch noch ein Dschinn befragt.",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4941,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4942,
        "text": "Die schwer Verfehlenden werden durch ihreMerkmale bekannt, dann wird an den Stirnhaaren und an den Füßen gezerrt.",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4943,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4944,
        "text": "Dies ist Dschahannam, welche die schwer Verfehlenden ableugnen.",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4945,
        "text": "Sie machen die Runde in ihr (Dschahannam) und mit extrem hoher Temperatur Siedendem.",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4946,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4947,
        "text": "Und für denjenigen, der sich vor der Vorstellung vor seinem HERRN fürchtet, gibt es zwei Dschanna -",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4948,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?! -",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4949,
        "text": "die von Bäumen sind.",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4950,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4951,
        "text": "In denen es zwei Quellen gibt, die fließen.",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4952,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4953,
        "text": "In denen es von jeder Obstart Zweiheit gibt.",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4954,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4955,
        "text": "Angelehnt sind sie auf Liegen, deren Inlett aus Istabraq ist. Und die Ernte beider Dschannat ist nahe.",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4956,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4957,
        "text": "In den beiden gibt es des Blicks Zurückhaltende, die weder ein Mensch, noch ein Dschinn vor ihnen deflorierte.",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4958,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4959,
        "text": "Als wären sie wie Rubinen und Korallen.",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4960,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4961,
        "text": "Gibt es etwa eine andere Belohnung für den Ihsan, als den Ihsan?!",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4962,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?",
        "numberInSurah": 61,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4963,
        "text": "Und außer ihnen sind noch zwei Dschannat.",
        "numberInSurah": 62,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4964,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 63,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4965,
        "text": "Sie sind dunkelgrün.",
        "numberInSurah": 64,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4966,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 65,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4967,
        "text": "In ihnen gibt es zwei Quellen, die sprudeln.",
        "numberInSurah": 66,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4968,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 67,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4969,
        "text": "In ihnen gibt es Obst, Datteln und Granatäpfel.",
        "numberInSurah": 68,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4970,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 69,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4971,
        "text": "In ihnen sind schöne Gute.",
        "numberInSurah": 70,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4972,
        "text": "Also welche von den Wohltaten eures HERRN leugnet ihr beide ab?!",
        "numberInSurah": 71,
        "juz": 27,
        "manzil": 7,
