{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 79,
    "name": "سُورَةُ النَّازِعَاتِ",
    "englishName": "An-Naazi'aat",
    "englishNameTranslation": "Those who drag forth",
    "revelationType": "Meccan",
    "numberOfAyahs": 46,
    "ayahs": [
      {
        "number": 5713,
        "text": "Bei den gründlichst Herausziehenden!",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5714,
        "text": "Bei den mit behutsamem Herausnehmen Herausnehmenden!",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5715,
        "text": "Bei den schwebend Schwebenden,",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5716,
        "text": "dann den im Wettlauf Führenden,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5717,
        "text": "dann den die Angelegenheit Regelnden!",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5718,
        "text": "An dem Tag, wenn das Bebende bebt,",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5719,
        "text": "dann ihr die Abhängige nachfolgt,",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5720,
        "text": "die Herzen an diesem Tag sind voller Furcht,",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5721,
        "text": "ihre Blicke sind vor Ehrfurcht gesenkt,",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5722,
        "text": "sie sagen: \"Werden wir etwa sicher zum vorherigen Zustand zurückgeführt,",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5723,
        "text": "auch dann nachdem wir modernde Knochen wurden?\"",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5724,
        "text": "Sie sagten: \"Dies ist dann sicher eine verlustreiche Umkehr.\"",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5725,
        "text": "Es ist doch nur ein einziger Schrei,",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5726,
        "text": "sogleich sind sie auf der flachen Ebene.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5727,
        "text": "Wurde dir die Mitteilung über Musa zuteil?!",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5728,
        "text": "Als sein HERR ihn im gereinigten Tuwa-Tal rief:",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5729,
        "text": "Gehe zu Pharao. Denn gewiß, er überschritt die Grenze,",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5730,
        "text": "so sag: \"Möchtest du, daß du dich läuterst,",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5731,
        "text": "und daß ich dich zu deinem HERRN rechtleite, dann du ehrfürchtig wirst?\"",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5732,
        "text": "Dann zeigte er ihm die große Aya.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5733,
        "text": "So leugnete er ab und widersetzte sich,",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5734,
        "text": "dann kehrte er den Rücken und ging,",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5735,
        "text": "so versammelte er (die Leute) und rief,",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5736,
        "text": "dann sagte er: \"Ich bin euer hoher HERR.\"",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5737,
        "text": "Dann belangte ALLAH ihn mit der harten Bestrafung für das Letzte und Erste.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5738,
        "text": "Gewiß, darin ist doch eine Lehre für jeden, der ehrfürchtig ist.",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5739,
        "text": "Seid ihr etwa schwieriger in der Schöpfung oder der Himmel?! ER richtete ihn ein,",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5740,
        "text": "ER erhöhte seine Dicke, dann ebnete ER ihn,",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5741,
        "text": "und ER verfinsterte seine Nacht und ER brachte seine Morgendämmerung hervor.",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5742,
        "text": "Und die Erde danach dehnte ER aus.",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5743,
        "text": "ER ließ aus ihr ihr Wasser und ihre Weide herauskommen.",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5744,
        "text": "Und die Berge ließ ER festigen",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5745,
        "text": "als Verbrauchsgut für euch und für eure An'am.",
        "numberInSurah": 33,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5746,
        "text": "Wenn dann die große Katastrophe kommt,",
        "numberInSurah": 34,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5747,
        "text": "an diesem Tag erinnert sich der Mensch dessen, was er erstrebte,",
        "numberInSurah": 35,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5748,
        "text": "und die Hölle wurde hervorgebracht für den, der sieht.",
        "numberInSurah": 36,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5749,
        "text": "Also hinsichtlich desjenigen, der die Grenze überschritt",
        "numberInSurah": 37,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5750,
        "text": "und das diesseitige Leben bevorzugte,",
        "numberInSurah": 38,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5751,
        "text": "so ist gewiß die Hölle die Unterkunft.",
        "numberInSurah": 39,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5752,
        "text": "Und hinsichtlich desjenigen, der die Vorstellung vor seinem HERRN fürchtet und die Seele der Zuneigung enthalten ließ,",
        "numberInSurah": 40,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5753,
        "text": "so ist gewiß die Dschanna die Unterkunft.",
        "numberInSurah": 41,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5754,
        "text": "Sie fragen dich nach der Stunde: \"Wann ist ihr Anbrechen?\"",
        "numberInSurah": 42,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5755,
        "text": "Und was hast du mit ihrer Erwähnung zu tun?!",
        "numberInSurah": 43,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5756,
        "text": "Bei deinem HERRN ist das Letzte über sie.",
        "numberInSurah": 44,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5757,
        "text": "Du bist doch nur ein Warner für denjenigen, der vor ihr Ehrfurcht hat.",
        "numberInSurah": 45,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5758,
        "text": "Als hätten sie, wenn sie sehen, nicht verweilt außer einer Nacht und ihrer Morgendämmerung.",
        "numberInSurah": 46,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "de.zaidan",
      "language": "de",
      "name": "Zaidan",
      "englishName": "Amir Zaidan",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}