  "data": {
    "number": 105,
    "name": "سُورَةُ الفِيلِ",
    "englishName": "Al-Fil",
    "englishNameTranslation": "The Elephant",
    "revelationType": "Meccan",
    "numberOfAyahs": 5,
    "ayahs": [
      {
        "number": 6189,
        "text": "Әй, Мухәммәд г-м күрмәдеңме, синең Раббың фил ияләреңә нинди җәза биргәнен? (Мухәммәд г-м туган елны Йәмән вәлисе Әбрәһә, хаҗ кылуны Мәккәдән туктатып үзенең шәһәренә күчерү өчен Кәгъбәтүлланы җимереп юк итәм, дип филле ґәскәр белән Мәккәгә килде. Ләкин теләгенә ирешә алмады, Аллаһ аларның өсләренә таш яудырып һәлак итте).",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 547,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6190,
        "text": "Әйә Раббың аларның мәкерләрен батыл кылып, тырышуларын бушка чыгарды түгелме?",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 547,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6191,
        "text": "Вә һәлак итмәк өчен алар өстенә төркем-төркем кошлар җибәрде.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 547,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6192,
        "text": "Кошлар аларны коелган таш белән атарлар.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 547,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6193,
        "text": "Аларны тапталган, чәйнәлгән салам кеби кылды.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 547,
        "hizbQuarter": 240,
        "sajda": false
      }
