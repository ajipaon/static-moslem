        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3708,
        "text": "إِنَّكَ لَمِنَ المُرسَلينَ",
        "numberInSurah": 3,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3709,
        "text": "عَلىٰ صِرٰطٍ مُستَقيمٍ",
        "numberInSurah": 4,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3710,
        "text": "تَنزيلَ العَزيزِ الرَّحيمِ",
        "numberInSurah": 5,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3711,
        "text": "لِتُنذِرَ قَومًا ما أُنذِرَ ءاباؤُهُم فَهُم غٰفِلونَ",
        "numberInSurah": 6,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3712,
        "text": "لَقَد حَقَّ القَولُ عَلىٰ أَكثَرِهِم فَهُم لا يُؤمِنونَ",
        "numberInSurah": 7,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3713,
        "text": "إِنّا جَعَلنا فى أَعنٰقِهِم أَغلٰلًا فَهِىَ إِلَى الأَذقانِ فَهُم مُقمَحونَ",
        "numberInSurah": 8,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3714,
        "text": "وَجَعَلنا مِن بَينِ أَيديهِم سَدًّا وَمِن خَلفِهِم سَدًّا فَأَغشَينٰهُم فَهُم لا يُبصِرونَ",
        "numberInSurah": 9,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3715,
        "text": "وَسَواءٌ عَلَيهِم ءَأَنذَرتَهُم أَم لَم تُنذِرهُم لا يُؤمِنونَ",
        "numberInSurah": 10,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3716,
        "text": "إِنَّما تُنذِرُ مَنِ اتَّبَعَ الذِّكرَ وَخَشِىَ الرَّحمٰنَ بِالغَيبِ ۖ فَبَشِّرهُ بِمَغفِرَةٍ وَأَجرٍ كَريمٍ",
        "numberInSurah": 11,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3717,
        "text": "إِنّا نَحنُ نُحىِ المَوتىٰ وَنَكتُبُ ما قَدَّموا وَءاثٰرَهُم ۚ وَكُلَّ شَيءٍ أَحصَينٰهُ فى إِمامٍ مُبينٍ",
        "numberInSurah": 12,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3718,
        "text": "وَاضرِب لَهُم مَثَلًا أَصحٰبَ القَريَةِ إِذ جاءَهَا المُرسَلونَ",
        "numberInSurah": 13,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3719,
        "text": "إِذ أَرسَلنا إِلَيهِمُ اثنَينِ فَكَذَّبوهُما فَعَزَّزنا بِثالِثٍ فَقالوا إِنّا إِلَيكُم مُرسَلونَ",
        "numberInSurah": 14,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3720,
        "text": "قالوا ما أَنتُم إِلّا بَشَرٌ مِثلُنا وَما أَنزَلَ الرَّحمٰنُ مِن شَيءٍ إِن أَنتُم إِلّا تَكذِبونَ",
        "numberInSurah": 15,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3721,
        "text": "قالوا رَبُّنا يَعلَمُ إِنّا إِلَيكُم لَمُرسَلونَ",
        "numberInSurah": 16,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3722,
        "text": "وَما عَلَينا إِلَّا البَلٰغُ المُبينُ",
        "numberInSurah": 17,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3723,
        "text": "قالوا إِنّا تَطَيَّرنا بِكُم ۖ لَئِن لَم تَنتَهوا لَنَرجُمَنَّكُم وَلَيَمَسَّنَّكُم مِنّا عَذابٌ أَليمٌ",
        "numberInSurah": 18,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3724,
        "text": "قالوا طٰئِرُكُم مَعَكُم ۚ أَئِن ذُكِّرتُم ۚ بَل أَنتُم قَومٌ مُسرِفونَ",
        "numberInSurah": 19,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3725,
        "text": "وَجاءَ مِن أَقصَا المَدينَةِ رَجُلٌ يَسعىٰ قالَ يٰقَومِ اتَّبِعُوا المُرسَلينَ",
        "numberInSurah": 20,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3726,
        "text": "اتَّبِعوا مَن لا يَسـَٔلُكُم أَجرًا وَهُم مُهتَدونَ",
        "numberInSurah": 21,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3727,
        "text": "وَما لِىَ لا أَعبُدُ الَّذى فَطَرَنى وَإِلَيهِ تُرجَعونَ",
        "numberInSurah": 22,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3728,
        "text": "ءَأَتَّخِذُ مِن دونِهِ ءالِهَةً إِن يُرِدنِ الرَّحمٰنُ بِضُرٍّ لا تُغنِ عَنّى شَفٰعَتُهُم شَيـًٔا وَلا يُنقِذونِ",
        "numberInSurah": 23,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3729,
        "text": "إِنّى إِذًا لَفى ضَلٰلٍ مُبينٍ",
        "numberInSurah": 24,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3730,
        "text": "إِنّى ءامَنتُ بِرَبِّكُم فَاسمَعونِ",
        "numberInSurah": 25,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3731,
        "text": "قيلَ ادخُلِ الجَنَّةَ ۖ قالَ يٰلَيتَ قَومى يَعلَمونَ",
        "numberInSurah": 26,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3732,
        "text": "بِما غَفَرَ لى رَبّى وَجَعَلَنى مِنَ المُكرَمينَ",
        "numberInSurah": 27,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3733,
        "text": "۞ وَما أَنزَلنا عَلىٰ قَومِهِ مِن بَعدِهِ مِن جُندٍ مِنَ السَّماءِ وَما كُنّا مُنزِلينَ",
        "numberInSurah": 28,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 382,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3734,
        "text": "إِن كانَت إِلّا صَيحَةً وٰحِدَةً فَإِذا هُم خٰمِدونَ",
        "numberInSurah": 29,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 382,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3735,
        "text": "يٰحَسرَةً عَلَى العِبادِ ۚ ما يَأتيهِم مِن رَسولٍ إِلّا كانوا بِهِ يَستَهزِءونَ",
        "numberInSurah": 30,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 382,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3736,
        "text": "أَلَم يَرَوا كَم أَهلَكنا قَبلَهُم مِنَ القُرونِ أَنَّهُم إِلَيهِم لا يَرجِعونَ",
        "numberInSurah": 31,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 382,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3737,
        "text": "وَإِن كُلٌّ لَمّا جَميعٌ لَدَينا مُحضَرونَ",
        "numberInSurah": 32,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 382,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3738,
        "text": "وَءايَةٌ لَهُمُ الأَرضُ المَيتَةُ أَحيَينٰها وَأَخرَجنا مِنها حَبًّا فَمِنهُ يَأكُلونَ",
        "numberInSurah": 33,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3739,
        "text": "وَجَعَلنا فيها جَنّٰتٍ مِن نَخيلٍ وَأَعنٰبٍ وَفَجَّرنا فيها مِنَ العُيونِ",
        "numberInSurah": 34,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3740,
        "text": "لِيَأكُلوا مِن ثَمَرِهِ وَما عَمِلَتهُ أَيديهِم ۖ أَفَلا يَشكُرونَ",
        "numberInSurah": 35,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3741,
        "text": "سُبحٰنَ الَّذى خَلَقَ الأَزوٰجَ كُلَّها مِمّا تُنبِتُ الأَرضُ وَمِن أَنفُسِهِم وَمِمّا لا يَعلَمونَ",
        "numberInSurah": 36,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3742,
        "text": "وَءايَةٌ لَهُمُ الَّيلُ نَسلَخُ مِنهُ النَّهارَ فَإِذا هُم مُظلِمونَ",
        "numberInSurah": 37,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3743,
        "text": "وَالشَّمسُ تَجرى لِمُستَقَرٍّ لَها ۚ ذٰلِكَ تَقديرُ العَزيزِ العَليمِ",
        "numberInSurah": 38,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3744,
        "text": "وَالقَمَرَ قَدَّرنٰهُ مَنازِلَ حَتّىٰ عادَ كَالعُرجونِ القَديمِ",
        "numberInSurah": 39,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3745,
        "text": "لَا الشَّمسُ يَنبَغى لَها أَن تُدرِكَ القَمَرَ وَلَا الَّيلُ سابِقُ النَّهارِ ۚ وَكُلٌّ فى فَلَكٍ يَسبَحونَ",
        "numberInSurah": 40,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3746,
        "text": "وَءايَةٌ لَهُم أَنّا حَمَلنا ذُرِّيَّتَهُم فِى الفُلكِ المَشحونِ",
        "numberInSurah": 41,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3747,
        "text": "وَخَلَقنا لَهُم مِن مِثلِهِ ما يَركَبونَ",
        "numberInSurah": 42,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3748,
        "text": "وَإِن نَشَأ نُغرِقهُم فَلا صَريخَ لَهُم وَلا هُم يُنقَذونَ",
        "numberInSurah": 43,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3749,
        "text": "إِلّا رَحمَةً مِنّا وَمَتٰعًا إِلىٰ حينٍ",
        "numberInSurah": 44,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3750,
        "text": "وَإِذا قيلَ لَهُمُ اتَّقوا ما بَينَ أَيديكُم وَما خَلفَكُم لَعَلَّكُم تُرحَمونَ",
        "numberInSurah": 45,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3751,
        "text": "وَما تَأتيهِم مِن ءايَةٍ مِن ءايٰتِ رَبِّهِم إِلّا كانوا عَنها مُعرِضينَ",
        "numberInSurah": 46,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3752,
        "text": "وَإِذا قيلَ لَهُم أَنفِقوا مِمّا رَزَقَكُمُ اللَّهُ قالَ الَّذينَ كَفَروا لِلَّذينَ ءامَنوا أَنُطعِمُ مَن لَو يَشاءُ اللَّهُ أَطعَمَهُ إِن أَنتُم إِلّا فى ضَلٰلٍ مُبينٍ",
        "numberInSurah": 47,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3753,
        "text": "وَيَقولونَ مَتىٰ هٰذَا الوَعدُ إِن كُنتُم صٰدِقينَ",
        "numberInSurah": 48,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3754,
        "text": "ما يَنظُرونَ إِلّا صَيحَةً وٰحِدَةً تَأخُذُهُم وَهُم يَخِصِّمونَ",
        "numberInSurah": 49,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3755,
        "text": "فَلا يَستَطيعونَ تَوصِيَةً وَلا إِلىٰ أَهلِهِم يَرجِعونَ",
        "numberInSurah": 50,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3756,
        "text": "وَنُفِخَ فِى الصّورِ فَإِذا هُم مِنَ الأَجداثِ إِلىٰ رَبِّهِم يَنسِلونَ",
        "numberInSurah": 51,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3757,
        "text": "قالوا يٰوَيلَنا مَن بَعَثَنا مِن مَرقَدِنا ۜ ۗ هٰذا ما وَعَدَ الرَّحمٰنُ وَصَدَقَ المُرسَلونَ",
        "numberInSurah": 52,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3758,
        "text": "إِن كانَت إِلّا صَيحَةً وٰحِدَةً فَإِذا هُم جَميعٌ لَدَينا مُحضَرونَ",
        "numberInSurah": 53,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3759,
        "text": "فَاليَومَ لا تُظلَمُ نَفسٌ شَيـًٔا وَلا تُجزَونَ إِلّا ما كُنتُم تَعمَلونَ",
        "numberInSurah": 54,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3760,
        "text": "إِنَّ أَصحٰبَ الجَنَّةِ اليَومَ فى شُغُلٍ فٰكِهونَ",
        "numberInSurah": 55,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3761,
        "text": "هُم وَأَزوٰجُهُم فى ظِلٰلٍ عَلَى الأَرائِكِ مُتَّكِـٔونَ",
        "numberInSurah": 56,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3762,
        "text": "لَهُم فيها فٰكِهَةٌ وَلَهُم ما يَدَّعونَ",
        "numberInSurah": 57,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3763,
        "text": "سَلٰمٌ قَولًا مِن رَبٍّ رَحيمٍ",
        "numberInSurah": 58,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3764,
        "text": "وَامتٰزُوا اليَومَ أَيُّهَا المُجرِمونَ",
        "numberInSurah": 59,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3765,
        "text": "۞ أَلَم أَعهَد إِلَيكُم يٰبَنى ءادَمَ أَن لا تَعبُدُوا الشَّيطٰنَ ۖ إِنَّهُ لَكُم عَدُوٌّ مُبينٌ",
        "numberInSurah": 60,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3766,
        "text": "وَأَنِ اعبُدونى ۚ هٰذا صِرٰطٌ مُستَقيمٌ",
        "numberInSurah": 61,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3767,
        "text": "وَلَقَد أَضَلَّ مِنكُم جِبِلًّا كَثيرًا ۖ أَفَلَم تَكونوا تَعقِلونَ",
        "numberInSurah": 62,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3768,
        "text": "هٰذِهِ جَهَنَّمُ الَّتى كُنتُم توعَدونَ",
        "numberInSurah": 63,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3769,
        "text": "اصلَوهَا اليَومَ بِما كُنتُم تَكفُرونَ",
        "numberInSurah": 64,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3770,
        "text": "اليَومَ نَختِمُ عَلىٰ أَفوٰهِهِم وَتُكَلِّمُنا أَيديهِم وَتَشهَدُ أَرجُلُهُم بِما كانوا يَكسِبونَ",
        "numberInSurah": 65,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3771,
        "text": "وَلَو نَشاءُ لَطَمَسنا عَلىٰ أَعيُنِهِم فَاستَبَقُوا الصِّرٰطَ فَأَنّىٰ يُبصِرونَ",
        "numberInSurah": 66,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3772,
        "text": "وَلَو نَشاءُ لَمَسَخنٰهُم عَلىٰ مَكانَتِهِم فَمَا استَطٰعوا مُضِيًّا وَلا يَرجِعونَ",
        "numberInSurah": 67,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3773,
        "text": "وَمَن نُعَمِّرهُ نُنَكِّسهُ فِى الخَلقِ ۖ أَفَلا يَعقِلونَ",
        "numberInSurah": 68,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3774,
        "text": "وَما عَلَّمنٰهُ الشِّعرَ وَما يَنبَغى لَهُ ۚ إِن هُوَ إِلّا ذِكرٌ وَقُرءانٌ مُبينٌ",
        "numberInSurah": 69,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3775,
        "text": "لِيُنذِرَ مَن كانَ حَيًّا وَيَحِقَّ القَولُ عَلَى الكٰفِرينَ",
        "numberInSurah": 70,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3776,
        "text": "أَوَلَم يَرَوا أَنّا خَلَقنا لَهُم مِمّا عَمِلَت أَيدينا أَنعٰمًا فَهُم لَها مٰلِكونَ",
        "numberInSurah": 71,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3777,
        "text": "وَذَلَّلنٰها لَهُم فَمِنها رَكوبُهُم وَمِنها يَأكُلونَ",
        "numberInSurah": 72,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3778,
        "text": "وَلَهُم فيها مَنٰفِعُ وَمَشارِبُ ۖ أَفَلا يَشكُرونَ",
        "numberInSurah": 73,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3779,
        "text": "وَاتَّخَذوا مِن دونِ اللَّهِ ءالِهَةً لَعَلَّهُم يُنصَرونَ",
        "numberInSurah": 74,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3780,
        "text": "لا يَستَطيعونَ نَصرَهُم وَهُم لَهُم جُندٌ مُحضَرونَ",
        "numberInSurah": 75,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3781,
        "text": "فَلا يَحزُنكَ قَولُهُم ۘ إِنّا نَعلَمُ ما يُسِرّونَ وَما يُعلِنونَ",
        "numberInSurah": 76,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3782,
        "text": "أَوَلَم يَرَ الإِنسٰنُ أَنّا خَلَقنٰهُ مِن نُطفَةٍ فَإِذا هُوَ خَصيمٌ مُبينٌ",
        "numberInSurah": 77,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3783,
        "text": "وَضَرَبَ لَنا مَثَلًا وَنَسِىَ خَلقَهُ ۖ قالَ مَن يُحىِ العِظٰمَ وَهِىَ رَميمٌ",
        "numberInSurah": 78,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3784,
        "text": "قُل يُحييهَا الَّذى أَنشَأَها أَوَّلَ مَرَّةٍ ۖ وَهُوَ بِكُلِّ خَلقٍ عَليمٌ",
        "numberInSurah": 79,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3785,
        "text": "الَّذى جَعَلَ لَكُم مِنَ الشَّجَرِ الأَخضَرِ نارًا فَإِذا أَنتُم مِنهُ توقِدونَ",
        "numberInSurah": 80,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3786,
        "text": "أَوَلَيسَ الَّذى خَلَقَ السَّمٰوٰتِ وَالأَرضَ بِقٰدِرٍ عَلىٰ أَن يَخلُقَ مِثلَهُم ۚ بَلىٰ وَهُوَ الخَلّٰقُ العَليمُ",
        "numberInSurah": 81,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3787,
        "text": "إِنَّما أَمرُهُ إِذا أَرادَ شَيـًٔا أَن يَقولَ لَهُ كُن فَيَكونُ",
        "numberInSurah": 82,
        "juz": 23,
        "manzil": 5,
        "page": 445,
        "ruku": 385,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3788,
        "text": "فَسُبحٰنَ الَّذى بِيَدِهِ مَلَكوتُ كُلِّ شَيءٍ وَإِلَيهِ تُرجَعونَ",
        "numberInSurah": 83,
        "juz": 23,
