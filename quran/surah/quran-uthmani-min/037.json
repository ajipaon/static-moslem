  "data": {
    "number": 37,
    "name": "سُورَةُ الصَّافَّاتِ",
    "englishName": "As-Saaffaat",
    "englishNameTranslation": "Those drawn up in Ranks",
    "revelationType": "Meccan",
    "numberOfAyahs": 182,
    "ayahs": [
      {
        "number": 3789,
        "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ وَالصّٰفّٰتِ صَفًّا",
        "numberInSurah": 1,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3790,
        "text": "فَالزّٰجِرٰتِ زَجرًا",
        "numberInSurah": 2,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3791,
        "text": "فَالتّٰلِيٰتِ ذِكرًا",
        "numberInSurah": 3,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3792,
        "text": "إِنَّ إِلٰهَكُم لَوٰحِدٌ",
        "numberInSurah": 4,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3793,
        "text": "رَبُّ السَّمٰوٰتِ وَالأَرضِ وَما بَينَهُما وَرَبُّ المَشٰرِقِ",
        "numberInSurah": 5,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3794,
        "text": "إِنّا زَيَّنَّا السَّماءَ الدُّنيا بِزينَةٍ الكَواكِبِ",
        "numberInSurah": 6,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3795,
        "text": "وَحِفظًا مِن كُلِّ شَيطٰنٍ مارِدٍ",
        "numberInSurah": 7,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3796,
        "text": "لا يَسَّمَّعونَ إِلَى المَلَإِ الأَعلىٰ وَيُقذَفونَ مِن كُلِّ جانِبٍ",
        "numberInSurah": 8,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3797,
        "text": "دُحورًا ۖ وَلَهُم عَذابٌ واصِبٌ",
        "numberInSurah": 9,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3798,
        "text": "إِلّا مَن خَطِفَ الخَطفَةَ فَأَتبَعَهُ شِهابٌ ثاقِبٌ",
        "numberInSurah": 10,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3799,
        "text": "فَاستَفتِهِم أَهُم أَشَدُّ خَلقًا أَم مَن خَلَقنا ۚ إِنّا خَلَقنٰهُم مِن طينٍ لازِبٍ",
        "numberInSurah": 11,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3800,
        "text": "بَل عَجِبتَ وَيَسخَرونَ",
        "numberInSurah": 12,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3801,
        "text": "وَإِذا ذُكِّروا لا يَذكُرونَ",
        "numberInSurah": 13,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3802,
        "text": "وَإِذا رَأَوا ءايَةً يَستَسخِرونَ",
        "numberInSurah": 14,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3803,
        "text": "وَقالوا إِن هٰذا إِلّا سِحرٌ مُبينٌ",
        "numberInSurah": 15,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3804,
        "text": "أَءِذا مِتنا وَكُنّا تُرابًا وَعِظٰمًا أَءِنّا لَمَبعوثونَ",
        "numberInSurah": 16,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3805,
        "text": "أَوَءاباؤُنَا الأَوَّلونَ",
        "numberInSurah": 17,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3806,
        "text": "قُل نَعَم وَأَنتُم دٰخِرونَ",
        "numberInSurah": 18,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3807,
        "text": "فَإِنَّما هِىَ زَجرَةٌ وٰحِدَةٌ فَإِذا هُم يَنظُرونَ",
        "numberInSurah": 19,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3808,
        "text": "وَقالوا يٰوَيلَنا هٰذا يَومُ الدّينِ",
        "numberInSurah": 20,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3809,
        "text": "هٰذا يَومُ الفَصلِ الَّذى كُنتُم بِهِ تُكَذِّبونَ",
        "numberInSurah": 21,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3810,
        "text": "۞ احشُرُوا الَّذينَ ظَلَموا وَأَزوٰجَهُم وَما كانوا يَعبُدونَ",
        "numberInSurah": 22,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3811,
        "text": "مِن دونِ اللَّهِ فَاهدوهُم إِلىٰ صِرٰطِ الجَحيمِ",
        "numberInSurah": 23,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3812,
        "text": "وَقِفوهُم ۖ إِنَّهُم مَسـٔولونَ",
        "numberInSurah": 24,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3813,
        "text": "ما لَكُم لا تَناصَرونَ",
        "numberInSurah": 25,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3814,
        "text": "بَل هُمُ اليَومَ مُستَسلِمونَ",
        "numberInSurah": 26,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3815,
        "text": "وَأَقبَلَ بَعضُهُم عَلىٰ بَعضٍ يَتَساءَلونَ",
        "numberInSurah": 27,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3816,
        "text": "قالوا إِنَّكُم كُنتُم تَأتونَنا عَنِ اليَمينِ",
        "numberInSurah": 28,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3817,
        "text": "قالوا بَل لَم تَكونوا مُؤمِنينَ",
        "numberInSurah": 29,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3818,
        "text": "وَما كانَ لَنا عَلَيكُم مِن سُلطٰنٍ ۖ بَل كُنتُم قَومًا طٰغينَ",
        "numberInSurah": 30,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3819,
        "text": "فَحَقَّ عَلَينا قَولُ رَبِّنا ۖ إِنّا لَذائِقونَ",
        "numberInSurah": 31,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3820,
        "text": "فَأَغوَينٰكُم إِنّا كُنّا غٰوينَ",
        "numberInSurah": 32,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3821,
        "text": "فَإِنَّهُم يَومَئِذٍ فِى العَذابِ مُشتَرِكونَ",
        "numberInSurah": 33,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3822,
        "text": "إِنّا كَذٰلِكَ نَفعَلُ بِالمُجرِمينَ",
        "numberInSurah": 34,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3823,
        "text": "إِنَّهُم كانوا إِذا قيلَ لَهُم لا إِلٰهَ إِلَّا اللَّهُ يَستَكبِرونَ",
        "numberInSurah": 35,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3824,
        "text": "وَيَقولونَ أَئِنّا لَتارِكوا ءالِهَتِنا لِشاعِرٍ مَجنونٍ",
        "numberInSurah": 36,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3825,
        "text": "بَل جاءَ بِالحَقِّ وَصَدَّقَ المُرسَلينَ",
        "numberInSurah": 37,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3826,
        "text": "إِنَّكُم لَذائِقُوا العَذابِ الأَليمِ",
        "numberInSurah": 38,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3827,
        "text": "وَما تُجزَونَ إِلّا ما كُنتُم تَعمَلونَ",
        "numberInSurah": 39,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3828,
        "text": "إِلّا عِبادَ اللَّهِ المُخلَصينَ",
        "numberInSurah": 40,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3829,
        "text": "أُولٰئِكَ لَهُم رِزقٌ مَعلومٌ",
        "numberInSurah": 41,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3830,
        "text": "فَوٰكِهُ ۖ وَهُم مُكرَمونَ",
        "numberInSurah": 42,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3831,
        "text": "فى جَنّٰتِ النَّعيمِ",
        "numberInSurah": 43,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3832,
        "text": "عَلىٰ سُرُرٍ مُتَقٰبِلينَ",
        "numberInSurah": 44,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3833,
        "text": "يُطافُ عَلَيهِم بِكَأسٍ مِن مَعينٍ",
        "numberInSurah": 45,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3834,
        "text": "بَيضاءَ لَذَّةٍ لِلشّٰرِبينَ",
        "numberInSurah": 46,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3835,
        "text": "لا فيها غَولٌ وَلا هُم عَنها يُنزَفونَ",
        "numberInSurah": 47,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3836,
        "text": "وَعِندَهُم قٰصِرٰتُ الطَّرفِ عينٌ",
        "numberInSurah": 48,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3837,
        "text": "كَأَنَّهُنَّ بَيضٌ مَكنونٌ",
        "numberInSurah": 49,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3838,
        "text": "فَأَقبَلَ بَعضُهُم عَلىٰ بَعضٍ يَتَساءَلونَ",
        "numberInSurah": 50,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3839,
        "text": "قالَ قائِلٌ مِنهُم إِنّى كانَ لى قَرينٌ",
        "numberInSurah": 51,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3840,
        "text": "يَقولُ أَءِنَّكَ لَمِنَ المُصَدِّقينَ",
        "numberInSurah": 52,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3841,
        "text": "أَءِذا مِتنا وَكُنّا تُرابًا وَعِظٰمًا أَءِنّا لَمَدينونَ",
        "numberInSurah": 53,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3842,
        "text": "قالَ هَل أَنتُم مُطَّلِعونَ",
        "numberInSurah": 54,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3843,
        "text": "فَاطَّلَعَ فَرَءاهُ فى سَواءِ الجَحيمِ",
        "numberInSurah": 55,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3844,
        "text": "قالَ تَاللَّهِ إِن كِدتَ لَتُردينِ",
        "numberInSurah": 56,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3845,
        "text": "وَلَولا نِعمَةُ رَبّى لَكُنتُ مِنَ المُحضَرينَ",
        "numberInSurah": 57,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3846,
        "text": "أَفَما نَحنُ بِمَيِّتينَ",
        "numberInSurah": 58,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3847,
        "text": "إِلّا مَوتَتَنَا الأولىٰ وَما نَحنُ بِمُعَذَّبينَ",
        "numberInSurah": 59,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3848,
        "text": "إِنَّ هٰذا لَهُوَ الفَوزُ العَظيمُ",
        "numberInSurah": 60,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3849,
        "text": "لِمِثلِ هٰذا فَليَعمَلِ العٰمِلونَ",
        "numberInSurah": 61,
        "juz": 23,
