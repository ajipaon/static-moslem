        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3852,
        "text": "إِنَّها شَجَرَةٌ تَخرُجُ فى أَصلِ الجَحيمِ",
        "numberInSurah": 64,
        "juz": 23,
