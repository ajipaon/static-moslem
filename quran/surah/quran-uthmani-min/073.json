        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5479,
        "text": "أَو زِد عَلَيهِ وَرَتِّلِ القُرءانَ تَرتيلًا",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5480,
        "text": "إِنّا سَنُلقى عَلَيكَ قَولًا ثَقيلًا",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5481,
        "text": "إِنَّ ناشِئَةَ الَّيلِ هِىَ أَشَدُّ وَطـًٔا وَأَقوَمُ قيلًا",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5482,
        "text": "إِنَّ لَكَ فِى النَّهارِ سَبحًا طَويلًا",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5483,
        "text": "وَاذكُرِ اسمَ رَبِّكَ وَتَبَتَّل إِلَيهِ تَبتيلًا",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5484,
        "text": "رَبُّ المَشرِقِ وَالمَغرِبِ لا إِلٰهَ إِلّا هُوَ فَاتَّخِذهُ وَكيلًا",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5485,
        "text": "وَاصبِر عَلىٰ ما يَقولونَ وَاهجُرهُم هَجرًا جَميلًا",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5486,
        "text": "وَذَرنى وَالمُكَذِّبينَ أُولِى النَّعمَةِ وَمَهِّلهُم قَليلًا",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 574,
        "ruku": 508,
        "hizbQuarter": 229,
        "sajda": false
      },
      {
        "number": 5487,
        "text": "إِنَّ لَدَينا أَنكالًا وَجَحيمًا",
