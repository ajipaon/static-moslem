  "data": {
    "number": 44,
    "name": "سُورَةُ الدُّخَانِ",
    "englishName": "Ad-Dukhaan",
    "englishNameTranslation": "The Smoke",
    "revelationType": "Meccan",
    "numberOfAyahs": 59,
    "ayahs": [
      {
        "number": 4415,
        "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ حم",
        "numberInSurah": 1,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4416,
        "text": "وَالكِتٰبِ المُبينِ",
        "numberInSurah": 2,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4417,
        "text": "إِنّا أَنزَلنٰهُ فى لَيلَةٍ مُبٰرَكَةٍ ۚ إِنّا كُنّا مُنذِرينَ",
        "numberInSurah": 3,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4418,
        "text": "فيها يُفرَقُ كُلُّ أَمرٍ حَكيمٍ",
        "numberInSurah": 4,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4419,
        "text": "أَمرًا مِن عِندِنا ۚ إِنّا كُنّا مُرسِلينَ",
        "numberInSurah": 5,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4420,
        "text": "رَحمَةً مِن رَبِّكَ ۚ إِنَّهُ هُوَ السَّميعُ العَليمُ",
        "numberInSurah": 6,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4421,
        "text": "رَبِّ السَّمٰوٰتِ وَالأَرضِ وَما بَينَهُما ۖ إِن كُنتُم موقِنينَ",
        "numberInSurah": 7,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4422,
        "text": "لا إِلٰهَ إِلّا هُوَ يُحيۦ وَيُميتُ ۖ رَبُّكُم وَرَبُّ ءابائِكُمُ الأَوَّلينَ",
        "numberInSurah": 8,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4423,
        "text": "بَل هُم فى شَكٍّ يَلعَبونَ",
        "numberInSurah": 9,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4424,
        "text": "فَارتَقِب يَومَ تَأتِى السَّماءُ بِدُخانٍ مُبينٍ",
        "numberInSurah": 10,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4425,
        "text": "يَغشَى النّاسَ ۖ هٰذا عَذابٌ أَليمٌ",
        "numberInSurah": 11,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4426,
        "text": "رَبَّنَا اكشِف عَنَّا العَذابَ إِنّا مُؤمِنونَ",
        "numberInSurah": 12,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4427,
        "text": "أَنّىٰ لَهُمُ الذِّكرىٰ وَقَد جاءَهُم رَسولٌ مُبينٌ",
        "numberInSurah": 13,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4428,
        "text": "ثُمَّ تَوَلَّوا عَنهُ وَقالوا مُعَلَّمٌ مَجنونٌ",
        "numberInSurah": 14,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4429,
        "text": "إِنّا كاشِفُوا العَذابِ قَليلًا ۚ إِنَّكُم عائِدونَ",
        "numberInSurah": 15,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4430,
        "text": "يَومَ نَبطِشُ البَطشَةَ الكُبرىٰ إِنّا مُنتَقِمونَ",
        "numberInSurah": 16,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4431,
        "text": "۞ وَلَقَد فَتَنّا قَبلَهُم قَومَ فِرعَونَ وَجاءَهُم رَسولٌ كَريمٌ",
        "numberInSurah": 17,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4432,
        "text": "أَن أَدّوا إِلَىَّ عِبادَ اللَّهِ ۖ إِنّى لَكُم رَسولٌ أَمينٌ",
        "numberInSurah": 18,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4433,
        "text": "وَأَن لا تَعلوا عَلَى اللَّهِ ۖ إِنّى ءاتيكُم بِسُلطٰنٍ مُبينٍ",
        "numberInSurah": 19,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4434,
        "text": "وَإِنّى عُذتُ بِرَبّى وَرَبِّكُم أَن تَرجُمونِ",
        "numberInSurah": 20,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4435,
        "text": "وَإِن لَم تُؤمِنوا لى فَاعتَزِلونِ",
        "numberInSurah": 21,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4436,
        "text": "فَدَعا رَبَّهُ أَنَّ هٰؤُلاءِ قَومٌ مُجرِمونَ",
        "numberInSurah": 22,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4437,
        "text": "فَأَسرِ بِعِبادى لَيلًا إِنَّكُم مُتَّبَعونَ",
        "numberInSurah": 23,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4438,
        "text": "وَاترُكِ البَحرَ رَهوًا ۖ إِنَّهُم جُندٌ مُغرَقونَ",
        "numberInSurah": 24,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4439,
        "text": "كَم تَرَكوا مِن جَنّٰتٍ وَعُيونٍ",
        "numberInSurah": 25,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4440,
        "text": "وَزُروعٍ وَمَقامٍ كَريمٍ",
        "numberInSurah": 26,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4441,
        "text": "وَنَعمَةٍ كانوا فيها فٰكِهينَ",
        "numberInSurah": 27,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4442,
        "text": "كَذٰلِكَ ۖ وَأَورَثنٰها قَومًا ءاخَرينَ",
        "numberInSurah": 28,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4443,
        "text": "فَما بَكَت عَلَيهِمُ السَّماءُ وَالأَرضُ وَما كانوا مُنظَرينَ",
        "numberInSurah": 29,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4444,
        "text": "وَلَقَد نَجَّينا بَنى إِسرٰءيلَ مِنَ العَذابِ المُهينِ",
        "numberInSurah": 30,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4445,
        "text": "مِن فِرعَونَ ۚ إِنَّهُ كانَ عالِيًا مِنَ المُسرِفينَ",
        "numberInSurah": 31,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4446,
        "text": "وَلَقَدِ اختَرنٰهُم عَلىٰ عِلمٍ عَلَى العٰلَمينَ",
        "numberInSurah": 32,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4447,
        "text": "وَءاتَينٰهُم مِنَ الءايٰتِ ما فيهِ بَلٰؤٌا۟ مُبينٌ",
        "numberInSurah": 33,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4448,
        "text": "إِنَّ هٰؤُلاءِ لَيَقولونَ",
        "numberInSurah": 34,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4449,
        "text": "إِن هِىَ إِلّا مَوتَتُنَا الأولىٰ وَما نَحنُ بِمُنشَرينَ",
        "numberInSurah": 35,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4450,
        "text": "فَأتوا بِـٔابائِنا إِن كُنتُم صٰدِقينَ",
        "numberInSurah": 36,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4451,
        "text": "أَهُم خَيرٌ أَم قَومُ تُبَّعٍ وَالَّذينَ مِن قَبلِهِم ۚ أَهلَكنٰهُم ۖ إِنَّهُم كانوا مُجرِمينَ",
        "numberInSurah": 37,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4452,
        "text": "وَما خَلَقنَا السَّمٰوٰتِ وَالأَرضَ وَما بَينَهُما لٰعِبينَ",
        "numberInSurah": 38,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4453,
        "text": "ما خَلَقنٰهُما إِلّا بِالحَقِّ وَلٰكِنَّ أَكثَرَهُم لا يَعلَمونَ",
        "numberInSurah": 39,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4454,
        "text": "إِنَّ يَومَ الفَصلِ ميقٰتُهُم أَجمَعينَ",
        "numberInSurah": 40,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4455,
        "text": "يَومَ لا يُغنى مَولًى عَن مَولًى شَيـًٔا وَلا هُم يُنصَرونَ",
        "numberInSurah": 41,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4456,
        "text": "إِلّا مَن رَحِمَ اللَّهُ ۚ إِنَّهُ هُوَ العَزيزُ الرَّحيمُ",
        "numberInSurah": 42,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4457,
        "text": "إِنَّ شَجَرَتَ الزَّقّومِ",
        "numberInSurah": 43,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4458,
        "text": "طَعامُ الأَثيمِ",
        "numberInSurah": 44,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4459,
        "text": "كَالمُهلِ يَغلى فِى البُطونِ",
        "numberInSurah": 45,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4460,
        "text": "كَغَلىِ الحَميمِ",
        "numberInSurah": 46,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4461,
        "text": "خُذوهُ فَاعتِلوهُ إِلىٰ سَواءِ الجَحيمِ",
        "numberInSurah": 47,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4462,
        "text": "ثُمَّ صُبّوا فَوقَ رَأسِهِ مِن عَذابِ الحَميمِ",
        "numberInSurah": 48,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4463,
        "text": "ذُق إِنَّكَ أَنتَ العَزيزُ الكَريمُ",
        "numberInSurah": 49,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4464,
        "text": "إِنَّ هٰذا ما كُنتُم بِهِ تَمتَرونَ",
        "numberInSurah": 50,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4465,
        "text": "إِنَّ المُتَّقينَ فى مَقامٍ أَمينٍ",
        "numberInSurah": 51,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4466,
        "text": "فى جَنّٰتٍ وَعُيونٍ",
        "numberInSurah": 52,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4467,
        "text": "يَلبَسونَ مِن سُندُسٍ وَإِستَبرَقٍ مُتَقٰبِلينَ",
        "numberInSurah": 53,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4468,
        "text": "كَذٰلِكَ وَزَوَّجنٰهُم بِحورٍ عينٍ",
        "numberInSurah": 54,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4469,
        "text": "يَدعونَ فيها بِكُلِّ فٰكِهَةٍ ءامِنينَ",
        "numberInSurah": 55,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4470,
        "text": "لا يَذوقونَ فيهَا المَوتَ إِلَّا المَوتَةَ الأولىٰ ۖ وَوَقىٰهُم عَذابَ الجَحيمِ",
        "numberInSurah": 56,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4471,
        "text": "فَضلًا مِن رَبِّكَ ۚ ذٰلِكَ هُوَ الفَوزُ العَظيمُ",
        "numberInSurah": 57,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4472,
        "text": "فَإِنَّما يَسَّرنٰهُ بِلِسانِكَ لَعَلَّهُم يَتَذَكَّرونَ",
        "numberInSurah": 58,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
