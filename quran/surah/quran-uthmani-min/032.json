  "data": {
    "number": 32,
    "name": "سُورَةُ السَّجۡدَةِ",
    "englishName": "As-Sajda",
    "englishNameTranslation": "The Prostration",
    "revelationType": "Meccan",
    "numberOfAyahs": 30,
    "ayahs": [
      {
        "number": 3504,
        "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ الم",
        "numberInSurah": 1,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3505,
        "text": "تَنزيلُ الكِتٰبِ لا رَيبَ فيهِ مِن رَبِّ العٰلَمينَ",
        "numberInSurah": 2,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3506,
        "text": "أَم يَقولونَ افتَرىٰهُ ۚ بَل هُوَ الحَقُّ مِن رَبِّكَ لِتُنذِرَ قَومًا ما أَتىٰهُم مِن نَذيرٍ مِن قَبلِكَ لَعَلَّهُم يَهتَدونَ",
        "numberInSurah": 3,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3507,
        "text": "اللَّهُ الَّذى خَلَقَ السَّمٰوٰتِ وَالأَرضَ وَما بَينَهُما فى سِتَّةِ أَيّامٍ ثُمَّ استَوىٰ عَلَى العَرشِ ۖ ما لَكُم مِن دونِهِ مِن وَلِىٍّ وَلا شَفيعٍ ۚ أَفَلا تَتَذَكَّرونَ",
        "numberInSurah": 4,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3508,
        "text": "يُدَبِّرُ الأَمرَ مِنَ السَّماءِ إِلَى الأَرضِ ثُمَّ يَعرُجُ إِلَيهِ فى يَومٍ كانَ مِقدارُهُ أَلفَ سَنَةٍ مِمّا تَعُدّونَ",
        "numberInSurah": 5,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3509,
        "text": "ذٰلِكَ عٰلِمُ الغَيبِ وَالشَّهٰدَةِ العَزيزُ الرَّحيمُ",
        "numberInSurah": 6,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3510,
        "text": "الَّذى أَحسَنَ كُلَّ شَيءٍ خَلَقَهُ ۖ وَبَدَأَ خَلقَ الإِنسٰنِ مِن طينٍ",
        "numberInSurah": 7,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3511,
        "text": "ثُمَّ جَعَلَ نَسلَهُ مِن سُلٰلَةٍ مِن ماءٍ مَهينٍ",
        "numberInSurah": 8,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3512,
        "text": "ثُمَّ سَوّىٰهُ وَنَفَخَ فيهِ مِن روحِهِ ۖ وَجَعَلَ لَكُمُ السَّمعَ وَالأَبصٰرَ وَالأَفـِٔدَةَ ۚ قَليلًا ما تَشكُرونَ",
        "numberInSurah": 9,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3513,
        "text": "وَقالوا أَءِذا ضَلَلنا فِى الأَرضِ أَءِنّا لَفى خَلقٍ جَديدٍ ۚ بَل هُم بِلِقاءِ رَبِّهِم كٰفِرونَ",
        "numberInSurah": 10,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 165,
        "sajda": false
      },
      {
        "number": 3514,
        "text": "۞ قُل يَتَوَفّىٰكُم مَلَكُ المَوتِ الَّذى وُكِّلَ بِكُم ثُمَّ إِلىٰ رَبِّكُم تُرجَعونَ",
        "numberInSurah": 11,
        "juz": 21,
        "manzil": 5,
        "page": 415,
        "ruku": 358,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3515,
        "text": "وَلَو تَرىٰ إِذِ المُجرِمونَ ناكِسوا رُءوسِهِم عِندَ رَبِّهِم رَبَّنا أَبصَرنا وَسَمِعنا فَارجِعنا نَعمَل صٰلِحًا إِنّا موقِنونَ",
        "numberInSurah": 12,
        "juz": 21,
        "manzil": 5,
        "page": 416,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3516,
        "text": "وَلَو شِئنا لَءاتَينا كُلَّ نَفسٍ هُدىٰها وَلٰكِن حَقَّ القَولُ مِنّى لَأَملَأَنَّ جَهَنَّمَ مِنَ الجِنَّةِ وَالنّاسِ أَجمَعينَ",
        "numberInSurah": 13,
        "juz": 21,
        "manzil": 5,
        "page": 416,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3517,
        "text": "فَذوقوا بِما نَسيتُم لِقاءَ يَومِكُم هٰذا إِنّا نَسينٰكُم ۖ وَذوقوا عَذابَ الخُلدِ بِما كُنتُم تَعمَلونَ",
        "numberInSurah": 14,
        "juz": 21,
        "manzil": 5,
        "page": 416,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3518,
        "text": "إِنَّما يُؤمِنُ بِـٔايٰتِنَا الَّذينَ إِذا ذُكِّروا بِها خَرّوا سُجَّدًا وَسَبَّحوا بِحَمدِ رَبِّهِم وَهُم لا يَستَكبِرونَ ۩",
        "numberInSurah": 15,
        "juz": 21,
        "manzil": 5,
        "page": 416,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": {
          "id": 10,
          "recommended": false,
          "obligatory": true
        }
      },
      {
        "number": 3519,
        "text": "تَتَجافىٰ جُنوبُهُم عَنِ المَضاجِعِ يَدعونَ رَبَّهُم خَوفًا وَطَمَعًا وَمِمّا رَزَقنٰهُم يُنفِقونَ",
        "numberInSurah": 16,
        "juz": 21,
        "manzil": 5,
        "page": 416,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3520,
        "text": "فَلا تَعلَمُ نَفسٌ ما أُخفِىَ لَهُم مِن قُرَّةِ أَعيُنٍ جَزاءً بِما كانوا يَعمَلونَ",
        "numberInSurah": 17,
        "juz": 21,
        "manzil": 5,
        "page": 416,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3521,
        "text": "أَفَمَن كانَ مُؤمِنًا كَمَن كانَ فاسِقًا ۚ لا يَستَوۥنَ",
        "numberInSurah": 18,
        "juz": 21,
        "manzil": 5,
        "page": 416,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3522,
        "text": "أَمَّا الَّذينَ ءامَنوا وَعَمِلُوا الصّٰلِحٰتِ فَلَهُم جَنّٰتُ المَأوىٰ نُزُلًا بِما كانوا يَعمَلونَ",
        "numberInSurah": 19,
        "juz": 21,
        "manzil": 5,
        "page": 416,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3523,
        "text": "وَأَمَّا الَّذينَ فَسَقوا فَمَأوىٰهُمُ النّارُ ۖ كُلَّما أَرادوا أَن يَخرُجوا مِنها أُعيدوا فيها وَقيلَ لَهُم ذوقوا عَذابَ النّارِ الَّذى كُنتُم بِهِ تُكَذِّبونَ",
        "numberInSurah": 20,
        "juz": 21,
        "manzil": 5,
        "page": 416,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3524,
        "text": "وَلَنُذيقَنَّهُم مِنَ العَذابِ الأَدنىٰ دونَ العَذابِ الأَكبَرِ لَعَلَّهُم يَرجِعونَ",
        "numberInSurah": 21,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3525,
        "text": "وَمَن أَظلَمُ مِمَّن ذُكِّرَ بِـٔايٰتِ رَبِّهِ ثُمَّ أَعرَضَ عَنها ۚ إِنّا مِنَ المُجرِمينَ مُنتَقِمونَ",
        "numberInSurah": 22,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 359,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3526,
        "text": "وَلَقَد ءاتَينا موسَى الكِتٰبَ فَلا تَكُن فى مِريَةٍ مِن لِقائِهِ ۖ وَجَعَلنٰهُ هُدًى لِبَنى إِسرٰءيلَ",
        "numberInSurah": 23,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 360,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3527,
        "text": "وَجَعَلنا مِنهُم أَئِمَّةً يَهدونَ بِأَمرِنا لَمّا صَبَروا ۖ وَكانوا بِـٔايٰتِنا يوقِنونَ",
        "numberInSurah": 24,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 360,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3528,
        "text": "إِنَّ رَبَّكَ هُوَ يَفصِلُ بَينَهُم يَومَ القِيٰمَةِ فيما كانوا فيهِ يَختَلِفونَ",
        "numberInSurah": 25,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 360,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3529,
        "text": "أَوَلَم يَهدِ لَهُم كَم أَهلَكنا مِن قَبلِهِم مِنَ القُرونِ يَمشونَ فى مَسٰكِنِهِم ۚ إِنَّ فى ذٰلِكَ لَءايٰتٍ ۖ أَفَلا يَسمَعونَ",
        "numberInSurah": 26,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 360,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3530,
        "text": "أَوَلَم يَرَوا أَنّا نَسوقُ الماءَ إِلَى الأَرضِ الجُرُزِ فَنُخرِجُ بِهِ زَرعًا تَأكُلُ مِنهُ أَنعٰمُهُم وَأَنفُسُهُم ۖ أَفَلا يُبصِرونَ",
        "numberInSurah": 27,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 360,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3531,
        "text": "وَيَقولونَ مَتىٰ هٰذَا الفَتحُ إِن كُنتُم صٰدِقينَ",
        "numberInSurah": 28,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 360,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3532,
        "text": "قُل يَومَ الفَتحِ لا يَنفَعُ الَّذينَ كَفَروا إيمٰنُهُم وَلا هُم يُنظَرونَ",
        "numberInSurah": 29,
        "juz": 21,
        "manzil": 5,
        "page": 417,
        "ruku": 360,
        "hizbQuarter": 166,
        "sajda": false
      },
      {
        "number": 3533,
        "text": "فَأَعرِض عَنهُم وَانتَظِر إِنَّهُم مُنتَظِرونَ",
        "numberInSurah": 30,
        "juz": 21,
        "manzil": 5,
