    "numberOfAyahs": 135,
    "ayahs": [
      {
        "number": 2349,
        "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ طه",
        "numberInSurah": 1,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2350,
        "text": "ما أَنزَلنا عَلَيكَ القُرءانَ لِتَشقىٰ",
        "numberInSurah": 2,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2351,
        "text": "إِلّا تَذكِرَةً لِمَن يَخشىٰ",
        "numberInSurah": 3,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2352,
        "text": "تَنزيلًا مِمَّن خَلَقَ الأَرضَ وَالسَّمٰوٰتِ العُلَى",
        "numberInSurah": 4,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2353,
        "text": "الرَّحمٰنُ عَلَى العَرشِ استَوىٰ",
        "numberInSurah": 5,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2354,
        "text": "لَهُ ما فِى السَّمٰوٰتِ وَما فِى الأَرضِ وَما بَينَهُما وَما تَحتَ الثَّرىٰ",
        "numberInSurah": 6,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2355,
        "text": "وَإِن تَجهَر بِالقَولِ فَإِنَّهُ يَعلَمُ السِّرَّ وَأَخفَى",
        "numberInSurah": 7,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2356,
        "text": "اللَّهُ لا إِلٰهَ إِلّا هُوَ ۖ لَهُ الأَسماءُ الحُسنىٰ",
        "numberInSurah": 8,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2357,
        "text": "وَهَل أَتىٰكَ حَديثُ موسىٰ",
        "numberInSurah": 9,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2358,
        "text": "إِذ رَءا نارًا فَقالَ لِأَهلِهِ امكُثوا إِنّى ءانَستُ نارًا لَعَلّى ءاتيكُم مِنها بِقَبَسٍ أَو أَجِدُ عَلَى النّارِ هُدًى",
        "numberInSurah": 10,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2359,
        "text": "فَلَمّا أَتىٰها نودِىَ يٰموسىٰ",
        "numberInSurah": 11,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2360,
        "text": "إِنّى أَنا۠ رَبُّكَ فَاخلَع نَعلَيكَ ۖ إِنَّكَ بِالوادِ المُقَدَّسِ طُوًى",
        "numberInSurah": 12,
        "juz": 16,
        "manzil": 4,
        "page": 312,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2361,
        "text": "وَأَنَا اختَرتُكَ فَاستَمِع لِما يوحىٰ",
        "numberInSurah": 13,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2362,
        "text": "إِنَّنى أَنَا اللَّهُ لا إِلٰهَ إِلّا أَنا۠ فَاعبُدنى وَأَقِمِ الصَّلوٰةَ لِذِكرى",
        "numberInSurah": 14,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2363,
        "text": "إِنَّ السّاعَةَ ءاتِيَةٌ أَكادُ أُخفيها لِتُجزىٰ كُلُّ نَفسٍ بِما تَسعىٰ",
        "numberInSurah": 15,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2364,
        "text": "فَلا يَصُدَّنَّكَ عَنها مَن لا يُؤمِنُ بِها وَاتَّبَعَ هَوىٰهُ فَتَردىٰ",
        "numberInSurah": 16,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2365,
        "text": "وَما تِلكَ بِيَمينِكَ يٰموسىٰ",
        "numberInSurah": 17,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2366,
        "text": "قالَ هِىَ عَصاىَ أَتَوَكَّؤُا۟ عَلَيها وَأَهُشُّ بِها عَلىٰ غَنَمى وَلِىَ فيها مَـٔارِبُ أُخرىٰ",
        "numberInSurah": 18,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2367,
        "text": "قالَ أَلقِها يٰموسىٰ",
        "numberInSurah": 19,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2368,
        "text": "فَأَلقىٰها فَإِذا هِىَ حَيَّةٌ تَسعىٰ",
        "numberInSurah": 20,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2369,
        "text": "قالَ خُذها وَلا تَخَف ۖ سَنُعيدُها سيرَتَهَا الأولىٰ",
        "numberInSurah": 21,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2370,
        "text": "وَاضمُم يَدَكَ إِلىٰ جَناحِكَ تَخرُج بَيضاءَ مِن غَيرِ سوءٍ ءايَةً أُخرىٰ",
        "numberInSurah": 22,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2371,
        "text": "لِنُرِيَكَ مِن ءايٰتِنَا الكُبرَى",
        "numberInSurah": 23,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2372,
        "text": "اذهَب إِلىٰ فِرعَونَ إِنَّهُ طَغىٰ",
        "numberInSurah": 24,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 270,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2373,
        "text": "قالَ رَبِّ اشرَح لى صَدرى",
        "numberInSurah": 25,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2374,
        "text": "وَيَسِّر لى أَمرى",
        "numberInSurah": 26,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2375,
        "text": "وَاحلُل عُقدَةً مِن لِسانى",
        "numberInSurah": 27,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2376,
        "text": "يَفقَهوا قَولى",
        "numberInSurah": 28,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2377,
        "text": "وَاجعَل لى وَزيرًا مِن أَهلى",
        "numberInSurah": 29,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2378,
        "text": "هٰرونَ أَخِى",
        "numberInSurah": 30,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2379,
        "text": "اشدُد بِهِ أَزرى",
        "numberInSurah": 31,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2380,
        "text": "وَأَشرِكهُ فى أَمرى",
        "numberInSurah": 32,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2381,
        "text": "كَى نُسَبِّحَكَ كَثيرًا",
        "numberInSurah": 33,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2382,
        "text": "وَنَذكُرَكَ كَثيرًا",
        "numberInSurah": 34,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2383,
        "text": "إِنَّكَ كُنتَ بِنا بَصيرًا",
        "numberInSurah": 35,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2384,
        "text": "قالَ قَد أوتيتَ سُؤلَكَ يٰموسىٰ",
        "numberInSurah": 36,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2385,
        "text": "وَلَقَد مَنَنّا عَلَيكَ مَرَّةً أُخرىٰ",
        "numberInSurah": 37,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2386,
        "text": "إِذ أَوحَينا إِلىٰ أُمِّكَ ما يوحىٰ",
        "numberInSurah": 38,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2387,
        "text": "أَنِ اقذِفيهِ فِى التّابوتِ فَاقذِفيهِ فِى اليَمِّ فَليُلقِهِ اليَمُّ بِالسّاحِلِ يَأخُذهُ عَدُوٌّ لى وَعَدُوٌّ لَهُ ۚ وَأَلقَيتُ عَلَيكَ مَحَبَّةً مِنّى وَلِتُصنَعَ عَلىٰ عَينى",
        "numberInSurah": 39,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2388,
        "text": "إِذ تَمشى أُختُكَ فَتَقولُ هَل أَدُلُّكُم عَلىٰ مَن يَكفُلُهُ ۖ فَرَجَعنٰكَ إِلىٰ أُمِّكَ كَى تَقَرَّ عَينُها وَلا تَحزَنَ ۚ وَقَتَلتَ نَفسًا فَنَجَّينٰكَ مِنَ الغَمِّ وَفَتَنّٰكَ فُتونًا ۚ فَلَبِثتَ سِنينَ فى أَهلِ مَديَنَ ثُمَّ جِئتَ عَلىٰ قَدَرٍ يٰموسىٰ",
        "numberInSurah": 40,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2389,
        "text": "وَاصطَنَعتُكَ لِنَفسِى",
        "numberInSurah": 41,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2390,
        "text": "اذهَب أَنتَ وَأَخوكَ بِـٔايٰتى وَلا تَنِيا فى ذِكرِى",
        "numberInSurah": 42,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2391,
        "text": "اذهَبا إِلىٰ فِرعَونَ إِنَّهُ طَغىٰ",
        "numberInSurah": 43,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2392,
        "text": "فَقولا لَهُ قَولًا لَيِّنًا لَعَلَّهُ يَتَذَكَّرُ أَو يَخشىٰ",
        "numberInSurah": 44,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2393,
        "text": "قالا رَبَّنا إِنَّنا نَخافُ أَن يَفرُطَ عَلَينا أَو أَن يَطغىٰ",
        "numberInSurah": 45,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2394,
        "text": "قالَ لا تَخافا ۖ إِنَّنى مَعَكُما أَسمَعُ وَأَرىٰ",
        "numberInSurah": 46,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2395,
        "text": "فَأتِياهُ فَقولا إِنّا رَسولا رَبِّكَ فَأَرسِل مَعَنا بَنى إِسرٰءيلَ وَلا تُعَذِّبهُم ۖ قَد جِئنٰكَ بِـٔايَةٍ مِن رَبِّكَ ۖ وَالسَّلٰمُ عَلىٰ مَنِ اتَّبَعَ الهُدىٰ",
        "numberInSurah": 47,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2396,
        "text": "إِنّا قَد أوحِىَ إِلَينا أَنَّ العَذابَ عَلىٰ مَن كَذَّبَ وَتَوَلّىٰ",
        "numberInSurah": 48,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2397,
        "text": "قالَ فَمَن رَبُّكُما يٰموسىٰ",
        "numberInSurah": 49,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2398,
        "text": "قالَ رَبُّنَا الَّذى أَعطىٰ كُلَّ شَيءٍ خَلقَهُ ثُمَّ هَدىٰ",
        "numberInSurah": 50,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2399,
        "text": "قالَ فَما بالُ القُرونِ الأولىٰ",
        "numberInSurah": 51,
        "juz": 16,
        "manzil": 4,
        "page": 314,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2400,
        "text": "قالَ عِلمُها عِندَ رَبّى فى كِتٰبٍ ۖ لا يَضِلُّ رَبّى وَلا يَنسَى",
        "numberInSurah": 52,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2401,
        "text": "الَّذى جَعَلَ لَكُمُ الأَرضَ مَهدًا وَسَلَكَ لَكُم فيها سُبُلًا وَأَنزَلَ مِنَ السَّماءِ ماءً فَأَخرَجنا بِهِ أَزوٰجًا مِن نَباتٍ شَتّىٰ",
        "numberInSurah": 53,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2402,
        "text": "كُلوا وَارعَوا أَنعٰمَكُم ۗ إِنَّ فى ذٰلِكَ لَءايٰتٍ لِأُولِى النُّهىٰ",
        "numberInSurah": 54,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2403,
        "text": "۞ مِنها خَلَقنٰكُم وَفيها نُعيدُكُم وَمِنها نُخرِجُكُم تارَةً أُخرىٰ",
        "numberInSurah": 55,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2404,
        "text": "وَلَقَد أَرَينٰهُ ءايٰتِنا كُلَّها فَكَذَّبَ وَأَبىٰ",
        "numberInSurah": 56,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2405,
        "text": "قالَ أَجِئتَنا لِتُخرِجَنا مِن أَرضِنا بِسِحرِكَ يٰموسىٰ",
        "numberInSurah": 57,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2406,
        "text": "فَلَنَأتِيَنَّكَ بِسِحرٍ مِثلِهِ فَاجعَل بَينَنا وَبَينَكَ مَوعِدًا لا نُخلِفُهُ نَحنُ وَلا أَنتَ مَكانًا سُوًى",
        "numberInSurah": 58,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2407,
        "text": "قالَ مَوعِدُكُم يَومُ الزّينَةِ وَأَن يُحشَرَ النّاسُ ضُحًى",
        "numberInSurah": 59,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2408,
        "text": "فَتَوَلّىٰ فِرعَونُ فَجَمَعَ كَيدَهُ ثُمَّ أَتىٰ",
        "numberInSurah": 60,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2409,
        "text": "قالَ لَهُم موسىٰ وَيلَكُم لا تَفتَروا عَلَى اللَّهِ كَذِبًا فَيُسحِتَكُم بِعَذابٍ ۖ وَقَد خابَ مَنِ افتَرىٰ",
        "numberInSurah": 61,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2410,
        "text": "فَتَنٰزَعوا أَمرَهُم بَينَهُم وَأَسَرُّوا النَّجوىٰ",
        "numberInSurah": 62,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2411,
        "text": "قالوا إِن هٰذٰنِ لَسٰحِرٰنِ يُريدانِ أَن يُخرِجاكُم مِن أَرضِكُم بِسِحرِهِما وَيَذهَبا بِطَريقَتِكُمُ المُثلىٰ",
        "numberInSurah": 63,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2412,
        "text": "فَأَجمِعوا كَيدَكُم ثُمَّ ائتوا صَفًّا ۚ وَقَد أَفلَحَ اليَومَ مَنِ استَعلىٰ",
        "numberInSurah": 64,
        "juz": 16,
        "manzil": 4,
        "page": 315,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2413,
        "text": "قالوا يٰموسىٰ إِمّا أَن تُلقِىَ وَإِمّا أَن نَكونَ أَوَّلَ مَن أَلقىٰ",
        "numberInSurah": 65,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2414,
        "text": "قالَ بَل أَلقوا ۖ فَإِذا حِبالُهُم وَعِصِيُّهُم يُخَيَّلُ إِلَيهِ مِن سِحرِهِم أَنَّها تَسعىٰ",
        "numberInSurah": 66,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2415,
        "text": "فَأَوجَسَ فى نَفسِهِ خيفَةً موسىٰ",
        "numberInSurah": 67,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2416,
        "text": "قُلنا لا تَخَف إِنَّكَ أَنتَ الأَعلىٰ",
        "numberInSurah": 68,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2417,
        "text": "وَأَلقِ ما فى يَمينِكَ تَلقَف ما صَنَعوا ۖ إِنَّما صَنَعوا كَيدُ سٰحِرٍ ۖ وَلا يُفلِحُ السّاحِرُ حَيثُ أَتىٰ",
        "numberInSurah": 69,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2418,
        "text": "فَأُلقِىَ السَّحَرَةُ سُجَّدًا قالوا ءامَنّا بِرَبِّ هٰرونَ وَموسىٰ",
        "numberInSurah": 70,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2419,
        "text": "قالَ ءامَنتُم لَهُ قَبلَ أَن ءاذَنَ لَكُم ۖ إِنَّهُ لَكَبيرُكُمُ الَّذى عَلَّمَكُمُ السِّحرَ ۖ فَلَأُقَطِّعَنَّ أَيدِيَكُم وَأَرجُلَكُم مِن خِلٰفٍ وَلَأُصَلِّبَنَّكُم فى جُذوعِ النَّخلِ وَلَتَعلَمُنَّ أَيُّنا أَشَدُّ عَذابًا وَأَبقىٰ",
        "numberInSurah": 71,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2420,
        "text": "قالوا لَن نُؤثِرَكَ عَلىٰ ما جاءَنا مِنَ البَيِّنٰتِ وَالَّذى فَطَرَنا ۖ فَاقضِ ما أَنتَ قاضٍ ۖ إِنَّما تَقضى هٰذِهِ الحَيوٰةَ الدُّنيا",
        "numberInSurah": 72,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2421,
        "text": "إِنّا ءامَنّا بِرَبِّنا لِيَغفِرَ لَنا خَطٰيٰنا وَما أَكرَهتَنا عَلَيهِ مِنَ السِّحرِ ۗ وَاللَّهُ خَيرٌ وَأَبقىٰ",
        "numberInSurah": 73,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2422,
        "text": "إِنَّهُ مَن يَأتِ رَبَّهُ مُجرِمًا فَإِنَّ لَهُ جَهَنَّمَ لا يَموتُ فيها وَلا يَحيىٰ",
        "numberInSurah": 74,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2423,
        "text": "وَمَن يَأتِهِ مُؤمِنًا قَد عَمِلَ الصّٰلِحٰتِ فَأُولٰئِكَ لَهُمُ الدَّرَجٰتُ العُلىٰ",
        "numberInSurah": 75,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2424,
        "text": "جَنّٰتُ عَدنٍ تَجرى مِن تَحتِهَا الأَنهٰرُ خٰلِدينَ فيها ۚ وَذٰلِكَ جَزاءُ مَن تَزَكّىٰ",
        "numberInSurah": 76,
        "juz": 16,
        "manzil": 4,
        "page": 316,
        "ruku": 272,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2425,
        "text": "وَلَقَد أَوحَينا إِلىٰ موسىٰ أَن أَسرِ بِعِبادى فَاضرِب لَهُم طَريقًا فِى البَحرِ يَبَسًا لا تَخٰفُ دَرَكًا وَلا تَخشىٰ",
        "numberInSurah": 77,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2426,
        "text": "فَأَتبَعَهُم فِرعَونُ بِجُنودِهِ فَغَشِيَهُم مِنَ اليَمِّ ما غَشِيَهُم",
        "numberInSurah": 78,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2427,
        "text": "وَأَضَلَّ فِرعَونُ قَومَهُ وَما هَدىٰ",
        "numberInSurah": 79,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2428,
        "text": "يٰبَنى إِسرٰءيلَ قَد أَنجَينٰكُم مِن عَدُوِّكُم وَوٰعَدنٰكُم جانِبَ الطّورِ الأَيمَنَ وَنَزَّلنا عَلَيكُمُ المَنَّ وَالسَّلوىٰ",
        "numberInSurah": 80,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2429,
        "text": "كُلوا مِن طَيِّبٰتِ ما رَزَقنٰكُم وَلا تَطغَوا فيهِ فَيَحِلَّ عَلَيكُم غَضَبى ۖ وَمَن يَحلِل عَلَيهِ غَضَبى فَقَد هَوىٰ",
        "numberInSurah": 81,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2430,
        "text": "وَإِنّى لَغَفّارٌ لِمَن تابَ وَءامَنَ وَعَمِلَ صٰلِحًا ثُمَّ اهتَدىٰ",
        "numberInSurah": 82,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2431,
        "text": "۞ وَما أَعجَلَكَ عَن قَومِكَ يٰموسىٰ",
        "numberInSurah": 83,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2432,
        "text": "قالَ هُم أُولاءِ عَلىٰ أَثَرى وَعَجِلتُ إِلَيكَ رَبِّ لِتَرضىٰ",
        "numberInSurah": 84,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2433,
        "text": "قالَ فَإِنّا قَد فَتَنّا قَومَكَ مِن بَعدِكَ وَأَضَلَّهُمُ السّامِرِىُّ",
        "numberInSurah": 85,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2434,
        "text": "فَرَجَعَ موسىٰ إِلىٰ قَومِهِ غَضبٰنَ أَسِفًا ۚ قالَ يٰقَومِ أَلَم يَعِدكُم رَبُّكُم وَعدًا حَسَنًا ۚ أَفَطالَ عَلَيكُمُ العَهدُ أَم أَرَدتُم أَن يَحِلَّ عَلَيكُم غَضَبٌ مِن رَبِّكُم فَأَخلَفتُم مَوعِدى",
        "numberInSurah": 86,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2435,
        "text": "قالوا ما أَخلَفنا مَوعِدَكَ بِمَلكِنا وَلٰكِنّا حُمِّلنا أَوزارًا مِن زينَةِ القَومِ فَقَذَفنٰها فَكَذٰلِكَ أَلقَى السّامِرِىُّ",
        "numberInSurah": 87,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2436,
        "text": "فَأَخرَجَ لَهُم عِجلًا جَسَدًا لَهُ خُوارٌ فَقالوا هٰذا إِلٰهُكُم وَإِلٰهُ موسىٰ فَنَسِىَ",
        "numberInSurah": 88,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 273,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2437,
        "text": "أَفَلا يَرَونَ أَلّا يَرجِعُ إِلَيهِم قَولًا وَلا يَملِكُ لَهُم ضَرًّا وَلا نَفعًا",
        "numberInSurah": 89,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 273,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2438,
        "text": "وَلَقَد قالَ لَهُم هٰرونُ مِن قَبلُ يٰقَومِ إِنَّما فُتِنتُم بِهِ ۖ وَإِنَّ رَبَّكُمُ الرَّحمٰنُ فَاتَّبِعونى وَأَطيعوا أَمرى",
        "numberInSurah": 90,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2439,
        "text": "قالوا لَن نَبرَحَ عَلَيهِ عٰكِفينَ حَتّىٰ يَرجِعَ إِلَينا موسىٰ",
        "numberInSurah": 91,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2440,
        "text": "قالَ يٰهٰرونُ ما مَنَعَكَ إِذ رَأَيتَهُم ضَلّوا",
        "numberInSurah": 92,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2441,
        "text": "أَلّا تَتَّبِعَنِ ۖ أَفَعَصَيتَ أَمرى",
        "numberInSurah": 93,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2442,
        "text": "قالَ يَبنَؤُمَّ لا تَأخُذ بِلِحيَتى وَلا بِرَأسى ۖ إِنّى خَشيتُ أَن تَقولَ فَرَّقتَ بَينَ بَنى إِسرٰءيلَ وَلَم تَرقُب قَولى",
        "numberInSurah": 94,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2443,
        "text": "قالَ فَما خَطبُكَ يٰسٰمِرِىُّ",
        "numberInSurah": 95,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2444,
        "text": "قالَ بَصُرتُ بِما لَم يَبصُروا بِهِ فَقَبَضتُ قَبضَةً مِن أَثَرِ الرَّسولِ فَنَبَذتُها وَكَذٰلِكَ سَوَّلَت لى نَفسى",
        "numberInSurah": 96,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2445,
        "text": "قالَ فَاذهَب فَإِنَّ لَكَ فِى الحَيوٰةِ أَن تَقولَ لا مِساسَ ۖ وَإِنَّ لَكَ مَوعِدًا لَن تُخلَفَهُ ۖ وَانظُر إِلىٰ إِلٰهِكَ الَّذى ظَلتَ عَلَيهِ عاكِفًا ۖ لَنُحَرِّقَنَّهُ ثُمَّ لَنَنسِفَنَّهُ فِى اليَمِّ نَسفًا",
        "numberInSurah": 97,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2446,
        "text": "إِنَّما إِلٰهُكُمُ اللَّهُ الَّذى لا إِلٰهَ إِلّا هُوَ ۚ وَسِعَ كُلَّ شَيءٍ عِلمًا",
        "numberInSurah": 98,
        "juz": 16,
        "manzil": 4,
        "page": 318,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2447,
        "text": "كَذٰلِكَ نَقُصُّ عَلَيكَ مِن أَنباءِ ما قَد سَبَقَ ۚ وَقَد ءاتَينٰكَ مِن لَدُنّا ذِكرًا",
        "numberInSurah": 99,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2448,
        "text": "مَن أَعرَضَ عَنهُ فَإِنَّهُ يَحمِلُ يَومَ القِيٰمَةِ وِزرًا",
        "numberInSurah": 100,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2449,
        "text": "خٰلِدينَ فيهِ ۖ وَساءَ لَهُم يَومَ القِيٰمَةِ حِملًا",
        "numberInSurah": 101,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2450,
        "text": "يَومَ يُنفَخُ فِى الصّورِ ۚ وَنَحشُرُ المُجرِمينَ يَومَئِذٍ زُرقًا",
        "numberInSurah": 102,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2451,
        "text": "يَتَخٰفَتونَ بَينَهُم إِن لَبِثتُم إِلّا عَشرًا",
        "numberInSurah": 103,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2452,
        "text": "نَحنُ أَعلَمُ بِما يَقولونَ إِذ يَقولُ أَمثَلُهُم طَريقَةً إِن لَبِثتُم إِلّا يَومًا",
        "numberInSurah": 104,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 274,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2453,
        "text": "وَيَسـَٔلونَكَ عَنِ الجِبالِ فَقُل يَنسِفُها رَبّى نَسفًا",
        "numberInSurah": 105,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 275,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2454,
        "text": "فَيَذَرُها قاعًا صَفصَفًا",
        "numberInSurah": 106,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 275,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2455,
        "text": "لا تَرىٰ فيها عِوَجًا وَلا أَمتًا",
        "numberInSurah": 107,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 275,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2456,
        "text": "يَومَئِذٍ يَتَّبِعونَ الدّاعِىَ لا عِوَجَ لَهُ ۖ وَخَشَعَتِ الأَصواتُ لِلرَّحمٰنِ فَلا تَسمَعُ إِلّا هَمسًا",
        "numberInSurah": 108,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 275,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2457,
        "text": "يَومَئِذٍ لا تَنفَعُ الشَّفٰعَةُ إِلّا مَن أَذِنَ لَهُ الرَّحمٰنُ وَرَضِىَ لَهُ قَولًا",
        "numberInSurah": 109,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 275,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2458,
        "text": "يَعلَمُ ما بَينَ أَيديهِم وَما خَلفَهُم وَلا يُحيطونَ بِهِ عِلمًا",
        "numberInSurah": 110,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 275,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2459,
        "text": "۞ وَعَنَتِ الوُجوهُ لِلحَىِّ القَيّومِ ۖ وَقَد خابَ مَن حَمَلَ ظُلمًا",
        "numberInSurah": 111,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 275,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2460,
        "text": "وَمَن يَعمَل مِنَ الصّٰلِحٰتِ وَهُوَ مُؤمِنٌ فَلا يَخافُ ظُلمًا وَلا هَضمًا",
        "numberInSurah": 112,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 275,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2461,
        "text": "وَكَذٰلِكَ أَنزَلنٰهُ قُرءانًا عَرَبِيًّا وَصَرَّفنا فيهِ مِنَ الوَعيدِ لَعَلَّهُم يَتَّقونَ أَو يُحدِثُ لَهُم ذِكرًا",
        "numberInSurah": 113,
        "juz": 16,
        "manzil": 4,
        "page": 319,
        "ruku": 275,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2462,
        "text": "فَتَعٰلَى اللَّهُ المَلِكُ الحَقُّ ۗ وَلا تَعجَل بِالقُرءانِ مِن قَبلِ أَن يُقضىٰ إِلَيكَ وَحيُهُ ۖ وَقُل رَبِّ زِدنى عِلمًا",
        "numberInSurah": 114,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 275,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2463,
        "text": "وَلَقَد عَهِدنا إِلىٰ ءادَمَ مِن قَبلُ فَنَسِىَ وَلَم نَجِد لَهُ عَزمًا",
        "numberInSurah": 115,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 275,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2464,
        "text": "وَإِذ قُلنا لِلمَلٰئِكَةِ اسجُدوا لِءادَمَ فَسَجَدوا إِلّا إِبليسَ أَبىٰ",
        "numberInSurah": 116,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2465,
        "text": "فَقُلنا يٰـٔادَمُ إِنَّ هٰذا عَدُوٌّ لَكَ وَلِزَوجِكَ فَلا يُخرِجَنَّكُما مِنَ الجَنَّةِ فَتَشقىٰ",
        "numberInSurah": 117,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2466,
        "text": "إِنَّ لَكَ أَلّا تَجوعَ فيها وَلا تَعرىٰ",
        "numberInSurah": 118,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2467,
        "text": "وَأَنَّكَ لا تَظمَؤُا۟ فيها وَلا تَضحىٰ",
        "numberInSurah": 119,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2468,
        "text": "فَوَسوَسَ إِلَيهِ الشَّيطٰنُ قالَ يٰـٔادَمُ هَل أَدُلُّكَ عَلىٰ شَجَرَةِ الخُلدِ وَمُلكٍ لا يَبلىٰ",
        "numberInSurah": 120,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2469,
        "text": "فَأَكَلا مِنها فَبَدَت لَهُما سَوءٰتُهُما وَطَفِقا يَخصِفانِ عَلَيهِما مِن وَرَقِ الجَنَّةِ ۚ وَعَصىٰ ءادَمُ رَبَّهُ فَغَوىٰ",
        "numberInSurah": 121,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2470,
        "text": "ثُمَّ اجتَبٰهُ رَبُّهُ فَتابَ عَلَيهِ وَهَدىٰ",
        "numberInSurah": 122,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2471,
        "text": "قالَ اهبِطا مِنها جَميعًا ۖ بَعضُكُم لِبَعضٍ عَدُوٌّ ۖ فَإِمّا يَأتِيَنَّكُم مِنّى هُدًى فَمَنِ اتَّبَعَ هُداىَ فَلا يَضِلُّ وَلا يَشقىٰ",
        "numberInSurah": 123,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2472,
        "text": "وَمَن أَعرَضَ عَن ذِكرى فَإِنَّ لَهُ مَعيشَةً ضَنكًا وَنَحشُرُهُ يَومَ القِيٰمَةِ أَعمىٰ",
        "numberInSurah": 124,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2473,
        "text": "قالَ رَبِّ لِمَ حَشَرتَنى أَعمىٰ وَقَد كُنتُ بَصيرًا",
        "numberInSurah": 125,
        "juz": 16,
        "manzil": 4,
        "page": 320,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2474,
        "text": "قالَ كَذٰلِكَ أَتَتكَ ءايٰتُنا فَنَسيتَها ۖ وَكَذٰلِكَ اليَومَ تُنسىٰ",
        "numberInSurah": 126,
        "juz": 16,
        "manzil": 4,
        "page": 321,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2475,
        "text": "وَكَذٰلِكَ نَجزى مَن أَسرَفَ وَلَم يُؤمِن بِـٔايٰتِ رَبِّهِ ۚ وَلَعَذابُ الءاخِرَةِ أَشَدُّ وَأَبقىٰ",
        "numberInSurah": 127,
        "juz": 16,
        "manzil": 4,
        "page": 321,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2476,
        "text": "أَفَلَم يَهدِ لَهُم كَم أَهلَكنا قَبلَهُم مِنَ القُرونِ يَمشونَ فى مَسٰكِنِهِم ۗ إِنَّ فى ذٰلِكَ لَءايٰتٍ لِأُولِى النُّهىٰ",
        "numberInSurah": 128,
        "juz": 16,
        "manzil": 4,
        "page": 321,
        "ruku": 276,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2477,
        "text": "وَلَولا كَلِمَةٌ سَبَقَت مِن رَبِّكَ لَكانَ لِزامًا وَأَجَلٌ مُسَمًّى",
        "numberInSurah": 129,
        "juz": 16,
        "manzil": 4,
        "page": 321,
        "ruku": 277,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2478,
        "text": "فَاصبِر عَلىٰ ما يَقولونَ وَسَبِّح بِحَمدِ رَبِّكَ قَبلَ طُلوعِ الشَّمسِ وَقَبلَ غُروبِها ۖ وَمِن ءانائِ الَّيلِ فَسَبِّح وَأَطرافَ النَّهارِ لَعَلَّكَ تَرضىٰ",
        "numberInSurah": 130,
        "juz": 16,
        "manzil": 4,
        "page": 321,
        "ruku": 277,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2479,
        "text": "وَلا تَمُدَّنَّ عَينَيكَ إِلىٰ ما مَتَّعنا بِهِ أَزوٰجًا مِنهُم زَهرَةَ الحَيوٰةِ الدُّنيا لِنَفتِنَهُم فيهِ ۚ وَرِزقُ رَبِّكَ خَيرٌ وَأَبقىٰ",
        "numberInSurah": 131,
        "juz": 16,
        "manzil": 4,
        "page": 321,
        "ruku": 277,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2480,
        "text": "وَأمُر أَهلَكَ بِالصَّلوٰةِ وَاصطَبِر عَلَيها ۖ لا نَسـَٔلُكَ رِزقًا ۖ نَحنُ نَرزُقُكَ ۗ وَالعٰقِبَةُ لِلتَّقوىٰ",
        "numberInSurah": 132,
        "juz": 16,
        "manzil": 4,
        "page": 321,
        "ruku": 277,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2481,
        "text": "وَقالوا لَولا يَأتينا بِـٔايَةٍ مِن رَبِّهِ ۚ أَوَلَم تَأتِهِم بَيِّنَةُ ما فِى الصُّحُفِ الأولىٰ",
        "numberInSurah": 133,
        "juz": 16,
        "manzil": 4,
        "page": 321,
        "ruku": 277,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2482,
        "text": "وَلَو أَنّا أَهلَكنٰهُم بِعَذابٍ مِن قَبلِهِ لَقالوا رَبَّنا لَولا أَرسَلتَ إِلَينا رَسولًا فَنَتَّبِعَ ءايٰتِكَ مِن قَبلِ أَن نَذِلَّ وَنَخزىٰ",
        "numberInSurah": 134,
        "juz": 16,
        "manzil": 4,
        "page": 321,
        "ruku": 277,
        "hizbQuarter": 128,
        "sajda": false
      },
      {
        "number": 2483,
        "text": "قُل كُلٌّ مُتَرَبِّصٌ فَتَرَبَّصوا ۖ فَسَتَعلَمونَ مَن أَصحٰبُ الصِّرٰطِ السَّوِىِّ وَمَنِ اهتَدىٰ",
        "numberInSurah": 135,
        "juz": 16,
