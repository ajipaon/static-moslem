        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4327,
        "text": "وَالكِتٰبِ المُبينِ",
        "numberInSurah": 2,
        "juz": 25,
        "manzil": 6,
        "page": 489,
        "ruku": 424,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4328,
        "text": "إِنّا جَعَلنٰهُ قُرءٰنًا عَرَبِيًّا لَعَلَّكُم تَعقِلونَ",
        "numberInSurah": 3,
        "juz": 25,
        "manzil": 6,
        "page": 489,
        "ruku": 424,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4329,
        "text": "وَإِنَّهُ فى أُمِّ الكِتٰبِ لَدَينا لَعَلِىٌّ حَكيمٌ",
        "numberInSurah": 4,
        "juz": 25,
        "manzil": 6,
        "page": 489,
        "ruku": 424,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4330,
        "text": "أَفَنَضرِبُ عَنكُمُ الذِّكرَ صَفحًا أَن كُنتُم قَومًا مُسرِفينَ",
        "numberInSurah": 5,
        "juz": 25,
        "manzil": 6,
        "page": 489,
        "ruku": 424,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4331,
        "text": "وَكَم أَرسَلنا مِن نَبِىٍّ فِى الأَوَّلينَ",
        "numberInSurah": 6,
        "juz": 25,
        "manzil": 6,
        "page": 489,
        "ruku": 424,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4332,
        "text": "وَما يَأتيهِم مِن نَبِىٍّ إِلّا كانوا بِهِ يَستَهزِءونَ",
        "numberInSurah": 7,
        "juz": 25,
        "manzil": 6,
        "page": 489,
        "ruku": 424,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4333,
        "text": "فَأَهلَكنا أَشَدَّ مِنهُم بَطشًا وَمَضىٰ مَثَلُ الأَوَّلينَ",
        "numberInSurah": 8,
        "juz": 25,
        "manzil": 6,
        "page": 489,
        "ruku": 424,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4334,
        "text": "وَلَئِن سَأَلتَهُم مَن خَلَقَ السَّمٰوٰتِ وَالأَرضَ لَيَقولُنَّ خَلَقَهُنَّ العَزيزُ العَليمُ",
        "numberInSurah": 9,
        "juz": 25,
        "manzil": 6,
        "page": 489,
        "ruku": 424,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4335,
        "text": "الَّذى جَعَلَ لَكُمُ الأَرضَ مَهدًا وَجَعَلَ لَكُم فيها سُبُلًا لَعَلَّكُم تَهتَدونَ",
        "numberInSurah": 10,
        "juz": 25,
        "manzil": 6,
        "page": 489,
        "ruku": 424,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4336,
        "text": "وَالَّذى نَزَّلَ مِنَ السَّماءِ ماءً بِقَدَرٍ فَأَنشَرنا بِهِ بَلدَةً مَيتًا ۚ كَذٰلِكَ تُخرَجونَ",
        "numberInSurah": 11,
        "juz": 25,
        "manzil": 6,
        "page": 490,
        "ruku": 424,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4337,
        "text": "وَالَّذى خَلَقَ الأَزوٰجَ كُلَّها وَجَعَلَ لَكُم مِنَ الفُلكِ وَالأَنعٰمِ ما تَركَبونَ",
        "numberInSurah": 12,
        "juz": 25,
        "manzil": 6,
        "page": 490,
        "ruku": 424,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4338,
        "text": "لِتَستَوۥا عَلىٰ ظُهورِهِ ثُمَّ تَذكُروا نِعمَةَ رَبِّكُم إِذَا استَوَيتُم عَلَيهِ وَتَقولوا سُبحٰنَ الَّذى سَخَّرَ لَنا هٰذا وَما كُنّا لَهُ مُقرِنينَ",
        "numberInSurah": 13,
        "juz": 25,
        "manzil": 6,
        "page": 490,
        "ruku": 424,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4339,
        "text": "وَإِنّا إِلىٰ رَبِّنا لَمُنقَلِبونَ",
        "numberInSurah": 14,
        "juz": 25,
        "manzil": 6,
        "page": 490,
        "ruku": 424,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4340,
        "text": "وَجَعَلوا لَهُ مِن عِبادِهِ جُزءًا ۚ إِنَّ الإِنسٰنَ لَكَفورٌ مُبينٌ",
        "numberInSurah": 15,
        "juz": 25,
        "manzil": 6,
        "page": 490,
        "ruku": 424,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4341,
        "text": "أَمِ اتَّخَذَ مِمّا يَخلُقُ بَناتٍ وَأَصفىٰكُم بِالبَنينَ",
        "numberInSurah": 16,
        "juz": 25,
        "manzil": 6,
        "page": 490,
        "ruku": 425,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4342,
        "text": "وَإِذا بُشِّرَ أَحَدُهُم بِما ضَرَبَ لِلرَّحمٰنِ مَثَلًا ظَلَّ وَجهُهُ مُسوَدًّا وَهُوَ كَظيمٌ",
        "numberInSurah": 17,
        "juz": 25,
        "manzil": 6,
        "page": 490,
        "ruku": 425,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4343,
        "text": "أَوَمَن يُنَشَّؤُا۟ فِى الحِليَةِ وَهُوَ فِى الخِصامِ غَيرُ مُبينٍ",
        "numberInSurah": 18,
        "juz": 25,
        "manzil": 6,
        "page": 490,
        "ruku": 425,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4344,
        "text": "وَجَعَلُوا المَلٰئِكَةَ الَّذينَ هُم عِبٰدُ الرَّحمٰنِ إِنٰثًا ۚ أَشَهِدوا خَلقَهُم ۚ سَتُكتَبُ شَهٰدَتُهُم وَيُسـَٔلونَ",
        "numberInSurah": 19,
        "juz": 25,
        "manzil": 6,
        "page": 490,
        "ruku": 425,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4345,
        "text": "وَقالوا لَو شاءَ الرَّحمٰنُ ما عَبَدنٰهُم ۗ ما لَهُم بِذٰلِكَ مِن عِلمٍ ۖ إِن هُم إِلّا يَخرُصونَ",
        "numberInSurah": 20,
        "juz": 25,
        "manzil": 6,
        "page": 490,
        "ruku": 425,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4346,
        "text": "أَم ءاتَينٰهُم كِتٰبًا مِن قَبلِهِ فَهُم بِهِ مُستَمسِكونَ",
        "numberInSurah": 21,
        "juz": 25,
        "manzil": 6,
        "page": 490,
        "ruku": 425,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4347,
        "text": "بَل قالوا إِنّا وَجَدنا ءاباءَنا عَلىٰ أُمَّةٍ وَإِنّا عَلىٰ ءاثٰرِهِم مُهتَدونَ",
        "numberInSurah": 22,
        "juz": 25,
        "manzil": 6,
        "page": 490,
        "ruku": 425,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4348,
        "text": "وَكَذٰلِكَ ما أَرسَلنا مِن قَبلِكَ فى قَريَةٍ مِن نَذيرٍ إِلّا قالَ مُترَفوها إِنّا وَجَدنا ءاباءَنا عَلىٰ أُمَّةٍ وَإِنّا عَلىٰ ءاثٰرِهِم مُقتَدونَ",
        "numberInSurah": 23,
        "juz": 25,
        "manzil": 6,
        "page": 491,
        "ruku": 425,
        "hizbQuarter": 196,
        "sajda": false
      },
      {
        "number": 4349,
        "text": "۞ قٰلَ أَوَلَو جِئتُكُم بِأَهدىٰ مِمّا وَجَدتُم عَلَيهِ ءاباءَكُم ۖ قالوا إِنّا بِما أُرسِلتُم بِهِ كٰفِرونَ",
        "numberInSurah": 24,
        "juz": 25,
        "manzil": 6,
        "page": 491,
        "ruku": 425,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4350,
        "text": "فَانتَقَمنا مِنهُم ۖ فَانظُر كَيفَ كانَ عٰقِبَةُ المُكَذِّبينَ",
        "numberInSurah": 25,
        "juz": 25,
        "manzil": 6,
        "page": 491,
        "ruku": 425,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4351,
        "text": "وَإِذ قالَ إِبرٰهيمُ لِأَبيهِ وَقَومِهِ إِنَّنى بَراءٌ مِمّا تَعبُدونَ",
        "numberInSurah": 26,
        "juz": 25,
        "manzil": 6,
        "page": 491,
        "ruku": 426,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4352,
        "text": "إِلَّا الَّذى فَطَرَنى فَإِنَّهُ سَيَهدينِ",
        "numberInSurah": 27,
        "juz": 25,
        "manzil": 6,
        "page": 491,
        "ruku": 426,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4353,
        "text": "وَجَعَلَها كَلِمَةً باقِيَةً فى عَقِبِهِ لَعَلَّهُم يَرجِعونَ",
        "numberInSurah": 28,
        "juz": 25,
        "manzil": 6,
        "page": 491,
        "ruku": 426,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4354,
        "text": "بَل مَتَّعتُ هٰؤُلاءِ وَءاباءَهُم حَتّىٰ جاءَهُمُ الحَقُّ وَرَسولٌ مُبينٌ",
        "numberInSurah": 29,
        "juz": 25,
        "manzil": 6,
        "page": 491,
        "ruku": 426,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4355,
        "text": "وَلَمّا جاءَهُمُ الحَقُّ قالوا هٰذا سِحرٌ وَإِنّا بِهِ كٰفِرونَ",
        "numberInSurah": 30,
        "juz": 25,
        "manzil": 6,
        "page": 491,
        "ruku": 426,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4356,
        "text": "وَقالوا لَولا نُزِّلَ هٰذَا القُرءانُ عَلىٰ رَجُلٍ مِنَ القَريَتَينِ عَظيمٍ",
        "numberInSurah": 31,
        "juz": 25,
        "manzil": 6,
        "page": 491,
        "ruku": 426,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4357,
        "text": "أَهُم يَقسِمونَ رَحمَتَ رَبِّكَ ۚ نَحنُ قَسَمنا بَينَهُم مَعيشَتَهُم فِى الحَيوٰةِ الدُّنيا ۚ وَرَفَعنا بَعضَهُم فَوقَ بَعضٍ دَرَجٰتٍ لِيَتَّخِذَ بَعضُهُم بَعضًا سُخرِيًّا ۗ وَرَحمَتُ رَبِّكَ خَيرٌ مِمّا يَجمَعونَ",
        "numberInSurah": 32,
        "juz": 25,
        "manzil": 6,
        "page": 491,
        "ruku": 426,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4358,
        "text": "وَلَولا أَن يَكونَ النّاسُ أُمَّةً وٰحِدَةً لَجَعَلنا لِمَن يَكفُرُ بِالرَّحمٰنِ لِبُيوتِهِم سُقُفًا مِن فِضَّةٍ وَمَعارِجَ عَلَيها يَظهَرونَ",
        "numberInSurah": 33,
        "juz": 25,
        "manzil": 6,
        "page": 491,
        "ruku": 426,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4359,
        "text": "وَلِبُيوتِهِم أَبوٰبًا وَسُرُرًا عَلَيها يَتَّكِـٔونَ",
        "numberInSurah": 34,
        "juz": 25,
        "manzil": 6,
        "page": 492,
        "ruku": 426,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4360,
        "text": "وَزُخرُفًا ۚ وَإِن كُلُّ ذٰلِكَ لَمّا مَتٰعُ الحَيوٰةِ الدُّنيا ۚ وَالءاخِرَةُ عِندَ رَبِّكَ لِلمُتَّقينَ",
        "numberInSurah": 35,
        "juz": 25,
        "manzil": 6,
        "page": 492,
        "ruku": 426,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4361,
        "text": "وَمَن يَعشُ عَن ذِكرِ الرَّحمٰنِ نُقَيِّض لَهُ شَيطٰنًا فَهُوَ لَهُ قَرينٌ",
        "numberInSurah": 36,
        "juz": 25,
        "manzil": 6,
        "page": 492,
        "ruku": 427,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4362,
        "text": "وَإِنَّهُم لَيَصُدّونَهُم عَنِ السَّبيلِ وَيَحسَبونَ أَنَّهُم مُهتَدونَ",
        "numberInSurah": 37,
        "juz": 25,
        "manzil": 6,
        "page": 492,
        "ruku": 427,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4363,
        "text": "حَتّىٰ إِذا جاءَنا قالَ يٰلَيتَ بَينى وَبَينَكَ بُعدَ المَشرِقَينِ فَبِئسَ القَرينُ",
        "numberInSurah": 38,
        "juz": 25,
        "manzil": 6,
        "page": 492,
        "ruku": 427,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4364,
        "text": "وَلَن يَنفَعَكُمُ اليَومَ إِذ ظَلَمتُم أَنَّكُم فِى العَذابِ مُشتَرِكونَ",
        "numberInSurah": 39,
        "juz": 25,
        "manzil": 6,
        "page": 492,
        "ruku": 427,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4365,
        "text": "أَفَأَنتَ تُسمِعُ الصُّمَّ أَو تَهدِى العُمىَ وَمَن كانَ فى ضَلٰلٍ مُبينٍ",
        "numberInSurah": 40,
        "juz": 25,
        "manzil": 6,
        "page": 492,
        "ruku": 427,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4366,
        "text": "فَإِمّا نَذهَبَنَّ بِكَ فَإِنّا مِنهُم مُنتَقِمونَ",
        "numberInSurah": 41,
        "juz": 25,
        "manzil": 6,
        "page": 492,
        "ruku": 427,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4367,
        "text": "أَو نُرِيَنَّكَ الَّذى وَعَدنٰهُم فَإِنّا عَلَيهِم مُقتَدِرونَ",
        "numberInSurah": 42,
        "juz": 25,
        "manzil": 6,
        "page": 492,
        "ruku": 427,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4368,
        "text": "فَاستَمسِك بِالَّذى أوحِىَ إِلَيكَ ۖ إِنَّكَ عَلىٰ صِرٰطٍ مُستَقيمٍ",
        "numberInSurah": 43,
        "juz": 25,
        "manzil": 6,
        "page": 492,
        "ruku": 427,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4369,
        "text": "وَإِنَّهُ لَذِكرٌ لَكَ وَلِقَومِكَ ۖ وَسَوفَ تُسـَٔلونَ",
        "numberInSurah": 44,
        "juz": 25,
        "manzil": 6,
        "page": 492,
        "ruku": 427,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4370,
        "text": "وَسـَٔل مَن أَرسَلنا مِن قَبلِكَ مِن رُسُلِنا أَجَعَلنا مِن دونِ الرَّحمٰنِ ءالِهَةً يُعبَدونَ",
        "numberInSurah": 45,
        "juz": 25,
        "manzil": 6,
        "page": 492,
        "ruku": 427,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4371,
        "text": "وَلَقَد أَرسَلنا موسىٰ بِـٔايٰتِنا إِلىٰ فِرعَونَ وَمَلَإِي۟هِ فَقالَ إِنّى رَسولُ رَبِّ العٰلَمينَ",
        "numberInSurah": 46,
        "juz": 25,
        "manzil": 6,
        "page": 492,
        "ruku": 428,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4372,
        "text": "فَلَمّا جاءَهُم بِـٔايٰتِنا إِذا هُم مِنها يَضحَكونَ",
        "numberInSurah": 47,
        "juz": 25,
        "manzil": 6,
        "page": 492,
        "ruku": 428,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4373,
        "text": "وَما نُريهِم مِن ءايَةٍ إِلّا هِىَ أَكبَرُ مِن أُختِها ۖ وَأَخَذنٰهُم بِالعَذابِ لَعَلَّهُم يَرجِعونَ",
        "numberInSurah": 48,
        "juz": 25,
        "manzil": 6,
        "page": 493,
        "ruku": 428,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4374,
        "text": "وَقالوا يٰأَيُّهَ السّاحِرُ ادعُ لَنا رَبَّكَ بِما عَهِدَ عِندَكَ إِنَّنا لَمُهتَدونَ",
        "numberInSurah": 49,
        "juz": 25,
        "manzil": 6,
        "page": 493,
        "ruku": 428,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4375,
        "text": "فَلَمّا كَشَفنا عَنهُمُ العَذابَ إِذا هُم يَنكُثونَ",
        "numberInSurah": 50,
        "juz": 25,
        "manzil": 6,
        "page": 493,
        "ruku": 428,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4376,
        "text": "وَنادىٰ فِرعَونُ فى قَومِهِ قالَ يٰقَومِ أَلَيسَ لى مُلكُ مِصرَ وَهٰذِهِ الأَنهٰرُ تَجرى مِن تَحتى ۖ أَفَلا تُبصِرونَ",
        "numberInSurah": 51,
        "juz": 25,
        "manzil": 6,
        "page": 493,
        "ruku": 428,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4377,
        "text": "أَم أَنا۠ خَيرٌ مِن هٰذَا الَّذى هُوَ مَهينٌ وَلا يَكادُ يُبينُ",
        "numberInSurah": 52,
        "juz": 25,
        "manzil": 6,
        "page": 493,
        "ruku": 428,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4378,
        "text": "فَلَولا أُلقِىَ عَلَيهِ أَسوِرَةٌ مِن ذَهَبٍ أَو جاءَ مَعَهُ المَلٰئِكَةُ مُقتَرِنينَ",
        "numberInSurah": 53,
        "juz": 25,
        "manzil": 6,
        "page": 493,
        "ruku": 428,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4379,
        "text": "فَاستَخَفَّ قَومَهُ فَأَطاعوهُ ۚ إِنَّهُم كانوا قَومًا فٰسِقينَ",
        "numberInSurah": 54,
        "juz": 25,
        "manzil": 6,
        "page": 493,
        "ruku": 428,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4380,
        "text": "فَلَمّا ءاسَفونَا انتَقَمنا مِنهُم فَأَغرَقنٰهُم أَجمَعينَ",
        "numberInSurah": 55,
        "juz": 25,
        "manzil": 6,
        "page": 493,
        "ruku": 428,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4381,
        "text": "فَجَعَلنٰهُم سَلَفًا وَمَثَلًا لِلءاخِرينَ",
        "numberInSurah": 56,
        "juz": 25,
        "manzil": 6,
        "page": 493,
        "ruku": 428,
        "hizbQuarter": 197,
        "sajda": false
      },
      {
        "number": 4382,
        "text": "۞ وَلَمّا ضُرِبَ ابنُ مَريَمَ مَثَلًا إِذا قَومُكَ مِنهُ يَصِدّونَ",
        "numberInSurah": 57,
        "juz": 25,
        "manzil": 6,
        "page": 493,
        "ruku": 429,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4383,
        "text": "وَقالوا ءَأٰلِهَتُنا خَيرٌ أَم هُوَ ۚ ما ضَرَبوهُ لَكَ إِلّا جَدَلًا ۚ بَل هُم قَومٌ خَصِمونَ",
        "numberInSurah": 58,
        "juz": 25,
        "manzil": 6,
        "page": 493,
        "ruku": 429,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4384,
        "text": "إِن هُوَ إِلّا عَبدٌ أَنعَمنا عَلَيهِ وَجَعَلنٰهُ مَثَلًا لِبَنى إِسرٰءيلَ",
        "numberInSurah": 59,
        "juz": 25,
        "manzil": 6,
        "page": 493,
        "ruku": 429,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4385,
        "text": "وَلَو نَشاءُ لَجَعَلنا مِنكُم مَلٰئِكَةً فِى الأَرضِ يَخلُفونَ",
        "numberInSurah": 60,
        "juz": 25,
        "manzil": 6,
        "page": 493,
        "ruku": 429,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4386,
        "text": "وَإِنَّهُ لَعِلمٌ لِلسّاعَةِ فَلا تَمتَرُنَّ بِها وَاتَّبِعونِ ۚ هٰذا صِرٰطٌ مُستَقيمٌ",
        "numberInSurah": 61,
        "juz": 25,
        "manzil": 6,
        "page": 494,
        "ruku": 429,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4387,
        "text": "وَلا يَصُدَّنَّكُمُ الشَّيطٰنُ ۖ إِنَّهُ لَكُم عَدُوٌّ مُبينٌ",
        "numberInSurah": 62,
        "juz": 25,
        "manzil": 6,
        "page": 494,
        "ruku": 429,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4388,
        "text": "وَلَمّا جاءَ عيسىٰ بِالبَيِّنٰتِ قالَ قَد جِئتُكُم بِالحِكمَةِ وَلِأُبَيِّنَ لَكُم بَعضَ الَّذى تَختَلِفونَ فيهِ ۖ فَاتَّقُوا اللَّهَ وَأَطيعونِ",
        "numberInSurah": 63,
        "juz": 25,
        "manzil": 6,
        "page": 494,
        "ruku": 429,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4389,
        "text": "إِنَّ اللَّهَ هُوَ رَبّى وَرَبُّكُم فَاعبُدوهُ ۚ هٰذا صِرٰطٌ مُستَقيمٌ",
        "numberInSurah": 64,
        "juz": 25,
        "manzil": 6,
        "page": 494,
        "ruku": 429,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4390,
        "text": "فَاختَلَفَ الأَحزابُ مِن بَينِهِم ۖ فَوَيلٌ لِلَّذينَ ظَلَموا مِن عَذابِ يَومٍ أَليمٍ",
        "numberInSurah": 65,
        "juz": 25,
        "manzil": 6,
        "page": 494,
        "ruku": 429,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4391,
        "text": "هَل يَنظُرونَ إِلَّا السّاعَةَ أَن تَأتِيَهُم بَغتَةً وَهُم لا يَشعُرونَ",
        "numberInSurah": 66,
        "juz": 25,
        "manzil": 6,
        "page": 494,
        "ruku": 429,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4392,
        "text": "الأَخِلّاءُ يَومَئِذٍ بَعضُهُم لِبَعضٍ عَدُوٌّ إِلَّا المُتَّقينَ",
        "numberInSurah": 67,
        "juz": 25,
        "manzil": 6,
        "page": 494,
        "ruku": 429,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4393,
        "text": "يٰعِبادِ لا خَوفٌ عَلَيكُمُ اليَومَ وَلا أَنتُم تَحزَنونَ",
        "numberInSurah": 68,
        "juz": 25,
        "manzil": 6,
        "page": 494,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4394,
        "text": "الَّذينَ ءامَنوا بِـٔايٰتِنا وَكانوا مُسلِمينَ",
        "numberInSurah": 69,
        "juz": 25,
        "manzil": 6,
        "page": 494,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4395,
        "text": "ادخُلُوا الجَنَّةَ أَنتُم وَأَزوٰجُكُم تُحبَرونَ",
        "numberInSurah": 70,
        "juz": 25,
        "manzil": 6,
        "page": 494,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4396,
        "text": "يُطافُ عَلَيهِم بِصِحافٍ مِن ذَهَبٍ وَأَكوابٍ ۖ وَفيها ما تَشتَهيهِ الأَنفُسُ وَتَلَذُّ الأَعيُنُ ۖ وَأَنتُم فيها خٰلِدونَ",
        "numberInSurah": 71,
        "juz": 25,
        "manzil": 6,
        "page": 494,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4397,
        "text": "وَتِلكَ الجَنَّةُ الَّتى أورِثتُموها بِما كُنتُم تَعمَلونَ",
        "numberInSurah": 72,
        "juz": 25,
        "manzil": 6,
        "page": 494,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4398,
        "text": "لَكُم فيها فٰكِهَةٌ كَثيرَةٌ مِنها تَأكُلونَ",
        "numberInSurah": 73,
        "juz": 25,
        "manzil": 6,
        "page": 494,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4399,
        "text": "إِنَّ المُجرِمينَ فى عَذابِ جَهَنَّمَ خٰلِدونَ",
        "numberInSurah": 74,
        "juz": 25,
        "manzil": 6,
        "page": 495,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4400,
        "text": "لا يُفَتَّرُ عَنهُم وَهُم فيهِ مُبلِسونَ",
        "numberInSurah": 75,
        "juz": 25,
        "manzil": 6,
        "page": 495,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4401,
        "text": "وَما ظَلَمنٰهُم وَلٰكِن كانوا هُمُ الظّٰلِمينَ",
        "numberInSurah": 76,
        "juz": 25,
        "manzil": 6,
        "page": 495,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4402,
        "text": "وَنادَوا يٰمٰلِكُ لِيَقضِ عَلَينا رَبُّكَ ۖ قالَ إِنَّكُم مٰكِثونَ",
        "numberInSurah": 77,
        "juz": 25,
        "manzil": 6,
        "page": 495,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4403,
        "text": "لَقَد جِئنٰكُم بِالحَقِّ وَلٰكِنَّ أَكثَرَكُم لِلحَقِّ كٰرِهونَ",
        "numberInSurah": 78,
        "juz": 25,
        "manzil": 6,
        "page": 495,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4404,
        "text": "أَم أَبرَموا أَمرًا فَإِنّا مُبرِمونَ",
        "numberInSurah": 79,
        "juz": 25,
        "manzil": 6,
        "page": 495,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4405,
        "text": "أَم يَحسَبونَ أَنّا لا نَسمَعُ سِرَّهُم وَنَجوىٰهُم ۚ بَلىٰ وَرُسُلُنا لَدَيهِم يَكتُبونَ",
        "numberInSurah": 80,
        "juz": 25,
        "manzil": 6,
        "page": 495,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4406,
        "text": "قُل إِن كانَ لِلرَّحمٰنِ وَلَدٌ فَأَنا۠ أَوَّلُ العٰبِدينَ",
        "numberInSurah": 81,
        "juz": 25,
        "manzil": 6,
        "page": 495,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4407,
        "text": "سُبحٰنَ رَبِّ السَّمٰوٰتِ وَالأَرضِ رَبِّ العَرشِ عَمّا يَصِفونَ",
        "numberInSurah": 82,
        "juz": 25,
        "manzil": 6,
        "page": 495,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4408,
        "text": "فَذَرهُم يَخوضوا وَيَلعَبوا حَتّىٰ يُلٰقوا يَومَهُمُ الَّذى يوعَدونَ",
        "numberInSurah": 83,
        "juz": 25,
        "manzil": 6,
        "page": 495,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4409,
        "text": "وَهُوَ الَّذى فِى السَّماءِ إِلٰهٌ وَفِى الأَرضِ إِلٰهٌ ۚ وَهُوَ الحَكيمُ العَليمُ",
        "numberInSurah": 84,
        "juz": 25,
        "manzil": 6,
        "page": 495,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4410,
        "text": "وَتَبارَكَ الَّذى لَهُ مُلكُ السَّمٰوٰتِ وَالأَرضِ وَما بَينَهُما وَعِندَهُ عِلمُ السّاعَةِ وَإِلَيهِ تُرجَعونَ",
        "numberInSurah": 85,
        "juz": 25,
        "manzil": 6,
        "page": 495,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4411,
        "text": "وَلا يَملِكُ الَّذينَ يَدعونَ مِن دونِهِ الشَّفٰعَةَ إِلّا مَن شَهِدَ بِالحَقِّ وَهُم يَعلَمونَ",
        "numberInSurah": 86,
        "juz": 25,
        "manzil": 6,
        "page": 495,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4412,
        "text": "وَلَئِن سَأَلتَهُم مَن خَلَقَهُم لَيَقولُنَّ اللَّهُ ۖ فَأَنّىٰ يُؤفَكونَ",
        "numberInSurah": 87,
        "juz": 25,
        "manzil": 6,
        "page": 495,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4413,
        "text": "وَقيلِهِ يٰرَبِّ إِنَّ هٰؤُلاءِ قَومٌ لا يُؤمِنونَ",
        "numberInSurah": 88,
        "juz": 25,
        "manzil": 6,
        "page": 495,
        "ruku": 430,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4414,
        "text": "فَاصفَح عَنهُم وَقُل سَلٰمٌ ۚ فَسَوفَ يَعلَمونَ",
