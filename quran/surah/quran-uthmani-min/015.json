  "code": 200,
  "status": "OK",
  "data": {
    "number": 15,
    "name": "سُورَةُ الحِجۡرِ",
    "englishName": "Al-Hijr",
    "englishNameTranslation": "The Rock",
    "revelationType": "Meccan",
    "numberOfAyahs": 99,
    "ayahs": [
      {
        "number": 1803,
        "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ الر ۚ تِلكَ ءايٰتُ الكِتٰبِ وَقُرءانٍ مُبينٍ",
        "numberInSurah": 1,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1804,
        "text": "رُبَما يَوَدُّ الَّذينَ كَفَروا لَو كانوا مُسلِمينَ",
        "numberInSurah": 2,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1805,
        "text": "ذَرهُم يَأكُلوا وَيَتَمَتَّعوا وَيُلهِهِمُ الأَمَلُ ۖ فَسَوفَ يَعلَمونَ",
        "numberInSurah": 3,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1806,
        "text": "وَما أَهلَكنا مِن قَريَةٍ إِلّا وَلَها كِتابٌ مَعلومٌ",
        "numberInSurah": 4,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1807,
        "text": "ما تَسبِقُ مِن أُمَّةٍ أَجَلَها وَما يَستَـٔخِرونَ",
        "numberInSurah": 5,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1808,
        "text": "وَقالوا يٰأَيُّهَا الَّذى نُزِّلَ عَلَيهِ الذِّكرُ إِنَّكَ لَمَجنونٌ",
        "numberInSurah": 6,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1809,
        "text": "لَو ما تَأتينا بِالمَلٰئِكَةِ إِن كُنتَ مِنَ الصّٰدِقينَ",
        "numberInSurah": 7,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1810,
        "text": "ما نُنَزِّلُ المَلٰئِكَةَ إِلّا بِالحَقِّ وَما كانوا إِذًا مُنظَرينَ",
        "numberInSurah": 8,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1811,
        "text": "إِنّا نَحنُ نَزَّلنَا الذِّكرَ وَإِنّا لَهُ لَحٰفِظونَ",
        "numberInSurah": 9,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1812,
        "text": "وَلَقَد أَرسَلنا مِن قَبلِكَ فى شِيَعِ الأَوَّلينَ",
        "numberInSurah": 10,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1813,
        "text": "وَما يَأتيهِم مِن رَسولٍ إِلّا كانوا بِهِ يَستَهزِءونَ",
        "numberInSurah": 11,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1814,
        "text": "كَذٰلِكَ نَسلُكُهُ فى قُلوبِ المُجرِمينَ",
        "numberInSurah": 12,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1815,
        "text": "لا يُؤمِنونَ بِهِ ۖ وَقَد خَلَت سُنَّةُ الأَوَّلينَ",
        "numberInSurah": 13,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1816,
        "text": "وَلَو فَتَحنا عَلَيهِم بابًا مِنَ السَّماءِ فَظَلّوا فيهِ يَعرُجونَ",
        "numberInSurah": 14,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1817,
        "text": "لَقالوا إِنَّما سُكِّرَت أَبصٰرُنا بَل نَحنُ قَومٌ مَسحورونَ",
        "numberInSurah": 15,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1818,
        "text": "وَلَقَد جَعَلنا فِى السَّماءِ بُروجًا وَزَيَّنّٰها لِلنّٰظِرينَ",
        "numberInSurah": 16,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1819,
        "text": "وَحَفِظنٰها مِن كُلِّ شَيطٰنٍ رَجيمٍ",
        "numberInSurah": 17,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1820,
        "text": "إِلّا مَنِ استَرَقَ السَّمعَ فَأَتبَعَهُ شِهابٌ مُبينٌ",
        "numberInSurah": 18,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1821,
        "text": "وَالأَرضَ مَدَدنٰها وَأَلقَينا فيها رَوٰسِىَ وَأَنبَتنا فيها مِن كُلِّ شَيءٍ مَوزونٍ",
        "numberInSurah": 19,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1822,
        "text": "وَجَعَلنا لَكُم فيها مَعٰيِشَ وَمَن لَستُم لَهُ بِرٰزِقينَ",
        "numberInSurah": 20,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1823,
        "text": "وَإِن مِن شَيءٍ إِلّا عِندَنا خَزائِنُهُ وَما نُنَزِّلُهُ إِلّا بِقَدَرٍ مَعلومٍ",
        "numberInSurah": 21,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1824,
        "text": "وَأَرسَلنَا الرِّيٰحَ لَوٰقِحَ فَأَنزَلنا مِنَ السَّماءِ ماءً فَأَسقَينٰكُموهُ وَما أَنتُم لَهُ بِخٰزِنينَ",
        "numberInSurah": 22,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1825,
        "text": "وَإِنّا لَنَحنُ نُحيۦ وَنُميتُ وَنَحنُ الوٰرِثونَ",
        "numberInSurah": 23,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1826,
        "text": "وَلَقَد عَلِمنَا المُستَقدِمينَ مِنكُم وَلَقَد عَلِمنَا المُستَـٔخِرينَ",
        "numberInSurah": 24,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1827,
        "text": "وَإِنَّ رَبَّكَ هُوَ يَحشُرُهُم ۚ إِنَّهُ حَكيمٌ عَليمٌ",
        "numberInSurah": 25,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1828,
        "text": "وَلَقَد خَلَقنَا الإِنسٰنَ مِن صَلصٰلٍ مِن حَمَإٍ مَسنونٍ",
        "numberInSurah": 26,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1829,
        "text": "وَالجانَّ خَلَقنٰهُ مِن قَبلُ مِن نارِ السَّمومِ",
        "numberInSurah": 27,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1830,
        "text": "وَإِذ قالَ رَبُّكَ لِلمَلٰئِكَةِ إِنّى خٰلِقٌ بَشَرًا مِن صَلصٰلٍ مِن حَمَإٍ مَسنونٍ",
        "numberInSurah": 28,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1831,
        "text": "فَإِذا سَوَّيتُهُ وَنَفَختُ فيهِ مِن روحى فَقَعوا لَهُ سٰجِدينَ",
        "numberInSurah": 29,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1832,
        "text": "فَسَجَدَ المَلٰئِكَةُ كُلُّهُم أَجمَعونَ",
        "numberInSurah": 30,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1833,
        "text": "إِلّا إِبليسَ أَبىٰ أَن يَكونَ مَعَ السّٰجِدينَ",
        "numberInSurah": 31,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1834,
        "text": "قالَ يٰإِبليسُ ما لَكَ أَلّا تَكونَ مَعَ السّٰجِدينَ",
        "numberInSurah": 32,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1835,
        "text": "قالَ لَم أَكُن لِأَسجُدَ لِبَشَرٍ خَلَقتَهُ مِن صَلصٰلٍ مِن حَمَإٍ مَسنونٍ",
        "numberInSurah": 33,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1836,
        "text": "قالَ فَاخرُج مِنها فَإِنَّكَ رَجيمٌ",
        "numberInSurah": 34,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1837,
        "text": "وَإِنَّ عَلَيكَ اللَّعنَةَ إِلىٰ يَومِ الدّينِ",
        "numberInSurah": 35,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1838,
        "text": "قالَ رَبِّ فَأَنظِرنى إِلىٰ يَومِ يُبعَثونَ",
        "numberInSurah": 36,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1839,
        "text": "قالَ فَإِنَّكَ مِنَ المُنظَرينَ",
        "numberInSurah": 37,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1840,
        "text": "إِلىٰ يَومِ الوَقتِ المَعلومِ",
        "numberInSurah": 38,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1841,
        "text": "قالَ رَبِّ بِما أَغوَيتَنى لَأُزَيِّنَنَّ لَهُم فِى الأَرضِ وَلَأُغوِيَنَّهُم أَجمَعينَ",
        "numberInSurah": 39,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1842,
        "text": "إِلّا عِبادَكَ مِنهُمُ المُخلَصينَ",
        "numberInSurah": 40,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1843,
        "text": "قالَ هٰذا صِرٰطٌ عَلَىَّ مُستَقيمٌ",
        "numberInSurah": 41,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1844,
        "text": "إِنَّ عِبادى لَيسَ لَكَ عَلَيهِم سُلطٰنٌ إِلّا مَنِ اتَّبَعَكَ مِنَ الغاوينَ",
        "numberInSurah": 42,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1845,
        "text": "وَإِنَّ جَهَنَّمَ لَمَوعِدُهُم أَجمَعينَ",
        "numberInSurah": 43,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1846,
        "text": "لَها سَبعَةُ أَبوٰبٍ لِكُلِّ بابٍ مِنهُم جُزءٌ مَقسومٌ",
        "numberInSurah": 44,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1847,
        "text": "إِنَّ المُتَّقينَ فى جَنّٰتٍ وَعُيونٍ",
        "numberInSurah": 45,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1848,
        "text": "ادخُلوها بِسَلٰمٍ ءامِنينَ",
        "numberInSurah": 46,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1849,
        "text": "وَنَزَعنا ما فى صُدورِهِم مِن غِلٍّ إِخوٰنًا عَلىٰ سُرُرٍ مُتَقٰبِلينَ",
        "numberInSurah": 47,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1850,
        "text": "لا يَمَسُّهُم فيها نَصَبٌ وَما هُم مِنها بِمُخرَجينَ",
        "numberInSurah": 48,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1851,
        "text": "۞ نَبِّئ عِبادى أَنّى أَنَا الغَفورُ الرَّحيمُ",
        "numberInSurah": 49,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1852,
        "text": "وَأَنَّ عَذابى هُوَ العَذابُ الأَليمُ",
        "numberInSurah": 50,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1853,
        "text": "وَنَبِّئهُم عَن ضَيفِ إِبرٰهيمَ",
        "numberInSurah": 51,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1854,
        "text": "إِذ دَخَلوا عَلَيهِ فَقالوا سَلٰمًا قالَ إِنّا مِنكُم وَجِلونَ",
        "numberInSurah": 52,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1855,
        "text": "قالوا لا تَوجَل إِنّا نُبَشِّرُكَ بِغُلٰمٍ عَليمٍ",
        "numberInSurah": 53,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1856,
        "text": "قالَ أَبَشَّرتُمونى عَلىٰ أَن مَسَّنِىَ الكِبَرُ فَبِمَ تُبَشِّرونَ",
        "numberInSurah": 54,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1857,
        "text": "قالوا بَشَّرنٰكَ بِالحَقِّ فَلا تَكُن مِنَ القٰنِطينَ",
        "numberInSurah": 55,
