  "data": {
    "number": 95,
    "name": "سُورَةُ التِّينِ",
    "englishName": "At-Tin",
    "englishNameTranslation": "The Fig",
    "revelationType": "Meccan",
    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6099,
        "text": "قسم به تین و زیتون (دو میوه معروف انجیر و زیت یا دو معبد بزرگ کعبه و بیت المقدس).",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6100,
        "text": "و قسم به طور سینا.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6101,
        "text": "و قسم به این شهر امن و امان (مکّه معظّم).",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6102,
        "text": "که ما انسان را در نیکوترین صورت (در مراتب وجود) بیافریدیم.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6103,
        "text": "سپس (به کیفر کفر و گناهش) به اسفل سافلین (جهنم و پست‌ترین رتبه امکان) برگردانیدیم.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6104,
        "text": "مگر آنان که ایمان آورده و نیکوکار شدند که به آنها پاداش دائمی (بهشت ابد) عطا کنیم.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6105,
        "text": "پس (ای انسان مشرک ناسپاس) چه تو را بر آن داشت که دین حق و روز جزا را تکذیب کنی؟",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6106,
        "text": "آیا خدا مقتدرترین و عادل‌ترین حکمفرمایان عالم نیست؟",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      }
    ],
