  "data": {
    "number": 107,
    "name": "سُورَةُ المَاعُونِ",
    "englishName": "Al-Maa'un",
    "englishNameTranslation": "Almsgiving",
    "revelationType": "Meccan",
    "numberOfAyahs": 7,
    "ayahs": [
      {
        "number": 6198,
        "text": "آیا دیدی آن کس را که روز جزا را انکار می‌کرد؟",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6199,
        "text": "این همان شخص (بی رحم) است که یتیم را (از در خود) به قهر می‌راند.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6200,
        "text": "و (کسی را) بر اطعام فقیر ترغیب نمی‌کند.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6201,
        "text": "پس وای بر آن نماز گزاران.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6202,
        "text": "که دل از یاد خدا غافل دارند.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6203,
        "text": "همانان که (اگر طاعتی کنند به) ریا و خود نمایی کنند.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6204,
        "text": "و زکات و احسان را (از فقیران و هر خیر کوچک را حتی قرض را هم از محتاجان) منع کنند.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      }
