    "numberOfAyahs": 5,
    "ayahs": [
      {
        "number": 6189,
        "text": "آیا ندیدی که خدای تو با اصحاب فیل (سپاه فیل سوار ابرهه) چه کرد؟",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 547,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6190,
        "text": "آیا کید و تدبیر آنها را (که برای خرابی کعبه اندیشیدند) تباه نکرد؟",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 547,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6191,
        "text": "و بر هلاک آنها مرغانی گروه گروه فرستاد.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 547,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6192,
        "text": "تا آن سپاه را به سنگهای سجّیل (دوزخی) سنگباران کردند.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 547,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6193,
        "text": "و تنشان را چون علفی زیر دندان حیوان خرد گردانید.",
        "numberInSurah": 5,
