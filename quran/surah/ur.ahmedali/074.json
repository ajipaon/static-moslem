{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 74,
    "name": "سُورَةُ المُدَّثِّرِ",
    "englishName": "Al-Muddaththir",
    "englishNameTranslation": "The Cloaked One",
    "revelationType": "Meccan",
    "numberOfAyahs": 56,
    "ayahs": [
      {
        "number": 5496,
        "text": "اے کپڑے میں لپٹنے والے",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5497,
        "text": "اٹھو پھر (کافروں کو) ڈراؤ",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5498,
        "text": "اور اپنے رب کی بڑائی بیان کرو",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5499,
        "text": "اور اپنے کپڑے پاک رکھو",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5500,
        "text": "اورمیل کچیل دور کرو",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5501,
        "text": "اوربدلہ پانے کی غرض سے احسان نہ کرو",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5502,
        "text": "اوراپنے رب کے لیے صبر کرو",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5503,
        "text": "پھر جب صور میں پھونکا جائے گا",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5504,
        "text": "پس وہ اس دن بڑا کٹھن دن ہو گا",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5505,
        "text": "کافروں پر وہ آسان نہ ہو گا",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5506,
        "text": "مجھے اور اس کو چھوڑ دو کہ جس کو میں نے اکیلا پیدا کیا",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5507,
        "text": "اوراس کوبڑھنے والا مال دیا",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5508,
        "text": "اور حاضر رہنے والے بیٹے دیئے",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5509,
        "text": "اور اس کے لیے ہر طرح کا سامان تیار کر دیا",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5510,
        "text": "پھر وہ طمع کرتا ہے کہ میں اور بڑھا دوں",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5511,
        "text": "ہرگز نہیں بےشک وہ ہماری آیات کا سخت مخالف ہے",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5512,
        "text": "عنقریب میں اسے اونچی گھاٹی پر چڑھاؤں گا",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5513,
        "text": "بے شک اس نے سوچا اور اندازہ لگایا",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5514,
        "text": "پھر اسے الله کی مار اس نے کیسا اندازہ لگایا",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5515,
        "text": "پھر اسے الله کی مار اس نے کیسا اندازہ لگایا",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5516,
        "text": "پھر اس نے دیکھا",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5517,
        "text": "پھر اس نے تیوری چڑھائی اور منہ بنایا",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5518,
        "text": "پھر پیٹھ پھیر لی اور تکبر کیا",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5519,
        "text": "پھر کہا یہ تو ایک جادو ہے جو چلا آتا ہے",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5520,
        "text": "یہ تو ہو نہ ہو آدمی کا کلام ہے",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5521,
        "text": "عنقریب اس کو دوزخ میں ڈالوں گا",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5522,
        "text": "اور آپ کو کیا خبر کہ دوزخ کیا ہے",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5523,
        "text": "نہ باقی رکھے اور نہ چھوڑے",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5524,
        "text": "آدمی کو جھلس دے",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5525,
        "text": "اس پر انیس (فرشتے) مقرر ہیں",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5526,
        "text": "اور ہم نے دوزخ پر فرشتے ہی رکھے ہیں اور ان کی تعداد کافروں کے لیے آزمائش بنائی ہے تاکہ جن کو کتاب دی گئی ہے وہ یقین کر لیں اور ایمان داروں کا ایمان بڑھے اورتاکہ اہلِ کتاب اور ایمان دار شک نہ کریں اور تاکہ جن کے دلوں میں (نفاق کی) بیماری ہے اورکافر یہ کہیں کہ الله کی اس بیان سے کیاغرض ہے اور الله اس طرح سے جسے چاہتاہے گمراہ کر تا ہے اور جسے چاہتا ہے ہدایت کرتا ہے اور آپ کے رب کے لشکروں کو اس کے سوا اور کوئی نہیں جانتا اور دوزخ (کا حال بیان کرنا) صرف آدمیوں کی نصیحت کے لیے ہے",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5527,
        "text": "نہیں نہیں قسم ہے چاند کی",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5528,
        "text": "اور رات کی جب وہ ڈھلے",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5529,
        "text": "اور صبح کی جب وہ روشن ہوجائے",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5530,
        "text": "کہ وہ (دوزخ) بڑی بڑی مصیبتوں میں سے ایک ہے",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5531,
        "text": "انسان کو ڈرانے والی ہے",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5532,
        "text": "تم میں سے ہر ایک کے لیے خواہ کوئی اس کے آگے آئے یا پیچھے ہٹے",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5533,
        "text": "ہر شخص اپنے اعمال کے سبب گروی ہے",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5534,
        "text": "مگر داہنے والے",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5535,
        "text": "باغوں میں ہو ں گے ایک دوسرے سے پوچھیں گے",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5536,
        "text": "گناہگارو ں کی نسبت",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5537,
        "text": "کس چیز نے تمہیں دوزخ میں ڈالا",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5538,
        "text": "وہ کہیں گے کہ ہم نمازی نہ تھے",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5539,
        "text": "اور نہ ہم مسکینوں کو کھانا کھلاتے تھے",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5540,
        "text": "اور ہم بکواس کرنے والوں کے ساتھ بکواس کیاکرتے تھے",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5541,
        "text": "اور ہم انصاف کے دن کو جھٹلایا کرتے تھے",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5542,
        "text": "یہاں تک کہ ہمیں موت آ پہنچی",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5543,
        "text": "پس ان کو سفارش کرنے والوں کی سفارش نفع نہ دے گی",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5544,
        "text": "پسانہیں کیا ہو گیا کہ وہ نصیحت سے منہ موڑرہے ہیں",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5545,
        "text": "گویا کہ وہ بدکنے والے گدھے ہیں",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5546,
        "text": "جو شیر سے بھاگے ہیں",
        "numberInSurah": 51,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5547,
        "text": "بلکہ ہر ایک آدمی ان میں سے چاہتا ہے کہ اسے کھلے ہوئے صحیفے دیئے جائیں",
        "numberInSurah": 52,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5548,
        "text": "ہرگز نہیں بلکہ وہ آخرت سے نہیں ڈرتے",
        "numberInSurah": 53,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5549,
        "text": "ہرگز نہیں بے شک یہ (قرآن) ایک نصیحت ہے",
        "numberInSurah": 54,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5550,
        "text": "(پس جو چاہے اس کو یاد کر لے",
        "numberInSurah": 55,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5551,
        "text": "اور کوئی بھی یاد نہیں کر سکتا مگر جبکہ الله ہی چاہے وہی جس سے ڈرنا چاہیے اوروہی بخشنے والا ہے",
        "numberInSurah": 56,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      }
    ],
    "edition": {
