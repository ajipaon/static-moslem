    "numberOfAyahs": 11,
    "ayahs": [
      {
        "number": 6080,
        "text": "دن کی روشنی کی قسم ہے",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6081,
        "text": "اور رات کی جب وہ چھا جائے",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6082,
        "text": "آپ کے رب نے نہ آپ کو چھوڑا ہے اور نہ بیزار ہوا ہے",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6083,
        "text": "اور البتہ آخرت آپ کے لیے دنیا سے بہتر ہے",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6084,
        "text": "اور آپ کا رب آپ کو (اتنا) دے گا کہ آپ خوش ہو جائیں گے",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6085,
        "text": "کیا اس نے آپ کو یتیم نہیں پایا تھا پھر جگہ دی",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6086,
        "text": "اور آپ کو (شریعت سے) بے خبر پایا پھر (شریعت کا) راستہ بتایا",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6087,
        "text": "اوراس نے آپ کو تنگدست پایا پھر غنی کر دیا",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6088,
        "text": "پھر یتیم کو دبایا نہ کرو",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6089,
        "text": "اورسائل کو جھڑکا نہ کرو",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6090,
        "text": "اورہر حال میں اپنے رب کے احسان کا ذکر کیا کرو",
        "numberInSurah": 11,
        "juz": 30,
