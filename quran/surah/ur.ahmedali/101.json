        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6159,
        "text": "وہ کھڑکھڑانے والی کیا ہے",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6160,
        "text": "اور آپ کو کیا خبر کہ وہ کھڑکھڑانے والی کیا ہے",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6161,
        "text": "جس دن لو گ بکھرے ہوئے پروانوں کی طرح ہوں گے",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6162,
        "text": "اور پہاڑ رنگی ہوئی دھنی ہوئی اُون کی طرح ہوں گے",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6163,
        "text": "تو جس کے اعمال (نیک) تول میں زیادہ ہوں گے",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6164,
        "text": "تو وہ خاطر خواہ عیش میں ہوگا",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6165,
        "text": "اور جس کے اعمال (نیک) تول میں کم ہوں گے",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6166,
        "text": "تواس کا ٹھکانا ہاویہ ہوگا",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6167,
        "text": "اور آپ کو کیا معلوم کہ وہ کیا چیز ہے",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6168,
        "text": "وہ دھکتی ہوئی آگ ہے",
        "numberInSurah": 11,
        "juz": 30,
