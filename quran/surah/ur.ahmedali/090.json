    "numberOfAyahs": 20,
    "ayahs": [
      {
        "number": 6024,
        "text": "ا س شہر کی قسم ہے",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6025,
        "text": "حالانکہ آپ اس شہر میں مقیم ہیں",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6026,
        "text": "اورباپ کی اور اس کی اولاد کی قسم ہے",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6027,
        "text": "کہ بےشک ہم نے انسان کو مصیبت میں پیدا کیا ہے",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6028,
        "text": "کیا وہ خیال کرتا ہےکہ اس پر کوئی بھی ہرگز قابو نہ پا سکے گا",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6029,
        "text": "کہتا ہے کہ میں نے مال برباد کر ڈالا",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6030,
        "text": "کیا وہ خیال کرتا ہے کہ اسے کسی نے بھی نہیں دیکھا",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6031,
        "text": "کیا ہم نے اس کے لیے دو آنکھیں نہیں بنائیں",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6032,
        "text": "اور زبان اور دو ہونٹ",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6033,
        "text": "اور ہم نے اسے دونوں راستے دکھائے",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6034,
        "text": "پس وہ (دین کی) گھاٹی میں سے نہ ہو کر نکلا",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6035,
        "text": "اور آپ کو کیا معلوم کہ وہ گھاٹی کیا ہے",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6036,
        "text": "گردن کا چھوڑانا",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6037,
        "text": "یا بھوک کے دن میں کھلانا",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6038,
        "text": "کسی رشتہ دار یتیم کو",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6039,
        "text": "یا کسی خاک نشین مسکین کو",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6040,
        "text": "پھر وہ ان میں سے ہو جو ایمان لائے اور انہوں نے ایک دوسرے کو صبر کی وصیت کی اور رحم کرنے کی وصیت کی",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6041,
        "text": "یہی لوگ دائیں والے ہیں",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6042,
        "text": "اور جنہوں نے ہماری آیتوں سے انکار کیا وہی بائیں والے ہیں",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6043,
        "text": "انہیں پر چاروں طرف سے بند کی ہوئی آگ ہے",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
