{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 75,
    "name": "سُورَةُ القِيَامَةِ",
    "englishName": "Al-Qiyaama",
    "englishNameTranslation": "The Resurrection",
    "revelationType": "Meccan",
    "numberOfAyahs": 40,
    "ayahs": [
      {
        "number": 5552,
        "text": "قیامت کے دن کی قسم ہے",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5553,
        "text": "اور پشیمان ہونے والے شخص کی قسم ہے",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5554,
        "text": "کیا انسان سمجھتا ہے کہ ہم اس کی ہڈیاں جمع نہ کریں گے",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5555,
        "text": "ہاں ہم تو اس پر قادر ہیں کہ اس کی پور پور درست کر دیں",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5556,
        "text": "بلکہ انسان تو چاہتا ہے کہ آئندہ بھی نافرمانی کرتا رہے",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5557,
        "text": "پوچھتا ہےکہ قیامت کا دن کب ہو گا",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5558,
        "text": "پس جب آنکھیں چندھیا جائیں گی",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5559,
        "text": "اور چاند بے نور ہو جائے گا",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5560,
        "text": "اور سورج اور چاند اکھٹے کیے جائیں گے",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5561,
        "text": "اس دن انسان کہے گا کہ بھاگنے کی جگہ کہاں ہے",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5562,
        "text": "ہر گز نہیں کہیں پناہ نہیں",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5563,
        "text": "اس دن آپ کے رب ہی کی طرف ٹھکانہ ہے",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5564,
        "text": "اس دن انسان کو بتا دیا جائے گا کہ وہ کیا لایا اور کیا چھوڑ آیا",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5565,
        "text": "بلکہ انسان اپنے اوپر خود شاہد ہے",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5566,
        "text": "گو وہ کتنے ہی بہانے پیش کرے",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5567,
        "text": "آپ (وحی ختم ہونے سے پہلے) قرآن پراپنی زبان نہ ہلایا کیجیئے تاکہ آپ اسے جلدی جلدی لیں",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5568,
        "text": "بے شک اس کا جمع کرنا اور پڑھا دینا ہمارے ذمہ ہے",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5569,
        "text": "پھر جب ہم ا سکی قرأت کر چکیں تو اس کی قرأت کا اتباع کیجیئے",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5570,
        "text": "پھر بے شک اس کا کھول کر بیان کرنا ہمارے ذمہ ہے",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5571,
        "text": "ہر گز نہیں بلکہ تم تو دنیا کو چاہتے ہو",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5572,
        "text": "اور آخرت کو چھوڑتے ہو",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5573,
        "text": "کئی چہرے اس دن تر و تازہ ہو ں گے",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5574,
        "text": "اپنے رب کی طرف دیکھتے ہوں گے",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5575,
        "text": "اور کتنے چہرے اس دن اداس ہو ں گے",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5576,
        "text": "خیال کر رہے ہوں گے کہ ان کے ساتھ کمر توڑ دینے والی سختی کی جائے گی",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5577,
        "text": "نہیں نہیں جب کہ جان گلے تک پہنچ جائے گی",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5578,
        "text": "اورلوگ کہیں گے کوئی جھاڑنے والا ہے",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5579,
        "text": "اور وہ خیال کرے گا کہ یہ وقت جدائی کا ہے",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5580,
        "text": "اور ایک پنڈلی دوسری پنڈلی سے لپٹ جائے گی",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5581,
        "text": "تیرے رب کی طرف اس دن چلنا ہوگا",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5582,
        "text": "پھر نہ تو اس نے تصدیق کی اور نہ نماز پڑھی",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5583,
        "text": "بلکہ جھٹلایا اورمنہ موڑا",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5584,
        "text": "پھر اپنے گھر والوں کی طرف اکڑتا ہوا چلا گیا",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5585,
        "text": "(اے انسان) تیرے لیے افسوس پرافسوس ہے",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5586,
        "text": "پھر تیرے لیے افسوس پر افسوس ہے",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5587,
        "text": "کیاانسان یہ سمجھ رہا ہے کہ وہ یونہی چھوڑ دیا جائے گا",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5588,
        "text": "کیا وہ ٹپکتی منی کی ایک بوند نہ تھا",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5589,
        "text": "پھر وہ لوتھڑا بنا پھر الله نے اسے بنا کر ٹھیک کیا",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5590,
        "text": "پھر اس نے مرد و عورت کا جوڑا بنایا",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5591,
        "text": "پھر کیا وہ الله مردے زندہ کردینے پر قادر نہیں",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      }
    ],
    "edition": {
