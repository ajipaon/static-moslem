        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6101,
        "text": "اور اس شہر (مکہ) کی جو امن والا ہے",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6102,
        "text": "بے شک ہم نے انسان کو بڑے عمدہ انداز میں پیدا کیا ہے",
        "numberInSurah": 4,
        "juz": 30,
