{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 78,
    "name": "سُورَةُ النَّبَإِ",
    "englishName": "An-Naba",
    "englishNameTranslation": "The Announcement",
    "revelationType": "Meccan",
    "numberOfAyahs": 40,
    "ayahs": [
      {
        "number": 5673,
        "text": "از چه چيز مى‌پرسند؟",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5674,
        "text": "از آن خبر بزرگ،",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5675,
        "text": "كه در آن اختلاف مى كنند.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5676,
        "text": "آرى، به زودى خواهند دانست.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5677,
        "text": "باز هم آرى، به زودى خواهند دانست.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5678,
        "text": "آيا ما زمين را بسترى نساختيم؟",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5679,
        "text": "و كوهها را ميخهايى؟",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5680,
        "text": "و شما را جفت جفت آفريديم.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5681,
        "text": "و خوابتان را آسايشتان گردانيديم.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5682,
        "text": "و شب را پوششتان قرار داديم.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5683,
        "text": "و روز را گاه طلب معيشت.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5684,
        "text": "و بر فراز سرتان هفت آسمان استوار بنا كرديم.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5685,
        "text": "و چراغى روشن آفريديم.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5686,
        "text": "از ابرهاى باران‌زاى آبى فراوان نازل كرديم،",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5687,
        "text": "تا بدان دانه و نبات برويانيم،",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5688,
        "text": "و بستانهاى انبوه.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5689,
        "text": "هر آينه روز داورى روزى است معين.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5690,
        "text": "روزى كه در صور دميده شود و شما فوج‌فوج بياييد.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5691,
        "text": "آسمان شكافته شود و هر شكاف درى باشد.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5692,
        "text": "و كوه‌ها روان شوند، و سراب گردند.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5693,
        "text": "جهنم در انتظار باشد.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5694,
        "text": "طاغيان را بازگشتنگاهى است.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5695,
        "text": "زمانى دراز در آنجا درنگ كنند.",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5696,
        "text": "نه خُنكى چشند و نه آب؛",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5697,
        "text": "جز آب جوشان و خون و چرك.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5698,
        "text": "اين كيفرى است برابر كردار.",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5699,
        "text": "زيرا آنان به روز حساب اميد نداشتند.",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5700,
        "text": "و آيات ما را به سختى تكذيب مى‌كردند.",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5701,
        "text": "و ما همه چيز را در كتابى شماره كرده‌ايم.",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5702,
        "text": "پس بچشيد كه جز به شكنجه شما نخواهيم افزود.",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5703,
        "text": "پرهيزگاران را جايى است در امان از هر آسيب؛",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5704,
        "text": "بستانها و تاكستانها،",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5705,
        "text": "و دخترانى همسال، با پستانهاى برآمده،",
        "numberInSurah": 33,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5706,
        "text": "و جامهاى پر.",
        "numberInSurah": 34,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5707,
        "text": "نه سخن بيهوده شنوند و نه دروغ.",
        "numberInSurah": 35,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5708,
        "text": "و اين پاداشى است كافى، از جانب پروردگارت.",
        "numberInSurah": 36,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5709,
        "text": "پروردگار آسمانها و زمين و آنچه ميان آنهاست. آن خداى رحمان، كه كس را به او ياراى خطاب نباشد.",
        "numberInSurah": 37,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5710,
        "text": "روزى كه روح و فرشتگان به صف مى‌ايستند، و كس سخن نمى‌گويد مگر آنكه خداى رحمان به او رخصت دهد و او سخن به صواب گويد.",
        "numberInSurah": 38,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5711,
        "text": "آن روز روزى است آمدنى. پس هر كه خواهد به سوى پروردگارش بازگردد.",
        "numberInSurah": 39,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5712,
        "text": "ما شما را از عذابى نزديك مى‌ترسانيم: روزى كه آدمى هر چه را پيشاپيش فرستاده است مى‌نگرد و كافر مى‌گويد: اى كاش من خاك مى‌بودم.",
        "numberInSurah": 40,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      }
    ],
