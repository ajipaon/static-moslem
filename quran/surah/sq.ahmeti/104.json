  "data": {
    "number": 104,
    "name": "سُورَةُ الهُمَزَةِ",
    "englishName": "Al-Humaza",
    "englishNameTranslation": "The Traducer",
    "revelationType": "Meccan",
    "numberOfAyahs": 9,
    "ayahs": [
      {
        "number": 6180,
        "text": "Mjerë për secilin që e ofendon e përqesh (njerëzit),",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6181,
        "text": "Që ka tubuar pasuri dhe që atë e ruan të mos i pakësohet.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6182,
        "text": "E mendon se pasuria e tij do të bëjë të përjetshëm.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6183,
        "text": "Jo, të mos medojë ashtu! Se ai pa tjetër do të hidhet në Hutame.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6184,
        "text": "E çka dini ti se ç’është Hutame?",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6185,
        "text": "Është zjarri i All-llahut i ndezur fort.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6186,
        "text": "Që depërton deri në loçkë të zemrës.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6187,
        "text": "Ai i mbyll ata, ua zë frymën.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6188,
        "text": "Ata janë të lidhur në pranga",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      }
    ],
