    "numberOfAyahs": 7,
    "ayahs": [
      {
        "number": 6198,
        "text": "A e ke parë ti (a e sheh) atë, që përgënjeshtron përgjegjësinë dhe llogarinënë botën tjetër?",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6199,
        "text": "Po ai është që e përzë në mënyrë të vrazhdë bonjakun.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6200,
        "text": "Dhe që nuk nxit për të ushqyer të varfërin.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6201,
        "text": "Pra shkatërrim është për ata që falen,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6202,
        "text": "të cilët ndaj namazit të tyre janë të pakujdesshëm.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6203,
        "text": "Ata që vetëm shtiren (sa për sy e faqe).",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6204,
        "text": "Dhe nuk japin as sendin më të vogël (as hua).",
        "numberInSurah": 7,
        "juz": 30,
