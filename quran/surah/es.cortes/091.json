  "data": {
    "number": 91,
    "name": "سُورَةُ الشَّمۡسِ",
    "englishName": "Ash-Shams",
    "englishNameTranslation": "The Sun",
    "revelationType": "Meccan",
    "numberOfAyahs": 15,
    "ayahs": [
      {
        "number": 6044,
        "text": "¡Por el sol y su claridad!",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6045,
        "text": "¡Por la luna cuando le sigue!",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6046,
        "text": "¡Por el día cuando lo muestra brillante!",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6047,
        "text": "¡Por la noche cuando lo vela!",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6048,
        "text": "¡Por el cielo y Quien lo ha edificado!",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6049,
        "text": "¡Por la tierra y Quien la ha extentido!",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6050,
        "text": "¡Por el alma y Quien le ha dado forma armoniosa,",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6051,
        "text": "instruyéndole sobre su propensión al pecado y su temor de Dios!",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6052,
        "text": "¡Bienaventurado quien la purifique!",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6053,
        "text": "¡Decepcionado, empero, quien la corrompa!",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6054,
        "text": "Los tamudeos, en su arrogancia, desmintieron.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6055,
        "text": "Cuando el más miserable de entre ellos se alzó.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6056,
        "text": "El enviado de Dios les dijo: «¡Dejad a la camella de Dios y que beba!»",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6057,
        "text": "Le desmintieron y la desjarretaron. Su Señor, entonces, les aniquiló por su pecado, a todos por igual,",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6058,
        "text": "sin temer las consecuencias de ello.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      }
    ],
