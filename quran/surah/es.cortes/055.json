        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4943,
        "text": "¿Cuál, pues, de los beneficios de vuestro Señor negaréis?",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
