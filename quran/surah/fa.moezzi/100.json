  "data": {
    "number": 100,
    "name": "سُورَةُ العَادِيَاتِ",
    "englishName": "Al-Aadiyaat",
    "englishNameTranslation": "The Chargers",
    "revelationType": "Meccan",
    "numberOfAyahs": 11,
    "ayahs": [
      {
        "number": 6147,
        "text": "سوگند به دوندگان به ستوه‌آمدن‌",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6148,
        "text": "پس آتش افروزندگان به نواختن سمها بر زمین‌",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6149,
        "text": "پس تاخت آرندگان به بامداد",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6150,
        "text": "پس برانگیزند بدان گردی‌",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6151,
        "text": "پس درآیند بدان در میان گروهی‌",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6152,
        "text": "همانا انسان است به پروردگار خویش ناسپاس‌",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6153,
        "text": "و همانا او است بر این گواه‌",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6154,
        "text": "و همانا او است در دوستی خواسته (یا اسبان) سرسخت‌",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6155,
        "text": "پس آیا نداند گاهی که انگیخته شود آنچه در گورها است‌",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6156,
        "text": "و فراهم شود آنچه در سینه‌ها است‌",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 542,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6157,
        "text": "که پروردگار ایشان است بدیشان آگاه‌",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 542,
        "hizbQuarter": 240,
        "sajda": false
      }
    ],
