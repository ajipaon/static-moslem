    "name": "سُورَةُ المَاعُونِ",
    "englishName": "Al-Maa'un",
    "englishNameTranslation": "Almsgiving",
    "revelationType": "Meccan",
    "numberOfAyahs": 7,
    "ayahs": [
      {
        "number": 6198,
        "text": "আপনি কি দেখেছেন তাকে, যে বিচারদিবসকে মিথ্যা বলে?",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6199,
        "text": "সে সেই ব্যক্তি, যে এতীমকে গলা ধাক্কা দেয়",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6200,
        "text": "এবং মিসকীনকে অন্ন দিতে উৎসাহিত করে না।",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6201,
        "text": "অতএব দুর্ভোগ সেসব নামাযীর,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6202,
        "text": "যারা তাদের নামায সম্বন্ধে বে-খবর;",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6203,
        "text": "যারা তা লোক-দেখানোর জন্য করে",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6204,
        "text": "এবং নিত্য ব্যবহার্য্য বস্তু অন্যকে দেয় না।",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
