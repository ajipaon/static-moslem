  "data": {
    "number": 100,
    "name": "سُورَةُ العَادِيَاتِ",
    "englishName": "Al-Aadiyaat",
    "englishNameTranslation": "The Chargers",
    "revelationType": "Meccan",
    "numberOfAyahs": 11,
    "ayahs": [
      {
        "number": 6147,
        "text": "শপথ উর্ধ্বশ্বাসে চলমান অশ্বসমূহের,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6148,
        "text": "অতঃপর ক্ষুরাঘাতে অগ্নিবিচ্ছুরক অশ্বসমূহের",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6149,
        "text": "অতঃপর প্রভাতকালে আক্রমণকারী অশ্বসমূহের",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6150,
        "text": "ও যারা সে সময়ে ধুলি উৎক্ষিপ্ত করে",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6151,
        "text": "অতঃপর যারা শক্রদলের অভ্যন্তরে ঢুকে পড়ে-",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6152,
        "text": "নিশ্চয় মানুষ তার পালনকর্তার প্রতি অকৃতজ্ঞ।",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6153,
        "text": "এবং সে অবশ্য এ বিষয়ে অবহিত",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6154,
        "text": "এবং সে নিশ্চিতই ধন-সম্পদের ভালবাসায় মত্ত।",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6155,
        "text": "সে কি জানে না, যখন কবরে যা আছে, তা উত্থিত হবে",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6156,
        "text": "এবং অন্তরে যা আছে, তা অর্জন করা হবে?",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 542,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6157,
        "text": "সেদিন তাদের কি হবে, সে সম্পর্কে তাদের পালনকর্তা সবিশেষ জ্ঞাত।",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 542,
        "hizbQuarter": 240,
        "sajda": false
      }
    ],
