{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 74,
    "name": "سُورَةُ المُدَّثِّرِ",
    "englishName": "Al-Muddaththir",
    "englishNameTranslation": "The Cloaked One",
    "revelationType": "Meccan",
    "numberOfAyahs": 56,
    "ayahs": [
      {
        "number": 5496,
        "text": "اي (پاڻتي) ڪپڙي ويڙھڻ وارا مڙس.",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5497,
        "text": "کڙو ٿي پوءِ ڊيڄار.",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5498,
        "text": "۽ پنھنجي پالڻھار جي وڏائي بيان ڪر.",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5499,
        "text": "۽ پنھنجا ڪپڙا پاڪ رک.",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5500,
        "text": "۽ پليتي کان پري رھ.",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5501,
        "text": "۽ (اھڙو) احسان نه ڪر جو زيادہ عوض گھرين.",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5502,
        "text": "۽ پنھنجي پالڻھار (جي حُڪم) لاءِ صبر اختيار ڪر.",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5503,
        "text": "پوءِ جڏھن صُور ۾ ڦوڪبو.",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5504,
        "text": "تڏھن اُن ڏينھن اوکو وقت ٿيندو.",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5505,
        "text": "ڪافرن تي اڻ سکيو (وقت) ھوندو.",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5506,
        "text": "مون کي ۽ جنھن کي اڪيلو پيدا ڪيم تنھن کي ڇڏي ڏي.",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5507,
        "text": "۽ اُن کي گھڻو مال ڏنم.",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5508,
        "text": "۽ (مجلسن ۾) حاضر ٿيڻ وارا پُٽ (به ڏنم).",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5509,
        "text": "۽ ان کي پوري طرح وڌايم.",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5510,
        "text": "وري (به) لالچ رکندو آھي ته (کيس) وڌيڪ ڏيان.",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5511,
        "text": "ائين نه آھي، بيشڪ اُھو اسان جي آيتن سان جھڳڙو ڪندڙ آھي.",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5512,
        "text": "جلد اُن کي وڏي سختي سان تڪليف ڏيندس.",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5513,
        "text": "بيشڪ اُن دل ۾ سوچ ڪئي ۽ ٺھراءُ ڪيو.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5514,
        "text": "پوءِ لعنت ھجيس شال ڪيئن ٺھراء ڪيائين.",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5515,
        "text": "وري به لعنت ھجيس شال ڪيئن ٺھراءُ ڪيائين.",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5516,
        "text": "وري (غور ڪري) ڏٺائين.",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5517,
        "text": "وري منھن ۾ شور وڌائين ۽ گھنڊ پاتائين.",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5518,
        "text": "وري پٺيرو ٿيو ۽ وڏائي ڪيائين.",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5519,
        "text": "پوءِ چيائين ته ھي (قرآن) رڳو جادو آھي جو (جادوگرن کان) ھليو اچي ٿو.",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5520,
        "text": "ھي (قرآن) رڳو ماڻھوءَ جو ڪلام آھي.",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5521,
        "text": "جلد اُن کي دوزخ ۾ وجھندس.",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5522,
        "text": "۽ توکي ڪھڙي شيءَ ڄاڻايو ته دوزخ ڇا آھي.",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5523,
        "text": "اُھو نڪي ڪجھ (گوشت) رھائي ٿو ۽ نڪي (ھڏا) ڇڏي ٿو.",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5524,
        "text": "ماڻھن کي (ساڙي) ڪارو ڪندڙ آھي.",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5525,
        "text": "اُن تي اُڻويہ ڄڻا دربان آھن.",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5526,
        "text": "۽ دوزخ جا دربان رڳو ملائڪ ڪيا اٿون، ۽ سندن ڳاڻيٽو رڳو ڪافرن لاءِ پرک ڪئي اٿون ته ڪتاب وارا يقين ڪن ۽ ايمان وارا پنھنجي ايمان ۾ وڌن ۽ ڪتاب وارا ۽ مؤمن شڪ ۾ نه پون ۽ ته جن جي دلين ۾ (منافقيءَ جي) بيماري آھي سي ۽ ڪافر چون ته ھن مثال ڏيڻ ۾ الله جو ڇا مطلب آھي، اھڙيءَ طرح الله جنھن کي گھري (تنھن کي) ڀلائيندو آھي ۽ جنھن کي گھري (تنھن کي) ھدايت ڪندو آھي، ۽ تنھنجي پالڻھار جا لشڪر اُن کانسواءِ (ٻيو) ڪونه ڄاڻندو آھي، ۽ ھيءَ (دوزخ) سڀ ڪنھن ماڻھوءَ لاءِ رڳو نصيحت آھي.",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5527,
        "text": "سچ چوان ٿو چنڊ جو قسم آھي.",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5528,
        "text": "۽ رات جو جڏھن پٺ ڦيري.",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5529,
        "text": "۽ صبح جو (قسم آھي) جڏھن روشن ٿئي.",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5530,
        "text": "ته بيشڪ دوزخ ھڪ وڏين شين مان آھي.",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5531,
        "text": "(سڀڪنھن) ماڻھو کي ڊيڄاريندڙ آھي.",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5532,
        "text": "اوھان مان انھي لاءِ جيڪو گھري ته (بھشت) لاءِ اڳي وڌي يا (دوزخ ۾) پٺ تي رھي.",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5533,
        "text": "سڀڪو جيءُ پنھنجي ڪئي ۾ ڳه پيل آھي.",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5534,
        "text": "پر سڄي پاسي وارا (ڇٽل آھن).",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5535,
        "text": "(بھشت جي) باغن ۾ ھوندا، گڏجي پڇندا.",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5536,
        "text": "ڏوھارين کان.",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5537,
        "text": "ته اوھان کي دوزخ ۾ ڪھڙي ڳالھ وڌو.",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5538,
        "text": "چوندا ته نڪي نمازين مان ھواسون.",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5539,
        "text": "۽ نڪي مسڪينن کي کارائيندا ھواسون.",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5540,
        "text": "۽ بڪواتن سان گڏ بڪوات ڪندا ڪندا ھواسون.",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5541,
        "text": "۽ بدلي جي ڏينھن کي ڪوڙ ڀانئيندا ھواسون.",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5542,
        "text": "تان جو اسان کي موت پھتو.",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5543,
        "text": "پوءِ کين پارت ڪندڙن جي پارت نفعو نه ڏيندي.",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5544,
        "text": "پوءِ انھن کي ڇا (ٿيو) آھي جو نصيحت کان منھن موڙيندڙ (ٿيا) آھن.",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5545,
        "text": "ڄڻ ته اُھي ٽھندڙ گڏھ آھن.",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5546,
        "text": "جي شينھن کان ڀڳا ھجن.",
        "numberInSurah": 51,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5547,
        "text": "بلڪ منجھائن ھر ھڪ مڙس گھرندو آھي ته کيس (آسمان) مان کليل ڪتاب ڏجن.",
        "numberInSurah": 52,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5548,
        "text": "ائين نه آھي، بلڪ آخرت کان نه ٿا ڊڄن.",
        "numberInSurah": 53,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5549,
        "text": "سچ آھي ته بيشڪ قرآن ھڪ نصيحت آھي.",
        "numberInSurah": 54,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5550,
        "text": "پوءِ جيڪو گُھري سو اُن کي پڙھي.",
        "numberInSurah": 55,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5551,
        "text": "۽ الله جي گھر کانسواءِ (ان کي) ياد ڪري نه سگھندا آھن، (الله) اُھو آھي جنھن کان ڊڄڻ گھرجي ۽ اھو بخشش جو به مالڪ آھي.",
        "numberInSurah": 56,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "sd.amroti",
