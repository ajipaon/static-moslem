  "data": {
    "number": 92,
    "name": "سُورَةُ اللَّيۡلِ",
    "englishName": "Al-Lail",
    "englishNameTranslation": "The Night",
    "revelationType": "Meccan",
    "numberOfAyahs": 21,
    "ayahs": [
      {
        "number": 6059,
        "text": "رات جو قسم آھي جڏھن ڍَڪي ٿي.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6060,
        "text": "۽ ڏينھن جو (قسم آھي) جڏھن روشن ٿئي ٿو.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6061,
        "text": "۽ اُن جو (قسم آھي) جنھن نر ۽ مادي خلقي.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6062,
        "text": "ته بيشڪ اوھان جي ڪوشش قسمين قسمين آھي.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6063,
        "text": "پوءِ جنھن (الله لڳ) ڏنو ۽ پرھيزگاري ڪئي.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6064,
        "text": "۽ چڱائي کي سچ ڄاتائين.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6065,
        "text": "تنھنکي (نجات جو) آسان رستو سھنجو ڪري ڏينداسين.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6066,
        "text": "۽ جنھن بخل ڪيو ۽ (پاڻ کي) بي پرواھ ڄاتائين.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6067,
        "text": "۽ چڱائي کي ڪوڙ ڀانيائين.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6068,
        "text": "تنھن کي اوکائيءَ (۾ پوڻ) لاءِ (رستو) آسان ڪري ڏينداسين.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6069,
        "text": "۽ جڏھن اونڌو ڪرندو ته سندس مال کانئس (ڪجھ) نه ٽاريندو.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6070,
        "text": "بيشڪ سڌو رستو ڏيکارڻ اسان تي (لازم) آھي.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6071,
        "text": "۽ بيشڪ آخرت ۽ دُنيا (ٻئي) اسان جي وس ۾ آھن.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6072,
        "text": "پوءِ اوھان کي باھ کان ڊيڄاريم جا ڄڀي ڪري ٿي.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6073,
        "text": "(اھڙي) وڏي نڀاڳي کانسواءِ (ٻيو) ڪو اُن ۾ ڪونه گھڙندو.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6074,
        "text": "جنھن (اسلام کي) ڪوڙ ڄاتو ۽ پٺيرو ٿيو.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6075,
        "text": "۽ اُن (باھ) کان انھي وڏي پرھيزگار کي پاسي ڪبو.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6076,
        "text": "جيڪو پنھنجو مال (الله جي واٽ ۾) ڏئي ٿو ته پاڪ ٿئي.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6077,
        "text": "۽ مٿس اھڙي ڪنھن جو ڪو احسان ٿيل نه آھي جو بدلو ڏجي.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6078,
        "text": "پر پنھنجي تمام مٿاھين پالڻھار جي رضامندي طلبڻ لاءِ (ڏئي ٿو).",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6079,
        "text": "۽ ضرور جلد خوش ٿيندو.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      }
    ],
