  "data": {
    "number": 55,
    "name": "سُورَةُ الرَّحۡمَٰن",
    "englishName": "Ar-Rahmaan",
    "englishNameTranslation": "The Beneficent",
    "revelationType": "Medinan",
    "numberOfAyahs": 78,
    "ayahs": [
      {
        "number": 4902,
        "text": "ٻاجھاري (الله).",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4903,
        "text": "قرآن سيکاريو.",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4904,
        "text": "ماڻھو کي پيدا ڪيائين.",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4905,
        "text": "ان کي پڌرو ڳالھائڻ سيکاريائين.",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4906,
        "text": "سج ۽ چنڊ پوري حساب تي ھلندا آھن.",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4907,
        "text": "۽ وليون ۽ وڻ (الله کي) سجدو ڪندا آھن.",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4908,
        "text": "۽ آسمان کي بلند ڪيائين ۽ انصاف جي ترازي رکيائين.",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4909,
        "text": "ھن لاءِ ته تور ۾ حد کان نه لنگھو.",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4910,
        "text": "۽ انصاف سان پورو توريندا رھو ۽ تور ۾ کوٽ نه ڪريو.",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4911,
        "text": "ماڻھن لاءِ زمين کي پکيڙيائين.",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4912,
        "text": "منجھس (ھر جنس) جو ميوو ۽ چپڙين واريون کجيون آھن.",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4913,
        "text": "۽ بُھ وارو اَنُّ ۽ سرھاڻيون (به اُن ۾) آھن.",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4914,
        "text": "پوءِ (اي جنّؤ ۽ ماڻھو) پنھنجي رب جي ڪھڙين نعمتن جو انڪار ڪندؤ.",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4915,
        "text": "ماڻھوءَ کي ٽِڪر جھڙي وڄندڙ مٽيءَ مان پيدا ڪيائين.",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4916,
        "text": "۽ جنّ کي باھ جي الانبي مان بڻايائين.",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4917,
        "text": "پوءِ (اي جنّؤ ۽ ماڻھو) پنھنجي پالڻھار جي ڪھڙين نعمتن جو انڪار ڪندؤ.",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4918,
        "text": "جو ٻنھي اوڀرن جو پالڻھار آھي ۽ ٻنھي اولھن جو پالڻھار آھي.",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4919,
        "text": "پوءِ (اي جنّؤ ۽ ماڻھو) پنھنجي پالڻھار جي ڪھڙين نعمتن جو انڪار ڪندؤ.",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4920,
        "text": "ٻن دريائن (کاري ۽ مٺيءَ) کي وھايائين جو پاڻ ۾ گڏجي ٿا وھن.",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4921,
        "text": "انھن ٻنھي جي وچ ۾ اوٽ آھي جو ھڪ ٻئي تي زيادتي نه ڪندا آھن.",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4922,
        "text": "پوءِ (اي جنّؤ ۽ ماڻھو) پنھنجي پالڻھار جي ڪھڙين نعمتن جو انڪار ڪندؤ.",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4923,
        "text": "انھن ٻنھي مان موتي ۽ مرجان نڪرندا آھن.",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4924,
        "text": "پوءِ (اي جنّو ۽ ماڻھو) پنھنجي پالڻھار جي ڪھڙين نعمتن جو انڪار ڪندؤ.",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4925,
        "text": "۽ سمنڊ ۾ ھلندڙ جھاز (سڙھ کنيل) جبلن وانگر اُچا الله جا آھن.",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4926,
        "text": "پوءِ (اي جنّو ۽ ماڻھؤ) پنھنجي پالڻھار جي ڪھڙين نعمتن جو انڪار ڪندؤ.",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4927,
        "text": "جيڪو به زمين تي آھي سو سڀ ناس ٿيندو.",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4928,
        "text": "۽ (اي پيغمبر) تنھنجي پالڻھار جي ذات باقي رھندي جو عزت وارو ۽ نعمتن وارو آھي.",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4929,
        "text": "پوءِ (اي جنّؤ ۽ ماڻھؤ) پنھنجي پالڻھار جي ڪھڙين نعمتن جو انڪار ڪندؤ.",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4930,
        "text": "جيڪو به آسمانن ۽ زمين ۾ آھي سو سڀڪو الله کان گھرندو آھي، سڀڪنھن ڏينھن اُھو ڪم ۾ آھي.",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4931,
        "text": "پوءِ (اي جنّو ۽ ماڻھؤ) پنھنجي پالڻھار جي ڪھڙين نعمتن جو انڪار ڪندؤ.",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4932,
        "text": "اي جنّو ۽ ماڻھؤ (اسين واندا ٿي) اوھان ڏانھن جلد متوجه ٿينداسون.",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4933,
        "text": "پوءِ (اي جنّؤ ۽ ماڻھؤ) پنھنجي پالڻھار جي ڪھڙين نعمتن جو انڪار ڪندؤ.",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4934,
        "text": "اي جِنّن ۽ ماڻھن جي ٽولي جيڪڏھن اوھين آسمانن ۽ زمين جي پاسن کان ٻاھر نڪري سگھندا آھيو ته نڪرو، وڏي ھمت کانسواءِ ٻي طرح نڪري نه سگھندؤ.",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4935,
        "text": "پوءِ (اي جنّؤ ۽ ماڻھؤ) پنھنجي پالڻھار جي ڪھڙين نعمتن جو انڪار ڪندؤ.",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4936,
        "text": "اوھان تي باھ جو اُلنبو ۽ دُونھون موڪلبو جو مقابلو ڪري نه سگھندؤ.",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4937,
        "text": "پوءِ (اي جنّؤ ۽ ماڻھؤ) پنھنجي پالڻھار جي ڪھڙين نعمتن جو انڪار ڪندؤ.",
