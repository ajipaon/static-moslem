  "code": 200,
  "status": "OK",
  "data": {
    "number": 89,
    "name": "سُورَةُ الفَجۡرِ",
    "englishName": "Al-Fajr",
    "englishNameTranslation": "The Dawn",
    "revelationType": "Meccan",
    "numberOfAyahs": 30,
    "ayahs": [
      {
        "number": 5994,
        "text": "پرھ جو قسم آھي.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5995,
        "text": "۽ ڏھن راتين جو قسم آھي.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5996,
        "text": "۽ ٻڌيءَ ۽ اِڪئيءَ جو قسم آھي.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5997,
        "text": "۽ رات جو قسم آھي جڏھن ھلي.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5998,
        "text": "ھان ڌيان واري لاءِ ھن (قسمن) ۾ بيشڪ پورو قسم آھي.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5999,
        "text": "نه ڏٺئي ڇا ته (قوم) عاد سان تنھنجي پالڻھار ڪيئن ڪيو.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6000,
        "text": "(جي) ارم وارا وڏن ٿنڀن (جي عمارتن) وارا ھوا.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6001,
        "text": "جن جھڙو شھرن ۾ ڪونه بڻايو ويو آھي.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6002,
        "text": "۽ (قوم) ثمُود سان (ڪيئن ڪيائين) جن ميدان (قرٰا) ۾ پھڻن جون ڇپون گھڙيون ھيون.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6003,
        "text": "۽ ميخن واري فرعون سان (ڪئن ڪيائين).",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6004,
        "text": "انھن (مڙني ٽولين) جن شھرن ۾ سرڪشي ڪئي.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6005,
        "text": "پوءِ منجھن گھڻو فساد ڪيائون.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6006,
        "text": "تنھنڪري تنھنجي پالڻھار مٿن عذاب جو چھبڪ وھايو.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6007,
        "text": "بيشڪ تنھنجو پالڻھار گھَٽَ (واري ھنڌ) ۾ (عملن کي ڏسندڙ) آھي.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6008,
        "text": "پوءِ ماڻھو کي جڏھن سندس پالڻھار (ھن طرح) پرکيندو آھي جو اُن کي مانوارو ڪندو آھي ۽ نعمت ڏيندو اٿس، ته چوندو آھي ته منھنجي پالڻھار مون کي نوازيو.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6009,
        "text": "۽ جڏھن کيس (ٻئي طرح ھيئن) پرکيندو آھي جو مٿس روزي تنگ ڪندو آھي ته چوندو آھي ته منھنجي پالڻھار مون کي بي مانو ڪيو.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6010,
        "text": "(ھرگز) ائين نه آھي بلڪ ڇوري ٻار کي تعظيم نه ڏيندا آھيو.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6011,
        "text": "۽ نڪي مسڪينن جي کارائڻ تي ھڪ ٻئي کي رغبت ڏياريندا آھيو.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6012,
        "text": "۽ مُئن جو مال سموروئي ڳھي ويندا آھيو.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6013,
        "text": "۽ گھڻي پيار سان مال کي پيارو رکندا آھيو.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6014,
        "text": "ائين نه ڪجي جڏھن زمين (جي اونچاڻ) ڪٽي ڪٽي سنئين ڪئي ويندي.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6015,
        "text": "۽ تنھنجو پالڻھار ايندو ۽ ملائڪ صِف صِف ٿي (ايندا).",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6016,
        "text": "۽ اُن ڏينھن دوزخ کي آڻبو، اُن ڏينھن ماڻھو نصيحت وٺندو ۽ اُنھيءَ مھل (اھا نصيحت وٺڻ) اُن کي ڪٿي فائدي واري آھي.",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 593,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6017,
        "text": "چوندو ته ھاءِ ارمان جيڪر پنھنجي (ھن ھميشه جي) حياتيءَ لاءَ (ڪجھ) اڳي موڪليان ھا.",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6018,
        "text": "پوءِ انھيءَ ڏينھن الله جي عذاب جھڙو ڪوبه ڪونه عذاب ڪندو.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6019,
        "text": "۽ نڪو ڪو الله جي قيد ڪرڻ جھڙو قيد ڪندو.",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6020,
        "text": "(مؤمنن کي چئبو ته) اي آرام وٺڻ وارا جيءُ.",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6021,
        "text": "تون پنھنجي پالڻھار ڏانھن موٽِ ھل (تون اُن کان) راضي ٿيل آھين (ھو توکان) راضي ٿيل آھي.",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6022,
        "text": "تنھنڪري (تون) منھنجي ٻانھن (جي ٽوليءَ) ۾ داخل ٿيءُ.",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 6023,
        "text": "۽ منھنجي بھشت ۾ گِھڙ.",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 531,
        "hizbQuarter": 237,
        "sajda": false
      }
    ],
