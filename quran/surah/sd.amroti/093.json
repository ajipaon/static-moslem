  "data": {
    "number": 93,
    "name": "سُورَةُ الضُّحَىٰ",
    "englishName": "Ad-Dhuhaa",
    "englishNameTranslation": "The Morning Hours",
    "revelationType": "Meccan",
    "numberOfAyahs": 11,
    "ayahs": [
      {
        "number": 6080,
        "text": "مٿي آيل ڏينھن جو قسم آھي.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6081,
        "text": "۽ رات جو (قسم) آھي، جڏھن ڍَڪي ٿي.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6082,
        "text": "ته (اي پيغمبر) تنھنجي پالڻھار توکي نه ڇڏيو ۽ نڪي ناراض ٿيو.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6083,
        "text": "۽ بيشڪ آخرت تولاءِ دنيا کان گھڻو چڱي آھي.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6084,
        "text": "۽ تنھنجو پالڻھار سگھوئي توکي (ايترو) ڏيندو جو (تون) خوش ٿيندين.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6085,
        "text": "توکي يتيم نه ڏٺائين ڇا پوءِ (توکي) جاءِ ڏنائين.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6086,
        "text": "۽ توکي ناواقف ڏٺائين پوءِ (شريعت جي) رستي لاتائين.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6087,
        "text": "۽ توکي مسڪين ڏٺائين پوءِ آسودو ڪيائين.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6088,
        "text": "تنھنڪري يتيم تي تون ڏاڍ نه ڪر.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6089,
        "text": "۽ سُوالي کي نه جھڻڪ.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6090,
        "text": "۽ پنھنجي پالڻھار جي نعمت جو بيان ڪندو رھ.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      }
    ],
