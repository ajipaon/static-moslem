        "hizbQuarter": 1,
        "sajda": false
      },
      {
        "number": 2,
        "text": "Çdo lavdërim i përket Allahut (Perëndisë), Zotit të Gjithësisë",
        "numberInSurah": 2,
        "juz": 1,
        "manzil": 1,
        "page": 1,
        "ruku": 1,
        "hizbQuarter": 1,
        "sajda": false
      },
      {
        "number": 3,
        "text": "Mëshirues dhe Përdëllimtar i Madh.",
        "numberInSurah": 3,
        "juz": 1,
        "manzil": 1,
        "page": 1,
        "ruku": 1,
        "hizbQuarter": 1,
        "sajda": false
      },
      {
        "number": 4,
        "text": "Sunduesit të ditës së gjukimit.",
        "numberInSurah": 4,
        "juz": 1,
        "manzil": 1,
        "page": 1,
        "ruku": 1,
        "hizbQuarter": 1,
        "sajda": false
      },
      {
        "number": 5,
        "text": "Vetëm Ty të adhurojmë dhe vetëm prej Teje ndihmë kërkojmë!",
        "numberInSurah": 5,
        "juz": 1,
        "manzil": 1,
        "page": 1,
        "ruku": 1,
        "hizbQuarter": 1,
        "sajda": false
      },
      {
        "number": 6,
        "text": "Udhëzona në rrugë të drejtë.",
