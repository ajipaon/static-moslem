{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 52,
    "name": "سُورَةُ الطُّورِ",
    "englishName": "At-Tur",
    "englishNameTranslation": "The Mount",
    "revelationType": "Meccan",
    "numberOfAyahs": 49,
    "ayahs": [
      {
        "number": 4736,
        "text": "Pasha malin e Turit (Sinajit),",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4737,
        "text": "dhe Librin e shkruar në rreshta,",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4738,
        "text": "të zhvilluar në lëkurë,",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4739,
        "text": "dhe Tempullin plot haxhinj,",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4740,
        "text": "dhe kupës së ngritur (të qiellit),",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4741,
        "text": "dhe detit të mbushur,",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4742,
        "text": "me të vërtetë, dënimi i Zotit tënd, do të ndodhë,",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4743,
        "text": "dhe askush nuk mund ta pengojë,",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4744,
        "text": "atë Ditë kur qielli do të lëkundet fuqishëm,",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4745,
        "text": "e malet të lëkunden me të madhe nga vendi!",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4746,
        "text": "Atë Ditë, mjerë për ata që kanë përgënjeshtruar,",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4747,
        "text": "që, të zhytur në gënjeshtra, argëtoheshin!",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4748,
        "text": "Atë Ditë, kur me forcë do të hidhen në zjarrin e xhehennemit.",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4749,
        "text": "(U thuhet atyre): “Ky është zjarri që e keni përgënjeshtruar,",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4750,
        "text": "pra, a është kjo magji, apo ju nuk shihni?",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4751,
        "text": "Përcëllohuni në të; e, duroni apo mos duroni, njësoj është për ju, - ky është ndëshkimi juaj vetëm për atë që keni bërë”.",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4752,
        "text": "Me të vërtetë, ata që kanë besuar do të gjenden në kopshtet e xhennetit dhe lumturi,",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4753,
        "text": "duke u kënaqur me atë që u ka dhënë Zoti i tyre, i ka ruajtur ata Zoti i tyre prej dënimit të skëterrës.",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4754,
        "text": "(Atyre u thuhet): “Hani e pni me kënaqësi, për atë që keni punuar!”",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4755,
        "text": "Të mbështetur në divane (kanape) të renditura, do t’i martojmë ata me hyrie, sysh të bukur.",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4756,
        "text": "E, ata që kanë besuar dhe u shkojnë pas pasardhësit e tyre në besim, do t’ju bashkojmë pasardhësit e tyre dhe nuk do t’ju pakësojmë asgjë nga veprat e tyre – çdo njeri është peng (përgjegjës) për atë që ka punuar –",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4757,
        "text": "dhe do t’ju zgjërojmë dhuntitë atyre me pemë dhe mish që ju ka ënda;",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4758,
        "text": "në të (xhennet) ia zgjasin njëri-tjetrit gotat e mbushura, aty nuk do të ketë biseda të kota, as mëkate, -",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4759,
        "text": "e, do të sillen rreth tyre shërbyes (gilmanët) djelmosha të ngjashëm me margaritarin e ruajtur,",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4760,
        "text": "dhe njëri-tjetrit i drejtohen duke e pyetur,",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4761,
        "text": "e do të thonë: “Ne, më parë – në familjet tona jemi frikësuar (nga dënimi),",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4762,
        "text": "e, Perëndia na ka dhuruar mëshirë dhe na ka ruajtur nga dënimi i rëndë (i frumës helmuese).",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4763,
        "text": "Ne, më parë (në jetën tokësore) e kemi adhuruar Atë, Ai, me të vërtetë, është bamirës dhe mëshirues”.",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 458,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4764,
        "text": "Andaj, këshillo ti (o Muhammed), se ti, me dhuntinë (profetninë) e Zotit tënd, nuk je as fallxhi as i çmendur.",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4765,
        "text": "Ose të thonë ata: “(Ai – Muhammedi është) vjershtar, të presim derisa ta zhdukë vdekja”.",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4766,
        "text": "Thuaju (atyre): “Pritni, se, me të vërtetë, edhe unë jam nga ata që presin!”",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 524,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4767,
        "text": "A mos u vie atyre kjo nga mendja e tyre, apo ata janë popull kryeneç (që e kanë kaluar kufirin)?",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4768,
        "text": "Apo thonë: “Ai e ka trilluar atë (Kur’anin)”. – Jo, por ata nuk besojnë;",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4769,
        "text": "andaj, le ta sjellin ata një fjalim të ngjashëm me të, nëse thonë të vërtetën!",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4770,
        "text": "A mos vallë, ata janë krijuar pa Krijues, apo mos janë ata krijues të vetvetes?!",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4771,
        "text": "A mos vallë ata kanë krijuar qiejt dhe Tokën?! Jo, por ata nuk duan të besojnë.",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4772,
        "text": "Vallë, a mos në ta është thesari i Zotit tënd, ose, vallë ata sundojnë?",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4773,
        "text": "Vallë, a mos kanë shkallë (me të cilat ngjiten lart) e përgjojnë?! E, le të sjell (ai) përgjuesi i tyre argument të qartë!",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4774,
        "text": "Apo, për Te janë vajzat, e për ju djemtë?!",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4775,
        "text": "Vallë a mos kërkon ti prej tyre shpërblim, e që ata të jenë të ngarkuar me këtë?!",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4776,
        "text": "Ose, a mos tek ata është sekreti, e ata e përshkruajnë?!",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4777,
        "text": "Apo dëshirojnë ngatërresë, e vetëm mohuesit do të jenë të ngatërruar?",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4778,
        "text": "Ose ata kanë zot (tjetër) pos Perëndisë? Perëndia është i Lartësuar mbi atë çka i përshkruajnë Atij!",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4779,
        "text": "Dhe, sikur të shihni se po bie një copë nga qielli, do të thoshin: “Retë e grumbulluara!”",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4780,
        "text": "E, lëri ata, derisa të mos takohen me Ditën në të cilën do të shkatërrohen.",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4781,
        "text": "Ditën në të cilën, dinakëritë e tyre nu do t’u bëjnë dobi asgjë dhe kur ata nuk do t’i ndihmojë askush.",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4782,
        "text": "E, me të vërtetë, për zullumqarët ka edhe dënim tjetër pos këtij; por (këtë) shumica e tyre nuk e dijnë.",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4783,
        "text": "E, ti duro për Vendimin e Zotit tënd! Me të vërtetë, ti je nën mbrojtjen Tonë; dhe lavdëroje duke e falenderuar Zotin tënd kur të ngritesh,",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4784,
        "text": "dhe, madhëroje Ate edhe natën edhe pas perëndimit të yjeve!",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 525,
        "ruku": 459,
        "hizbQuarter": 210,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "sq.nahi",
      "language": "sq",
      "name": "Efendi Nahi",
      "englishName": "Hasan Efendi Nahi",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}