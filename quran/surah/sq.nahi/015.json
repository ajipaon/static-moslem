{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 15,
    "name": "سُورَةُ الحِجۡرِ",
    "englishName": "Al-Hijr",
    "englishNameTranslation": "The Rock",
    "revelationType": "Meccan",
    "numberOfAyahs": 99,
    "ayahs": [
      {
        "number": 1803,
        "text": "Elif Lâm Râ. Këto janë ajetet e Librit, të Kur’anit të qartë!",
        "numberInSurah": 1,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1804,
        "text": "Shpeshherë mohuesit do të dëshironin që të ishin muslimanë.",
        "numberInSurah": 2,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1805,
        "text": "Lëri ata që të hanë dhe le të kënaqen, dhe lë t’i mashtrojë shpresa; ata do ta dinë (fundin)!",
        "numberInSurah": 3,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1806,
        "text": "Na nuk i kemi zhdukur banorët e asnjë vendi, për pa ardhur koha e caktuar,",
        "numberInSurah": 4,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1807,
        "text": "asnjë popull nuk mund as ta shpejtojë as ta ngadalësojë (caktimin Tonë për të).",
        "numberInSurah": 5,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1808,
        "text": "Ata thanë: “O ti, që të është shpallë Kur’ani, ti je, me të vërtetë, i marrë!",
        "numberInSurah": 6,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1809,
        "text": "Të na sillshe engjëjt, nëse je i drejtë?”",
        "numberInSurah": 7,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1810,
        "text": "Na i dërgojmë engjëjt vetëm me të Vërtetën (me dënim të premtuar), dhe atëherë nuk u jepet afat për të pritur (por dënimi merr fund).",
        "numberInSurah": 8,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1811,
        "text": "Na, me të vërtetë, kemi shpallë Kur’anin dhe, Na, me të vërtetë, do ta ruajmë atë (origjinalitetin e tij).",
        "numberInSurah": 9,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1812,
        "text": "Na, me të vërtetë, u kemi dërguar pejgamberë edh para teje, - grupeve (popujve) të mëparshëm,",
        "numberInSurah": 10,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1813,
        "text": "dhe asnjë pejgamber nuk ka ardhur e që ata të mos talleshin me të.",
        "numberInSurah": 11,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1814,
        "text": "Ja, kështu Na, fusim (talljen) në zemrat e mëkatarëve,",
        "numberInSurah": 12,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1815,
        "text": "ata nuk e besojnë atë (profetin Muhammed), e madje, ka kaluar ligji (mbi dënimin) e popujve të kaluar (për tallje me pejgamberët).",
        "numberInSurah": 13,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1816,
        "text": "Sikur Ne ta hapnim një derë të qiellit dhe ata të ngjiteshin nëpër të,",
        "numberInSurah": 14,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1817,
        "text": "do të thonin (ata): “Sytë na kanë qenë të mjegulluar, na jemi popull i magjepsur!”",
        "numberInSurah": 15,
        "juz": 14,
        "manzil": 3,
        "page": 262,
        "ruku": 218,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1818,
        "text": "Na kemi krijuar hyllësi në qiej dhe i kemi zbukuruar ato për shikuesin,",
        "numberInSurah": 16,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1819,
        "text": "dhe i kemi ruajtur nga çdo djall i mallkuar;",
        "numberInSurah": 17,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1820,
        "text": "përpos atij që vjedh të dëgjuarën, e arrinë atë një flakë e dukshme.",
        "numberInSurah": 18,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1821,
        "text": "Dhe Tokën e kemi shtrirë dhe nëpër të kemi shpërndarë male të forta, dhe kemi bërë që në të, të zhvillohet çdo gjë në masa proporcionale,",
        "numberInSurah": 19,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1822,
        "text": "dhe kemi bërë për ju në të (Tokë) sendet që u japin gjallëri dhe për ata që nuk i mbani (furnizoni) ju.",
        "numberInSurah": 20,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1823,
        "text": "Nuk ekziston asnjë send, e që të mos gjendet ajo në thesarin Tonë, e nga ajo Na japim vetëm aq sa është e nevojshme.",
        "numberInSurah": 21,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1824,
        "text": "Na dërgojmë erërat për të frytnuar, e zbresim shiun nga qielli e ju kemi furnizuar me ujë – dhe depot e tij (ujit) nuk gjenden tek ju.",
        "numberInSurah": 22,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1825,
        "text": "Dhe, me të vërtetë, Na japim gjëllëri dhe vdekje, dhe vetëm Na jemi trashëgimtar (të përhershëm),",
        "numberInSurah": 23,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1826,
        "text": "dhe vetëm Na dimë për ata që kanë qenë para jush, dhe vetëm Na dimë për ata që do të vijnë pas jush,",
        "numberInSurah": 24,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1827,
        "text": "dhe, me të vërtetë, Zoti yt, do t’i tubojë; Ai, me të vërtetë, është i Plotëdijshëm dhe i Gjithëdijshëm.",
        "numberInSurah": 25,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 219,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1828,
        "text": "Na kemi krijuar njeriun prej një balte të fortë – të zezë, së cilës i është dhënë forma,",
        "numberInSurah": 26,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1829,
        "text": "kurse xhindet i kemi krijuar që më parë nga zjarri i flakët.",
        "numberInSurah": 27,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1830,
        "text": "(Kujtoje) kur Zoti yt u tha engjëjve: “Unë do ta krijojë njeriun nga balta e zezë e fortë, e ndryshueshme (e figuruar),",
        "numberInSurah": 28,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1831,
        "text": "e kur t’ia jap formën dhe i jap shpirtin (e bëhet i gjallë), ju bëni sexhde atij!”",
        "numberInSurah": 29,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1832,
        "text": "Të gjithë engjëjt, bashkarisht, i bën sexhde atij,",
        "numberInSurah": 30,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1833,
        "text": "përveç Iblisit. Ai refuzoi t’u bashkohet atyre që bënin sexhde.",
        "numberInSurah": 31,
        "juz": 14,
        "manzil": 3,
        "page": 263,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1834,
        "text": "(Perëndia) tha: “O Iblis, ç’pate ti që nuk u bëre me ata që bënë sexhde?”",
        "numberInSurah": 32,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1835,
        "text": "(Iblisi) tha: “Nuk jam unë ai që t’i bëjë sexhde një njeriu të cilin Ti e ke krijuar nga balta e zezë, e fortë, e ndryshueshme (e figuruar), së cilës i është dhënë forma.",
        "numberInSurah": 33,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1836,
        "text": "(Perëndia) tha: “Atëherë, dil prej xhennetit, ti, në të vërtetë, je i mallkuar,",
        "numberInSurah": 34,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1837,
        "text": "dhe le të jetë mallkimi mbi ty deri në Ditën e Kijametit!”",
        "numberInSurah": 35,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1838,
        "text": "Iblisi tha: “Zoti im, më jep afat deri në ditën kur do të ringjallen ata (njerëzit)!”",
        "numberInSurah": 36,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1839,
        "text": "(Zoti) tha: “Ty, me të vërtetë, të është dhënë afat",
        "numberInSurah": 37,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1840,
        "text": "deri në ditën e kohës së caktuar”.",
        "numberInSurah": 38,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1841,
        "text": "(Iblisi) tha: “Zoti im, meqë më ke shpie në humbje, unë në Tokë do t’ju zbukuroj atyre sendet dhe të gjithë do t’i shpie në mashtrim,",
        "numberInSurah": 39,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1842,
        "text": "përveç robërve Tu të sinqertë”.",
        "numberInSurah": 40,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1843,
        "text": "(Perëndia) tha: “Kjo është rruga ime e drejtë.",
        "numberInSurah": 41,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1844,
        "text": "Ti, me të vërtetë, nuk ke kurrfarë pushteti ndaj robërve të Mi, përveç atyre të humburve, që të pasojnë ty”.",
        "numberInSurah": 42,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1845,
        "text": "Dhe, me të vërtetë, xhehennemi është vendbanimi i tyre i premtuar, për të gjithë:",
        "numberInSurah": 43,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1846,
        "text": "ai i ka shtatë dyer, e për çdo derë ka pjesë të caktuar”.",
        "numberInSurah": 44,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 220,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1847,
        "text": "Me të vërtetë, ata që ruhen nga mëkatet, do të gjenden në kopshtet e xhennetit pranë burimeve.",
        "numberInSurah": 45,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1848,
        "text": "(Ju thuhet): “Hyni në to me përshëndetje dhe të siguruar (prej frikës)”.",
        "numberInSurah": 46,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1849,
        "text": "Dhe, Ne, do ta shporrim urrejtjen nga zemrat e tyre (që kanë pasur më parë), ata do të rrinë ulur si vëllezër në kanape (kolltuqe) – njëri përballë tjetrit,",
        "numberInSurah": 47,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1850,
        "text": "aty, nuk do t’i gërget lodhja, ata prej atyhit kurrë nuk do të dëbohen.",
        "numberInSurah": 48,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1851,
        "text": "Lajmëroji robërit e Mi (o Muhammed!) se Unë, me të vërtetë, jam Ai që falë dhe mëshiroj,",
        "numberInSurah": 49,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 105,
        "sajda": false
      },
      {
        "number": 1852,
        "text": "e që dënimi Im (për të këqinjtë), është me të vërtetë, dënim i dhembshëm!",
        "numberInSurah": 50,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1853,
        "text": "Dhe lajmëroji për mysafirët e Ibrahimit,",
        "numberInSurah": 51,
        "juz": 14,
        "manzil": 3,
        "page": 264,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1854,
        "text": "kur, hynë tek ai dhe thanë: “Selam!” (Ibrahimi) tha: “Na po frikohemi nga Ju”.",
        "numberInSurah": 52,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1855,
        "text": "Ata thanë: “Mos u friko se na po të sihariqojmë ty me një djalë të dijshëm”.",
        "numberInSurah": 53,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1856,
        "text": "(Ibrahimi) tha: “A mos vallë do të më sihariqoni, tani, kur më ka goditur pleqëria? Me çka do të më sihariqoni mua?”",
        "numberInSurah": 54,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1857,
        "text": "Ata thanë: “Na do të sihariqojmë ty me një të Vërtetë që do të realizohet, e mos u bën shpresëhumbur!”",
        "numberInSurah": 55,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1858,
        "text": "(Ibrahimi) tha: “Kush mund ta humbë shpresën nga mëshira e Zotit të vet, përpos atyre që janë të humbur” –",
        "numberInSurah": 56,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1859,
        "text": "dhe pyeti (Ibrahimi): “E, çfarë është qëllimi juaj, o të dërguar?”",
        "numberInSurah": 57,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1860,
        "text": "(Ata) thanë: “Na jemi dërguar te një popull mëkatarë, -",
        "numberInSurah": 58,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1861,
        "text": "përpos familjes së Lutit (që nuk janë mëkatarë). Na do t’i shpëtojmë të gjithë ata (anëtarët e familjes së Lutit) –",
        "numberInSurah": 59,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1862,
        "text": "përveç gruas së tij; për atë, Na kemi caktuar dënim, të cilin do ta shiojë ajo”.",
        "numberInSurah": 60,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 221,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1863,
        "text": "Dhe, kur të dërguarit erdhën te Luti,",
        "numberInSurah": 61,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1864,
        "text": "(Luti) tha: “Ju, me të vërtetë, jeni njerz të panjohur!”",
        "numberInSurah": 62,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1865,
        "text": "Ata thanë: “Jo, na të kemi sjellur atë, në të cilën ata (mohuesit nga populli yt) dyshojnë,",
        "numberInSurah": 63,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1866,
        "text": "të sjellim atë që do të ngjajë me siguri, e ne, me të vërtetë, jemi të sinqertë.",
        "numberInSurah": 64,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1867,
        "text": "Udhëto ti me familjen tënde, në një kohë të natës, e shko ti pas tyre, e askush prej jush të mos shikojë prapa, por vazhdoni në drejtim të urdhëruar!”",
        "numberInSurah": 65,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1868,
        "text": "Dhe Ne, ia kemi shpallur atij, atë që do të ngjajë: që ata të gjithë – deri në të mbramin, do të zhduken, në agim.",
        "numberInSurah": 66,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1869,
        "text": "Dhe, erdhën të gëzueshëm banorët e qytetit.",
        "numberInSurah": 67,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1870,
        "text": "(Luti) tha: “Këta janë mysafirët e mi, andaj mos më turpëroni,",
        "numberInSurah": 68,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1871,
        "text": "dhe druajuni Perëndisë, dhe mos më poshtëroni mua!”",
        "numberInSurah": 69,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1872,
        "text": "Ata (populli i Lutit) thanë: “A nuk të kemi ndaluar na ty, që të pranosh mysafirë?”",
        "numberInSurah": 70,
        "juz": 14,
        "manzil": 3,
        "page": 265,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1873,
        "text": "(Luti) tha: “Ja këto vajzat e (popullit) tim! Nëse doni të bëni çka synoni, (martohuni me to).",
        "numberInSurah": 71,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1874,
        "text": "(O Muhammed!) Pasha jetën tënde, me të vërtetë, ata janë déjur në iluzionet e veta.",
        "numberInSurah": 72,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1875,
        "text": "Dhe, ata i kaplojë një zë i tmerrshëm kur lindir Dielli,",
        "numberInSurah": 73,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1876,
        "text": "dhe Ne, kemi bërë që, ajo që gjendej lartë – të bie poshtë, duke lëshuar në ta si shi gurësh prej dheut të pjekur, -",
        "numberInSurah": 74,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1877,
        "text": "Këto, me të vërtetë, janë argumente për ata që vëzhgojnë me kujdes,",
        "numberInSurah": 75,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1878,
        "text": "me të vërtetë, ato (qytete) janë në rrugë, kur gjenden edhe tani.",
        "numberInSurah": 76,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1879,
        "text": "Kjo, me të vërtetë, është dokument për besimtarët.",
        "numberInSurah": 77,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1880,
        "text": "Por, edhe banorët e Ejkes kanë qenë zullumqarë,",
        "numberInSurah": 78,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1881,
        "text": "andaj, i kemi ndëshkuar, dhe që të dy janë në rrugë – të dukshme.",
        "numberInSurah": 79,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 222,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1882,
        "text": "Edhe banorët e Hixhrës, pejgamberët i konsideruan për gënjeshtarë.",
        "numberInSurah": 80,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1883,
        "text": "Dhe Ne, u patëm dhënë atyre dokumentet Tona, por ata shmangeshin prej tyre.",
        "numberInSurah": 81,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1884,
        "text": "Ata gëdhendnin shtëpia me gurë-malesh, duke besuar se nuk do t’i gjejë asgjë,",
        "numberInSurah": 82,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1885,
        "text": "andaj, edhe ata në agim i goditi zë i tmerrshëm",
        "numberInSurah": 83,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1886,
        "text": "dhe nuk patën farë dobie nga ajo që patën ndërtuar (banesat)",
        "numberInSurah": 84,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1887,
        "text": "Na kemi krijuar qiejt dhe Tokën dhe atë që gjendet në mes tyre – në mënyrë të mrekullueshme. Me të vërtetë, do të vie Kijameti, andaj sillu ti (o Muhammed) me sjellje të bukura (zemërgjerësi),",
        "numberInSurah": 85,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1888,
        "text": "me të vërtetë, Zoti yt është i vetmi Krijues dhe i Gjithëdijshëm. –",
        "numberInSurah": 86,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1889,
        "text": "Na të kemi shpallë ty shtatë ajete, që përsëriten dhe Kur’anin e madhërueshëm. –",
        "numberInSurah": 87,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1890,
        "text": "Mos i drejto sytë e tu kah ato kënaqësi që Na ua kemi dhënë disave prej tyre dhe mos u hidhëro për ta (në mos besofshin). E, ndaj besimtarëve bëhu i butë",
        "numberInSurah": 88,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1891,
        "text": "dhe thuaj: “Unë, në të vërtetë, jam vetëm paralajmërues i qartë!” –",
        "numberInSurah": 89,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1892,
        "text": "(Ashtu u kemi dërguar cënim) atyre, të cilët e kanë ndarë Librin,",
        "numberInSurah": 90,
        "juz": 14,
        "manzil": 3,
        "page": 266,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1893,
        "text": "e të cilët e kanë ndarë Kur’anin pjesë-pjesë (diçka kanë besuar e diçka jo).",
        "numberInSurah": 91,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1894,
        "text": "Dhe, kështu, pasha Zotin tënd, Na me të vërtetë, do t’i pyesim ata të gjithë,",
        "numberInSurah": 92,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1895,
        "text": "për atë që kanë punuar!",
        "numberInSurah": 93,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1896,
        "text": "Ti haptazi prediko atë që të është urdhëruar dhe shmangiu idhujtarëve,",
        "numberInSurah": 94,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1897,
        "text": "Na të mjaftojmë ty, kundër atyre që të tallin (ty),",
        "numberInSurah": 95,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1898,
        "text": "të cilët përveç Perëndisë, marrin edhe zot tjetër, dhe do ta dinë ata mirë (fundin)!",
        "numberInSurah": 96,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1899,
        "text": "Na e dimë mirë, që ty të ngushtohet zemra, për atë që thonë ata,",
        "numberInSurah": 97,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1900,
        "text": "andaj, madhëroje dhe falenderoje Zotin tënd dhe bëhu prej atyre që falin namazin!",
        "numberInSurah": 98,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      },
      {
        "number": 1901,
        "text": "Dhe, adhuroje Zotin tënd përderisa të arrijë gjendja e sigurtë (vdekja)!",
        "numberInSurah": 99,
        "juz": 14,
        "manzil": 3,
        "page": 267,
        "ruku": 223,
        "hizbQuarter": 106,
        "sajda": false
      }
    ],
