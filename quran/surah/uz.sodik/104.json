  "data": {
    "number": 104,
    "name": "سُورَةُ الهُمَزَةِ",
    "englishName": "Al-Humaza",
    "englishNameTranslation": "The Traducer",
    "revelationType": "Meccan",
    "numberOfAyahs": 9,
    "ayahs": [
      {
        "number": 6180,
        "text": "Ҳар бир обрў тўкувчи ва айбловчига «вайл» бўлсин.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6181,
        "text": "У мол жамлади ва уни санаб турди.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6182,
        "text": "У, албатта, уни абадий қолдирур, деб ҳисоблар.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6183,
        "text": "Йўқ! Албатта у «ҳутома»га хор-зор этиб ташланар.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6184,
        "text": "«Ҳутома» қандоқ нарса эканини сенга нима билдирар?",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6185,
        "text": "У Аллоҳнинг шиддатли, ўчмас оловидир.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6186,
        "text": "Қалбгача етиб борадир.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6187,
        "text": "Албатта, у уларни устидан қоплангандир.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6188,
        "text": "Узун-узун устунларга боғлангандирлар.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      }
    ],
