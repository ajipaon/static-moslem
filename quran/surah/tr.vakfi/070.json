{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 70,
    "name": "سُورَةُ المَعَارِجِ",
    "englishName": "Al-Ma'aarij",
    "englishNameTranslation": "The Ascending Stairways",
    "revelationType": "Meccan",
    "numberOfAyahs": 44,
    "ayahs": [
      {
        "number": 5376,
        "text": "Bir soran inecek azabı sordu:",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5377,
        "text": "İnkarcılar için; ki onu savacak yoktur,",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5378,
        "text": "Yükselme derecelerinin sahibi olan Allah katından.",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5379,
        "text": "Melekler ve Ruh (Cebrail), oraya, miktarı (dünya senesi ile) ellibin yıl olan bir günde yükselip çıkar.",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5380,
        "text": "(Resulüm!) Şimdi sen güzelce sabret.",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5381,
        "text": "Doğrusu onlar, o azabı (ihtimalden) uzak görüyorlar.",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5382,
        "text": "Biz ise onu yakın görmekteyiz.",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5383,
        "text": "O gün gökyüzü, erimiş maden gibi olur.",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5384,
        "text": "Dağlar da atılmış yüne döner.",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5385,
        "text": "Dost, dostu sormaz.",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 568,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5386,
        "text": "Birbirlerine gösterilirler (fakat herkes kendi derdindedir). Günahkar kimse ister ki, o günün azabından (kurtuluş için), oğullarını,",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5387,
        "text": "Karısını ve kardeşini,",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5388,
        "text": "Kendisini koruyup barındıran tüm ailesini",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5389,
        "text": "Ve yeryüzünde kim varsa hepsini fidye olarak versin de, tek kendini kurtarsın.",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5390,
        "text": "Fakat ne mümkün! Bilinmeli ki, o (cehennem) alevlenen bir ateştir.",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5391,
        "text": "Derileri kavurup soyar.",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5392,
        "text": "Yüz çevirip geri döneni, (kendine) çağırır!",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5393,
        "text": "(Servet) toplayıp yığan kimseyi!.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 227,
        "sajda": false
      },
      {
        "number": 5394,
        "text": "Gerçekten insan, pek hırslı (ve sabırsız) yaratılmıştır.",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5395,
        "text": "Kendisine fenalık dokunduğunda sızlanır, feryat eder.",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5396,
        "text": "Ona imkan verildiğinde ise pinti kesilir.",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5397,
        "text": "Ancak şunlar öyle değildir: Namaz kılanlar,",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5398,
        "text": "Ki, onlar namazlarında devamlıdırlar (ihmal göstermezler;).",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5399,
        "text": "Mallarında, belli bir hak vardır,",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5400,
        "text": "Saile ve mahruma(vermek için).",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5401,
        "text": "Ceza (ve hesap) gününün doğruluğuna inananlar;",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5402,
        "text": "Rab'lerinin azabından korkanlar,",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5403,
        "text": "Ki Rab'lerinin azabı(na karşı) emin olunamaz;",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5404,
        "text": "Irzlarını koruyanlar",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5405,
        "text": "Ancak eşlerine ve cariyelerine karşı müstesna; çünkü onlar kınanmaz;",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5406,
        "text": "Bundan öteye (geçmek) isteyenler ise, onlar taşkınların ta kendileridir,",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5407,
        "text": "Emanetlerine ve ahitlerine riayet edenler;",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5408,
        "text": "Şahitliklerini (dosdoğru) yapanlar;",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5409,
        "text": "Namazlarını koruyanlar;",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5410,
        "text": "İşte bunlar, cennetlerde ağırlanırlar.",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 502,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5411,
        "text": "(Resulüm!) O kafirlere ne oluyor ki, sana doğru koşuyorlar?",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5412,
        "text": "Bölük bölük sağından ve solundan(gelip etrafını sarıyorlar)",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5413,
        "text": "Onlardan her biri nimet cennetine sokulacağını mı umuyor?",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5414,
        "text": "Hayır (hiç ummasınlar!) Şüphesiz biz onları, kendilerinin de bildikleri şeyden yarattık (fakat ibret almadılar, imana gelmediler).",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 569,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5415,
        "text": "Doğuların ve batıların Rabbine yemin ederim ki, bizim gücümüz yeter:",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5416,
        "text": "Şüphesiz onların yerine daha iyilerini getirmeye bizim gücümüz yeter ve kimse bizim önümüze geçemez.",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5417,
        "text": "Ama sen onları (şimdilik) bırak da, tehdit edildikleri günlerine kavuşuncaya dek dalsınlar, oynayadursunlar.",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5418,
        "text": "O gün onlar, sanki dikili bir şeye koşuyorlar gibi, kabirlerinden fırlaya fırlaya çıkarlar.",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      },
      {
        "number": 5419,
        "text": "Gözleri horluktan aşağı düşmüş ve kendileri zillete bürünmüş bir halde. İşte bu, onların tehdit edilegeldikleri gündür!",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 570,
        "ruku": 503,
        "hizbQuarter": 228,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "tr.vakfi",
      "language": "tr",
      "name": "Diyanet Vakfı",
      "englishName": "Diyanet Vakfi",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}