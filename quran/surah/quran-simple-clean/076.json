  "data": {
    "number": 76,
    "name": "سُورَةُ الإِنسَانِ",
    "englishName": "Al-Insaan",
    "englishNameTranslation": "Man",
    "revelationType": "Medinan",
    "numberOfAyahs": 31,
    "ayahs": [
      {
        "number": 5592,
        "text": "بسم الله الرحمن الرحيم هل أتى على الإنسان حين من الدهر لم يكن شيئا مذكورا",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5593,
        "text": "إنا خلقنا الإنسان من نطفة أمشاج نبتليه فجعلناه سميعا بصيرا",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5594,
        "text": "إنا هديناه السبيل إما شاكرا وإما كفورا",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5595,
        "text": "إنا أعتدنا للكافرين سلاسل وأغلالا وسعيرا",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5596,
        "text": "إن الأبرار يشربون من كأس كان مزاجها كافورا",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5597,
        "text": "عينا يشرب بها عباد الله يفجرونها تفجيرا",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5598,
        "text": "يوفون بالنذر ويخافون يوما كان شره مستطيرا",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5599,
        "text": "ويطعمون الطعام على حبه مسكينا ويتيما وأسيرا",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5600,
        "text": "إنما نطعمكم لوجه الله لا نريد منكم جزاء ولا شكورا",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5601,
        "text": "إنا نخاف من ربنا يوما عبوسا قمطريرا",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5602,
        "text": "فوقاهم الله شر ذلك اليوم ولقاهم نضرة وسرورا",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5603,
        "text": "وجزاهم بما صبروا جنة وحريرا",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5604,
        "text": "متكئين فيها على الأرائك ۖ لا يرون فيها شمسا ولا زمهريرا",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5605,
        "text": "ودانية عليهم ظلالها وذللت قطوفها تذليلا",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5606,
        "text": "ويطاف عليهم بآنية من فضة وأكواب كانت قواريرا",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5607,
        "text": "قوارير من فضة قدروها تقديرا",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5608,
        "text": "ويسقون فيها كأسا كان مزاجها زنجبيلا",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5609,
        "text": "عينا فيها تسمى سلسبيلا",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5610,
        "text": "۞ ويطوف عليهم ولدان مخلدون إذا رأيتهم حسبتهم لؤلؤا منثورا",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5611,
        "text": "وإذا رأيت ثم رأيت نعيما وملكا كبيرا",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5612,
        "text": "عاليهم ثياب سندس خضر وإستبرق ۖ وحلوا أساور من فضة وسقاهم ربهم شرابا طهورا",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5613,
        "text": "إن هذا كان لكم جزاء وكان سعيكم مشكورا",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 514,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5614,
        "text": "إنا نحن نزلنا عليك القرآن تنزيلا",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 515,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5615,
        "text": "فاصبر لحكم ربك ولا تطع منهم آثما أو كفورا",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 515,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5616,
        "text": "واذكر اسم ربك بكرة وأصيلا",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 579,
        "ruku": 515,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5617,
        "text": "ومن الليل فاسجد له وسبحه ليلا طويلا",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 515,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5618,
        "text": "إن هؤلاء يحبون العاجلة ويذرون وراءهم يوما ثقيلا",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 515,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5619,
        "text": "نحن خلقناهم وشددنا أسرهم ۖ وإذا شئنا بدلنا أمثالهم تبديلا",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 515,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5620,
        "text": "إن هذه تذكرة ۖ فمن شاء اتخذ إلى ربه سبيلا",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 515,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5621,
        "text": "وما تشاءون إلا أن يشاء الله ۚ إن الله كان عليما حكيما",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 515,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5622,
        "text": "يدخل من يشاء في رحمته ۚ والظالمين أعد لهم عذابا أليما",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 515,
        "hizbQuarter": 232,
        "sajda": false
      }
