    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6131,
        "text": "بسم الله الرحمن الرحيم لم يكن الذين كفروا من أهل الكتاب والمشركين منفكين حتى تأتيهم البينة",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 598,
        "ruku": 540,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6132,
        "text": "رسول من الله يتلو صحفا مطهرة",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 598,
        "ruku": 540,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6133,
        "text": "فيها كتب قيمة",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 598,
        "ruku": 540,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6134,
        "text": "وما تفرق الذين أوتوا الكتاب إلا من بعد ما جاءتهم البينة",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 598,
        "ruku": 540,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6135,
        "text": "وما أمروا إلا ليعبدوا الله مخلصين له الدين حنفاء ويقيموا الصلاة ويؤتوا الزكاة ۚ وذلك دين القيمة",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 598,
        "ruku": 540,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6136,
        "text": "إن الذين كفروا من أهل الكتاب والمشركين في نار جهنم خالدين فيها ۚ أولئك هم شر البرية",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 598,
        "ruku": 540,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6137,
        "text": "إن الذين آمنوا وعملوا الصالحات أولئك هم خير البرية",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 598,
        "ruku": 540,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6138,
        "text": "جزاؤهم عند ربهم جنات عدن تجري من تحتها الأنهار خالدين فيها أبدا ۖ رضي الله عنهم ورضوا عنه ۚ ذلك لمن خشي ربه",
        "numberInSurah": 8,
