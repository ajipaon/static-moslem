{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 36,
    "name": "سُورَةُ يسٓ",
    "englishName": "Yaseen",
    "englishNameTranslation": "Yaseen",
    "revelationType": "Meccan",
    "numberOfAyahs": 83,
    "ayahs": [
      {
        "number": 3706,
        "text": "بسم الله الرحمن الرحيم يس",
        "numberInSurah": 1,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3707,
        "text": "والقرآن الحكيم",
        "numberInSurah": 2,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3708,
        "text": "إنك لمن المرسلين",
        "numberInSurah": 3,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3709,
        "text": "على صراط مستقيم",
        "numberInSurah": 4,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3710,
        "text": "تنزيل العزيز الرحيم",
        "numberInSurah": 5,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3711,
        "text": "لتنذر قوما ما أنذر آباؤهم فهم غافلون",
        "numberInSurah": 6,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3712,
        "text": "لقد حق القول على أكثرهم فهم لا يؤمنون",
        "numberInSurah": 7,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3713,
        "text": "إنا جعلنا في أعناقهم أغلالا فهي إلى الأذقان فهم مقمحون",
        "numberInSurah": 8,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3714,
        "text": "وجعلنا من بين أيديهم سدا ومن خلفهم سدا فأغشيناهم فهم لا يبصرون",
        "numberInSurah": 9,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3715,
        "text": "وسواء عليهم أأنذرتهم أم لم تنذرهم لا يؤمنون",
        "numberInSurah": 10,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3716,
        "text": "إنما تنذر من اتبع الذكر وخشي الرحمن بالغيب ۖ فبشره بمغفرة وأجر كريم",
        "numberInSurah": 11,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3717,
        "text": "إنا نحن نحيي الموتى ونكتب ما قدموا وآثارهم ۚ وكل شيء أحصيناه في إمام مبين",
        "numberInSurah": 12,
        "juz": 22,
        "manzil": 5,
        "page": 440,
        "ruku": 381,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3718,
        "text": "واضرب لهم مثلا أصحاب القرية إذ جاءها المرسلون",
        "numberInSurah": 13,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3719,
        "text": "إذ أرسلنا إليهم اثنين فكذبوهما فعززنا بثالث فقالوا إنا إليكم مرسلون",
        "numberInSurah": 14,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3720,
        "text": "قالوا ما أنتم إلا بشر مثلنا وما أنزل الرحمن من شيء إن أنتم إلا تكذبون",
        "numberInSurah": 15,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3721,
        "text": "قالوا ربنا يعلم إنا إليكم لمرسلون",
        "numberInSurah": 16,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3722,
        "text": "وما علينا إلا البلاغ المبين",
        "numberInSurah": 17,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3723,
        "text": "قالوا إنا تطيرنا بكم ۖ لئن لم تنتهوا لنرجمنكم وليمسنكم منا عذاب أليم",
        "numberInSurah": 18,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3724,
        "text": "قالوا طائركم معكم ۚ أئن ذكرتم ۚ بل أنتم قوم مسرفون",
        "numberInSurah": 19,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3725,
        "text": "وجاء من أقصى المدينة رجل يسعى قال يا قوم اتبعوا المرسلين",
        "numberInSurah": 20,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3726,
        "text": "اتبعوا من لا يسألكم أجرا وهم مهتدون",
        "numberInSurah": 21,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3727,
        "text": "وما لي لا أعبد الذي فطرني وإليه ترجعون",
        "numberInSurah": 22,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3728,
        "text": "أأتخذ من دونه آلهة إن يردن الرحمن بضر لا تغن عني شفاعتهم شيئا ولا ينقذون",
        "numberInSurah": 23,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3729,
        "text": "إني إذا لفي ضلال مبين",
        "numberInSurah": 24,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3730,
        "text": "إني آمنت بربكم فاسمعون",
        "numberInSurah": 25,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3731,
        "text": "قيل ادخل الجنة ۖ قال يا ليت قومي يعلمون",
        "numberInSurah": 26,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3732,
        "text": "بما غفر لي ربي وجعلني من المكرمين",
        "numberInSurah": 27,
        "juz": 22,
        "manzil": 5,
        "page": 441,
        "ruku": 382,
        "hizbQuarter": 176,
        "sajda": false
      },
      {
        "number": 3733,
        "text": "۞ وما أنزلنا على قومه من بعده من جند من السماء وما كنا منزلين",
        "numberInSurah": 28,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 382,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3734,
        "text": "إن كانت إلا صيحة واحدة فإذا هم خامدون",
        "numberInSurah": 29,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 382,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3735,
        "text": "يا حسرة على العباد ۚ ما يأتيهم من رسول إلا كانوا به يستهزئون",
        "numberInSurah": 30,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 382,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3736,
        "text": "ألم يروا كم أهلكنا قبلهم من القرون أنهم إليهم لا يرجعون",
        "numberInSurah": 31,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 382,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3737,
        "text": "وإن كل لما جميع لدينا محضرون",
        "numberInSurah": 32,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 382,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3738,
        "text": "وآية لهم الأرض الميتة أحييناها وأخرجنا منها حبا فمنه يأكلون",
        "numberInSurah": 33,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3739,
        "text": "وجعلنا فيها جنات من نخيل وأعناب وفجرنا فيها من العيون",
        "numberInSurah": 34,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3740,
        "text": "ليأكلوا من ثمره وما عملته أيديهم ۖ أفلا يشكرون",
        "numberInSurah": 35,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3741,
        "text": "سبحان الذي خلق الأزواج كلها مما تنبت الأرض ومن أنفسهم ومما لا يعلمون",
        "numberInSurah": 36,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3742,
        "text": "وآية لهم الليل نسلخ منه النهار فإذا هم مظلمون",
        "numberInSurah": 37,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3743,
        "text": "والشمس تجري لمستقر لها ۚ ذلك تقدير العزيز العليم",
        "numberInSurah": 38,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3744,
        "text": "والقمر قدرناه منازل حتى عاد كالعرجون القديم",
        "numberInSurah": 39,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3745,
        "text": "لا الشمس ينبغي لها أن تدرك القمر ولا الليل سابق النهار ۚ وكل في فلك يسبحون",
        "numberInSurah": 40,
        "juz": 23,
        "manzil": 5,
        "page": 442,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3746,
        "text": "وآية لهم أنا حملنا ذريتهم في الفلك المشحون",
        "numberInSurah": 41,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3747,
        "text": "وخلقنا لهم من مثله ما يركبون",
        "numberInSurah": 42,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3748,
        "text": "وإن نشأ نغرقهم فلا صريخ لهم ولا هم ينقذون",
        "numberInSurah": 43,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3749,
        "text": "إلا رحمة منا ومتاعا إلى حين",
        "numberInSurah": 44,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3750,
        "text": "وإذا قيل لهم اتقوا ما بين أيديكم وما خلفكم لعلكم ترحمون",
        "numberInSurah": 45,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3751,
        "text": "وما تأتيهم من آية من آيات ربهم إلا كانوا عنها معرضين",
        "numberInSurah": 46,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3752,
        "text": "وإذا قيل لهم أنفقوا مما رزقكم الله قال الذين كفروا للذين آمنوا أنطعم من لو يشاء الله أطعمه إن أنتم إلا في ضلال مبين",
        "numberInSurah": 47,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3753,
        "text": "ويقولون متى هذا الوعد إن كنتم صادقين",
        "numberInSurah": 48,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3754,
        "text": "ما ينظرون إلا صيحة واحدة تأخذهم وهم يخصمون",
        "numberInSurah": 49,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3755,
        "text": "فلا يستطيعون توصية ولا إلى أهلهم يرجعون",
        "numberInSurah": 50,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 383,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3756,
        "text": "ونفخ في الصور فإذا هم من الأجداث إلى ربهم ينسلون",
        "numberInSurah": 51,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3757,
        "text": "قالوا يا ويلنا من بعثنا من مرقدنا ۜ ۗ هذا ما وعد الرحمن وصدق المرسلون",
        "numberInSurah": 52,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3758,
        "text": "إن كانت إلا صيحة واحدة فإذا هم جميع لدينا محضرون",
        "numberInSurah": 53,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3759,
        "text": "فاليوم لا تظلم نفس شيئا ولا تجزون إلا ما كنتم تعملون",
        "numberInSurah": 54,
        "juz": 23,
        "manzil": 5,
        "page": 443,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3760,
        "text": "إن أصحاب الجنة اليوم في شغل فاكهون",
        "numberInSurah": 55,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3761,
        "text": "هم وأزواجهم في ظلال على الأرائك متكئون",
        "numberInSurah": 56,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3762,
        "text": "لهم فيها فاكهة ولهم ما يدعون",
        "numberInSurah": 57,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3763,
        "text": "سلام قولا من رب رحيم",
        "numberInSurah": 58,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3764,
        "text": "وامتازوا اليوم أيها المجرمون",
        "numberInSurah": 59,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 177,
        "sajda": false
      },
      {
        "number": 3765,
        "text": "۞ ألم أعهد إليكم يا بني آدم أن لا تعبدوا الشيطان ۖ إنه لكم عدو مبين",
        "numberInSurah": 60,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3766,
        "text": "وأن اعبدوني ۚ هذا صراط مستقيم",
        "numberInSurah": 61,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3767,
        "text": "ولقد أضل منكم جبلا كثيرا ۖ أفلم تكونوا تعقلون",
        "numberInSurah": 62,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3768,
        "text": "هذه جهنم التي كنتم توعدون",
        "numberInSurah": 63,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3769,
        "text": "اصلوها اليوم بما كنتم تكفرون",
        "numberInSurah": 64,
        "juz": 23,
        "manzil": 5,
        "page": 444,
        "ruku": 384,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
