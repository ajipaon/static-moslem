        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5558,
        "text": "فإذا برق البصر",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5559,
        "text": "وخسف القمر",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5560,
        "text": "وجمع الشمس والقمر",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5561,
        "text": "يقول الإنسان يومئذ أين المفر",
