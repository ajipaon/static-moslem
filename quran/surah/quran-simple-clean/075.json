    "name": "سُورَةُ القِيَامَةِ",
    "englishName": "Al-Qiyaama",
    "englishNameTranslation": "The Resurrection",
    "revelationType": "Meccan",
    "numberOfAyahs": 40,
    "ayahs": [
      {
        "number": 5552,
        "text": "بسم الله الرحمن الرحيم لا أقسم بيوم القيامة",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5553,
        "text": "ولا أقسم بالنفس اللوامة",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5554,
        "text": "أيحسب الإنسان ألن نجمع عظامه",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5555,
        "text": "بلى قادرين على أن نسوي بنانه",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5556,
        "text": "بل يريد الإنسان ليفجر أمامه",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5557,
        "text": "يسأل أيان يوم القيامة",
