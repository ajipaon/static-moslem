  "data": {
    "number": 96,
    "name": "سُورَةُ العَلَقِ",
    "englishName": "Al-Alaq",
    "englishNameTranslation": "The Clot",
    "revelationType": "Meccan",
    "numberOfAyahs": 19,
    "ayahs": [
      {
        "number": 6107,
        "text": "Lies im Namen deines Herrn, Der erschaffen hat,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6108,
        "text": "den Menschen erschaffen hat aus einem Anhängsel.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6109,
        "text": "Lies, und dein Herr ist der Edelste,",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6110,
        "text": "Der (das Schreiben) mit dem Schreibrohr gelehrt hat,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6111,
        "text": "den Menschen gelehrt hat, was er nicht wußte.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6112,
        "text": "Keineswegs! Der Mensch lehnt sich wahrlich auf,",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6113,
        "text": "daß er von sich meint, unbedürftig zu sein.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6114,
        "text": "Gewiß, zu deinem Herrn wird die Rückkehr sein.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6115,
        "text": "Siehst du denjenigen, der abhält",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6116,
        "text": "einen Diener, wenn er betet?",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6117,
        "text": "Siehst du! Obwohl er nach der Rechtleitung verfährt,",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6118,
        "text": "oder die Gottesfurcht gebietet?",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6119,
        "text": "Siehst du! Wie (wäre es) wenn er (die Botschaft) für Lüge erklärt und sich abkehrt?",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6120,
        "text": "Weiß er denn nicht, daß Allah sieht?",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6121,
        "text": "Keineswegs! Wenn er nicht aufhört, werden Wir ihn ganz gewiß an der Stirnlocke packen und ziehen,",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6122,
        "text": "einer Stirnlocke, einer lügnerischen, einer verfehlt handelnden.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6123,
        "text": "So soll er doch seine Genossen rufen.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6124,
        "text": "Wir werden die (Höllen)wache rufen.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6125,
        "text": "Keineswegs! Gehorche ihm nicht, sondern wirf dich nieder und sei (Allah) nah!",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 538,
        "hizbQuarter": 239,
        "sajda": {
          "id": 15,
          "recommended": false,
          "obligatory": true
        }
      }
    ],
