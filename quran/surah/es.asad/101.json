  "data": {
    "number": 101,
    "name": "سُورَةُ القَارِعَةِ",
    "englishName": "Al-Qaari'a",
    "englishNameTranslation": "The Calamity",
    "revelationType": "Meccan",
    "numberOfAyahs": 11,
    "ayahs": [
      {
        "number": 6158,
        "text": "¡OH, la calamidad repentina!",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6159,
        "text": "¡Que terrible la calamidad repentina!",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6160,
        "text": "¿Y qué puede hacerte concebir lo que será esa calamidad repentina?",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6161,
        "text": "[Ocurrirá] el Día en que los hombres parezcan polillas revoloteando confusas,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6162,
        "text": "y las montañas parezcan copos de lana cardada....",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6163,
        "text": "Y entonces, aquel cuyo peso [de buenas acciones] sea grande en la balanza",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6164,
        "text": "gozará de una vida placentera;",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6165,
        "text": "pero aquel cuyo peso sea leve en la balanza",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6166,
        "text": "se verá cercado por un abismo.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6167,
        "text": "¿Y qué puede hacerte concebir lo que será ese [abismo]?",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6168,
        "text": "¡Un fuego que arde intensamente!",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      }
