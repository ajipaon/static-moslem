        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4952,
        "text": "¿Cuál, pues, de los poderes de vuestro Sustentador negaréis?",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
