    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6139,
        "text": "Cuando la Tierra tiemble con su temblor",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
