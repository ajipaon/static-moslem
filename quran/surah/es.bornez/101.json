      },
      {
        "number": 6159,
        "text": "¿Qué es la catástrofe?",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6160,
        "text": "¿Y qué te hará entender lo que es la catástrofe?",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6161,
        "text": "Es el día en que los seres humanos estarán como un tumulto de langostas desorientadas y dispersas",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6162,
        "text": "y las montañas como lana de colores deshilachada.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6163,
        "text": "Entonces, aquel cuya balanza sea pesada",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6164,
        "text": "disfrutará de una vida satisfactoria",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6165,
        "text": "y aquel cuya balanza sea ligera",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6166,
        "text": "tendrá por hogar el abismo.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6167,
        "text": "¿Y qué te hará entender lo que es el abismo?",
        "numberInSurah": 10,
        "juz": 30,
