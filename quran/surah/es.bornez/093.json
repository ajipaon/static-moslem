  "data": {
    "number": 93,
    "name": "سُورَةُ الضُّحَىٰ",
    "englishName": "Ad-Dhuhaa",
    "englishNameTranslation": "The Morning Hours",
    "revelationType": "Meccan",
    "numberOfAyahs": 11,
    "ayahs": [
      {
        "number": 6080,
        "text": "Juro por la mañana cuando resplandece",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6081,
        "text": "y por la noche cuando todo está en calma,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6082,
        "text": "que tu Señor no te ha abandonado ni está enojado contigo.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6083,
        "text": "Y, ciertamente, la otra vida será mejor para ti que la primera",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6084,
        "text": "y pronto te dará tu Señor y estarás satisfecho.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6085,
        "text": "¿Acaso no te encontró huérfano y te amparó?",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6086,
        "text": "¿Y te encontró perdido y te guió?",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6087,
        "text": "¿Y te encontró pobre y te enriqueció?",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6088,
        "text": "Así pues, no oprimas al huérfano",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6089,
        "text": "ni alejes al mendigo",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6090,
        "text": "y proclama la merced de tu Señor.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 535,
        "hizbQuarter": 238,
        "sajda": false
      }
    ],
