  "data": {
    "number": 104,
    "name": "سُورَةُ الهُمَزَةِ",
    "englishName": "Al-Humaza",
    "englishNameTranslation": "The Traducer",
    "revelationType": "Meccan",
    "numberOfAyahs": 9,
    "ayahs": [
      {
        "number": 6180,
        "text": "हर ताना देने वाले चुग़लख़ोर की ख़राबी है",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6181,
        "text": "जो माल को जमा करता है और गिन गिन कर रखता है",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6182,
        "text": "वह समझता है कि उसका माल उसे हमेशा ज़िन्दा बाक़ी रखेगा",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6183,
        "text": "हरगिज़ नहीं वह तो ज़रूर हुतमा में डाला जाएगा",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6184,
        "text": "और तुमको क्या मालूम हतमा क्या है",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6185,
        "text": "वह ख़ुदा की भड़काई हुई आग है जो (तलवे से लगी तो) दिलों तक चढ़ जाएगी",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6186,
        "text": "ये लोग आग के लम्बे सुतूनो",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6187,
        "text": "में डाल कर बन्द कर दिए",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6188,
        "text": "जाएँगे",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 601,
        "ruku": 546,
        "hizbQuarter": 240,
        "sajda": false
      }
