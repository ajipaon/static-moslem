  "data": {
    "number": 82,
    "name": "سُورَةُ الانفِطَارِ",
    "englishName": "Al-Infitaar",
    "englishNameTranslation": "The Cleaving",
    "revelationType": "Meccan",
    "numberOfAyahs": 19,
    "ayahs": [
      {
        "number": 5830,
        "text": "जब आसमान तर्ख़ जाएगा",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5831,
        "text": "और जब तारे झड़ पड़ेंगे",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5832,
        "text": "और जब दरिया बह (कर एक दूसरे से मिल) जाएँगे",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5833,
        "text": "और जब कब्रें उखाड़ दी जाएँगी",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5834,
        "text": "तब हर शख़्श को मालूम हो जाएगा कि उसने आगे क्या भेजा था और पीछे क्या छोड़ा था",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5835,
        "text": "ऐ इन्सान तुम्हें अपने परवरदिगार के बारे में किस चीज़ ने धोका दिया",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5836,
        "text": "जिसने तुझे पैदा किया तो तुझे दुरूस्त बनाया और मुनासिब आज़ा दिए",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5837,
        "text": "और जिस सूरत में उसने चाहा तेरे जोड़ बन्द मिलाए",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5838,
        "text": "हाँ बात ये है कि तुम लोग जज़ा (के दिन) को झुठलाते हो",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5839,
        "text": "हालॉकि तुम पर निगेहबान मुक़र्रर हैं",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5840,
        "text": "बुर्ज़ुग लोग (फरिश्ते सब बातों को) लिखने वाले (केरामन क़ातेबीन)",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5841,
        "text": "जो कुछ तुम करते हो वह सब जानते हैं",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5842,
        "text": "बेशक नेको कार (बेहिश्त की) नेअमतों में होंगे",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5843,
        "text": "और बदकार लोग यक़ीनन जहन्नुम में जज़ा के दिन",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5844,
        "text": "उसी में झोंके जाएँगे",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5845,
        "text": "और वह लोग उससे छुप न सकेंगे",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5846,
        "text": "और तुम्हें क्या मालूम कि जज़ा का दिन क्या है",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5847,
        "text": "फिर तुम्हें क्या मालूम कि जज़ा का दिन क्या चीज़ है",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5848,
        "text": "उस दिन कोई शख़्श किसी शख़्श की भलाई न कर सकेगा और उस दिन हुक्म सिर्फ ख़ुदा ही का होगा",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      }
