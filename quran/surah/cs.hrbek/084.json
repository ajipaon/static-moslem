    "englishNameTranslation": "The Splitting Open",
    "revelationType": "Meccan",
    "numberOfAyahs": 25,
    "ayahs": [
      {
        "number": 5885,
        "text": "Až se nebe rozštěpí",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5886,
        "text": "a uposlechne Pána svého a své povinnosti zadost učiní,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5887,
        "text": "až se země zarovná",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5888,
        "text": "a vyvrhne, co v ní je, a bude prázdná",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5889,
        "text": "a uposlechne Pána svého a své povinnosti zadost učiní",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5890,
        "text": "Člověče, věru se usilovně snažíš o Pána svého a vskutku s Ním dojde k setkání",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5891,
        "text": "Komu bude kniha jeho dána do pravice,",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5892,
        "text": "tomu zúčtováno bude počtem snadným",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5893,
        "text": "a on radostně se navrátí k lidem svým.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5894,
        "text": "A ten, jemuž za záda bude kniha jeho dána,",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5895,
        "text": "ten volat bude po zničení",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5896,
        "text": "a plát bude ve šlehajícím plameni.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5897,
        "text": "Žil věru s rodinou svou v radosti na zemi,",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5898,
        "text": "a že k Bohu se nenavrátí, se domníval.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5899,
        "text": "Ba nikoliv! Vždyť Pán jeho jej pozoroval!",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5900,
        "text": "Hle, přísahám při soumraku,",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5901,
        "text": "při noci a při tom, co zahaluje,",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5902,
        "text": "a při měsíci, když vchází do úplňku,",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5903,
        "text": "že vskutku stoupat budete stupeň po stupínku!",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5904,
        "text": "Co je jim tedy, že nevěří",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5905,
        "text": "a k zemi se nevrhají, když Korán jim přednášejí?",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": {
          "id": 14,
          "recommended": true,
          "obligatory": false
        }
      },
      {
        "number": 5906,
        "text": "Ba co víc, ti, kdož nevěří, za lživý jej prohlašují.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5907,
        "text": "Však Bůh nejlépe ví, co skrývají!",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5908,
        "text": "Ohlas jim zvěst radostnou o trestu bolestném, jenž stihne je -",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5909,
        "text": "kromě těch, kdož uvěřili a zbožné skutky konali, a těm náleží odměna, jež se neupomíná.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 526,
        "hizbQuarter": 236,
        "sajda": false
      }
