{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 83,
    "name": "سُورَةُ المُطَفِّفِينَ",
    "englishName": "Al-Mutaffifin",
    "englishNameTranslation": "Defrauding",
    "revelationType": "Meccan",
    "numberOfAyahs": 36,
    "ayahs": [
      {
        "number": 5849,
        "text": "Běda těm, kdo míru zkracují,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5850,
        "text": "kteří když jim je měřeno plnou míru od lidí žádají,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5851,
        "text": "však když pro druhé měří či váží, ztrátu jim působí!",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5852,
        "text": "Což si myslí, že nebudou vzkříšeni",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5853,
        "text": "pro den nesmírný,",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5854,
        "text": "pro den, kdy lidé před Pána vesmíru předstoupí?",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5855,
        "text": "Však pozor, vždyť věru kniha hříšníků je v Sidždžínu!",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5856,
        "text": "Víš ty vůbec, co je to Sidždžín?",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5857,
        "text": "Kniha označkovaná!",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5858,
        "text": "Běda v ten den těm, kdož za lež prohlašují",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5859,
        "text": "a za výmysl den soudný pokládají!",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5860,
        "text": "Nepokládá jej za výmysl nikdo kromě všech protivníků hříšných,",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5861,
        "text": "kteří - když jsou jim přednášena Naše znamení - říkají: \"To jsou povídačky starých!\"",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5862,
        "text": "Však pozor! Naopak srdce jejich zrezivěla od toho, co si vysloužila.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5863,
        "text": "Však pozor! Věru že budou v ten den od svého Pána odděleni závěsem",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5864,
        "text": "a potom budou hořet v ohni pekelném",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5865,
        "text": "a posléze jim bude řečeno: \"Hle, zde máte, co nazývali jste výmyslem!\"",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5866,
        "text": "Však pozor! Vždyť věru kniha zbožných je v ´Illijún!",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5867,
        "text": "Víš ty vůbec, co je to ´Illijún?",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5868,
        "text": "Kniha označkovaná,",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5869,
        "text": "na vlastní oči ji spatří ten, kdo k Pánu bude přiblížen.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5870,
        "text": "A zbožní vskutku budou ve slastech,",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5871,
        "text": "na pohovkách ležíce se budou rozhlížet.",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5872,
        "text": "Na jejich tvářích uzříš svěžest slastí,",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5873,
        "text": "napájeni budou vínem silným, zapečetěným",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5874,
        "text": "z pižma pečetí - o to nechť soutěží ti, kdož soutěží -",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5875,
        "text": "s vodou Tasnímu smíseným,",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5876,
        "text": "pramene, z něhož pijí jen přiblížení.",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5877,
        "text": "Věru že se hříšníci posmívali těm, kdož uvěřili,",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5878,
        "text": "a když je míjeli; vzájemně na sebe pomrkávali,",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5879,
        "text": "a když se ke svým rodinám vrátili, smích si tropili,",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5880,
        "text": "a když je viděli, volali: \"Věru jsou to zbloudilí!\",",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5881,
        "text": "A přec nebyli posláni k věřícím, aby je střežili.",
        "numberInSurah": 33,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5882,
        "text": "Dnes však se nevěřícím posmívají ti, kdož věřili,",
        "numberInSurah": 34,
        "juz": 30,
        "manzil": 7,
        "page": 588,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5883,
        "text": "a na pohovkách ležíce se rozhlížejí.",
        "numberInSurah": 35,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5884,
        "text": "Dostalo se nevěřícím odměny za to, co činili?",
        "numberInSurah": 36,
        "juz": 30,
        "manzil": 7,
        "page": 589,
        "ruku": 525,
        "hizbQuarter": 235,
        "sajda": false
      }
    ],
