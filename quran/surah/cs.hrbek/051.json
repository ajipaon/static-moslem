{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 51,
    "name": "سُورَةُ الذَّارِيَاتِ",
    "englishName": "Adh-Dhaariyat",
    "englishNameTranslation": "The Winnowing Winds",
    "revelationType": "Meccan",
    "numberOfAyahs": 60,
    "ayahs": [
      {
        "number": 4676,
        "text": "Při rychle běžících,",
        "numberInSurah": 1,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4677,
        "text": "břímě nesoucích,",
        "numberInSurah": 2,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4678,
        "text": "lehce plynoucích",
        "numberInSurah": 3,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4679,
        "text": "a rozkaz rozdělujících!",
        "numberInSurah": 4,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4680,
        "text": "To, co vám je slíbeno, je vskutku pravdivé",
        "numberInSurah": 5,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4681,
        "text": "a soud dozajista nastane!",
        "numberInSurah": 6,
        "juz": 26,
        "manzil": 7,
        "page": 520,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4682,
        "text": "Při nebi plném drah hvězdných,",
        "numberInSurah": 7,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4683,
        "text": "vy libujete si v řečech rozporných,",
        "numberInSurah": 8,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4684,
        "text": "od nichž odvrátí se ten, kdo byl odvrácen.",
        "numberInSurah": 9,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4685,
        "text": "Nechť zhynou lháři prolhaní,",
        "numberInSurah": 10,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4686,
        "text": "kteří v hlouposti své jsou lhostejní",
        "numberInSurah": 11,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4687,
        "text": "a ptají se: \"Kdy dostaví se den poslední?\"",
        "numberInSurah": 12,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4688,
        "text": "V den, kdy budou ohněm zkoušeni!",
        "numberInSurah": 13,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4689,
        "text": "\"Okuste tuto svou zkoušku! Tohle je to, co přáli jste si uspíšit!\"",
        "numberInSurah": 14,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4690,
        "text": "Však v zahradách a u pramenů bohabojní budou dlít",
        "numberInSurah": 15,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4691,
        "text": "přijímajíce to, co Pán jejich jim dává, neb před tím dobré skutky konali",
        "numberInSurah": 16,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4692,
        "text": "a jen malou část noci spávali",
        "numberInSurah": 17,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4693,
        "text": "a za úsvitu o odpuštění prosívali",
        "numberInSurah": 18,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4694,
        "text": "a ze svých majetků vždy patřičně žebrákům a nemajetným dávali.",
        "numberInSurah": 19,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4695,
        "text": "Na zemi znamení jsou pro věřící přesvědčené,",
        "numberInSurah": 20,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4696,
        "text": "a také ve vašich duších. Což jasně to nezříte?",
        "numberInSurah": 21,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4697,
        "text": "A na nebesích je váš příděl obživy a rovněž to, co vám je slíbeno.",
        "numberInSurah": 22,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4698,
        "text": "Při Pánu nebes i země, toto je skutečnost stejná jako to, že hovoříte.",
        "numberInSurah": 23,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 455,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4699,
        "text": "Zdaž doneslo se ti o Abrahamových poctěných hostech vyprávění?",
        "numberInSurah": 24,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4700,
        "text": "Hle, vešli k němu a pravili \"Mír\", a on odvětil: \"I vám mír, lidé neznámí!\"",
        "numberInSurah": 25,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4701,
        "text": "A k rodině své odešel a tučné tele připravil",
        "numberInSurah": 26,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4702,
        "text": "a se slovy \"Což jíst nebudete?\" jim je předložil.",
        "numberInSurah": 27,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4703,
        "text": "A pocítil strach před nimi, však oni mu pak pravili \"Neboj se!\" a zvěst o narození chlapce moudrého mu oznámili.",
        "numberInSurah": 28,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4704,
        "text": "Pak žena jeho přiběhla vzrušena a do tváře se bila a volala: \"Vždyť já neplodná jsem již stařena!\"",
        "numberInSurah": 29,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4705,
        "text": "I odpověděli: \"Takto Pán tvůj děl a On věru moudrý je i vševědoucí.\"",
        "numberInSurah": 30,
        "juz": 26,
        "manzil": 7,
        "page": 521,
        "ruku": 456,
        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4706,
        "text": "A otázal se Abraham: \"Jaká věc vás přivádí, vyslanci?\"",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4707,
        "text": "Odvětili: \"Posláni jsme byli mezi lidi hříšné,",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4708,
        "text": "abychom na ně seslali kameny z hlíny pálené,",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4709,
        "text": "u Pána tvého jmény přestupníků označené.\"",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4710,
        "text": "Ty z nich, kdož věřící byli, jsme odtamtud vyvedli,",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4711,
        "text": "však dům jen jediný těch, kdož do vůle Boží se odevzdali, jsme tam nalezli.",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4712,
        "text": "A zanechali jsme je jako znamení těm, kdo trestu bolestného se obávají.",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4713,
        "text": "... A o Mojžíšovi; hle, vyslali jsme jej k Faraónovi se zjevnou pravomocí,",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4714,
        "text": "ten však se obrátil i se svou oporou a prohlásil: \"Kouzelník je to nebo blázen!\"",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4715,
        "text": "A uchvátili jsme jej i vojska jeho a do moře jsme je uvrhli, neb pokárání byl hoden.",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4716,
        "text": "... A o Ádovcích; hle, vyslali jsme proti nim vichřici zničující,",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4717,
        "text": "jež nic z toho, nad čím se převalila, nenechala, aby v prach to neproměnila.",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4718,
        "text": "... A o Thamúdovcích; hle, bylo jim řečeno: Užívejte si ještě nějakou chvíli!",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4719,
        "text": "Však rozkazu Pána svého neposlechli a zemětřesení je postihlo, zatímco se dívali,",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4720,
        "text": "a nebyli schopni se zpříma postavit a nebylo jim pomoci.",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4721,
        "text": "... A o lidu Noemově před nimi, a to byli hanebníci!",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 456,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4722,
        "text": "Nebe jsme důkladně postavili, vždyť My jsme rozšiřovateli,",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4723,
        "text": "a zemi jsme rozprostřeli - a jak výteční jsme v tom byli!",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4724,
        "text": "A každou věc jsme v párech stvořili - snad si to připomenete!",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4725,
        "text": "K Bohu se utíkejte, neb já jsem od Něho pro vás varovatel zjevný,",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4726,
        "text": "a nepřidružujte k Bohu žádné jiné božstvo, neb já jsem od Něho pro vás varovatel zjevný!",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 522,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4727,
        "text": "A stejně tak nepřišel k těm, kdož před těmito byli, žádný posel, aby o něm neprohlásili: \"Kouzelník je to nebo blázen!\"",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4728,
        "text": "Což po sobě to dědí? Nikoliv, však lid je to vzpurný.",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4729,
        "text": "Odvrať se proto od nich, a nebudeš za to kárán!",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4730,
        "text": "Připomínej, vždyť připomenutí je věřícím prospěšné!",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4731,
        "text": "A džiny a lidi jsem jedině proto stvořil, aby Mne uctívali,",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4732,
        "text": "a nechci od nich žádný příděl obživy a netoužím po tom, aby Mne živili,",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4733,
        "text": "vždyť Bůh sám je živitel, vládce síly a nezviklatelný!",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4734,
        "text": "Věru těm, kdož křivdili, se dostane přídělu podobného, přídělu jim rovných před nimi; nechť se tedy o uspíšení nesnaží!",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      },
      {
        "number": 4735,
        "text": "Běda nevěřícím v ten jejich den, jenž byl jim přislíben!",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 523,
        "ruku": 457,
        "hizbQuarter": 209,
        "sajda": false
      }
    ],
