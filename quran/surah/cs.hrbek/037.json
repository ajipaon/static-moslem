        "sajda": false
      },
      {
        "number": 3928,
        "text": "když na loď naloženou se uchýlil",
        "numberInSurah": 140,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3929,
        "text": "a vrhal los a byl tím, jenž ztratil,",
        "numberInSurah": 141,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3930,
        "text": "a pohltila jej ryba, zatímco pokárání zasloužil.",
        "numberInSurah": 142,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3931,
        "text": "A kdyby nebyl z těch, kdož Boha oslavují,",
        "numberInSurah": 143,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3932,
        "text": "byl by jistě v břiše jejím zůstal až do dne vzkříšení.",
        "numberInSurah": 144,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3933,
        "text": "A vyvrhli jsme jej chorého na břeh pustý",
        "numberInSurah": 145,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3934,
        "text": "a dali jsme nad ním vyrůst keři tykvovému.",
        "numberInSurah": 146,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3935,
        "text": "Potom jsme jej ke sto tisícům či více nevěřícím poslali,",
        "numberInSurah": 147,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3936,
        "text": "a uvěřili a užívání života jsme jim na čas popřáli.",
        "numberInSurah": 148,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3937,
        "text": "Dotaž se jich, proč Pán tvůj má dcery mít, když oni přece mají syny,",
        "numberInSurah": 149,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3938,
        "text": "či zda byli svědky toho, že stvořili jsme anděly jako ženy?",
        "numberInSurah": 150,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3939,
        "text": "Což není to jen výmysl jejich, když hovoří:",
        "numberInSurah": 151,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3940,
        "text": "\"Bůh zplodil\" - a věru jsou to lháři!",
        "numberInSurah": 152,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3941,
        "text": "A že by si místo synů vyvolil dcery?",
        "numberInSurah": 153,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3942,
        "text": "Co je s vámi, že máte takové názory?",
        "numberInSurah": 154,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3943,
        "text": "Což se nevzpamatujete?",
        "numberInSurah": 155,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3944,
        "text": "Anebo snad pravomoc zjevnou k tomu máte?",
        "numberInSurah": 156,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3945,
        "text": "Předložte tedy Písmo své, jste-li pravdomluvní!",
        "numberInSurah": 157,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3946,
        "text": "A učinili Boha s džiny příbuzným, však džinové dobře již vědí, že sami budou před Pána předvedeni.",
        "numberInSurah": 158,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3947,
        "text": "Oč slavnější je Bůh než to, co lživě Mu připisují,",
        "numberInSurah": 159,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3948,
        "text": "kromě služebníků Božích upřímných.",
        "numberInSurah": 160,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3949,
        "text": "A ani vy, ani to, co uctíváte,",
        "numberInSurah": 161,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3950,
        "text": "nikoho proti Bohu vzbouřit nemůžete",
        "numberInSurah": 162,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3951,
        "text": "kromě toho, jenž hořet bude v pekle.",
        "numberInSurah": 163,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3952,
        "text": "Není mezi námi nikoho, kdo by neměl místo určené,",
        "numberInSurah": 164,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3953,
        "text": "my věru pak jsme ti, kdo v řadách stojí,",
        "numberInSurah": 165,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3954,
        "text": "a také ti, kdož Pána svého oslavují!",
        "numberInSurah": 166,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3955,
        "text": "Nevěřící sice říkají:",
        "numberInSurah": 167,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3956,
        "text": "\"Kdybychom měli nějaké připomenutí od předků svých dávných,",
        "numberInSurah": 168,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3957,
        "text": "věru bychom byli také ze služebníků Božích upřímných!\"",
        "numberInSurah": 169,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3958,
        "text": "Však přesto v ně nevěří a záhy poznají!",
        "numberInSurah": 170,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3959,
        "text": "Slovo Naše již dříve bylo dáno služebníkům Našim poslaným,",
        "numberInSurah": 171,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3960,
        "text": "že od Nás pomoc vždy dostanou",
        "numberInSurah": 172,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3961,
        "text": "a že vojska Naše pro ně vítězství přinesou.",
        "numberInSurah": 173,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3962,
        "text": "Odvrať se od nich na dobu krátkou",
        "numberInSurah": 174,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3963,
        "text": "a pozoruj je; však oni také záhy zpozorují!",
        "numberInSurah": 175,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3964,
        "text": "Cožpak doopravdy uspíšení trestu Našeho požadují?",
        "numberInSurah": 176,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3965,
        "text": "Až pak trest na jejich nádvoří sestoupí, špatné to bude jitro pro ty, kdož byli varováni marně.",
        "numberInSurah": 177,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3966,
        "text": "Odvrať se tedy od nich dočasně",
        "numberInSurah": 178,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3967,
        "text": "a pozoruj je, však oni také záhy zpozorují!",
        "numberInSurah": 179,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3968,
        "text": "Oč slavnější je Pán tvůj, Pán moci veškeré, než to, co mu lživě připisují!",
        "numberInSurah": 180,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3969,
        "text": "Mír budiž všem vyslancům Božím",
        "numberInSurah": 181,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3970,
        "text": "a chvála Bohu, Pánu lidstva!",
        "numberInSurah": 182,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      }
