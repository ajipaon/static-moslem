    "revelationType": "Meccan",
    "numberOfAyahs": 182,
    "ayahs": [
      {
        "number": 3789,
        "text": "Při těch, kdo v řadách stoji,",
        "numberInSurah": 1,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
