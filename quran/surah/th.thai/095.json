    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6099,
        "text": "ขอสาบานด้วยต้นมะเดื่อและต้นมะกอก",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6100,
        "text": "และด้วยภูเขาฎูรซีนาย",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6101,
        "text": "และด้วยเมืองนี้ที่ปลอดภัย",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6102,
        "text": "โดยแน่นอนเราได้บังเกิดมนุษย์มาในรูปแบบที่สวยงามยิ่ง",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6103,
        "text": "แล้วเราได้ให้เขากลับสู่สภาพที่ตกต่ำยิ่ง",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6104,
        "text": "นอกจากบรรดาผู้ศรัทธาและประกอบสิ่งดีงามทั้งหลาย โดยที่สำหรับพวกเขาจะได้รับรางวัลอย่างไม่มีที่สิ้นสุด",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6105,
        "text": "ดังนั้น อะไรเล่าเขาจึงปฏิเสธเจ้า หลังจากนี้เกี่ยวกับการตอบแทน",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6106,
        "text": "มิใช่อัลลอฮฺดอกหรือ เป็นผู้ตัดสินที่ดีเยี่ยมในหมู่ผู้ตัดสินทั้งหลาย",
        "numberInSurah": 8,
