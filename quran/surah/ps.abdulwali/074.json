{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 74,
    "name": "سُورَةُ المُدَّثِّرِ",
    "englishName": "Al-Muddaththir",
    "englishNameTranslation": "The Cloaked One",
    "revelationType": "Meccan",
    "numberOfAyahs": 56,
    "ayahs": [
      {
        "number": 5496,
        "text": "اى په جامه كې نغښتونكیه!",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5497,
        "text": "ته پورته شه، نو (خلق) ووېروه",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5498,
        "text": "او نو د خپل رب لويي بیانوه",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5499,
        "text": "او نو خپلې جامې پاكې ساته",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5500,
        "text": "او ګناه (او بتان) نو ترك كړه",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5501,
        "text": "او (داسې) احسان مه كوه چې بدل (او ډېرول) يې غواړې",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5502,
        "text": "او خاص د خپل رب لپاره، پس صبر كوه",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5503,
        "text": "نو كله چې په شپېلۍ كې پوكى وكړى شي",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5504,
        "text": "نو په دغه وخت كې دغه ډېره سخته ورځ ده",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5505,
        "text": "په كافرانو باندې اسانه نه ده",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5506,
        "text": "ته پرېږده ما او هغه څوك چې ما دى يواځې پیدا كړى دى",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5507,
        "text": "او (بیا مې) ده ته ډېر زیات مال وركړ",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5508,
        "text": "او زامن حاضر (باشه)",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5509,
        "text": "او ده ته مې فراخي وركړه، ډېره فراخي",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5510,
        "text": "بیا دى طمع لري چې زه به ده ته لا زیات وركړم",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5511,
        "text": "هیڅكله هم نه، بېشكه دى زمونږ له ایتونو سره ډېر عناد كوونكى (مخالف) و",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5512,
        "text": "ژر به زه ده ته ډېر سخت تكلیف وركړم",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 575,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5513,
        "text": "بېشكه ده فكر وكړ او اندازه يې وكړه",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5514,
        "text": "پس دى دې هلاك كړى شي، ده څنګه اندازه وكړه؟",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5515,
        "text": "بیا دې هلاك كړى شي، ده څنګه اندازه وكړه",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5516,
        "text": "بیا يې وكتل",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5517,
        "text": "بیا يې مخ تریو كړ او وچولى (تندى) يې بوټ كړ",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5518,
        "text": "بیا يې شا كړه او لويي يې وكړه",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5519,
        "text": "نو ده وویل: نه دى دا (قرآن) مګر داسې جادو چې رانقل كولى شي",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5520,
        "text": "نه دى دا مګر د انسان قول",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5521,
        "text": "ژر به زه دى د دوزخ اور ته داخل كړم",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5522,
        "text": "او ته څه شي پوه كړې چې سقر (د دوزخ اور) څه شى دى",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5523,
        "text": "دى به نه باقي ساتي او نه به پرېږدي",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5524,
        "text": "انسان لره ډېر سوځوونكى دى",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5525,
        "text": "په ده باندې نولس (ملايك) مقرر دي",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5526,
        "text": "او مونږ د دوزخ څوكيداران نه دي ګرځولي مګر ملايك او مونږ د دوى شمېر نه دى ګرځولى مګر ازمېښت د هغو كسانو لپاره چې كافران شوي دي، د دې لپاره چې هغه كسان یقین وكړي چې كتاب ورته وركړى شوى دى او د هغو كسانو ایمان زیات شي چې ایمان يې راوړى دى او شك ونه كړي هغه كسان چې كتاب ورته وركړى شوى دى او مومنان۔ او د دې لپاره چې هغه كسان ووايي چې د هغوى په زړونو كې مرض دى او كافران: الله په دې سره د مثال په لحاظ څه اراده كړې ده۔. همداسې الله ګمراه كوي چا ته چې وغواړي او هدایت وركوي چا ته چې وغواړي۔ او ستا د رب لښكرې نه پېژني مګر هم دى. او نه دى دا مګر د خلقو لپاره نصیحت (او پند)",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 510,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5527,
        "text": "هیڅكله داسې نه ده، قسم دى په سپوږمۍ",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5528,
        "text": "او په شپې باندې كله چې شا كړي",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5529,
        "text": "او په سبا باندې كله چې روښانه شي",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5530,
        "text": "بېشكه دغه (اور د سقر) یقینًا په لویو (بلاګانو) كې یو دى",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5531,
        "text": "چې انسان لره وېروونكى دى",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5532,
        "text": "هغه چا لره چې په تاسو كې غواړي چې وړاندې شي، یا وروسته شي",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5533,
        "text": "هر نفس به د خپلو كړو په بدل كې ګاڼه (ګرو) وي",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5534,
        "text": "غیر له ښي اړخ والاو نه",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5535,
        "text": "(دوى به) په جنتونو كې وي، یو تر بله به پوښتنې كوي",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5536,
        "text": "د مجرمانو (كافرانو) په باره كې",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5537,
        "text": "(مومنان به مجرمانو ته وايي:) تاسو په دوزخ كې څه شي داخل كړي یئ؟",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5538,
        "text": "دوى به ووايي: مونږ له لمونځ كوونكو ځنې نه وو",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5539,
        "text": "او مسكین ته به مو طعام نه وركاوه",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5540,
        "text": "او له باطل ویونكو سره به مونږ په باطلو خبرو كې ننوتلو",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5541,
        "text": "او مونږ به د بدلې ورځ دروغ ګڼله",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5542,
        "text": "تر دې پورې چې مونږ ته یقین راغى",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 576,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5543,
        "text": "نو دوى ته د سفارش كوونكو سفارش فايده نه رسوي",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5544,
        "text": "نو په دوى څه شوي دي چې له نصیحت (قرآن) نه مخ ګرځوونكي دي",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5545,
        "text": "ګویا كې دوى تښتېدونكي ځنګلي خره دي",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5546,
        "text": "چې له زمري نه تښتي",
        "numberInSurah": 51,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5547,
        "text": "بلكې په دوى كې هر یو سړى غواړي چې ده ته خورې كړى شوې صحیفې (پاڼې) وركړى شي",
        "numberInSurah": 52,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5548,
        "text": "هیڅكله داسې نه ده۔ بلكې دوى له اخرت نه نه وېرېږي",
        "numberInSurah": 53,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5549,
        "text": "هیڅكله داسې نه ده۔، بېشكه دا (قرآن) نصیحت دى",
        "numberInSurah": 54,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5550,
        "text": "نو څوك چې غواړي (نو) له ده نه دې پند واخلي",
        "numberInSurah": 55,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      },
      {
        "number": 5551,
        "text": "او دوى نصیحت (او پند) نه اخلي مګر كه الله يې وغواړي. هم دى د وېرې لايق دى او د بخښنې اهل دى",
        "numberInSurah": 56,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 511,
        "hizbQuarter": 230,
        "sajda": false
      }
    ],
