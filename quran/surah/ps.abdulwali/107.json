        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6199,
        "text": "نو همدغه هغه كس دى چې یتیم ته دیكې وركوي",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
