{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 80,
    "name": "سُورَةُ عَبَسَ",
    "englishName": "Abasa",
    "englishNameTranslation": "He frowned",
    "revelationType": "Meccan",
    "numberOfAyahs": 42,
    "ayahs": [
      {
        "number": 5759,
        "text": "Surat astı ve döndü;",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5760,
        "text": "Kör geldi diye.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5761,
        "text": "Ne bilirsin belki o arınacak?",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5762,
        "text": "Yahut öğüt dinleyecek de öğüt, kendisine yarayacak.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5763,
        "text": "Kendisini zengin görüp tenezzül etmeyene gelince;",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5764,
        "text": "Sen ona yöneliyorsun.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5765,
        "text": "Onun arınmamasından sana ne?",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5766,
        "text": "Fakat koşarak sana gelen,",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5767,
        "text": "Saygılı olarak gelmişken,",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5768,
        "text": "Sen onunla ilgilenmiyorsun.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5769,
        "text": "Hayır (olmaz böyle şey); o (ayetler), bir hatırlatmadır.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5770,
        "text": "Dileyen onu düşünüp öğüt alır.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5771,
        "text": "(O öğüt) Sahifeler içindedir: Değer verilen,",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5772,
        "text": "Saygı ile yükseltilen, tertemiz (sayfalar)",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5773,
        "text": "Yazıcıların ellerinde:",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5774,
        "text": "Değerli, iyi (yazıcıların).",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5775,
        "text": "Kahrolası insan, ne kadar da nankördür!",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5776,
        "text": "(Allah) Onu hangi şeyden yarattı?",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5777,
        "text": "Nutfe (sperm)den. Onu yarattı, ona biçim verdi.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5778,
        "text": "Sonra ona yolu kolaylaştırdı.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5779,
        "text": "Sonra onu öldürdü, kabre koydurdu.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5780,
        "text": "Sonra dilediği zaman onu diriltip kaldırdı.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5781,
        "text": "Hayır, insan, O'nun kendisine emrettiğini yapmadı.",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5782,
        "text": "İnsan şu yiyeceğine baksın.",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5783,
        "text": "Biz suyu iyice döktük.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5784,
        "text": "Sonra toprağı güzelce yardık da,",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5785,
        "text": "Orada bitirdik: Dane,",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5786,
        "text": "Üzüm, yonca,",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5787,
        "text": "Zeytin, hurma,",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5788,
        "text": "İri ve gür bahçeler,",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5789,
        "text": "Meyva ve çayır;",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5790,
        "text": "Sizin ve hayvanlarınızın geçimi için.",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5791,
        "text": "Çarpınca kulakları sağır eden o gürültü geldiği zaman,",
        "numberInSurah": 33,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5792,
        "text": "İşte o gün kişi kaçar: kardeşinden,",
        "numberInSurah": 34,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5793,
        "text": "Anasından, babasından,",
        "numberInSurah": 35,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5794,
        "text": "Eşinden ve oğullarından.",
        "numberInSurah": 36,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5795,
        "text": "O gün, onlardan her kişinin, kendisine yeter derecede işi vardır.",
        "numberInSurah": 37,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5796,
        "text": "Yüzler var ki o gün parıl parıl,",
        "numberInSurah": 38,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5797,
        "text": "Güleç, sevinçli.",
        "numberInSurah": 39,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5798,
        "text": "Yüzler de var ki o gün tozlanmış.",
        "numberInSurah": 40,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5799,
        "text": "Onları karanlık bürümüş (öylesine üzgün, öylesine dertli).",
        "numberInSurah": 41,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5800,
        "text": "İşte onlar kafirler, Hak'tan sapanlardır.",
        "numberInSurah": 42,
        "juz": 30,
        "manzil": 7,
        "page": 585,
        "ruku": 522,
        "hizbQuarter": 234,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "tr.ates",
      "language": "tr",
      "name": "Süleyman Ateş",
      "englishName": "Suleyman Ates",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}