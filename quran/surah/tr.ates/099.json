    "number": 99,
    "name": "سُورَةُ الزَّلۡزَلَةِ",
    "englishName": "Az-Zalzala",
    "englishNameTranslation": "The Earthquake",
    "revelationType": "Medinan",
    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6139,
        "text": "Yer o yaman sarsıntı ile sarsıldığı,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6140,
        "text": "Yer (bağrındaki) ağırlıklarını çıkardığı,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6141,
        "text": "Ve insan: \"Ona ne oluyor?\" dediği zaman!",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6142,
        "text": "İşte o gün (yer), haberlerini söyler.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6143,
        "text": "Çünkü Rabbin ona vahyetmiştir (onun için başından geçenleri anlatır).",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6144,
        "text": "O gün insanlar, ayrı ayrı gruplar halinde (Yüce Divana) çıkarlar ki, yaptıkları işler kendilerine gösterilsin.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6145,
        "text": "Artık kim zerre ağırlığınca hayır yapmışsa onu görür.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 541,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6146,
        "text": "Ve kim zerre ağırlığınca şer yapmışsa onu görür.",
