{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 75,
    "name": "سُورَةُ القِيَامَةِ",
    "englishName": "Al-Qiyaama",
    "englishNameTranslation": "The Resurrection",
    "revelationType": "Meccan",
    "numberOfAyahs": 40,
    "ayahs": [
      {
        "number": 5552,
        "text": "Yoo, kıyamet gününe and içerim,",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5553,
        "text": "Yoo, daima, kendini kınayan nefse and içerim.",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5554,
        "text": "İnsan kendisinin kemiklerini bir araya toplamayacağımızı mı sanıyor?",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5555,
        "text": "Evet, toplarız, onun parmak uçlarnı düzenlemeğe gücümüz yeter.",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5556,
        "text": "Fakat insan, devamlı suç işleyerek ilerisini berbadetmek ister.",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5557,
        "text": "Kıyamet günü nerede? diye sorup durur.",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5558,
        "text": "Ama göz (güneş gibi ortaya çıkan gerçeğin karşısında) kamaştığı,",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5559,
        "text": "Ay tutulduğu,",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5560,
        "text": "Güneş ve Ay bir araya toplandığı zaman!",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5561,
        "text": "(Evet) O gün insan: \"Kaçacak yer neresi?\" der.",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5562,
        "text": "Hayır, sığınacak yer yoktur.",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5563,
        "text": "O gün varıp durulacak yer, ancak Rabbinin huzurudur (ey insan).",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5564,
        "text": "(O zaman) İnsanın yapıp öne sürdüğü, (yapmayıp) geri bıraktığı herşey kendisine haber verilir.",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5565,
        "text": "Doğrusu insan kendi nefsini görür,",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5566,
        "text": "Birtakım özürler ortaya atsa da.",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5567,
        "text": "(Ey Muhammed,) Onu hemen okumak için diline depretme.",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5568,
        "text": "Onu (senin kalbinde) toplamak ve (sana) okumak bize düşer.",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5569,
        "text": "O halde sana Kur'an'ı okuduğumuz zaman onun okunuşunu izle.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5570,
        "text": "Sonra onu açıklamak da bize düşer.",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5571,
        "text": "Hayır, siz çabuk(geçen şu dünyay)ı seviyorsunuz da,",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5572,
        "text": "Ahireti bırakıyorsunuz.",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5573,
        "text": "Yüzler var ki o gün ışıl ışıl parlar,",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5574,
        "text": "Rabbine bakar.",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5575,
        "text": "Yüzler de var ki o gün asıktır.",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5576,
        "text": "Kendisine bel kemiklerini kıran(bela)nın yapılacağını anlar.",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5577,
        "text": "Hayır, ne zaman ki can, köprücük kemiklerine dayanır,",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5578,
        "text": "Ve (başında bulunanlar tarafından): \"Kim afsun yapar acaba? denir,",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5579,
        "text": "Ve kendisi artık bunun, ayrılık zamanı olduğunu anlar,",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5580,
        "text": "Ve bacak bacağa dolaşır.",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5581,
        "text": "İşte o gün, sevk Rabbinedir (can, Allah'ın huzuruna sevk edilir).",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5582,
        "text": "Ne sadaka verdi, ne de namaz kıldı.",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5583,
        "text": "Fakat yalanladı, döndü.",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5584,
        "text": "Sonra çalım satarak ailesine gitti.",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5585,
        "text": "Yazık sana yazık!",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5586,
        "text": "Yine yazık sana yazık!",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5587,
        "text": "İnsan, başı boş bırakılacağını mı sanır?",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5588,
        "text": "Kendisi dökülen meniden bir nutfe (sperm) değil miydi?",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5589,
        "text": "Sonra alaka (rahme asılan embriyo) oldu da (Rabbi onu) yarattı, düzenledi.",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5590,
        "text": "O(meni)den iki çifti: Erkeği ve dişiyi var etti.",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5591,
        "text": "Şimdi bun(ları yapan Allah)ın ölüleri diriltmeğe gücü yetmez mi?",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      }
    ],
