{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 53,
    "name": "سُورَةُ النَّجۡمِ",
    "englishName": "An-Najm",
    "englishNameTranslation": "The Star",
    "revelationType": "Meccan",
    "numberOfAyahs": 62,
    "ayahs": [
      {
        "number": 4785,
        "text": "Aşağı kayan yıldıza andolsun ki:",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4786,
        "text": "Arkadaşınız sapmadı, azmadı.",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4787,
        "text": "O heva'dan konuşmaz.",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4788,
        "text": "O(nun okuduğu Kur'an) kendisine vahyedilen vahiyden başka bir şey değildir.",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4789,
        "text": "Onu, mühtiş kuvvetleri olan biri öğretti;",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4790,
        "text": "Üstün akıl sahibi (melek). Doğruldu;",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4791,
        "text": "Kendisi yüksek ufukta iken.",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4792,
        "text": "Sonra yaklaştı, (yere doğru) sarktı.",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4793,
        "text": "(Muhammed ile arasındaki mesafe) İki yay uzunluğu kadar, yahut daha az kaldı.",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4794,
        "text": "Kuluna, vahyettiğini vahyetti.",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4795,
        "text": "Gönül gördüğünde yanılmadı (yalan söylemedi, gerçeği gördü).",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4796,
        "text": "Onun gördüğünden kuşku mu duyuyorsunuz?",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4797,
        "text": "Andolsun, onu bir inişinde daha görmüştü;",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4798,
        "text": "Sidretü'l-Münteha(uzak ağaç)ın yanında,",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4799,
        "text": "Ki onun yanında oturulacak bahçe vardır.",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4800,
        "text": "Sidre'yi kaplayan kaplıyordu.",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4801,
        "text": "(Muhammed'in) Göz(ü) şaşmadı ve azmadı.",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4802,
        "text": "Andolsun, Rabbinin büyük ayetlerinden bazılarını gördü.",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4803,
        "text": "Gördünüz mü o Lat ve 'Uzza'yı?",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4804,
        "text": "Ve üçüncü(leri olan) öteki (put) Menat'ı?",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4805,
        "text": "Demek erkek size, kadın Allah'a mı?",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4806,
        "text": "O halde bu insafsızca bir taksim!",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4807,
        "text": "Onlar, sizin ve babalarınızın, (tanrı) diye isimlendirdiğiniz (boş, kavramsız) isimlerden başka bir şey değildir. Allah, onlara hiçbir güç (tanrı oldukları hakkında hiçbir delil) indirmemiştir. O(putlara tapa)nlar zanna ve nefislerin hevesine uyuyorlar. Oysa kendilerine, Rableri tarafından yol gösterici gelmiştir.",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4808,
        "text": "Yoksa insan, her arzu ettiğine sahip mi olacaktır?",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4809,
        "text": "Son da ilk de (ahiret de, dünya da) Allah'ındır.",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 460,
        "hizbQuarter": 210,
        "sajda": false
      },
      {
        "number": 4810,
        "text": "Göklerde nice melek var ki onların şefa'ati hiçbir işe yaramaz. Meğer Allah'ın dilediği ve razı olduğu kimseye izin verdikten sonra olsun (ancak o zaman şefa'atin faydası olur).",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 526,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4811,
        "text": "Ahirete inanmayanlar, meleklere dişilerin adlarını takıyorlar.",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4812,
        "text": "Onların bu hususta bir bilgileri yoktur. Sadece zanna uyuyorlar. Zan ise haktan hiçbir gerçek kazandırmaz. (Zan ile gerçeğe ulaşılmaz.)",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4813,
        "text": "Bizi anmaktan yüz çeviren ve dünya hayatından başka bir şey istemeyen kimseden yüz çevir.",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4814,
        "text": "İşte onların erişebilecekleri bilgi (sınırı) budur. (Bundan ötesine akılları ermez). Şüphesiz Rabbin, yolundan sapanı da iyi bilir ve O, yola geleni de iyi bilir.",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4815,
        "text": "Göklerde ve yerde bulunan herşey Allah'ındır. (Bunları yaratmıştır) Ki kötülük edenleri, yaptıklarıyle cezalandırsın, güzel davrananları da güzellikle mükafatlandırsın.",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4816,
        "text": "Onlar, günahın büyüklerinden ve çirkin işlerden kaçınırlar, yalnız bazı küçük hatalar işleyebilirler. Şüphesiz Rabbinin affı geniştir (O kendisine yönelen kulunu affeder). O sizi daha iyi bilir: Gerek sizi topraktan inşa ettiği, gerek annelerinizin karınlarında bulunduğunuz zaman biçim verdiği sırada (sizin her halinizi bilmiştir), artık kendinizi övüp yüceltmeyin, çünkü O, korunanı daha iyi bilir.",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 461,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4817,
        "text": "Gördün mü şu adamı ki arkasını döndü?",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4818,
        "text": "Azıcık verdi, gerisini elinde sıkı sıkı tuttu?",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4819,
        "text": "Gayb'ın bilgisi kendi yanında da o mu (alemin esrarını) görüyor?",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4820,
        "text": "Yoksa kendisine haber mi verilmedi: Musa'nın sahifelerinde bulunan,",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4821,
        "text": "Ve çok vefalı İbrahim'in (sahifelerinde bulunan şu gerçekler):",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4822,
        "text": "Ki hiçbir günahkar, başkasının günah yükünü yüklenmez.",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4823,
        "text": "İnsana çalışmasından başka bir şey yoktur.",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4824,
        "text": "Ve çalışması da yakında görülecektir.",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4825,
        "text": "Sonra ona tastamam karşılığı verilecektir.",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4826,
        "text": "Ve sonunda senin Rabbine varılacaktır.",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4827,
        "text": "Güldüren de O'dur, ağlatan da O'dur.",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4828,
        "text": "Öldüren de O'dur, yaşatan da O'dur.",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 527,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4829,
        "text": "O yarattı iki çifti: erkeği ve dişiyi,",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4830,
        "text": "Atıldığı zaman nutfe(sperm)den.",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4831,
        "text": "Şüphesiz tekrar yaratmak da O'nun işidir.",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4832,
        "text": "Zengin eden O'dur, bol verip memnun eden O.",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4833,
        "text": "(Taptıkları) Şi'ra(yıldızı)nın Rabbi O'dur.",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4834,
        "text": "O helak etti, önce gelen 'Ad'ı,",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4835,
        "text": "Semud'u, komadı (onları).",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4836,
        "text": "Önceden de Nuh kavmini (helak etmişti). Çünkü onlar daha zalim ve azgın idiler.",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4837,
        "text": "Altı üstüne getirilen kentleri (Lut kavminin oturduğu bölgeleri) devirip yıktı.",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4838,
        "text": "Onların üstüne neler çöktü, neler!",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4839,
        "text": "O halde Rabbinin hangi ni'metinden kuşku duyuyorsun?",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4840,
        "text": "Bu (Kur'an veya peygamber) de ilk uyarıcılar gibi bir uyarıcıdır.",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4841,
        "text": "O yaklaşıcı, yaklaştı.",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4842,
        "text": "Onu Allah'tan başka açacak (geldiği zaman kaldıracak, vaktini erteleyecek veya onun ne zaman geleceğini belirleyecek) kimse yoktur.",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4843,
        "text": "Şimdi siz bu söze mi hayret ediyorsunuz?",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4844,
        "text": "Ve gülüyorsunuz da ağlamıyorsunuz?",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4845,
        "text": "Ve siz baş kaldırıyorsunuz?",
        "numberInSurah": 61,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4846,
        "text": "Haydi Allah'a secde edin ve kulluk edin!",
        "numberInSurah": 62,
        "juz": 27,
        "manzil": 7,
        "page": 528,
        "ruku": 462,
        "hizbQuarter": 211,
        "sajda": {
          "id": 13,
          "recommended": false,
          "obligatory": true
        }
      }
    ],
