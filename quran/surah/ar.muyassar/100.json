    "revelationType": "Meccan",
    "numberOfAyahs": 11,
    "ayahs": [
      {
        "number": 6147,
        "text": "أقسم الله تعالى بالخيل الجاريات في سبيله نحو العدوِّ، حين يظهر صوتها من سرعة عَدْوِها. ولا يجوز للمخلوق أن يقسم إلا بالله، فإن القسم بغير الله شرك.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6148,
        "text": "فالخيل اللاتي تنقدح النار من صلابة حوافرها؛ من شدَّة عَدْوها.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6149,
        "text": "فالمغيرات على الأعداء عند الصبح.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6150,
        "text": "فهيَّجْنَ بهذا العَدْو غبارًا.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6151,
        "text": "فتوسَّطن بركبانهن جموع الأعداء.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6152,
        "text": "إن الإنسان لِنعم ربه لَجحود، وإنه بجحوده ذلك لمقر. وإنه لحب المال لشديد.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 599,
        "ruku": 542,
        "hizbQuarter": 239,
        "sajda": false
