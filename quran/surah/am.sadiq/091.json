        "page": 595,
        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6054,
        "text": "ሠሙድ ወሰን በማለፍዋ አስተባበለች፡፡",
        "numberInSurah": 11,
