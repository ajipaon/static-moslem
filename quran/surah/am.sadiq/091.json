        "ruku": 533,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6053,
        "text": "(በኀጢኣት) የሸፈናትም ሰው በእውነት አፈረ፡፡",
        "numberInSurah": 10,
        "juz": 30,
