    "ayahs": [
      {
        "number": 5932,
        "text": "በሰማዩ በሌሊት መጪውም እምላለሁ፡፡",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5933,
        "text": "የሌሊት መጪውም ምን እንደ ኾነ ምን አሳወቀህ?",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5934,
        "text": "ጨለማን ቀዳጁ ኮከብ ነው፡፡",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5935,
        "text": "ነፍስ ሁሉ በእርሷ ላይ ጠባቂ ያለባት እንጅ ሌላ አይደለችም፡፡",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5936,
        "text": "ሰውም ከምን እንደተፈጠረ ይመልከት፡፡",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5937,
        "text": "ከተስፈንጣሪ ውሃ ተፈጠረ፡፡",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5938,
        "text": "ከጀርባና ከእርግብግብቶች መካከል የሚወጣ ከኾነ (ውሃ)፡፡",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5939,
        "text": "እርሱ (አላህ) በመመለሱ ላይ በእርግጥ ቻይ ነው፡፡",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5940,
        "text": "ምስጢሮች በሚገለጡበት ቀን፡፡",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5941,
        "text": "ለእርሱም (ለሰው) ኀይልና ረዳት ምንም የለውም፡፡",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5942,
        "text": "የመመለስ ባለቤት በኾነችው ሰማይም እምላለሁ፡፡",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5943,
        "text": "(በበቃይ) የመሰንጠቅ ባለቤት በኾነችው ምድርም፤",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5944,
        "text": "እርሱ (እውነትን ከውሸት) የሚለይ ቃል ነው፡፡",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5945,
        "text": "እርሱም ቀልድ አይደለም፡፡",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5946,
        "text": "እነርሱ በእርግጥ ተንኮልን ይሠራሉ፡፡",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 528,
        "hizbQuarter": 236,
        "sajda": false
      },
      {
        "number": 5947,
        "text": "(እኔ) ተንኮልንም እመልሳለሁ፡፡",
        "numberInSurah": 16,
        "juz": 30,
