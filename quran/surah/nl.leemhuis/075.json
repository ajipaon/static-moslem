{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 75,
    "name": "سُورَةُ القِيَامَةِ",
    "englishName": "Al-Qiyaama",
    "englishNameTranslation": "The Resurrection",
    "revelationType": "Meccan",
    "numberOfAyahs": 40,
    "ayahs": [
      {
        "number": 5552,
        "text": "Niet dan? Ik zweer bij de opstandingsdag!",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5553,
        "text": "Niet dan? Ik zweer bij de ziel die zichzelf verwijten maakt!",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5554,
        "text": "Rekent de mens erop dat Wij zijn beenderen niet bijeen zullen brengen?",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5555,
        "text": "Zeker, Wij zijn in staat om zijn vingertoppen te vormen.",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5556,
        "text": "Maar de mens wenst er maar op los te leven.",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5557,
        "text": "Hij vraagt wanneer de opstandingsdag zal zijn.",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5558,
        "text": "Wanneer dan de ogen verblind worden",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5559,
        "text": "en de maan wordt verduisterd",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5560,
        "text": "en de zon en de maan worden samengevoegd,",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5561,
        "text": "op die dag zal de mens zeggen: \"Waar is een toevluchtsoord?\"",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5562,
        "text": "Welnee, er is geen schuilplaats.",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5563,
        "text": "Bij jouw Heer is op die dag de verblijfplaats.",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5564,
        "text": "Aan de mensen zal op die dag worden meegedeeld wat zij vroeger en later gedaan hebben.",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5565,
        "text": "Ja zeker, de mens zal een duidelijk bewijs tegen zichzelf zijn,",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5566,
        "text": "ook al draagt hij zijn verontschuldigingen aan.",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5567,
        "text": "Beweeg niet je tong ermee om je ermee te haasten.",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5568,
        "text": "Het is Onze taak hem te verzamelen en voor te lezen.",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5569,
        "text": "En als Wij hem voorlezen volg dan de voorlezing ervan.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5570,
        "text": "En dan is het Onze taak hem uiteen te zetten.",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 577,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5571,
        "text": "Maar nee, jullie beminnen het snel voorbijgaande",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5572,
        "text": "en veronachtzamen het hiernamaals.",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5573,
        "text": "Stralende gezichten zijn er op die dag",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5574,
        "text": "die naar hun Heer kijken.",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5575,
        "text": "En grijnzende gezichten zijn er op die dag",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5576,
        "text": "die wel vermoeden dat hun iets ontzettends wordt aangedaan.",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5577,
        "text": "Ja zeker, wanneer [door de levensadem] het sleutelbeen bereikt wordt",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5578,
        "text": "en er gezegd wordt: \"Wie kan bezweren?\"",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5579,
        "text": "en [de betrokkene] vermoedt dat het het afscheid is",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5580,
        "text": "en de benen over elkaar gelegd zijn,",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5581,
        "text": "op die dag wordt men naar jouw Heer gedreven.",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 512,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5582,
        "text": "Hij dacht immers dat het niet waar was en bad de salaat niet!",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5583,
        "text": "Maar hij loochende het en keerde zich af.",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5584,
        "text": "Toen ging hij snoevend naar zijn familie.",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5585,
        "text": "Wacht maar jij, wacht maar,",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5586,
        "text": "En nog eens: Wacht maar jij, wacht maar.",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5587,
        "text": "Rekent de mens erop dat hij ongemoeid gelaten wordt?",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5588,
        "text": "Was hij niet een druppel zaad dat werd uitgestort?",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5589,
        "text": "Daarna was hij een bloedklonter. Toen schiep Hij, vormde",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5590,
        "text": "en maakte er de twee geslachten uit: het mannelijke en het vrouwelijke.",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      },
      {
        "number": 5591,
        "text": "Is dat niet Hij die in staat is de doden levend te maken?",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 578,
        "ruku": 513,
        "hizbQuarter": 231,
        "sajda": false
      }
    ],
