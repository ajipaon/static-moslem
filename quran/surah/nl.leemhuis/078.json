{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 78,
    "name": "سُورَةُ النَّبَإِ",
    "englishName": "An-Naba",
    "englishNameTranslation": "The Announcement",
    "revelationType": "Meccan",
    "numberOfAyahs": 40,
    "ayahs": [
      {
        "number": 5673,
        "text": "Waarover ondervragen zij elkaar?",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5674,
        "text": "Over de geweldige mededeling",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5675,
        "text": "waarover zij het oneens zijn.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5676,
        "text": "Nee toch, zij zullen het weten!",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5677,
        "text": "Nog eens, nee toch, zij zullen het weten!",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5678,
        "text": "Hebben Wij niet de aarde tot een wiegebed gemaakt",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5679,
        "text": "en de bergen tot tentpinnen?",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5680,
        "text": "Wij hebben jullie in paren geschapen.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5681,
        "text": "Wij hebben jullie slaap gemaakt om uit te rusten.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5682,
        "text": "Wij hebben de nacht gemaakt als een omhulling.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5683,
        "text": "Wij hebben de dag gemaakt voor de levensbehoeften.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5684,
        "text": "Wij hebben boven jullie zeven hechte [hemelen] gebouwd.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5685,
        "text": "Wij hebben een schitterende lamp gemaakt.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5686,
        "text": "En Wij hebben uit de regenwolken water in stromen laten neerdalen",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5687,
        "text": "om daarmee graan en planten voort te brengen",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5688,
        "text": "en dichtbegroeide tuinen.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5689,
        "text": "De dag van de schifting is een afgesproken tijd,",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5690,
        "text": "de dag waarop op de bazuin geblazen wordt. Dan zullen jullie in groepen komen",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5691,
        "text": "en wordt de hemel geopend en hij is een en al poorten.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5692,
        "text": "En de bergen worden in beweging gezet en zijn als een luchtspiegeling.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5693,
        "text": "En de hel is een hinderlaag,",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5694,
        "text": "waar de onbeschaamden naar terugkeren,",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5695,
        "text": "waar zij voortdurend zullen blijven.",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5696,
        "text": "Zij zullen daarin noch koelte noch iets om te drinken proeven,",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5697,
        "text": "maar gloeiend water en etter,",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5698,
        "text": "als een passende vergelding.",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5699,
        "text": "Zij verwachtten geen afrekening",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5700,
        "text": "en zij loochenden Onze tekenen herhaaldelijk.",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5701,
        "text": "En alles hebben Wij opgesomd in een boek.",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5702,
        "text": "Proeft dan; jullie bestraffing wordt alleen maar erger.",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 582,
        "ruku": 518,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5703,
        "text": "Maar voor de godvrezenden is er een triomf,",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5704,
        "text": "boomgaarden en wijnstokken,",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5705,
        "text": "rondborstige gezellinnen die even oud zijn",
        "numberInSurah": 33,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5706,
        "text": "en een vol gevulde beker.",
        "numberInSurah": 34,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5707,
        "text": "Zij horen daar geen geklets en geen loochening.",
        "numberInSurah": 35,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5708,
        "text": "Als beloning van jouw Heer, als gift en als afrekening",
        "numberInSurah": 36,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5709,
        "text": "van de Heer van de hemelen en de aarde en wat er tussen beide is, de Erbarmer, die zij niet kunnen tegenspreken",
        "numberInSurah": 37,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5710,
        "text": "op de dag dat de geest en de engelen in een rij staan. Spreken zullen slechts zij aan wie de Erbarmer het toestaat en die zeggen wat juist is.",
        "numberInSurah": 38,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5711,
        "text": "Dat is de dag van de waarheid. Wie wil begeeft zich op de terugweg naar zijn Heer.",
        "numberInSurah": 39,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5712,
        "text": "Wij hebben jullie gewaarschuwd voor een nabije bestraffing op de dag dat de mens kijkt naar wat zijn handen vroeger gedaan hebben en waarop de ongelovige zegt: \"O wee, was ik maar stof!\"",
        "numberInSurah": 40,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 519,
        "hizbQuarter": 233,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "nl.leemhuis",
      "language": "nl",
      "name": "Leemhuis",
      "englishName": "Leemhuis",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}