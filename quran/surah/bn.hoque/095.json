    "ayahs": [
      {
        "number": 6099,
        "text": "ভাবো ডুমুরেব, আর জলপাইয়ের কথা;",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6100,
        "text": "আর সিনাই পর্বতের কথা,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6101,
        "text": "আর এই নিরাপদ নগরের কথা!",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6102,
        "text": "সুনিশ্চয় আমরা মানুষকে সৃষ্টি করেছি শ্রেষ্ঠ-সুন্দর আকৃতিতে।",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6103,
        "text": "তারপর আমরা তাকে পরিণত করি হীনদের মধ্যে হীনতমে, --",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6104,
        "text": "তারা ব্যতীত যারা ঈমান এনেছে ও সৎকাজ করছে, তাদের জন্য তবে রয়েছে বাধা-বিরতিবিহীন প্রতিদান।",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6105,
        "text": "তবে কী যা এরপরে তোমাকে বিচারসন্বন্ধে মিথ্যারোপ করতে দেয়?",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 597,
        "ruku": 537,
        "hizbQuarter": 239,
        "sajda": false
      },
      {
        "number": 6106,
        "text": "আল্লাহ্ কি বিচারকদের মধ্যে সর্বশ্রেষ্ঠ বিচারক নন?",
        "numberInSurah": 8,
        "juz": 30,
