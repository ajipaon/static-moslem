    "numberOfAyahs": 8,
    "ayahs": [
      {
        "number": 6169,
        "text": "প্রাচুর্যের প্রতিযোগিতা তোমাদের মতিভ্রম ঘটায়, --",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6170,
        "text": "যতক্ষণ না তোমরা কবরে আসো।",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6171,
        "text": "না! শীঘ্রই তোমরা জানতে পারবে!",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6172,
        "text": "আবার বলি, -- না, শীঘ্রই তোমরা জানতে পারবে!",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6173,
        "text": "না, যদি তোমরা জানতে নিশ্চিত জ্ঞানে!",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6174,
        "text": "তোমরা তো ভয়ংকর আগুন দেখবেই।",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6175,
        "text": "আবার বলি, তোমরা অবশ্যই এটি দেখবে নিশ্চিত দৃষ্টিতে।",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6176,
        "text": "এরপর সেইদিন তোমাদের অবশ্যই জিজ্ঞাসা করা হবে অবদান সম্পর্কে।",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 544,
        "hizbQuarter": 240,
        "sajda": false
      }
