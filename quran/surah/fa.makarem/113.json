        "sajda": false
      },
      {
        "number": 6227,
        "text": "از شرّ تمام آنچه آفریده است؛",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 555,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6228,
        "text": "و از شرّ هر موجود شرور هنگامی که شبانه وارد می‌شود؛",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 555,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6229,
        "text": "و از شرّ آنها که با افسون در گره‌ها می‌دمند (و هر تصمیمی را سست می‌کنند)؛",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 555,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6230,
        "text": "و از شرّ هر حسودی هنگامی که حسد می‌ورزد!",
        "numberInSurah": 5,
