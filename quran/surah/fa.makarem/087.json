  "code": 200,
  "status": "OK",
  "data": {
    "number": 87,
    "name": "سُورَةُ الأَعۡلَىٰ",
    "englishName": "Al-A'laa",
    "englishNameTranslation": "The Most High",
    "revelationType": "Meccan",
    "numberOfAyahs": 19,
    "ayahs": [
      {
        "number": 5949,
        "text": "منزّه شمار نام پروردگار بلندمرتبه‌ات را!",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5950,
        "text": "همان خداوندی که آفرید و منظم کرد،",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5951,
        "text": "و همان که اندازه‌گیری کرد و هدایت نمود،",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5952,
        "text": "و آن کس را که چراگاه را به وجود آورد،",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5953,
        "text": "سپس آن را خشک و تیره قرار داد!",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5954,
        "text": "ما بزودی (قرآن را) بر تو می‌خوانیم و هرگز فراموش نخواهی کرد،",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5955,
        "text": "مگر آنچه را خدا بخواهد، که او آشکار و نهان را می‌داند!",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5956,
        "text": "و ما تو را برای انجام هر کار خیر آماده می‌کنیم!",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5957,
        "text": "پس تذکّر ده اگر تذکّر مفید باشد!",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5958,
        "text": "و بزودی کسی که از خدا می‌ترسد متذکّر می‌شود.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5959,
        "text": "امّا بدبخت‌ترین افراد از آن دوری می‌گزیند،",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5960,
        "text": "همان کسی که در آتش بزرگ وارد می‌شود،",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5961,
        "text": "سپس در آن آتش نه می‌میرد و نه زنده می‌شود!",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5962,
        "text": "به یقین کسی که پاکی جست (و خود را تزکیه کرد)، رستگار شد.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5963,
        "text": "و (آن که) نام پروردگارش را یاد کرد سپس نماز خواند!",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 591,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5964,
        "text": "ولی شما زندگی دنیا را مقدم می‌دارید،",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5965,
        "text": "در حالی که آخرت بهتر و پایدارتر است!",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5966,
        "text": "این دستورها در کتب آسمانی پیشین (نیز) آمده است،",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      },
      {
        "number": 5967,
        "text": "در کتب ابراهیم و موسی.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 592,
        "ruku": 529,
        "hizbQuarter": 237,
        "sajda": false
      }
    ],
