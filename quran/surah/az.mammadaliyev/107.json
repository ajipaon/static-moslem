    "revelationType": "Meccan",
    "numberOfAyahs": 7,
    "ayahs": [
      {
        "number": 6198,
        "text": "(Ya Peyğəmbər!) Dini (haqq-hesab gününü) yalan hesab edəni (Əbu Cəhli) gördünmü?",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6199,
        "text": "O elə adamdır ki, yetimi itələyib qovar (haqqını verməz);",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6200,
        "text": "Və (xalqı) yoxsulu yedirtməyə rəğbətləndirməz (nə özü fəqiri yedirdər, nə də özgəsini qoyar).",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6201,
        "text": "Vay halına o namaz qılanların ki,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6202,
        "text": "Onlar öz namazlarından qafildirlər (səhlənkarlıqları üzündən namazlarını vaxtlı-vaxtında qılmazlar);",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6203,
        "text": "Onlar (namazlarında) riyakarlıq edər,",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6204,
        "text": "Və (xalqa) zəkat verməyi qadağan edərlər (yaxud xəsislik göstərib bir iş üçün qonum-qonşuya lazım olan qab-qacağı verməkdən imtina edərlər).",
        "numberInSurah": 7,
        "juz": 30,
