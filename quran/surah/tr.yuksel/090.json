  "data": {
    "number": 90,
    "name": "سُورَةُ البَلَدِ",
    "englishName": "Al-Balad",
    "englishNameTranslation": "The City",
    "revelationType": "Meccan",
    "numberOfAyahs": 20,
    "ayahs": [
      {
        "number": 6024,
        "text": "And içerim bu kente,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6025,
        "text": "Ki sen bu kentte oturmaktasın.",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6026,
        "text": "Doğurana ve doğurduğuna da andolsun.",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6027,
        "text": "İnsanı zorluklar arasında (direnmesi için) yarattık.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6028,
        "text": "Kendisine güç yetiremiyeceğimizi mi sanıyor?",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6029,
        "text": "(Övünerek) \"Çok para harcadım,\" diyor.",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6030,
        "text": "Kimsenin kendisini görmediğini mi sanıyor?",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6031,
        "text": "Ona vermedik mi: İki göz,",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6032,
        "text": "Bir dil ve iki dudak?",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6033,
        "text": "Ona iki yolu göstermedik mi?",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6034,
        "text": "Ne var ki zor yola katlanamadı.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6035,
        "text": "Zor yolun ne olduğunu bilir misin?",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6036,
        "text": "Köleleri özgürlüklerine kavuşturmaktır;",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6037,
        "text": "Kıtlık anında doyurmaktır:",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6038,
        "text": "Akraba bir öksüzü,",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6039,
        "text": "Yahut düşkün bir yoksulu...",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6040,
        "text": "Dahası, birbirlerine sabır ve sevgiyi öğütleyen inananlardan olmaktır.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6041,
        "text": "Nitekim mutlular onlardır.",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6042,
        "text": "Ayet ve mucizelerimizi inkar edenlerse talihsizlerdir.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6043,
        "text": "Onlar ateşe kapatılacaklardır.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 594,
        "ruku": 532,
        "hizbQuarter": 238,
        "sajda": false
      }
    ],
