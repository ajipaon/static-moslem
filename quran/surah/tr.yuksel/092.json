{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 92,
    "name": "سُورَةُ اللَّيۡلِ",
    "englishName": "Al-Lail",
    "englishNameTranslation": "The Night",
    "revelationType": "Meccan",
    "numberOfAyahs": 21,
    "ayahs": [
      {
        "number": 6059,
        "text": "Andolsun bürüdüğü zaman geceye,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6060,
        "text": "Ortaya çıktığı zaman gündüze,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6061,
        "text": "Erkeği ve dişiyi yaratana andolsun ki,",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6062,
        "text": "İşleriniz çeşit çeşittir.",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6063,
        "text": "Kim verir ve erdemli davranır,",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6064,
        "text": "Ve iyiyi, güzeli doğrularsa,",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6065,
        "text": "Ona iyice kolaylaştırırız.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6066,
        "text": "Fakat, kim cimrilik edip zenginlik taslar,",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6067,
        "text": "Ve iyiyi, güzeli yalanlarsa,",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6068,
        "text": "Onu da zora yöneltiriz.",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6069,
        "text": "Düştüğü vakit kurtaramaz parası/malı onu.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6070,
        "text": "Doğruya biz iletiriz;",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6071,
        "text": "Sonu da ilki de biz kontrol ederiz.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6072,
        "text": "Ben sizi alevli bir ateşe karşı uyardım.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 595,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6073,
        "text": "Oraya talihsiz olandan başkası girmez.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6074,
        "text": "O ki yalanladı ve sırtını döndü.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6075,
        "text": "Erdemli ise ondan uzak tutulacaktır.",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6076,
        "text": "O ki malını vererek temizlenir;",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6077,
        "text": "Hiç kimseden de buna karşılık bir iyilik beklemez.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6078,
        "text": "Sadece En Yüce olan Rabbinin rızasını gözetir.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      },
      {
        "number": 6079,
        "text": "Kendisi de yakında mutlu olacaktır.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 596,
        "ruku": 534,
        "hizbQuarter": 238,
        "sajda": false
      }
    ],
