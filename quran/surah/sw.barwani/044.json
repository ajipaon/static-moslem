{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 44,
    "name": "سُورَةُ الدُّخَانِ",
    "englishName": "Ad-Dukhaan",
    "englishNameTranslation": "The Smoke",
    "revelationType": "Meccan",
    "numberOfAyahs": 59,
    "ayahs": [
      {
        "number": 4415,
        "text": "H'a Mim",
        "numberInSurah": 1,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4416,
        "text": "Naapa kwa Kitabu kinacho bainisha,",
        "numberInSurah": 2,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4417,
        "text": "Hakika tumekiteremsha katika usiku ulio barikiwa. Hakika Sisi ni Waonyaji.",
        "numberInSurah": 3,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4418,
        "text": "Katika usiku huu hubainishwa kila jambo la hikima,",
        "numberInSurah": 4,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4419,
        "text": "Jambo litokalo kwetu. Hakika Sisi ndio wenye kutuma.",
        "numberInSurah": 5,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4420,
        "text": "Ni rehema itokayo kwa Mola wako Mlezi. Hakika Yeye ni Mwenye kusikia Mwenye kujua.",
        "numberInSurah": 6,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4421,
        "text": "Mola Mlezi wa mbingu na ardhi na vilio baina yao, ikiwa nyinyi mna yakini.",
        "numberInSurah": 7,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4422,
        "text": "Hakuna mungu ila Yeye; anahuisha na anafisha- Mola Mlezi wenu na Mola Mlezi wa baba zenu wa mwanzo.",
        "numberInSurah": 8,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4423,
        "text": "Lakini wao wanacheza katika shaka.",
        "numberInSurah": 9,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4424,
        "text": "Basi ingoje siku ambayo mbingu zitakapo leta moshi ulio dhaahiri,",
        "numberInSurah": 10,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4425,
        "text": "Utakao wafunika watu: Hii ni adhabu chungu!",
        "numberInSurah": 11,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4426,
        "text": "Mola wetu Mlezi! Tuondolee adhabu hii. Hakika tutaamini.",
        "numberInSurah": 12,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4427,
        "text": "Nini faida ya kukumbuka kwao? Na alikwisha wafikia Mtume mwenye kubainisha.",
        "numberInSurah": 13,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4428,
        "text": "Na wao wakamgeuzia uso, na wakasema: Huyu amefunzwa, naye ni mwendawazimu.",
        "numberInSurah": 14,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4429,
        "text": "Hakika sisi tutaiondoa adhabu kidogo, lakini nyinyi kwa yakini mtarejea vile vile!",
        "numberInSurah": 15,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4430,
        "text": "Siku tutayo yashambulia mashambulio makubwa, bila ya shaka Sisi ni wenye kutesa.",
        "numberInSurah": 16,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 198,
        "sajda": false
      },
      {
        "number": 4431,
        "text": "Na hakika kabla ya hawa tuliwafanyia mtihani watu wa Firauni, na aliwafikia Mtume Mtukufu.",
        "numberInSurah": 17,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4432,
        "text": "Akasema: Nipeni waja wa Mwenyezi Mungu; kwa hakika mimi kwenu ni Mtume Muaminifu.",
        "numberInSurah": 18,
        "juz": 25,
        "manzil": 6,
        "page": 496,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4433,
        "text": "Na msimfanyie kiburi Mwenyezi Mungu; hakika mimi nitakuleteeni uthibitisho ulio wazi.",
        "numberInSurah": 19,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4434,
        "text": "Nami najikinga kwa Mola wangu Mlezi na ndiye Mola wenu Mlezi pia, ili msinipige mawe.",
        "numberInSurah": 20,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4435,
        "text": "Na ikiwa hamniamini, basi jitengeni nami.",
        "numberInSurah": 21,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4436,
        "text": "Ndipo akamwomba Mola wake Mlezi: Hakika watu hawa ni wakosefu.",
        "numberInSurah": 22,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4437,
        "text": "Mwenyezi Mungu akasema: Basi nenda na waja wangu usiku. Kwa yakini mtafuatwa.",
        "numberInSurah": 23,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4438,
        "text": "Na iache bahari vivyo hivyo imeachana, hakika wao hao ni jeshi litakalo zamishwa.",
        "numberInSurah": 24,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4439,
        "text": "Mabustani mangapi, na chemchem ngapi waliziacha!",
        "numberInSurah": 25,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4440,
        "text": "Na mimea na vyeo vitukufu!",
        "numberInSurah": 26,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4441,
        "text": "Na neema walizo kuwa wakijistareheshea!",
        "numberInSurah": 27,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4442,
        "text": "Ndio hivyo! Na tukawarithisha haya watu wenginewe.",
        "numberInSurah": 28,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4443,
        "text": "La mbingu wala ardhi hazikuwalilia, wala hawakupewa muhula.",
        "numberInSurah": 29,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 431,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4444,
        "text": "Na bila ya shaka tuliwaokoa Wana wa Israili katika adhabu ya kuwadhalilisha,",
        "numberInSurah": 30,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4445,
        "text": "Ya Firauni. Hakika yeye alikuwa jeuri katika wenye kupindukia mipaka.",
        "numberInSurah": 31,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4446,
        "text": "Na tuliwakhiari kwa ujuzi wetu kuliko walimwengu wenginewe.",
        "numberInSurah": 32,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4447,
        "text": "Na tukawapa katika ishara zenye majaribio yaliyo wazi.",
        "numberInSurah": 33,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4448,
        "text": "Hakika hawa wanasema:",
        "numberInSurah": 34,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4449,
        "text": "Hapana ila kufa kwetu mara ya kwanza tu, wala sisi hatufufuliwi.",
        "numberInSurah": 35,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4450,
        "text": "Basi warudisheni baba zetu, ikiwa nyinyi mnasema kweli.",
        "numberInSurah": 36,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4451,
        "text": "Je! Ni wao bora au watu wa Tubbaa' na walio kuwa kabla yao? Tuliwaangamiza. Hakika hao walikuwa wakosefu.",
        "numberInSurah": 37,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4452,
        "text": "Na hatukuziumba mbingu na ardhi na vilivyomo baina yake kwa mchezo.",
        "numberInSurah": 38,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4453,
        "text": "Hatukuviumba hivyo ila kwa Haki, lakini wengi wao hawajui.",
        "numberInSurah": 39,
        "juz": 25,
        "manzil": 6,
        "page": 497,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4454,
        "text": "Hakika siku ya Uamuzi ni wakati ulio wekwa kwa wao wote.",
        "numberInSurah": 40,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4455,
        "text": "Siku ambayo rafiki hatamfaa rafiki yake kwa chochote, wala hawatanusuriwa.",
        "numberInSurah": 41,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4456,
        "text": "Isipo kuwa atakaye mrehemu Mwenyezi Mungu. Hakika Yeye ndiye Mwenye nguvu, Mwenye kurehemu.",
        "numberInSurah": 42,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 432,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4457,
        "text": "Hakika Mti wa Zaqqum,",
        "numberInSurah": 43,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4458,
        "text": "Ni chakula cha mwenye dhambi.",
        "numberInSurah": 44,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4459,
        "text": "Kama shaba iliyo yayushwa, hutokota matumboni",
        "numberInSurah": 45,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4460,
        "text": "Kama kutokota kwa maji ya moto.",
        "numberInSurah": 46,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4461,
        "text": "(Itasemwa:) Mkamateni na mtupeni katikati ya Jahannamu!",
        "numberInSurah": 47,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4462,
        "text": "Kisha mmwagieni juu ya kichwa chake adhabu ya maji yanayo chemka.",
        "numberInSurah": 48,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4463,
        "text": "Onja! Ati wewe ndiye mwenye nguvu, mtukufu!",
        "numberInSurah": 49,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4464,
        "text": "Hakika haya ndiyo mliyo kuwa mkiyatilia shaka.",
        "numberInSurah": 50,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4465,
        "text": "Hakika wachamngu watakuwa katika mahali pa amani,",
        "numberInSurah": 51,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4466,
        "text": "Katika mabustani na chemchem,",
        "numberInSurah": 52,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4467,
        "text": "Watavaa hariri nyepesi na hariri nzito wakikabiliana,",
        "numberInSurah": 53,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4468,
        "text": "Hivi ndivyo itakavyo kuwa, na tutawaoza mahurilaini.",
        "numberInSurah": 54,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4469,
        "text": "Humo watataka kila aina ya matunda, na wakae kwa amani.",
        "numberInSurah": 55,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4470,
        "text": "Humo hawataonja mauti ila mauti yale ya kwanza, na (Mwenyezi Mungu) atawalinda na adhabu ya Jahannamu,",
        "numberInSurah": 56,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4471,
        "text": "Kuwa ni fadhila zitokazo kwa Mola wako Mlezi. Huko ndiko kufuzu kukubwa.",
        "numberInSurah": 57,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4472,
        "text": "Basi tumeifanya nyepesi hii Qur'ani kwa ulimi wako, ili wapate kukumbuka.",
        "numberInSurah": 58,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4473,
        "text": "Ngoja tu, na wao wangoje pia.",
        "numberInSurah": 59,
        "juz": 25,
        "manzil": 6,
        "page": 498,
        "ruku": 433,
        "hizbQuarter": 199,
        "sajda": false
      }
    ],
