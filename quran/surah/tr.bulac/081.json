{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 81,
    "name": "سُورَةُ التَّكۡوِيرِ",
    "englishName": "At-Takwir",
    "englishNameTranslation": "The Overthrowing",
    "revelationType": "Meccan",
    "numberOfAyahs": 29,
    "ayahs": [
      {
        "number": 5801,
        "text": "Güneş, köreltildiği zaman,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5802,
        "text": "Yıldızlar, bulanıklaşıp-döküldüğü zaman,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5803,
        "text": "Dağlar, yürütüldüğü zaman,",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5804,
        "text": "Gebe develer, kendi başına terk edildiği zaman,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5805,
        "text": "Vahşi-hayvanlar, toplandığı zaman,",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5806,
        "text": "Denizler, tutuşturulduğu zaman,",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5807,
        "text": "Nefisler, birleştiği zaman,",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5808,
        "text": "Ve 'diri diri toprağa gömülen kızcağıza' sorulduğu zaman:",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5809,
        "text": "\"Hangi suçtan dolayı öldürüldü?\"",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5810,
        "text": "Sahifeler (amel defterleri) açıldığı zaman,",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5811,
        "text": "Gök, sıyrılıp-yüzüldüğü zaman",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5812,
        "text": "Cehennem ateşi çılgınca kızıştırıldığı zaman,",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5813,
        "text": "Cennet de yakınlaştırıldığı zaman,",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5814,
        "text": "(Artık her) Nefis, neyi hazırladığını bilip-öğrenmiştir.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5815,
        "text": "Artık hayır; yemin ederim (gündüz) sinip (gece) dönen (gezegen)lere,",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5816,
        "text": "Bir akış içinde yerini alanlara;",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5817,
        "text": "Kararmaya ilk başladığı zaman, geceye andolsun,",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5818,
        "text": "Ve nefes almaya başladığı zaman, sabaha;",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5819,
        "text": "Şüphesiz o (Kur'an), üstün onur sahibi bir elçinin gerçekten (Allah'tan getirdiği) sözüdür;",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5820,
        "text": "(Bu elçi,) Bir güç sahibidir, arşın sahibi Katında şereflidir.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5821,
        "text": "Ona itaat edilir, sonra güvenilirdir.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5822,
        "text": "Sizin sahibiniz bir deli değildir.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5823,
        "text": "Andolsun o (peygamber), onu apaçık bir ufukta görmüştür.",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5824,
        "text": "O, gayb (haberlerin)e karşı (söylediklerinden dolayı) suçlanamaz (ya da cimrilikte bulunup kıskançlık yapmaz.)",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5825,
        "text": "O (Kur'an) da kovulmuş şeytanın sözü değildir.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5826,
        "text": "Şu halde, siz nereye kaçıp-gidiyorsunuz?",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5827,
        "text": "O (Kur'an), alemler için yalnızca bir zikirdir;",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5828,
        "text": "Sizden dosdoğru bir yön (istikamet) tutturmak dileyenler için.",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      },
      {
        "number": 5829,
        "text": "Alemlerin Rabbi olan Allah dilemedikçe siz dileyemezsiniz.",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 586,
        "ruku": 523,
        "hizbQuarter": 234,
        "sajda": false
      }
    ],
    "edition": {
      "identifier": "tr.bulac",
      "language": "tr",
      "name": "Alİ Bulaç",
      "englishName": "Alİ Bulaç",
      "format": "text",
      "type": "translation",
      "direction": "ltr"
    }
  }
}