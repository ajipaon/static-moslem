{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 82,
    "name": "سُورَةُ الانفِطَارِ",
    "englishName": "Al-Infitaar",
    "englishNameTranslation": "The Cleaving",
    "revelationType": "Meccan",
    "numberOfAyahs": 19,
    "ayahs": [
      {
        "number": 5830,
        "text": "Gök, çatlayıp-yarıldığı zaman,",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5831,
        "text": "Yıldızlar, dağılıp-yayıldığı zaman,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5832,
        "text": "Denizler, fışkırtılıp-taşırıldığı zaman,",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5833,
        "text": "Ve kabirlerin içi 'deşilip dışa atıldığı' zaman;",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5834,
        "text": "(Artık her) Nefis önceden takdim ettiklerini ve ertelediklerini bilip-öğrenmiştir.",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5835,
        "text": "Ey insan, 'üstün kerem sahibi' olan Rabbine karşı seni aldatıp-yanıltan nedir?",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5836,
        "text": "Ki O, seni yarattı, 'sana bir düzen içinde biçim verdi' ve seni bir itidal üzere kıldı.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5837,
        "text": "Dilediği bir surette seni tertib etti.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5838,
        "text": "Asla, hayır; siz dini yalanlıyorsunuz;",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5839,
        "text": "Oysa gerçekten sizin üzerinizde koruyucular var,",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5840,
        "text": "'Şerefli-üstün' yazıcılar.",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5841,
        "text": "Her yapmakta olduğunuzu bilirler.",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5842,
        "text": "Şüphesiz ebrar olanlar, elbette nimetler(le donatılmış cennetler) içindedirler.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5843,
        "text": "Ve şüphesiz facir (kötü) olanlar da, elbette çılgınca yanan ateşin içindedirler.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5844,
        "text": "Onlar, din günü oraya yollanırlar.",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5845,
        "text": "Ve ondan ayrılıp-kaybolacak değildirler.",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5846,
        "text": "Din gününü sana bildiren şey nedir?",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5847,
        "text": "Ve yine din gününü sana bildiren şey nedir?",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      },
      {
        "number": 5848,
        "text": "Hiçbir nefsin bir başka nefse herhangi bir şeye güç yetiremeyeceği gündür; o gün emir yalnızca Allah'ındır.",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 587,
        "ruku": 524,
        "hizbQuarter": 235,
        "sajda": false
      }
    ],
