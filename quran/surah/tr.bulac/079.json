{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 79,
    "name": "سُورَةُ النَّازِعَاتِ",
    "englishName": "An-Naazi'aat",
    "englishNameTranslation": "Those who drag forth",
    "revelationType": "Meccan",
    "numberOfAyahs": 46,
    "ayahs": [
      {
        "number": 5713,
        "text": "Ta en derinden acıyla sökerek çıkaranlara andolsun.",
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5714,
        "text": "Yumuşacık çekip alanlara,",
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5715,
        "text": "Yüzdükçe yüzerek gidenlere,",
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5716,
        "text": "Öncü olarak yarışıp geçenlere,",
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5717,
        "text": "Derken işi bir düzen içinde evirip çevirenlere,",
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5718,
        "text": "O sarsıntının sarsacağı gün,",
        "numberInSurah": 6,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5719,
        "text": "Arkasından onu diğer bir sarsıntı izleyecek.",
        "numberInSurah": 7,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5720,
        "text": "O gün yürekler (dehşet içinde) hoplayacak.",
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5721,
        "text": "Gözler zillet içinde düşecek.",
        "numberInSurah": 9,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5722,
        "text": "Derler ki: \"Biz çukurda iken, gerçekten biz mi yeniden (diriltilip) döndürüleceğiz?\"",
        "numberInSurah": 10,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5723,
        "text": "\"Biz çürüyüp dağılmış kemikler olduğumuz zaman mı?\"",
        "numberInSurah": 11,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5724,
        "text": "Derler ki: \"Şu durumda, zararına bir dönüştür bu.\"",
        "numberInSurah": 12,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5725,
        "text": "Oysa bu, yalnızca tek bir haykırıştır.",
        "numberInSurah": 13,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5726,
        "text": "Bir de bakarsın ki, onlar, yerin üstündedirler.",
        "numberInSurah": 14,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5727,
        "text": "Musa'nın haberi sana geldi mi?",
        "numberInSurah": 15,
        "juz": 30,
        "manzil": 7,
        "page": 583,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5728,
        "text": "Hani Rabbi ona, kutsal vadi Tuva'da seslenmişti:",
        "numberInSurah": 16,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5729,
        "text": "\"Firavun'a git; çünkü o, azdı.\"",
        "numberInSurah": 17,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5730,
        "text": "Ona de ki: “Temizlenmek ister misin?\"",
        "numberInSurah": 18,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5731,
        "text": "\"Seni Rabbine yönelteyim, böylece (O'ndan) korkmuş olursun.\"",
        "numberInSurah": 19,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5732,
        "text": "(Musa) Ona büyük mucizeyi gösterdi.",
        "numberInSurah": 20,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5733,
        "text": "Fakat o, yalanladı ve isyan etti.",
        "numberInSurah": 21,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5734,
        "text": "Sonra (karşı yönde) çaba harcayıp sırtını döndü.",
        "numberInSurah": 22,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5735,
        "text": "Sonunda (yardımcı güçlerini) topladı, seslendi;",
        "numberInSurah": 23,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5736,
        "text": "Dedi ki: \"Sizin en yüce Rabbiniz benim.\"",
        "numberInSurah": 24,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5737,
        "text": "Böylelikle Allah onu, ahiret ve dünya azabıyla yakaladı.",
        "numberInSurah": 25,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5738,
        "text": "Gerçekten bunda 'içi titreyerek korkacak' kimse için elbette bir ibret (ders) vardır.",
        "numberInSurah": 26,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 520,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5739,
        "text": "Yaratmak bakımından siz mi daha güçsünüz yoksa gök mü? (Allah) Onu bina etti.",
        "numberInSurah": 27,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5740,
        "text": "Boyunu yükseltti, ona belli bir düzen verdi.",
        "numberInSurah": 28,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5741,
        "text": "Gecesini kararttı, kuşluğunu açığa-çıkardı.",
        "numberInSurah": 29,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5742,
        "text": "Bundan sonra yeryüzünü serip döşedi.",
        "numberInSurah": 30,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5743,
        "text": "Ondan da suyunu ve otlağını çıkardı.",
        "numberInSurah": 31,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5744,
        "text": "Dağlarını dikip-oturttu;",
        "numberInSurah": 32,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5745,
        "text": "Size ve hayvanlarınıza bir yarar (meta) olmak üzere.",
        "numberInSurah": 33,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5746,
        "text": "Ancak o, 'herşeyi batırıp gömen büyük-felaket’ (kıyamet) geldiği zaman.",
        "numberInSurah": 34,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5747,
        "text": "O gün, insan, neye çaba harcadığını düşünüp-anlar.",
        "numberInSurah": 35,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5748,
        "text": "Görebilenler için cehennem de sergilenmiştir.",
        "numberInSurah": 36,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5749,
        "text": "Artık kim taşkınlık edip-azarsa,",
        "numberInSurah": 37,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5750,
        "text": "Ve dünya hayatını seçerse,",
        "numberInSurah": 38,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5751,
        "text": "Şüphesiz cehennem, (onun için) bir barınma yeridir.",
        "numberInSurah": 39,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5752,
        "text": "Kim Rabbinin makamından korkar ve nefsi heva (istek ve tutkular) dan sakındırırsa,",
        "numberInSurah": 40,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5753,
        "text": "Artık şüphesiz cennet, (onun için) bir barınma yeridir.",
        "numberInSurah": 41,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5754,
        "text": "\"O ne zaman demir atacak?\" diye, sana kıyamet-saatini soruyorlar.",
        "numberInSurah": 42,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5755,
        "text": "Onunla ilgili bilgi vermekten yana, sende ne var ki…",
        "numberInSurah": 43,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5756,
        "text": "En sonunda o (ve onunla ilgili bilgi), Rabbine aittir.",
        "numberInSurah": 44,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5757,
        "text": "Sen, yalnızca ondan 'içi titreyerek korkanlar' için bir uyarıcısın.",
        "numberInSurah": 45,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      },
      {
        "number": 5758,
        "text": "Onu gördükleri gün, sanki, bir akşam veya bir kuşluk-vaktinden başkasını yaşamamış gibidirler.",
        "numberInSurah": 46,
        "juz": 30,
        "manzil": 7,
        "page": 584,
        "ruku": 521,
        "hizbQuarter": 233,
        "sajda": false
      }
    ],
