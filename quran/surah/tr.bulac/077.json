{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 77,
    "name": "سُورَةُ المُرۡسَلَاتِ",
    "englishName": "Al-Mursalaat",
    "englishNameTranslation": "The Emissaries",
    "revelationType": "Meccan",
    "numberOfAyahs": 50,
    "ayahs": [
      {
        "number": 5623,
        "text": "Birbiri ardınca gönderilenlere andolsun;",
        "numberInSurah": 1,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5624,
        "text": "Derken kökünden koparıp savuranlara.",
        "numberInSurah": 2,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5625,
        "text": "Yaydıkça yayanlara.",
        "numberInSurah": 3,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5626,
        "text": "Böylece ayırdıkça ayıranlara,",
        "numberInSurah": 4,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5627,
        "text": "Zikr (vahy, öğüt) bırakanlara;",
        "numberInSurah": 5,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5628,
        "text": "Özür (suçu, eksikliği ortadan kaldırmak) veya uyarmak için.",
        "numberInSurah": 6,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5629,
        "text": "Şüphesiz, size vaadedilen gerçekleşecektir.",
        "numberInSurah": 7,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5630,
        "text": "Yıldızlar 'örtülüp (ışıkları) silindiği' zaman,",
        "numberInSurah": 8,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5631,
        "text": "Gök yarıldığı zaman",
        "numberInSurah": 9,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5632,
        "text": "Dağlar, kökünden sökülüp savurulduğu zaman,",
        "numberInSurah": 10,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5633,
        "text": "Ve resuller de (şahitlik için) belli bir vakitte getirildiği zaman",
        "numberInSurah": 11,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5634,
        "text": "(Bu,) Hangi gün için ertelenmişti?",
        "numberInSurah": 12,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5635,
        "text": "(Mü'mini müşrikten, haklıyı haksızdan) Ayırma günü için.",
        "numberInSurah": 13,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5636,
        "text": "Bu ayırma gününü sana ne bildirdi?",
        "numberInSurah": 14,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5637,
        "text": "O gün, yalanlayanların vay haline.",
        "numberInSurah": 15,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5638,
        "text": "Biz, öncekileri helak etmedik mi?",
        "numberInSurah": 16,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5639,
        "text": "Sonra arkadan gelenleri onların izinde yürüteceğiz.",
        "numberInSurah": 17,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5640,
        "text": "İşte Biz, suçlu-günahkarlara böyle yapıyoruz.",
        "numberInSurah": 18,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5641,
        "text": "O gün, yalanlayanların vay haline.",
        "numberInSurah": 19,
        "juz": 29,
        "manzil": 7,
        "page": 580,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5642,
        "text": "Sizi basbayağı bir sudan yaratmadık mı?",
        "numberInSurah": 20,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5643,
        "text": "Sonra onu savunması sağlam bir karar yerine yerleştirdik.",
        "numberInSurah": 21,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5644,
        "text": "Belli bir süreye kadar;",
        "numberInSurah": 22,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5645,
        "text": "İşte (buna) güç yetirdik. Demek ki, Biz ne güzel güç yetirenleriz.",
        "numberInSurah": 23,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5646,
        "text": "O gün, yalanlayanların vay haline.",
        "numberInSurah": 24,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5647,
        "text": "Biz yeryüzünü bir toplanma yeri kılmadık mı?",
        "numberInSurah": 25,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5648,
        "text": "Dirilere ve ölülere.",
        "numberInSurah": 26,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5649,
        "text": "Ve onda sabit yüksek dağlar var etmedik mi? Size tatlı bir su içirmedik mi?",
        "numberInSurah": 27,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5650,
        "text": "O gün, yalanlayanların vay haline.",
        "numberInSurah": 28,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5651,
        "text": "Kendisini yalanladığınız (azab)a gidin.",
        "numberInSurah": 29,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5652,
        "text": "Üç dala ayrılmış bir gölgeye gidin.",
        "numberInSurah": 30,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5653,
        "text": "Ne gölge altında barındırır, ne (yakıcı) alevden korur.",
        "numberInSurah": 31,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5654,
        "text": "Gerçekten o, sanki her biri saray olan bir kıvılcım saçar.",
        "numberInSurah": 32,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5655,
        "text": "Her biri, sanki sapsarı erkek deve sürüleri gibidir.",
        "numberInSurah": 33,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5656,
        "text": "O gün, yalanlayanların vay haline.",
        "numberInSurah": 34,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5657,
        "text": "Bu, onların konuşamayacakları bir gündür.",
        "numberInSurah": 35,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5658,
        "text": "Ve onlara özür beyan etmeleri için izin verilmez.",
        "numberInSurah": 36,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5659,
        "text": "O gün, yalanlayanların vay haline.",
        "numberInSurah": 37,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5660,
        "text": "Bu, hüküm günüdür; sizi ve öncekileri 'birarada topladık.'",
        "numberInSurah": 38,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5661,
        "text": "Şayet kurabileceğiniz hileli bir düzeniniz varsa, durmaksızın bana karşı kurun.",
        "numberInSurah": 39,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5662,
        "text": "O gün, yalanlayanların vay haline.",
        "numberInSurah": 40,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 516,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5663,
        "text": "Şüphesiz muttaki olanlar, gölgeliklerde ve pınar-başlarındadır;",
        "numberInSurah": 41,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5664,
        "text": "Ve canlarının çekip-arzu ettiği meyveler (arasındadırlar).",
        "numberInSurah": 42,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5665,
        "text": "Yaptıklarınıza karşılık olmak üzere, afiyetle yiyin ve için.",
        "numberInSurah": 43,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5666,
        "text": "Elbette Biz, 'iyi ve güzel' davrananları işte böyle ödüllendiririz.",
        "numberInSurah": 44,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5667,
        "text": "O gün, yalanlayanların vay haline.",
        "numberInSurah": 45,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5668,
        "text": "(Sizler de dünyada) Yiyin ve biraz yararlanın. Çünkü siz, suçlu-günahkar kimselersiniz.",
        "numberInSurah": 46,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5669,
        "text": "O gün, yalanlayanların vay haline.",
        "numberInSurah": 47,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5670,
        "text": "Onlara: \"Rüku edin\" denildiği zaman, rüku etmezler.",
        "numberInSurah": 48,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5671,
        "text": "O gün, yalanlayanların vay haline.",
        "numberInSurah": 49,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      },
      {
        "number": 5672,
        "text": "Artık onlar, bundan sonra hangi söze inanacaklar?",
        "numberInSurah": 50,
        "juz": 29,
        "manzil": 7,
        "page": 581,
        "ruku": 517,
        "hizbQuarter": 232,
        "sajda": false
      }
    ],
