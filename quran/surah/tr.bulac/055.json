{
  "code": 200,
  "status": "OK",
  "data": {
    "number": 55,
    "name": "سُورَةُ الرَّحۡمَٰن",
    "englishName": "Ar-Rahmaan",
    "englishNameTranslation": "The Beneficent",
    "revelationType": "Medinan",
    "numberOfAyahs": 78,
    "ayahs": [
      {
        "number": 4902,
        "text": "Rahman (olan Allah)",
        "numberInSurah": 1,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4903,
        "text": "Kur'an'ı öğretti.",
        "numberInSurah": 2,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4904,
        "text": "İnsanı yarattı.",
        "numberInSurah": 3,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4905,
        "text": "Ona beyanı öğretti.",
        "numberInSurah": 4,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4906,
        "text": "Güneş ve ay (belli) bir hesap iledir.",
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4907,
        "text": "Bitki ve ağaç (O'na) secde etmektedirler.",
        "numberInSurah": 6,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4908,
        "text": "Gökyüzü, Onu da yükseltti ve mizanı koydu.",
        "numberInSurah": 7,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4909,
        "text": "Sakın mizanda 'haksızlık ve taşkınlık yapmayın.'",
        "numberInSurah": 8,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4910,
        "text": "Tartıyı adaletle tutup-doğrultun ve tartıyı noksan tutmayın.",
        "numberInSurah": 9,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4911,
        "text": "Yere gelince, onu da (yaratılmış bütün) varlıklar için alçalttı-koydu.",
        "numberInSurah": 10,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4912,
        "text": "Onda meyveler ve salkımlı hurmalıklar var.",
        "numberInSurah": 11,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4913,
        "text": "Yapraklı taneler ve güzel kokulu bitkiler.",
        "numberInSurah": 12,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4914,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 13,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4915,
        "text": "İnsanı, ateşte pişmiş gibi kuru bir çamurdan yarattı.",
        "numberInSurah": 14,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4916,
        "text": "Cann'ı (cinni) da 'yalın-dumansız bir ateşten' yarattı.",
        "numberInSurah": 15,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4917,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 16,
        "juz": 27,
        "manzil": 7,
        "page": 531,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4918,
        "text": "O, iki doğunun da Rabbidir, iki batının da Rabbidir.",
        "numberInSurah": 17,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4919,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 18,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4920,
        "text": "Birbirleriyle kavuşmak üzere iki denizi salıverdi.",
        "numberInSurah": 19,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4921,
        "text": "İkisi arasında bir engel (berzah) vardır; birbirlerinin sınırını geçmezler.",
        "numberInSurah": 20,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4922,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 21,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4923,
        "text": "İkisinden de inci ve mercan çıkar.",
        "numberInSurah": 22,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4924,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 23,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4925,
        "text": "Denizde koca dağlar gibi yükselen gemiler O'nundur.",
        "numberInSurah": 24,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4926,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 25,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 466,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4927,
        "text": "(Yer) Üzerindeki herşey yok olucudur;",
        "numberInSurah": 26,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4928,
        "text": "Celal ve ikram sahibi olan Rabbinin yüzü (Kendisi) baki kalacaktır.",
        "numberInSurah": 27,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4929,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 28,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4930,
        "text": "Göklerde ve yerde olan ne varsa O'ndan ister. O, her gün bir iştedir.",
        "numberInSurah": 29,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4931,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 30,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4932,
        "text": "Ey (yeryüzüne yükletilmiş) iki ağırlık (olan ins ve cin), yakında (ahirette hesabınızı görmek üzere) sizin için de vakit bulacağız.",
        "numberInSurah": 31,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4933,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 32,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4934,
        "text": "Ey cin ve ins toplulukları, eğer göklerin ve yerin bucaklarından aşıp-geçmeye güç yetirebilirseniz, hemen aşın; ancak 'üstün bir güç (sultan)' olmaksızın aşamazsınız.",
        "numberInSurah": 33,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4935,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 34,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4936,
        "text": "İkinizin de üzerine ateşten yalın bir alev ve (bakır gibi erimiş) kıpkızıl bir duman salıverilir de 'kurtulup-başaramazsınız.'",
        "numberInSurah": 35,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4937,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 36,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4938,
        "text": "Sonra gök yarılıp yağ gibi erimiş olarak kıpkırmızı bir gül olduğu zaman;",
        "numberInSurah": 37,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4939,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 38,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4940,
        "text": "İşte o gün, ne insana, ne cinne günahından sorulmaz.",
        "numberInSurah": 39,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4941,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 40,
        "juz": 27,
        "manzil": 7,
        "page": 532,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4942,
        "text": "(Çünkü o gün) Suçlu-günahkarlar, simalarından tanınır da alınlarından ve ayaklarından yakalanırlar.",
        "numberInSurah": 41,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4943,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 42,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4944,
        "text": "İşte bu, suçlu-günahkarların kendisini yalanladıkları cehennemdir.",
        "numberInSurah": 43,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4945,
        "text": "Onlar, kendisiyle alabildiğine kaynar hale getirilmiş su arasında dönüp-dolaşırlar.",
        "numberInSurah": 44,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4946,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 45,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 467,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4947,
        "text": "Rabbin makamından korkan kimse için ise iki cennet vardır.",
        "numberInSurah": 46,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4948,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 47,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4949,
        "text": "Çeşit çeşit 'inceliklere ve güzelliklere' (veya her türden sık ağaçlara) sahiptirler.",
        "numberInSurah": 48,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4950,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 49,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4951,
        "text": "İkisinde de akmakta olan iki pınar vardır.",
        "numberInSurah": 50,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4952,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 51,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4953,
        "text": "İkisinde de her meyveden iki çift vardır.",
        "numberInSurah": 52,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4954,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 53,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4955,
        "text": "Astarları, ağır işlenmiş atlastan yataklar üzerinde yaslanırlar. İki cennetin de meyve-devşirmesi (ordakilere) yakın (kolay)dır.",
        "numberInSurah": 54,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4956,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 55,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4957,
        "text": "Orada bakışlarını yalnızca eşlerine çevirmiş kadınlar vardır ki, bunlardan önce kendilerine ne bir insan, ne bir cin dokunmuştur.",
        "numberInSurah": 56,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4958,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 57,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4959,
        "text": "Sanki onlar yakut ve mercan gibidirler.",
        "numberInSurah": 58,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4960,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 59,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4961,
        "text": "İhsanın karşılığı ihsandan başkası mıdır?",
        "numberInSurah": 60,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4962,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 61,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4963,
        "text": "Bu-ikisinin ötesinde iki cennet daha var.",
        "numberInSurah": 62,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4964,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 63,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4965,
        "text": "Alabildiğine yemyeşildirler.",
        "numberInSurah": 64,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4966,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 65,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4967,
        "text": "İçlerinde durmaksızın fışkırıp-akan iki pınar vardır.",
        "numberInSurah": 66,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4968,
        "text": "Şu halde Rabbinizin hangi nimetlerini yalanlayabilirsiniz?",
        "numberInSurah": 67,
        "juz": 27,
        "manzil": 7,
        "page": 533,
        "ruku": 468,
        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4969,
        "text": "İçlerinde (her türden) meyve, eşsiz-hurma ve eşsiz-nar vardır.",
        "numberInSurah": 68,
        "juz": 27,
        "manzil": 7,
        "page": 534,
        "ruku": 468,
